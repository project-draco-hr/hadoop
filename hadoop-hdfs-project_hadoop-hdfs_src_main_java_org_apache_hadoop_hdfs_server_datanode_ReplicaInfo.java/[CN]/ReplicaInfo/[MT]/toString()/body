{
  return getClass().getSimpleName() + ", " + super.toString()+ ", "+ getState()+ "\n  getNumBytes()     = "+ getNumBytes()+ "\n  getBytesOnDisk()  = "+ getBytesOnDisk()+ "\n  getVisibleLength()= "+ getVisibleLength()+ "\n  getVolume()       = "+ getVolume()+ "\n  getBlockFile()    = "+ getBlockFile();
}
