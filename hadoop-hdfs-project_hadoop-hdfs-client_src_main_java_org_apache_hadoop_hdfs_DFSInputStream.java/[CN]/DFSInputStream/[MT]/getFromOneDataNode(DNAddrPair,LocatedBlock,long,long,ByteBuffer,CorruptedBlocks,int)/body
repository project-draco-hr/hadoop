{
  final SpanId parentSpanId=Tracer.getCurrentSpanId();
  return new Callable<ByteBuffer>(){
    @Override public ByteBuffer call() throws Exception {
      try (TraceScope ignored=dfsClient.getTracer().newScope("hedgedRead" + hedgedReadId,parentSpanId)){
        actualGetFromOneDataNode(datanode,block,start,end,bb,corruptedBlocks);
        return bb;
      }
     }
  }
;
}
