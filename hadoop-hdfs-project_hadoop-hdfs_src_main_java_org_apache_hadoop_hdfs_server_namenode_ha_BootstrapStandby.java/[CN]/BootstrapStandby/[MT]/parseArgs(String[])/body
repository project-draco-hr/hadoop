{
  for (  String arg : args) {
    if ("-force".equals(arg)) {
      force=true;
    }
 else     if ("-nonInteractive".equals(arg)) {
      interactive=false;
    }
 else     if ("-skipSharedEditsCheck".equals(arg)) {
      skipSharedEditsCheck=true;
    }
 else {
      printUsage();
      throw new HadoopIllegalArgumentException("Illegal argument: " + arg);
    }
  }
}
