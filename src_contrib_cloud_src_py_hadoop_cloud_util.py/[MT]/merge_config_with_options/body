def merge_config_with_options(section_name, config, options):
    '\n  Merge configuration options with a dictionary of options.\n  Keys in the options dictionary take precedence.\n  '
    res = {}
    try:
        for (key, value) in config.items(section_name):
            if (value.find('\n') != (-1)):
                res[key] = value.split('\n')
            else:
                res[key] = value
    except ConfigParser.NoSectionError:
        pass
    for key in options:
        if (options[key] != None):
            res[key] = options[key]
    return res
