{
  Preconditions.checkArgument(targetPos >= 0,"Cannot seek to negative offset.");
  checkStream();
  try {
    boolean result=((Seekable)in).seekToNewSource(targetPos);
    resetStreamOffset(targetPos);
    return result;
  }
 catch (  ClassCastException e) {
    throw new UnsupportedOperationException("This stream does not support " + "seekToNewSource.");
  }
}
