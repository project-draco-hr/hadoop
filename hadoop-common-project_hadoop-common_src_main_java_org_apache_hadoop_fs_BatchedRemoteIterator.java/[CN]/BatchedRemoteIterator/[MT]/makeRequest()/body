{
  idx=0;
  entries=null;
  entries=makeRequest(prevKey,maxRepliesPerRequest);
  if (entries.size() > maxRepliesPerRequest) {
    throw new IOException("invalid number of replies returned: got " + entries.size() + ", expected "+ maxRepliesPerRequest+ " at most.");
  }
  if (entries.size() == 0) {
    entries=null;
  }
}
