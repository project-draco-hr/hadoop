{
  byte buf[]=new byte[4096];
  long bytesRemaining=count;
  int bytesRead;
  int bytesToRead;
  in.seek(start);
  while (true) {
    bytesToRead=(int)(bytesRemaining < buf.length ? bytesRemaining : buf.length);
    bytesRead=in.read(buf,0,bytesToRead);
    if (bytesRead == -1) {
      break;
    }
    os.write(buf,0,bytesRead);
    bytesRemaining-=bytesRead;
    if (bytesRemaining <= 0) {
      break;
    }
  }
}
