{
  Set<NodeId> nodes=getNodesOnWhichAttemptRan(app);
  for (  NodeId nodeId : nodes) {
    app.dispatcher.getEventHandler().handle(new RMNodeCleanAppEvent(nodeId,app.applicationId));
  }
  app.finishTime=System.currentTimeMillis();
  app.dispatcher.getEventHandler().handle(new RMAppManagerEvent(app.applicationId,RMAppManagerEventType.APP_COMPLETED));
}
