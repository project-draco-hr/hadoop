{
  submitApplicationAttempt();
  applicationAttempt.handle(new RMAppAttemptEvent(applicationAttempt.getAppAttemptId(),RMAppAttemptEventType.APP_ACCEPTED));
  if (unmanagedAM) {
    assertEquals(RMAppAttemptState.LAUNCHED_UNMANAGED_SAVING,applicationAttempt.getAppAttemptState());
    applicationAttempt.handle(new RMAppAttemptNewSavedEvent(applicationAttempt.getAppAttemptId(),null));
  }
  testAppAttemptScheduledState();
}
