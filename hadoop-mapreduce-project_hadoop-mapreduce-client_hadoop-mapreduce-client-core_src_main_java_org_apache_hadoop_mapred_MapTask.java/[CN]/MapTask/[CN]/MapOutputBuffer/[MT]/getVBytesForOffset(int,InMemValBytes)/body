{
  final int nextindex=kvoff == kvend ? bufend : kvmeta.get((kvoff - NMETA + kvmeta.capacity() + KEYSTART) % kvmeta.capacity());
  int vallen=(nextindex >= kvmeta.get(kvoff + VALSTART)) ? nextindex - kvmeta.get(kvoff + VALSTART) : (bufvoid - kvmeta.get(kvoff + VALSTART)) + nextindex;
  vbytes.reset(kvbuffer,kvmeta.get(kvoff + VALSTART),vallen);
}
