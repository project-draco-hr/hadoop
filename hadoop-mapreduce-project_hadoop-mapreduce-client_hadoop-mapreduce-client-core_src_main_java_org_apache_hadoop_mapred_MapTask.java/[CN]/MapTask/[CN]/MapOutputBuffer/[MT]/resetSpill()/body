{
  final int e=equator;
  bufstart=bufend=e;
  final int aligned=e - (e % METASIZE);
  kvstart=kvend=(int)(((long)aligned - METASIZE + kvbuffer.length) % kvbuffer.length) / 4;
  LOG.info("(RESET) equator " + e + " kv "+ kvstart+ "("+ (kvstart * 4)+ ")"+ " kvi "+ kvindex+ "("+ (kvindex * 4)+ ")");
}
