{
  return new Callable<ByteBuffer>(){
    @Override public ByteBuffer call() throws Exception {
      try {
        byte[] buf=bb.array();
        int offset=bb.position();
        actualGetFromOneDataNode(datanode,block,start,end,buf,offset,corruptedBlockMap);
        return bb;
      }
  finally {
        latch.countDown();
      }
    }
  }
;
}
