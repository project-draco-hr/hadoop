{
  this.limit=limit;
  this.flowRunScanner=internalScanner;
  if (internalScanner instanceof RegionScanner) {
    this.regionScanner=(RegionScanner)internalScanner;
  }
  this.action=action;
  if (env == null) {
    this.appFinalValueRetentionThreshold=YarnConfiguration.DEFAULT_APP_FINAL_VALUE_RETENTION_THRESHOLD;
    this.region=null;
  }
 else {
    this.region=env.getRegion();
    Configuration hbaseConf=env.getConfiguration();
    this.appFinalValueRetentionThreshold=hbaseConf.getLong(YarnConfiguration.APP_FINAL_VALUE_RETENTION_THRESHOLD,YarnConfiguration.DEFAULT_APP_FINAL_VALUE_RETENTION_THRESHOLD);
  }
}
