{
  if ((currentColumnCells == null) || (currentColumnCells.size() == 0)) {
    return 0;
  }
  if (currentAggOp == null) {
    cells.addAll(currentColumnCells);
    return currentColumnCells.size();
  }
switch (currentAggOp) {
case MIN:
case MAX:
    cells.addAll(currentColumnCells);
  return currentColumnCells.size();
case SUM:
case SUM_FINAL:
Cell sumCell=processSummation(currentColumnCells);
cells.add(sumCell);
return 1;
default :
cells.addAll(currentColumnCells);
return currentColumnCells.size();
}
}
