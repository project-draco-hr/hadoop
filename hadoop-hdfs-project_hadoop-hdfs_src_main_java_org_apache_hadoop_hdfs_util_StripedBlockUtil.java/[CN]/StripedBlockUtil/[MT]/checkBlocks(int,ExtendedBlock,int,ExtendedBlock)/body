{
  if (!blocki.getBlockPoolId().equals(blockj.getBlockPoolId())) {
    throw new IOException("Block pool IDs mismatched: block" + j + "="+ blockj+ ", block"+ i+ "="+ blocki);
  }
  if (blocki.getBlockId() - i != blockj.getBlockId() - j) {
    throw new IOException("Block IDs mismatched: block" + j + "="+ blockj+ ", block"+ i+ "="+ blocki);
  }
  if (blocki.getGenerationStamp() != blockj.getGenerationStamp()) {
    throw new IOException("Generation stamps mismatched: block" + j + "="+ blockj+ ", block"+ i+ "="+ blocki);
  }
}
