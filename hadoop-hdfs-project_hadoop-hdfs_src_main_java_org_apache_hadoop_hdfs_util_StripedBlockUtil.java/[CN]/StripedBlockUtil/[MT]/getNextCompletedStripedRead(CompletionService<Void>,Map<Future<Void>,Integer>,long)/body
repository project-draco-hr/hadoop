{
  Preconditions.checkArgument(!futures.isEmpty());
  Preconditions.checkArgument(threshold > 0);
  Future<Void> future=null;
  try {
    future=readService.poll(threshold,TimeUnit.MILLISECONDS);
    if (future != null) {
      future.get();
      return new StripedReadResult(futures.remove(future),StripedReadResult.SUCCESSFUL);
    }
 else {
      return new StripedReadResult(StripedReadResult.TIMEOUT);
    }
  }
 catch (  ExecutionException e) {
    return new StripedReadResult(futures.remove(future),StripedReadResult.FAILED);
  }
catch (  CancellationException e) {
    return new StripedReadResult(futures.remove(future),StripedReadResult.CANCELLED);
  }
}
