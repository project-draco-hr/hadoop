{
  NMContext context=new NMContext(new NMContainerTokenSecretManager(conf),new NMTokenSecretManagerInNM(),null,new ApplicationACLsManager(conf),stateStore,false){
    public int getHttpPort(){
      return HTTP_PORT;
    }
  }
;
  MasterKey masterKey=new MasterKeyPBImpl();
  masterKey.setKeyId(123);
  masterKey.setBytes(ByteBuffer.wrap(new byte[]{new Integer(123).byteValue()}));
  context.getContainerTokenSecretManager().setMasterKey(masterKey);
  context.getNMTokenSecretManager().setMasterKey(masterKey);
  return context;
}
