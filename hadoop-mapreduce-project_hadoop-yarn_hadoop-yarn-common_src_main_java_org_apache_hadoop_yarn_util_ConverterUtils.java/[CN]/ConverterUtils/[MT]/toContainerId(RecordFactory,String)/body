{
  Iterator<String> it=_split(containerIdStr).iterator();
  it.next();
  ApplicationAttemptId appAttemptID=toApplicationAttemptId(recordFactory,it);
  ContainerId containerId=recordFactory.newRecordInstance(ContainerId.class);
  containerId.setApplicationAttemptId(appAttemptID);
  containerId.setId(Integer.parseInt(it.next()));
  return containerId;
}
