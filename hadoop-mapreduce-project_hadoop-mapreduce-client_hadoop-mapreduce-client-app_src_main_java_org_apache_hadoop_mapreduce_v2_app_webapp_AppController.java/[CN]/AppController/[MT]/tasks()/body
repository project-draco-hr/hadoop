{
  try {
    requireJob();
  }
 catch (  Exception e) {
    renderText(e.getMessage());
    return;
  }
  if (app.getJob() != null) {
    try {
      String tt=$(TASK_TYPE);
      tt=tt.isEmpty() ? "All" : StringUtils.capitalize(MRApps.taskType(tt).toString().toLowerCase(Locale.US));
      setTitle(join(tt," Tasks for ",$(JOB_ID)));
    }
 catch (    Exception e) {
      badRequest(e.getMessage());
    }
  }
  render(tasksPage());
}
