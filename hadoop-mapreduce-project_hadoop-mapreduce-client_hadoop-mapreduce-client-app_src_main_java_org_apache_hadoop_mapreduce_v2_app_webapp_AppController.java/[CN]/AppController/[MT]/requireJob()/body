{
  try {
    if ($(JOB_ID).isEmpty()) {
      throw new RuntimeException("missing job ID");
    }
    JobId jobID=MRApps.toJobID($(JOB_ID));
    app.setJob(app.context.getJob(jobID));
    if (app.getJob() == null) {
      notFound($(JOB_ID));
    }
    Job job=app.context.getJob(jobID);
    checkAccess(job);
  }
 catch (  Exception e) {
    badRequest(e.getMessage() == null ? e.getClass().getName() : e.getMessage());
  }
}
