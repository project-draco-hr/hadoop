{
  Collection<JobInProgress> jips=mgr.getJobQueue(queueName).getWaitingJobs();
  for (  JobInProgress jip : jips) {
    if (jip.getStatus().getRunState() == JobStatus.RUNNING) {
      JobStatusChangeEvent evt=new JobStatusChangeEvent(jip,EventType.RUN_STATE_CHANGED,jip.getStatus());
      mgr.jobUpdated(evt);
    }
  }
}
