{
  JobQueuesManager mgr=scheduler.jobQueuesManager;
  FakeJobInProgress job=taskTrackerManager.submitJobAndInit(JobStatus.RUNNING,1,1,"default","u1");
  assertTrue("Running jobs list does not contain submitted job",mgr.getJobQueue("default").getRunningJobs().contains(job));
  taskTrackerManager.finalizeJob(job,JobStatus.KILLED);
  assertFalse("Running jobs list does not contain submitted job",mgr.getJobQueue("default").getRunningJobs().contains(job));
}
