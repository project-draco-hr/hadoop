{
  assertTrue("Observed and expected queues are not of same length.",expectedOrder.length == observedOrder.length);
  int i=0;
  for (  String expectedQ : expectedOrder) {
    assertTrue("Observed and expected queues are not in the same order. " + "Differ at index " + i + ". Got "+ observedOrder[i]+ " instead of "+ expectedQ,expectedQ.equals(observedOrder[i]));
    i++;
  }
}
