{
  checkOpen();
  CreateFlag.validate(flag);
  DFSOutputStream result=primitiveAppend(src,flag,buffersize,progress);
  if (result == null) {
    result=new DFSOutputStream(this,src,absPermission,flag,createParent,replication,blockSize,progress,buffersize,bytesPerChecksum);
  }
  leaserenewer.put(src,result,this);
  return result;
}
