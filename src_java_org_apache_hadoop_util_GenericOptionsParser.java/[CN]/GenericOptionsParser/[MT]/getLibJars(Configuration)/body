{
  String jars=conf.get("tmpjars");
  if (jars == null) {
    return null;
  }
  String[] files=jars.split(",");
  URL[] cp=new URL[files.length];
  for (int i=0; i < cp.length; i++) {
    Path tmp=new Path(files[i]);
    cp[i]=FileSystem.getLocal(conf).pathToFile(tmp).toURI().toURL();
  }
  return cp;
}
