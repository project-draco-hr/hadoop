{
  Method init=null;
  try {
    init=RecordBench.class.getDeclaredMethod("init" + toCamelCase(type) + "s",new Class[]{Record[].class});
  }
 catch (  NoSuchMethodException ex) {
    throw new RuntimeException(ex);
  }
  Record[] records=new Record[numRecords];
  times.init=System.nanoTime();
  try {
    init.invoke(null,new Object[]{records});
  }
 catch (  Exception ex) {
    throw new RuntimeException(ex);
  }
  times.init=System.nanoTime() - times.init;
  return records;
}
