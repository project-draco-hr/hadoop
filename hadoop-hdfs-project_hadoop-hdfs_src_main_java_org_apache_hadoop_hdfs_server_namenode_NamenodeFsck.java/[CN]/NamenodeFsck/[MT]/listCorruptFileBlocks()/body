{
  final List<String> corrputBlocksFiles=namenode.getNamesystem().listCorruptFileBlocksWithSnapshot(path,snapshottableDirs,currentCookie);
  int numCorruptFiles=corrputBlocksFiles.size();
  String filler;
  if (numCorruptFiles > 0) {
    filler=Integer.toString(numCorruptFiles);
  }
 else   if (currentCookie[0].equals("0")) {
    filler="no";
  }
 else {
    filler="no more";
  }
  out.println("Cookie:\t" + currentCookie[0]);
  for (  String s : corrputBlocksFiles) {
    out.println(s);
  }
  out.println("\n\nThe filesystem under path '" + path + "' has "+ filler+ " CORRUPT files");
  out.println();
}
