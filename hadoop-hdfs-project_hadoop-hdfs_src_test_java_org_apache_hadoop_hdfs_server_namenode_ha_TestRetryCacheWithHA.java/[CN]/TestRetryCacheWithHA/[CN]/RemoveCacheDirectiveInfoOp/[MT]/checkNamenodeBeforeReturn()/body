{
  for (int i=0; i < CHECKTIMES; i++) {
    RemoteIterator<CacheDirectiveEntry> iter=dfs.listCacheDirectives(new CacheDirectiveInfo.Builder().setPool(directive.getPool()).setPath(directive.getPath()).build());
    if (!iter.hasNext()) {
      return true;
    }
    Thread.sleep(1000);
  }
  return false;
}
