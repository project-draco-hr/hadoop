{
synchronized (block) {
synchronized (movedBlocks) {
      if (isGoodBlockCandidate(source,target,block)) {
        this.block=block;
        if (chooseProxySource()) {
          movedBlocks.add(block);
          if (LOG.isDebugEnabled()) {
            LOG.debug("Decided to move " + this);
          }
          return true;
        }
      }
    }
  }
  return false;
}
