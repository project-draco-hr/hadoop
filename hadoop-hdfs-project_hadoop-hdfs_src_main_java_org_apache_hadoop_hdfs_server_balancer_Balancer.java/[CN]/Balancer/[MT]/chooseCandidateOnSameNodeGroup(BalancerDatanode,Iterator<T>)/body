{
  if (dn.isMoveQuotaFull()) {
    for (; candidates.hasNext(); ) {
      final T c=candidates.next();
      if (!c.isMoveQuotaFull()) {
        candidates.remove();
        continue;
      }
      if (cluster.isOnSameNodeGroup(dn.getDatanode(),c.getDatanode())) {
        return c;
      }
    }
  }
  return null;
}
