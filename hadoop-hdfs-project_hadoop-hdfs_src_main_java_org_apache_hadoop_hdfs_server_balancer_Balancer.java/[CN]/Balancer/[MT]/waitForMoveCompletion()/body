{
  boolean shouldWait;
  do {
    shouldWait=false;
    for (    BalancerDatanode.StorageGroup target : targets) {
      if (!target.getBalancerDatanode().isPendingQEmpty()) {
        shouldWait=true;
        break;
      }
    }
    if (shouldWait) {
      try {
        Thread.sleep(blockMoveWaitTime);
      }
 catch (      InterruptedException ignored) {
      }
    }
  }
 while (shouldWait);
}
