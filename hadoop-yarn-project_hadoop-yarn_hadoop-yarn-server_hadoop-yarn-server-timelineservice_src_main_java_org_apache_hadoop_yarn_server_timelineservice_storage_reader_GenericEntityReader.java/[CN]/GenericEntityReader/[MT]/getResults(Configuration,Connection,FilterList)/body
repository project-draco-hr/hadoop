{
  Scan scan=new Scan();
  TimelineReaderContext context=getContext();
  scan.setRowPrefixFilter(EntityRowKey.getRowKeyPrefix(context.getClusterId(),context.getUserId(),context.getFlowName(),context.getFlowRunId(),context.getAppId(),context.getEntityType()));
  scan.setMaxVersions(Integer.MAX_VALUE);
  if (filterList != null && !filterList.getFilters().isEmpty()) {
    scan.setFilter(filterList);
  }
  return table.getResultScanner(hbaseConf,conn,scan);
}
