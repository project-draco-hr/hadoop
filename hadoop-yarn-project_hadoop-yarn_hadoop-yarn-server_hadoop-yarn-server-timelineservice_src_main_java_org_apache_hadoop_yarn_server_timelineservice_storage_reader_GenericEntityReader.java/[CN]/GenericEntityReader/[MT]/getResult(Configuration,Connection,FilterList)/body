{
  TimelineReaderContext context=getContext();
  byte[] rowKey=EntityRowKey.getRowKey(context.getClusterId(),context.getUserId(),context.getFlowName(),context.getFlowRunId(),context.getAppId(),context.getEntityType(),context.getEntityId());
  Get get=new Get(rowKey);
  get.setMaxVersions(Integer.MAX_VALUE);
  if (filterList != null && !filterList.getFilters().isEmpty()) {
    get.setFilter(filterList);
  }
  return table.getResult(hbaseConf,conn,get);
}
