{
  if (canAssign(priority,node,NodeType.OFF_SWITCH,reservedContainer)) {
    return assignContainer(clusterResource,node,priority,offSwitchResourceRequest,NodeType.OFF_SWITCH,reservedContainer,allocatedContainer,schedulingMode,currentResoureLimits);
  }
  return new CSAssignment(Resources.none(),NodeType.OFF_SWITCH);
}
