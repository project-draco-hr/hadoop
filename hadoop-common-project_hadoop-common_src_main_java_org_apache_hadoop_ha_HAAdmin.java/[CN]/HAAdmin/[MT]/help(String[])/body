{
  if (argv.length != 2) {
    printUsage(errOut,"-help");
    return -1;
  }
  String cmd=argv[1];
  if (!cmd.startsWith("-")) {
    cmd="-" + cmd;
  }
  UsageInfo usageInfo=USAGE.get(cmd);
  if (usageInfo == null) {
    errOut.println(cmd + ": Unknown command");
    printUsage(errOut);
    return -1;
  }
  errOut.println(cmd + " [" + usageInfo.args+ "]: "+ usageInfo.help);
  return 1;
}
