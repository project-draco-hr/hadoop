{
  if (conf instanceof JobConf) {
    this.conf=(JobConf)conf;
  }
 else {
    this.conf=new JobConf(conf);
  }
  this.jobId=jobId;
  this.credentials=this.conf.getCredentials();
  try {
    this.ugi=UserGroupInformation.getCurrentUser();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
