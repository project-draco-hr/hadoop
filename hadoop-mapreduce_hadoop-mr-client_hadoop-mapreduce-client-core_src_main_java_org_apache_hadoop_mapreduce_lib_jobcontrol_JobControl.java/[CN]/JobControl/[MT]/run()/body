{
  this.runnerState=ThreadState.RUNNING;
  while (true) {
    while (this.runnerState == ThreadState.SUSPENDED) {
      try {
        Thread.sleep(5000);
      }
 catch (      Exception e) {
      }
    }
    try {
      checkRunningJobs();
      checkWaitingJobs();
      startReadyJobs();
    }
 catch (    Exception e) {
      this.runnerState=ThreadState.STOPPED;
    }
    if (this.runnerState != ThreadState.RUNNING && this.runnerState != ThreadState.SUSPENDED) {
      break;
    }
    try {
      Thread.sleep(5000);
    }
 catch (    Exception e) {
    }
    if (this.runnerState != ThreadState.RUNNING && this.runnerState != ThreadState.SUSPENDED) {
      break;
    }
  }
  this.runnerState=ThreadState.STOPPED;
}
