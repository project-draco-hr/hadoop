{
  String nsIds=conf.get(DFSConfigKeys.DFS_NAMESERVICES);
  if (nsIds == null) {
    conf.set(DFSConfigKeys.DFS_NAMESERVICES,logicalName);
  }
 else {
    conf.set(DFSConfigKeys.DFS_NAMESERVICES,nsIds + "," + logicalName);
  }
  conf.set(DFSUtil.addKeySuffixes(HdfsClientConfigKeys.DFS_HA_NAMENODES_KEY_PREFIX,logicalName),"nn1,nn2");
  conf.set(HdfsClientConfigKeys.Failover.PROXY_PROVIDER_KEY_PREFIX + "." + logicalName,ConfiguredFailoverProxyProvider.class.getName());
  conf.setInt(DFSConfigKeys.DFS_REPLICATION_KEY,1);
}
