{
  for (  String arg : args) {
    System.out.println("Working on " + arg);
    LineReader reader=makeStream(unquote(arg));
    Text line=new Text();
    int size=reader.readLine(line);
    while (size > 0) {
      System.out.println("Got: " + line.toString());
      size=reader.readLine(line);
    }
    reader.close();
  }
}
