{
  ReservationDefinition rDef=ReservationSystemTestUtil.createSimpleReservationDefinition(start,start + f.length * step,f.length * step);
  assertTrue(plan.toString(),plan.addReservation(new InMemoryReservationAllocation(ReservationSystemTestUtil.getNewReservationId(),rDef,"user_fixed","dedicated",start,start + f.length * step,ReservationSystemTestUtil.generateAllocation(start,step,f),res,minAlloc)));
}
