{
  int numJobsInScenario=initializeScenario2();
  ReservationDefinition rr1=createReservationDefinition(10L,15 * step,new ReservationRequest[]{ReservationRequest.newInstance(Resource.newInstance(1024,1),20,20,step),ReservationRequest.newInstance(Resource.newInstance(1024,1),20,20,step)},ReservationRequestInterpreter.R_ORDER_NO_GAP,"u1");
  try {
    ReservationId reservationID=ReservationSystemTestUtil.getNewReservationId();
    agent.createReservation(reservationID,"u1",plan,rr1);
    fail();
  }
 catch (  PlanningException e) {
  }
  assertTrue("Agent-based allocation should have failed",plan.getAllReservations().size() == numJobsInScenario);
}
