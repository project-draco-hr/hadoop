{
  modCount++;
  size--;
  E entry=node.removeEntryRight();
  if (node.isEmpty()) {
    deleteNode(node);
  }
 else   if (node.prev != null && (Node.NODE_SIZE - 1 - node.prev.rightIndex) >= node.size) {
    node.prev.addEntriesRight(node);
    deleteNode(node);
  }
 else   if (node.next != null && node.next.leftIndex >= node.size) {
    node.next.addEntriesLeft(node);
    deleteNode(node);
  }
 else   if (node.next != null && node.next.size < (Node.NODE_SIZE - 1 - node.rightIndex)) {
    node.addEntriesRight(node.next);
    deleteNode(node.next);
  }
  return entry;
}
