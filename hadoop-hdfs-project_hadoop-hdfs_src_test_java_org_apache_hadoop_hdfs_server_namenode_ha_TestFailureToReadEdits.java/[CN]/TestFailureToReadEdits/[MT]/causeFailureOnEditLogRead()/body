{
  FSEditLog spyEditLog=spy(nn1.getNamesystem().getEditLogTailer().getEditLog());
  LimitedEditLogAnswer answer=new LimitedEditLogAnswer();
  doAnswer(answer).when(spyEditLog).selectInputStreams(anyLong(),anyLong(),(MetaRecoveryContext)anyObject(),anyBoolean());
  nn1.getNamesystem().getEditLogTailer().setEditLog(spyEditLog);
  return answer;
}
