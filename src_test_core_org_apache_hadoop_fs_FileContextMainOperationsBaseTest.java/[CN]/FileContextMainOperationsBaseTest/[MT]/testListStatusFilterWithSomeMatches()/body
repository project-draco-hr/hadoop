{
  Path[] testDirs={getTestRootPath(TEST_DIR_AAA),getTestRootPath(TEST_DIR_AXA),getTestRootPath(TEST_DIR_AXX),getTestRootPath(TEST_DIR_AAA2)};
  if (fc.exists(testDirs[0]) == false) {
    for (    Path path : testDirs) {
      fc.mkdir(path,FsPermission.getDefault(),true);
    }
  }
  FileStatus[] filteredPaths=fc.util().listStatus(getTestRootPath("test/hadoop"),TEST_X_FILTER);
  Assert.assertEquals(2,filteredPaths.length);
  Assert.assertTrue(getTestRootPath(TEST_DIR_AXA).equals(filteredPaths[0].getPath()) || getTestRootPath(TEST_DIR_AXA).equals(filteredPaths[1].getPath()));
  Assert.assertTrue(getTestRootPath(TEST_DIR_AXX).equals(filteredPaths[0].getPath()) || getTestRootPath(TEST_DIR_AXX).equals(filteredPaths[1].getPath()));
}
