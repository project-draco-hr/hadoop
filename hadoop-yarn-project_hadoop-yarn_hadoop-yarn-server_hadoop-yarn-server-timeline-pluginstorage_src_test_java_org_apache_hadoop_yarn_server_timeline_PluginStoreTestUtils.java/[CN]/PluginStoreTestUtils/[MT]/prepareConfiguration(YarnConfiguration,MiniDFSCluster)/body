{
  conf.set(CommonConfigurationKeysPublic.FS_DEFAULT_NAME_KEY,dfsCluster.getURI().toString());
  conf.setFloat(YarnConfiguration.TIMELINE_SERVICE_VERSION,1.5f);
  conf.setLong(YarnConfiguration.TIMELINE_SERVICE_ENTITYGROUP_FS_STORE_SCAN_INTERVAL_SECONDS,1);
  conf.set(YarnConfiguration.TIMELINE_SERVICE_STORE,EntityGroupFSTimelineStore.class.getName());
  return conf;
}
