{
  allNodesLabelUpdateInterval=conf.getLong(YarnConfiguration.RM_NODE_LABELS_PROVIDER_FETCH_INTERVAL_MS,YarnConfiguration.DEFAULT_RM_NODE_LABELS_PROVIDER_FETCH_INTERVAL_MS);
  rmNodeLabelsMappingProvider=createRMNodeLabelsMappingProvider(conf);
  addService(rmNodeLabelsMappingProvider);
  super.serviceInit(conf);
}
