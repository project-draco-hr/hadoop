{
  return (dominant) ? Math.max((float)resource.getMemorySize() / clusterResource.getMemorySize(),(float)resource.getVirtualCores() / clusterResource.getVirtualCores()) : Math.min((float)resource.getMemorySize() / clusterResource.getMemorySize(),(float)resource.getVirtualCores() / clusterResource.getVirtualCores());
}
