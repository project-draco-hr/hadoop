{
  if (!protocol.equals(RPC.getProtocolName(JournalWireProtocol.class))) {
    throw new IOException("Namenode Serverside implements " + RPC.getProtocolName(JournalWireProtocol.class) + ". The following requested protocol is unknown: "+ protocol);
  }
  return ProtocolSignature.getProtocolSignature(clientMethodsHash,JournalWireProtocol.versionID,JournalWireProtocol.class);
}
