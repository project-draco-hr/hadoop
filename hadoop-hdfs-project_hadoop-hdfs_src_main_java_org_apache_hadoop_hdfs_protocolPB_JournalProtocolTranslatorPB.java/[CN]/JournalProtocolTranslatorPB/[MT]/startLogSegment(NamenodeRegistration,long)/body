{
  StartLogSegmentRequestProto req=StartLogSegmentRequestProto.newBuilder().setJournalInfo(PBHelper.convertToJournalInfo(registration)).setTxid(txid).build();
  try {
    rpcProxy.startLogSegment(NULL_CONTROLLER,req);
  }
 catch (  ServiceException e) {
