{
  JournalRequestProto req=JournalRequestProto.newBuilder().setJournalInfo(PBHelper.convert(journalInfo)).setEpoch(epoch).setFirstTxnId(firstTxnId).setNumTxns(numTxns).setRecords(PBHelper.getByteString(records)).build();
  try {
    rpcProxy.journal(NULL_CONTROLLER,req);
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
