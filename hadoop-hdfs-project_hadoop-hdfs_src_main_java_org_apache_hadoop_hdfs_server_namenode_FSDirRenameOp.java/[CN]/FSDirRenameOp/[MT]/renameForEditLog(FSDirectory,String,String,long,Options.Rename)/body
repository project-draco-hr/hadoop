{
  BlocksMapUpdateInfo collectedBlocks=new BlocksMapUpdateInfo();
  final INodesInPath srcIIP=fsd.getINodesInPath4Write(src,false);
  final INodesInPath dstIIP=fsd.getINodesInPath4Write(dst,false);
  unprotectedRenameTo(fsd,src,dst,srcIIP,dstIIP,timestamp,collectedBlocks,options);
  if (!collectedBlocks.getToDeleteList().isEmpty()) {
    fsd.getFSNamesystem().getBlockManager().removeBlocksAndUpdateSafemodeTotal(collectedBlocks);
  }
}
