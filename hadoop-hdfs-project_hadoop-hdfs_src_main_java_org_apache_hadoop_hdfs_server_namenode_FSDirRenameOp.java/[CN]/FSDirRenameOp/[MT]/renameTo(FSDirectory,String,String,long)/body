{
  if (NameNode.stateChangeLog.isDebugEnabled()) {
    NameNode.stateChangeLog.debug("DIR* FSDirectory.renameTo: " + src + " to "+ dst);
  }
  boolean stat=false;
  fsd.writeLock();
  try {
    stat=unprotectedRenameTo(fsd,src,dst,mtime);
  }
  finally {
    fsd.writeUnlock();
  }
  return stat;
}
