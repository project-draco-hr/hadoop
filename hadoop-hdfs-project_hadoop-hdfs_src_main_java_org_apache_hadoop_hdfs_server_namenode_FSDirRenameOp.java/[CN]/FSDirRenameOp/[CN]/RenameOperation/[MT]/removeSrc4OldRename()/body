{
  final long removedSrc=fsd.removeLastINode(srcIIP);
  if (removedSrc == -1) {
    NameNode.stateChangeLog.warn("DIR* FSDirRenameOp.unprotectedRenameTo: " + "failed to rename " + src + " to "+ dst+ " because the source"+ " can not be removed");
    return false;
  }
 else {
    fsd.updateCountForDelete(srcChild,srcIIP);
    srcIIP=INodesInPath.replace(srcIIP,srcIIP.length() - 1,null);
    return true;
  }
}
