{
  try {
    byte[] data=db.get(bytes(TIMELINE_STORE_VERSION_KEY));
    if (data == null || data.length == 0) {
      return getCurrentVersion();
    }
    Version version=new VersionPBImpl(VersionProto.parseFrom(data));
    return version;
  }
 catch (  DBException e) {
    throw new IOException(e);
  }
}
