{
  ApplicationReport report=null;
  try {
    report=client.getApplicationReport(id);
  }
 catch (  ApplicationNotFoundException e) {
    return false;
  }
catch (  IOException e) {
    throw new YarnException(e);
  }
  if (report == null) {
    return false;
  }
  return ACTIVE_STATES.contains(report.getYarnApplicationState());
}
