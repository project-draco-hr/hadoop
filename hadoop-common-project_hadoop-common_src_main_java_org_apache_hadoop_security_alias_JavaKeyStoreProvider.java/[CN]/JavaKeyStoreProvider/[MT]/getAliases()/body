{
  readLock.lock();
  try {
    ArrayList<String> list=new ArrayList<String>();
    String alias=null;
    try {
      Enumeration<String> e=keyStore.aliases();
      while (e.hasMoreElements()) {
        alias=e.nextElement();
        list.add(alias);
      }
    }
 catch (    KeyStoreException e) {
      throw new IOException("Can't get alias " + alias + " from "+ path,e);
    }
    return list;
  }
  finally {
    readLock.unlock();
  }
}
