{
  long startPos=reader.getReader().bytesRead;
  boolean hasNext=reader.nextRawKey();
  long endPos=reader.getReader().bytesRead;
  totalBytesProcessed+=endPos - startPos;
  mergeProgress.set(totalBytesProcessed * progPerByte);
  if (hasNext) {
    adjustTop();
  }
 else {
    pop();
    reader.close();
  }
}
