{
  String[] args=new String[5];
  args[0]="--language";
  args[1]=this.language;
  args[2]="--destdir";
  args[3]=this.dest.getPath();
  args[4]=file.getPath();
  int retVal=Rcc.driver(args);
  if (retVal != 0 && failOnError) {
    throw new BuildException("Hadoop record compiler returned error code " + retVal);
  }
}
