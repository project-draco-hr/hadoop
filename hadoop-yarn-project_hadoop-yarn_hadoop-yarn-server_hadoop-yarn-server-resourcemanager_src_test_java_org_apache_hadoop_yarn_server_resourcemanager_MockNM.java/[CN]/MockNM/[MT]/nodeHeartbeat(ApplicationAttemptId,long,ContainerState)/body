{
  HashMap<ApplicationId,List<ContainerStatus>> nodeUpdate=new HashMap<ApplicationId,List<ContainerStatus>>(1);
  ContainerStatus containerStatus=BuilderUtils.newContainerStatus(BuilderUtils.newContainerId(attemptId,containerId),containerState,"Success",0);
  ArrayList<ContainerStatus> containerStatusList=new ArrayList<ContainerStatus>(1);
  containerStatusList.add(containerStatus);
  Log.info("ContainerStatus: " + containerStatus);
  nodeUpdate.put(attemptId.getApplicationId(),containerStatusList);
  return nodeHeartbeat(nodeUpdate,true);
}
