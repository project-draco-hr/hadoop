{
  if (UserGroupInformation.isSecurityEnabled()) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Number of TokenIdentifiers in the UGI from RPC: " + remoteUgi.getTokenIdentifiers().size());
    }
    return selectContainerTokenIdentifier(remoteUgi);
  }
 else {
    try {
      return BuilderUtils.newContainerTokenIdentifier(container.getContainerToken());
    }
 catch (    IOException e) {
      throw RPCUtil.getRemoteException(e);
    }
  }
}
