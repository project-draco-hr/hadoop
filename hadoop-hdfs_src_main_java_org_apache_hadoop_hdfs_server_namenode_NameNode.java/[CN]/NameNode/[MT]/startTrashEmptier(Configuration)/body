{
  long trashInterval=conf.getLong(CommonConfigurationKeys.FS_TRASH_INTERVAL_KEY,CommonConfigurationKeys.FS_TRASH_INTERVAL_DEFAULT);
  if (trashInterval == 0)   return;
  this.emptier=new Thread(new Trash(conf).getEmptier(),"Trash Emptier");
  this.emptier.setDaemon(true);
  this.emptier.start();
}
