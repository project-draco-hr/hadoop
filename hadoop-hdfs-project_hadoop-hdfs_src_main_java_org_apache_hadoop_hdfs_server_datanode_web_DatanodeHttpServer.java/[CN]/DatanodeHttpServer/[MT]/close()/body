{
  bossGroup.shutdownGracefully();
  workerGroup.shutdownGracefully();
  if (sslFactory != null) {
    sslFactory.destroy();
  }
  if (externalHttpChannel != null) {
    externalHttpChannel.close();
  }
  try {
    infoServer.stop();
  }
 catch (  Exception e) {
    throw new IOException(e);
  }
}
