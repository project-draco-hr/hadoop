{
  return executor.submit(new Callable<RemoteEditLogManifest>(){
    @Override public RemoteEditLogManifest call() throws IOException {
      GetEditLogManifestResponseProto ret=getProxy().getEditLogManifest(journalId,fromTxnId,forReading,inProgressOk);
      httpPort=ret.getHttpPort();
      return PBHelper.convert(ret.getManifest());
    }
  }
);
}
