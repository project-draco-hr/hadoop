{
  final EnumSet<E> set=EnumSet.noneOf(enumClass);
  if (!str.isEmpty()) {
    for (int i, j=0; j >= 0; ) {
      i=j > 0 ? j + 1 : 0;
      j=str.indexOf(',',i);
      final String sub=j >= 0 ? str.substring(i,j) : str.substring(i);
      set.add(Enum.valueOf(enumClass,sub.trim().toUpperCase()));
    }
  }
  return set;
}
