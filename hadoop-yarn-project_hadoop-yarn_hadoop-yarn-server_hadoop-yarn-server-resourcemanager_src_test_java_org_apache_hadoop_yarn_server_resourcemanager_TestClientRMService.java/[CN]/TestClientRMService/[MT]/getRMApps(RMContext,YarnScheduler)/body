{
  ConcurrentHashMap<ApplicationId,RMApp> apps=new ConcurrentHashMap<ApplicationId,RMApp>();
  ApplicationId applicationId1=getApplicationId(1);
  ApplicationId applicationId2=getApplicationId(2);
  ApplicationId applicationId3=getApplicationId(3);
  YarnConfiguration config=new YarnConfiguration();
  apps.put(applicationId1,getRMApp(rmContext,yarnScheduler,applicationId1,config,"testqueue",10,3));
  apps.put(applicationId2,getRMApp(rmContext,yarnScheduler,applicationId2,config,"a",20,2));
  apps.put(applicationId3,getRMApp(rmContext,yarnScheduler,applicationId3,config,"testqueue",40,5));
  return apps;
}
