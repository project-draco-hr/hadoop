{
  Connection conn=null;
  try {
    conn=ConnectionFactory.createConnection(hbaseConf);
    Admin admin=conn.getAdmin();
    if (admin == null) {
      throw new IOException("Cannot create table since admin is null");
    }
    createTimelineEntityTable(admin,hbaseConf);
  }
  finally {
    if (conn != null) {
      conn.close();
    }
  }
}
