{
  set(YarnWebParams.APP_STATE,StringHelper.cjoin(RMAppState.NEW.toString(),RMAppState.SUBMITTED.toString(),RMAppState.ACCEPTED.toString(),RMAppState.RUNNING.toString()));
  ResourceManager rm=getInstance(ResourceManager.class);
  ResourceScheduler rs=rm.getResourceScheduler();
  if (rs == null || rs instanceof CapacityScheduler) {
    setTitle("Capacity Scheduler");
    render(CapacitySchedulerPage.class);
    return;
  }
  setTitle("Default Scheduler");
  render(DefaultSchedulerPage.class);
}
