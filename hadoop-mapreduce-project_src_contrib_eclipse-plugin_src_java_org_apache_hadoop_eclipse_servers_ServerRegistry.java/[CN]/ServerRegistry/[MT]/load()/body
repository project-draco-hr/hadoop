{
  Map<String,HadoopServer> map=new TreeMap<String,HadoopServer>();
  for (  File file : saveDir.listFiles()) {
    try {
      HadoopServer server=new HadoopServer(file);
      map.put(server.getLocationName(),server);
    }
 catch (    Exception exn) {
      System.err.println(exn);
    }
  }
  this.servers=map;
}
