{
  this.writeLock.lock();
  try {
    response.addAllContainersToCleanup(new ArrayList<ContainerId>(this.containersToClean));
    response.addAllApplicationsToCleanup(this.finishedApplications);
    response.addFinishedContainersPulledByAM(new ArrayList<ContainerId>(this.finishedContainersPulledByAM));
    this.containersToClean.clear();
    this.finishedApplications.clear();
    this.finishedContainersPulledByAM.clear();
  }
  finally {
    this.writeLock.unlock();
  }
}
