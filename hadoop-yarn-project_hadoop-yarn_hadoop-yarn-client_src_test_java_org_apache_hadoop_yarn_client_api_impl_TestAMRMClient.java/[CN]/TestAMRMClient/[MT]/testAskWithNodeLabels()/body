{
  AMRMClientImpl<ContainerRequest> client=new AMRMClientImpl<ContainerRequest>();
  client.addContainerRequest(new ContainerRequest(Resource.newInstance(1024,1),null,null,Priority.UNDEFINED,true,"x && y"));
  Assert.assertEquals(1,client.ask.size());
  Assert.assertEquals("x && y",client.ask.iterator().next().getNodeLabelExpression());
  client.addContainerRequest(new ContainerRequest(Resource.newInstance(1024,1),null,null,Priority.UNDEFINED,true,"x && y"));
  client.addContainerRequest(new ContainerRequest(Resource.newInstance(1024,1),null,null,Priority.UNDEFINED,true,"a && b"));
  Assert.assertEquals(1,client.ask.size());
  Assert.assertEquals("a && b",client.ask.iterator().next().getNodeLabelExpression());
}
