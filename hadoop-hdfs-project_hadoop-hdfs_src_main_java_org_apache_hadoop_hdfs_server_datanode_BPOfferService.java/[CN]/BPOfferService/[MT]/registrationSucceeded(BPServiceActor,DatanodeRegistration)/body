{
  if (bpRegistration != null) {
    checkNSEquality(bpRegistration.storageInfo.getNamespaceID(),reg.storageInfo.getNamespaceID(),"namespace ID");
    checkNSEquality(bpRegistration.storageInfo.getClusterID(),reg.storageInfo.getClusterID(),"cluster ID");
  }
 else {
    bpRegistration=reg;
  }
  dn.bpRegistrationSucceeded(bpRegistration,getBlockPoolId());
}
