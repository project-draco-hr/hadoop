{
  StorageReport[] report={new StorageReport(bpRegistration.getStorageID(),false,dn.data.getCapacity(),dn.data.getDfsUsed(),dn.data.getRemaining(),dn.data.getBlockPoolUsed(getBlockPoolId()))};
  return bpNamenode.sendHeartbeat(bpRegistration,report,dn.xmitsInProgress.get(),dn.getXceiverCount(),dn.data.getNumFailedVolumes());
}
