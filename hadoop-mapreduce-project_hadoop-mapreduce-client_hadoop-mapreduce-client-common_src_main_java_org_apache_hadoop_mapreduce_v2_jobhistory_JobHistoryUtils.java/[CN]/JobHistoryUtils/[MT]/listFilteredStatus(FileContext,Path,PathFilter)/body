{
  List<FileStatus> fsList=remoteIterToList(fc.listStatus(root));
  if (filter == null) {
    return fsList;
  }
 else {
    List<FileStatus> filteredList=new LinkedList<FileStatus>();
    for (    FileStatus fs : fsList) {
      if (filter.accept(fs.getPath())) {
        filteredList.add(fs);
      }
    }
    return filteredList;
  }
}
