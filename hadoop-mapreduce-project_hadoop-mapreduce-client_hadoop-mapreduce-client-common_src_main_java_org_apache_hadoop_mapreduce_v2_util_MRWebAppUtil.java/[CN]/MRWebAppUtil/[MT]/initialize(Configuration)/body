{
  setSSLEnabledInYARN(conf.getBoolean(CommonConfigurationKeysPublic.HADOOP_SSL_ENABLED_KEY,CommonConfigurationKeysPublic.HADOOP_SSL_ENABLED_DEFAULT));
  setSSLEnabledInJHS(conf.getBoolean(JHAdminConfig.MR_HS_SSL_ENABLED,JHAdminConfig.DEFAULT_MR_HS_SSL_ENABLED));
  setSSLEnabledInMRAM(conf.getBoolean(MRConfig.SSL_ENABLED_KEY,MRConfig.SSL_ENABLED_KEY_DEFAULT));
}
