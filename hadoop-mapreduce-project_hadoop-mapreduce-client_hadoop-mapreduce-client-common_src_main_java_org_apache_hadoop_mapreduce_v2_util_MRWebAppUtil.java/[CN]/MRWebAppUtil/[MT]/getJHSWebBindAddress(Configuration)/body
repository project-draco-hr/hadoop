{
  if (httpPolicyInJHS == Policy.HTTPS_ONLY) {
    return conf.getSocketAddr(JHAdminConfig.MR_HISTORY_WEBAPP_HTTPS_ADDRESS,JHAdminConfig.DEFAULT_MR_HISTORY_WEBAPP_HTTPS_ADDRESS,JHAdminConfig.DEFAULT_MR_HISTORY_WEBAPP_HTTPS_PORT);
  }
 else {
    return conf.getSocketAddr(JHAdminConfig.MR_HISTORY_WEBAPP_ADDRESS,JHAdminConfig.DEFAULT_MR_HISTORY_WEBAPP_ADDRESS,JHAdminConfig.DEFAULT_MR_HISTORY_WEBAPP_PORT);
  }
}
