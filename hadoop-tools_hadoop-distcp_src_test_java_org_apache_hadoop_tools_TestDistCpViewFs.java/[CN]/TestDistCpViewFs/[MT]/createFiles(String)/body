{
  String e;
  for (  String entry : entries) {
    if ((new Path(entry)).isAbsolute()) {
      e=entry;
    }
 else {
      e=root + "/" + entry;
    }
    OutputStream out=fs.create(new Path(e));
    try {
      out.write((e).getBytes());
      out.write("\n".getBytes());
    }
  finally {
      out.close();
    }
  }
}
