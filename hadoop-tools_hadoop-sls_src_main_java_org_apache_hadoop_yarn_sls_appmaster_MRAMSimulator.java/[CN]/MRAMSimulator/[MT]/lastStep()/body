{
  super.lastStep();
  allMaps.clear();
  allReduces.clear();
  assignedMaps.clear();
  assignedReduces.clear();
  pendingFailedMaps.clear();
  pendingFailedReduces.clear();
  pendingMaps.clear();
  pendingReduces.clear();
  scheduledMaps.clear();
  scheduledReduces.clear();
  responseQueue.clear();
}
