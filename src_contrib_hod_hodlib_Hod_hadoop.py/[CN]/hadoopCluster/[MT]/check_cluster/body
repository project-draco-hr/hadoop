def check_cluster(self, clusterInfo):
    status = 0
    if ('mapred' in clusterInfo):
        mapredAddress = clusterInfo['mapred'][7:]
        hdfsAddress = clusterInfo['hdfs'][7:]
        status = get_cluster_status(hdfsAddress, mapredAddress)
        if (status == 0):
            status = 12
    else:
        status = 15
    return status
