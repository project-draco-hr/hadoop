{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Sending signal " + signal.getValue() + " to pid "+ pid+ " as user "+ user);
  }
  if (!containerIsAlive(pid)) {
    return false;
  }
  try {
    killContainer(pid,signal);
  }
 catch (  IOException e) {
    if (!containerIsAlive(pid)) {
      return false;
    }
    throw e;
  }
  return true;
}
