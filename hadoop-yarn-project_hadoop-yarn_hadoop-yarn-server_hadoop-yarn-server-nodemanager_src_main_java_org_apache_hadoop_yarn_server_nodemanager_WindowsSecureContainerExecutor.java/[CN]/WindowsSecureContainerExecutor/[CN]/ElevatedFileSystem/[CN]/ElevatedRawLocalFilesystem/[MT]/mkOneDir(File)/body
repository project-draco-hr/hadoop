{
  Path path=new Path(p2f.getAbsolutePath());
  if (LOG.isDebugEnabled()) {
    LOG.debug(String.format("EFS:mkOneDir: %s",path));
  }
  boolean ret=false;
  try {
    Native.Elevated.mkdir(path);
    ret=true;
  }
 catch (  Throwable e) {
    if (LOG.isDebugEnabled()) {
      LOG.debug(String.format("EFS:mkOneDir: %s",org.apache.hadoop.util.StringUtils.stringifyException(e)));
    }
  }
  return ret;
}
