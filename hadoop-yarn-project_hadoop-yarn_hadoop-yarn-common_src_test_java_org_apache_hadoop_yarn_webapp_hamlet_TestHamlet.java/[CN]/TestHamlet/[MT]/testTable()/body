{
  Hamlet h=newHamlet().title("test table").link("style.css");
  TABLE t=h.table("#id");
  for (int i=0; i < 3; ++i) {
    t.tr().td("1").td("2")._();
  }
  t._();
  PrintWriter out=h.getWriter();
  out.flush();
  assertEquals(0,h.nestLevel);
  verify(out).print("<table");
  verify(out).print("</table>");
  verify(out,atLeast(1)).print("</td>");
  verify(out,atLeast(1)).print("</tr>");
}
