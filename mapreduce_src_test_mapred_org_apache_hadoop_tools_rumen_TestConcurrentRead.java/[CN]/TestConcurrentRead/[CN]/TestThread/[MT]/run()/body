{
  try {
    startSignal.await();
    for (int i=0; i < repeat; ++i) {
      try {
        readAndCompare();
      }
 catch (      Throwable e) {
        errors.put(getName(),e);
        break;
      }
    }
    doneSignal.countDown();
  }
 catch (  Throwable e) {
    errors.put(getName(),e);
  }
}
