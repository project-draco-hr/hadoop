{
  FSDataOutputStream out=null;
  try {
    String broken="{ broken { [[]} broken";
    out=PluginStoreTestUtils.createLogFile(logPath,fs);
    out.write(broken.getBytes(Charset.forName("UTF-8")));
    out.close();
    out=null;
  }
  finally {
    if (out != null) {
      out.close();
    }
  }
}
