{
  statistics.incrementWriteOps(1);
  storageStatistics.incrementOpCounter(OpType.PRIMITIVE_CREATE);
  final DFSOutputStream dfsos=dfs.primitiveCreate(getPathName(fixRelativePart(f)),absolutePermission,flag,true,replication,blockSize,progress,bufferSize,checksumOpt);
  return dfs.createWrappedOutputStream(dfsos,statistics);
}
