{
  statistics.incrementReadOps(1);
  storageStatistics.incrementOpCounter(OpType.GET_FILE_CHECKSUM);
  Path absF=fixRelativePart(f);
  return new FileSystemLinkResolver<FileChecksum>(){
    @Override public FileChecksum doCall(    final Path p) throws IOException {
      return dfs.getFileChecksum(getPathName(p),Long.MAX_VALUE);
    }
    @Override public FileChecksum next(    final FileSystem fs,    final Path p) throws IOException {
      return fs.getFileChecksum(p);
    }
  }
.resolve(this,absF);
}
