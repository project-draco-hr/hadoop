{
  WebResource r=resource();
  MockNM nm1=rm.registerNode("h1:1234",5120);
  MockNM nm2=rm.registerNode("h2:1235",5121);
  rm.sendNodeStarted(nm1);
  rm.NMwaitForState(nm1.getNodeId(),NodeState.RUNNING);
  rm.NMwaitForState(nm2.getNodeId(),NodeState.NEW);
  ClientResponse response=r.path("ws").path("v1").path("cluster").path("nodes").queryParam("state","UNHEALTHY").accept(MediaType.APPLICATION_JSON).get(ClientResponse.class);
  assertEquals(MediaType.APPLICATION_JSON_TYPE,response.getType());
  JSONObject json=response.getEntity(JSONObject.class);
  assertEquals("incorrect number of elements",1,json.length());
  assertEquals("nodes is not null",JSONObject.NULL,json.get("nodes"));
}
