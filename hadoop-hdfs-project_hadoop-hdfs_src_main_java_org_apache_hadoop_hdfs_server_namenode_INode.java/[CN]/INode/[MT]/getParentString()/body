{
  final INodeReference parentRef=getParentReference();
  if (parentRef != null) {
    return "parentRef=" + parentRef.getLocalName() + "->";
  }
 else {
    final INodeDirectory parentDir=getParent();
    if (parentDir != null) {
      return "parentDir=" + parentDir.getLocalName() + "/";
    }
 else {
      return "parent=null";
    }
  }
}
