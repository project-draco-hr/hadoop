{
  Content.Counts counts=computeContentSummary(summary).getCounts();
  final Quota.Counts q=getQuotaCounts();
  return new ContentSummary(counts.get(Content.LENGTH),counts.get(Content.FILE) + counts.get(Content.SYMLINK),counts.get(Content.DIRECTORY),q.get(Quota.NAMESPACE),counts.get(Content.DISKSPACE),q.get(Quota.DISKSPACE));
}
