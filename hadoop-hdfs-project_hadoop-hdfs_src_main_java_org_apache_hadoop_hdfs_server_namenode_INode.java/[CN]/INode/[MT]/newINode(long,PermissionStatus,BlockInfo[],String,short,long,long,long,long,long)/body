{
  if (symlink.length() != 0) {
    return new INodeSymlink(id,symlink,modificationTime,atime,permissions);
  }
 else   if (blocks == null) {
    if (nsQuota >= 0 || dsQuota >= 0) {
      return new INodeDirectoryWithQuota(id,permissions,modificationTime,nsQuota,dsQuota);
    }
    return new INodeDirectory(id,permissions,modificationTime);
  }
  return new INodeFile(id,permissions,blocks,replication,modificationTime,atime,preferredBlockSize);
}
