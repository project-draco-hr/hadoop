{
  final Content.Counts current=computeContentSummary(new Content.CountsMap()).getCounts(Key.CURRENT);
  return new ContentSummary(current.get(Content.LENGTH),current.get(Content.FILE) + current.get(Content.SYMLINK),current.get(Content.DIRECTORY),getNsQuota(),current.get(Content.DISKSPACE),getDsQuota());
}
