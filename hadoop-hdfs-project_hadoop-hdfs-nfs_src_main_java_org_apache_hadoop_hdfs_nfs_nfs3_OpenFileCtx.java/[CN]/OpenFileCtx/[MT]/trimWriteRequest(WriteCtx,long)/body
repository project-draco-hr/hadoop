{
  long offset=writeCtx.getOffset();
  if (LOG.isDebugEnabled()) {
    int count=writeCtx.getCount();
    LOG.debug(String.format("Trim request [%d-%d)," + " current offset %d," + " drop the overlapped section [%d-%d)"+ " and write new data [%d-%d)",offset,(offset + count),currentOffset,offset,(currentOffset),currentOffset,(offset + count)));
  }
  writeCtx.trimWrite((int)(currentOffset - offset));
}
