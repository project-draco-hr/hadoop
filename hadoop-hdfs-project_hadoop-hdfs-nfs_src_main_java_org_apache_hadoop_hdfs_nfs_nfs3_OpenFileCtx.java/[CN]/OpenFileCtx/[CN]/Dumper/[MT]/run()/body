{
  while (activeState && enabledDump) {
    if (nonSequentialWriteInMemory.get() >= DUMP_WRITE_WATER_MARK) {
      dump();
    }
synchronized (OpenFileCtx.this) {
      if (nonSequentialWriteInMemory.get() < DUMP_WRITE_WATER_MARK) {
        try {
          OpenFileCtx.this.wait();
        }
 catch (        InterruptedException e) {
        }
      }
    }
  }
}
