{
  SchedulingMode sm=SchedulingMode.parse(FairSchedulingMode.class.getName());
  assertTrue("Invalid scheduler name",sm.getName().equals(FairSchedulingMode.NAME));
  sm=SchedulingMode.parse(FairSchedulingMode.class.getCanonicalName());
  assertTrue("Invalid scheduler name",sm.getName().equals(FairSchedulingMode.NAME));
  sm=SchedulingMode.getInstance(FairSchedulingMode.class);
  assertTrue("Invalid scheduler name",sm.getName().equals(FairSchedulingMode.NAME));
  sm=SchedulingMode.parse("fair");
  assertTrue("Invalid scheduler name",sm.getName().equals(FairSchedulingMode.NAME));
  sm=SchedulingMode.parse("fifo");
  assertTrue("Invalid scheduler name",sm.getName().equals(FifoSchedulingMode.NAME));
}
