{
  namesystem.getAclConfigFlag().checkForFsImage();
  AclFeature aclFeature=null;
  if (LayoutVersion.supports(Feature.EXTENDED_ACL,imgVersion)) {
    AclFsImageProto p=AclFsImageProto.parseDelimitedFrom((DataInputStream)in);
    aclFeature=new AclFeature(PBHelper.convertAclEntry(p.getEntriesList()));
  }
  return aclFeature;
}
