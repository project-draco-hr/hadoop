{
  if (clazz != null) {
    return ReflectionUtils.newInstance(clazz,conf);
  }
  try {
    if (Shell.LINUX) {
      return new LinuxResourceCalculatorPlugin();
    }
    if (Shell.WINDOWS) {
      return new WindowsResourceCalculatorPlugin();
    }
  }
 catch (  SecurityException se) {
    return null;
  }
  return null;
}
