{
  if (sslConf == null) {
    sslConf=new Configuration(false);
  }
  boolean needsClientAuth=YarnConfiguration.YARN_SSL_CLIENT_HTTPS_NEED_AUTH_DEFAULT;
  sslConf.addResource(YarnConfiguration.YARN_SSL_SERVER_RESOURCE_DEFAULT);
  return builder.needsClientAuth(needsClientAuth).keyPassword(getPassword(sslConf,WEB_APP_KEY_PASSWORD_KEY)).keyStore(sslConf.get("ssl.server.keystore.location"),getPassword(sslConf,WEB_APP_KEYSTORE_PASSWORD_KEY),sslConf.get("ssl.server.keystore.type","jks")).trustStore(sslConf.get("ssl.server.truststore.location"),getPassword(sslConf,WEB_APP_TRUSTSTORE_PASSWORD_KEY),sslConf.get("ssl.server.truststore.type","jks"));
}
