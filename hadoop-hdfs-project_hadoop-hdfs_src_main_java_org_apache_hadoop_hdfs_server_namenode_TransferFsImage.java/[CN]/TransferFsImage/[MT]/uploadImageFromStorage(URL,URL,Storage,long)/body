{
  String fileid=GetImageServlet.getParamStringToPutImage(txid,myNNAddress,storage);
  try {
    TransferFsImage.getFileClient(fsName,fileid,null,null,false);
  }
 catch (  HttpGetFailedException e) {
    if (e.getResponseCode() == HttpServletResponse.SC_CONFLICT) {
      LOG.info("Image upload with txid " + txid + " conflicted with a previous image upload to the "+ "same NameNode. Continuing...",e);
      return;
    }
 else {
      throw e;
    }
  }
  LOG.info("Uploaded image with txid " + txid + " to namenode at "+ fsName);
}
