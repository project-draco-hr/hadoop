{
  commonPreHead(html);
  String tid=$(TASK_ID);
  String activeNav="3";
  if (tid == null || tid.isEmpty()) {
    activeNav="2";
  }
  set(initID(ACCORDION,"nav"),"{autoHeight:false, active:" + activeNav + "}");
  set(DATATABLES_SELECTOR,"#counters .dt-counters");
  set(initSelector(DATATABLES),"{bJQueryUI:true, sDom:'t', iDisplayLength:-1}");
}
