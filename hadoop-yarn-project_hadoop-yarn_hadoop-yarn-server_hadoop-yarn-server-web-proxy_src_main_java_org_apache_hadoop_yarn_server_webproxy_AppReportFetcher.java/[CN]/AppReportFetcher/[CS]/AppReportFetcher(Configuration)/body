{
  this.conf=conf;
  YarnRPC rpc=YarnRPC.create(this.conf);
  InetSocketAddress rmAddress=conf.getSocketAddr(YarnConfiguration.RM_ADDRESS,YarnConfiguration.DEFAULT_RM_ADDRESS,YarnConfiguration.DEFAULT_RM_PORT);
  LOG.info("Connecting to ResourceManager at " + rmAddress);
  applicationsManager=(ApplicationClientProtocol)rpc.getProxy(ApplicationClientProtocol.class,rmAddress,this.conf);
  LOG.info("Connected to ResourceManager at " + rmAddress);
}
