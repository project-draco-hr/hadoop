{
  if (conf.getBoolean(YarnConfiguration.APPLICATION_HISTORY_ENABLED,YarnConfiguration.DEFAULT_APPLICATION_HISTORY_ENABLED)) {
    isAHSEnabled=true;
  }
  this.conf=conf;
  try {
    applicationsManager=ClientRMProxy.createRMProxy(conf,ApplicationClientProtocol.class);
    if (isAHSEnabled) {
      historyManager=getAHSProxy(conf);
    }
 else {
      this.historyManager=null;
    }
  }
 catch (  IOException e) {
    throw new YarnRuntimeException(e);
  }
}
