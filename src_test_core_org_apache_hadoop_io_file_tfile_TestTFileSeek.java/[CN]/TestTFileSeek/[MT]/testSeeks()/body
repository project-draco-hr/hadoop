{
  String[] supported=TFile.getSupportedCompressionAlgorithms();
  boolean proceed=false;
  for (  String c : supported) {
    if (c.equals(options.compress)) {
      proceed=true;
      break;
    }
  }
  if (!proceed) {
    System.out.println("Skipped for " + options.compress);
    return;
  }
  if (options.doCreate()) {
    createTFile();
  }
  if (options.doRead()) {
    seekTFile();
  }
}
