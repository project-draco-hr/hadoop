{
  if (isCreation) {
    TestDirectoryTree.Node newChild=new TestDirectoryTree.Node(changedPath,node.level + 1,node,node.fs);
    newChild.initFileList(BLOCKSIZE,REPLICATION,seed,2);
    node.nonSnapshotChildren.add(newChild);
  }
 else {
    TestDirectoryTree.Node childToDelete=node.nonSnapshotChildren.remove(node.nonSnapshotChildren.size() - 1);
    node.fs.delete(childToDelete.nodePath,true);
  }
}
