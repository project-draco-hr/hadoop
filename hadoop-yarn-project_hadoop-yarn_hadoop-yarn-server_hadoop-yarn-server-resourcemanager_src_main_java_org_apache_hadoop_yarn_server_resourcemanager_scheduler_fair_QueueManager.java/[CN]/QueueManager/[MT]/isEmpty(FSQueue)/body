{
  if (queue instanceof FSLeafQueue) {
    FSLeafQueue leafQueue=(FSLeafQueue)queue;
    return queue.getNumRunnableApps() == 0 && leafQueue.getNonRunnableAppSchedulables().isEmpty();
  }
 else {
    for (    FSQueue child : queue.getChildQueues()) {
      if (!isEmpty(child)) {
        return false;
      }
    }
    return true;
  }
}
