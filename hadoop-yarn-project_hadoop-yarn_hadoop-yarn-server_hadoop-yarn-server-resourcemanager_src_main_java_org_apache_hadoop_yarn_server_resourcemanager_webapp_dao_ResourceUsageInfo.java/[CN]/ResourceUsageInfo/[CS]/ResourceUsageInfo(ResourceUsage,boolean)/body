{
  if (resourceUsage == null) {
    return;
  }
  for (  String partitionName : resourceUsage.getNodePartitionsSet()) {
    resourceUsagesByPartition.add(new PartitionResourceUsageInfo(partitionName,new ResourceInfo(resourceUsage.getUsed(partitionName)),new ResourceInfo(resourceUsage.getReserved(partitionName)),new ResourceInfo(resourceUsage.getPending(partitionName)),considerAMUsage ? new ResourceInfo(resourceUsage.getAMUsed(partitionName)) : null));
  }
}
