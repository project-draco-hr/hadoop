{
  if (logContext.getContainerType() == ContainerType.APPLICATION_MASTER || logContext.getExitCode() != 0) {
    return true;
  }
  if ((logContext.getContainerId().getContainerId() & ContainerId.CONTAINER_ID_BITMASK) < minThreshold + 2) {
    return true;
  }
  return (sampleRate != 0 && logContext.getContainerId().hashCode() % (1 / sampleRate) == 0);
}
