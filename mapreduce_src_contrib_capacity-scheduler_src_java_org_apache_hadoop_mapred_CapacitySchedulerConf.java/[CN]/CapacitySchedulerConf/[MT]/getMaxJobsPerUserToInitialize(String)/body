{
  String raw=getProperty(queue,MAXIMUM_INITIALIZED_JOBS_PER_USER_PROPERTY);
  int maxJobsPerUser=getInt(raw,defaultMaxJobsPerUsersToInitialize);
  if (maxJobsPerUser <= 0) {
    throw new IllegalArgumentException("Invalid maximum jobs per user configuration " + maxJobsPerUser);
  }
  return maxJobsPerUser;
}
