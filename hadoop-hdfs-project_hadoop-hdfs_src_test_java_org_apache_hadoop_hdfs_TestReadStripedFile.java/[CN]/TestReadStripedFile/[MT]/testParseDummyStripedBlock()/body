{
  LocatedStripedBlock lsb=createDummyLocatedBlock();
  LocatedBlock[] blocks=DFSStripedInputStream.parseStripedBlockGroup(lsb,GROUP_SIZE,CELLSIZE);
  assertEquals(GROUP_SIZE,blocks.length);
  for (int j=0; j < GROUP_SIZE; j++) {
    assertFalse(blocks[j].isStriped());
    assertEquals(j,BlockIdManager.getBlockIndex(blocks[j].getBlock().getLocalBlock()));
    assertEquals(j * CELLSIZE,blocks[j].getStartOffset());
  }
}
