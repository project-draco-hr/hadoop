{
  String truststoreLocation=BASEDIR + "/testcorrupt.jks";
  OutputStream os=new FileOutputStream(truststoreLocation);
  os.write(1);
  os.close();
  ReloadingX509TrustManager tm=new ReloadingX509TrustManager("jks",truststoreLocation,"password",10);
  try {
    tm.init();
  }
  finally {
    tm.destroy();
  }
}
