{
  if ((expected == null && actual != null) || (expected != null && actual == null)) {
    return false;
  }
  if (expected == null && actual == null) {
    return true;
  }
  if (expected.length != actual.length) {
    return false;
  }
  for (int i=0; i < expected.length; i++) {
    if (expected[i] != actual[i]) {
      return false;
    }
  }
  return true;
}
