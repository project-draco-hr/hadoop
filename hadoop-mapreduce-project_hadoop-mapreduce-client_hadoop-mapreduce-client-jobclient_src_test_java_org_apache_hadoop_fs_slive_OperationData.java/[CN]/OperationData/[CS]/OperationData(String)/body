{
  String pieces[]=Helper.getTrimmedStrings(data);
  distribution=Distribution.UNIFORM;
  percent=null;
  if (pieces.length == 1) {
    percent=(Double.parseDouble(pieces[0]) / 100.0d);
  }
 else   if (pieces.length >= 2) {
    percent=(Double.parseDouble(pieces[0]) / 100.0d);
    distribution=Distribution.valueOf(pieces[1].toUpperCase());
  }
}
