{
  return new ContainerLauncherImpl(context){
    @Override protected ContainerManager getCMProxy(    String containerManagerBindAddr,    ContainerToken containerToken) throws IOException {
      try {
synchronized (this) {
          wait();
        }
      }
 catch (      InterruptedException e) {
        LOG.info(e);
        if (!swallowInterrupts) {
          throw new IOException(e);
        }
 else {
          Thread.currentThread().interrupt();
        }
      }
      return null;
    }
  }
;
}
