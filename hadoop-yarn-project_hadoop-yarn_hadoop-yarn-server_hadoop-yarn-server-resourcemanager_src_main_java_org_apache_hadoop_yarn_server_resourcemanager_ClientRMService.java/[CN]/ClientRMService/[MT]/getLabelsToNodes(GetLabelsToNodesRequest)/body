{
  RMNodeLabelsManager labelsMgr=rmContext.getNodeLabelManager();
  if (request.getNodeLabels() == null || request.getNodeLabels().isEmpty()) {
    return GetLabelsToNodesResponse.newInstance(labelsMgr.getLabelsInfoToNodes());
  }
 else {
    return GetLabelsToNodesResponse.newInstance(labelsMgr.getLabelsInfoToNodes(request.getNodeLabels()));
  }
}
