def toc(list):
    tocout = []
    header = ()
    for i in list:
        if (header != i.getinter()):
            header = i.getinter()
            line = ('  * %s\n' % i.headerbuild())
            tocout.append(line)
        line = ('    * [%s](#%s)\n' % (i.getname().replace('_', '\\_'), i.getname()))
        tocout.append(line)
    return tocout
