def getdocpage(self):
    line = ('### `%s`\n\n* Synopsis\n\n```\n%s\n```\n\n* Description\n\n%s\n\n* Returns\n\n%s\n\n| Classification | Level |\n| :--- | :--- |\n| Audience | %s |\n| Stability | %s |\n| Replaceable | %s |\n\n' % (self.getname(), self.getusage(), self.getdesc(), self.getreturn(), self.getaudience(), self.getstability(), self.getreplace()))
    return line
