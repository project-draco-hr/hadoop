{
  stripedBlocks=new MultipleBlockingQueue<>(numAllBlocks,1,conf.getStripedWriteMaxSecondsGetStripedBlock());
  endBlocks=new MultipleBlockingQueue<>(numDataBlocks,1,conf.getStripedWriteMaxSecondsGetEndedBlock());
  updateBlocks=new MultipleBlockingQueue<>(numAllBlocks,1,conf.getStripedWriteMaxSecondsGetStripedBlock());
