{
  String[] ret=new String[indices.length];
  Text t=new Text();
  DataInputBuffer dib=new DataInputBuffer();
  for (int i=0; i < ret.length; ++i) {
    int ii=indices[i];
    dib.reset(bytes,offsets[ii],((ii + 1 == indices.length) ? eob : offsets[ii + 1]) - offsets[ii]);
    t.readFields(dib);
    ret[i]=t.toString();
  }
  return ret;
}
