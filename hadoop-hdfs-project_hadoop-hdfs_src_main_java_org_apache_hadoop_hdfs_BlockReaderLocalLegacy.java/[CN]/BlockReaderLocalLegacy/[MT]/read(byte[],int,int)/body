{
  if (LOG.isTraceEnabled()) {
    LOG.trace("read off " + off + " len "+ len);
  }
  if (!verifyChecksum) {
    return dataIn.read(buf,off,len);
  }
  int nRead=fillSlowReadBuffer(slowReadBuff.capacity());
  if (nRead > 0) {
    nRead=Math.min(len,nRead);
    slowReadBuff.get(buf,off,nRead);
  }
  return nRead;
}
