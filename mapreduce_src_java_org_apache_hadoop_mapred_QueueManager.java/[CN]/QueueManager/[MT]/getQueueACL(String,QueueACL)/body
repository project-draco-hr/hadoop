{
  if (areAclsEnabled) {
    Queue q=leafQueues.get(queueName);
    if (q != null) {
      return q.getAcls().get(toFullPropertyName(queueName,qACL.getAclName()));
    }
 else {
      LOG.warn("Queue " + queueName + " is not present.");
      return null;
    }
  }
  return new AccessControlList("*");
}
