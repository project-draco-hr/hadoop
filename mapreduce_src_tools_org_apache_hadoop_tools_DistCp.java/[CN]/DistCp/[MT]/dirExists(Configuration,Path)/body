{
  FileSystem destFileSys=dst.getFileSystem(conf);
  FileStatus status=null;
  try {
    status=destFileSys.getFileStatus(dst);
  }
 catch (  FileNotFoundException e) {
    return false;
  }
  if (status.isFile()) {
    throw new FileAlreadyExistsException("Not a dir: " + dst + " is a file.");
  }
  return true;
}
