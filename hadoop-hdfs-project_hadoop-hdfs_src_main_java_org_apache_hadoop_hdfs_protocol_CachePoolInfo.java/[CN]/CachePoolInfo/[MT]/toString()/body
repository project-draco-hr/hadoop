{
  return new StringBuilder().append("{").append("poolName:").append(poolName).append(", ownerName:").append(ownerName).append(", groupName:").append(groupName).append(", mode:").append((mode == null) ? "null" : String.format("0%03o",mode.toShort())).append(", limit:").append(limit).append("}").toString();
}
