{
  String key=getApplicationAttemptNodeKey(attemptId);
  if (LOG.isDebugEnabled()) {
    LOG.debug("Storing state for attempt " + attemptId + " at "+ key);
  }
  try {
    db.put(bytes(key),attemptStateData.getProto().toByteArray());
  }
 catch (  DBException e) {
    throw new IOException(e);
  }
}
