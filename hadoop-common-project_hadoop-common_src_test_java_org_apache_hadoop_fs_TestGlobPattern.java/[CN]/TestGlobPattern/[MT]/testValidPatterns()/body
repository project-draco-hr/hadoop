{
  assertMatch(true,"*","^$","foo","bar");
  assertMatch(true,"?","?","^","[","]","$");
  assertMatch(true,"foo*","foo","food","fool");
  assertMatch(true,"f*d","fud","food");
  assertMatch(true,"*d","good","bad");
  assertMatch(true,"\\*\\?\\[\\{\\\\","*?[{\\");
  assertMatch(true,"[]^-]","]","-","^");
  assertMatch(true,"]","]");
  assertMatch(true,"^.$()|+","^.$()|+");
  assertMatch(true,"[^^]",".","$","[","]");
  assertMatch(false,"[^^]","^");
  assertMatch(true,"[!!-]","^","?");
  assertMatch(false,"[!!-]","!","-");
  assertMatch(true,"{[12]*,[45]*,[78]*}","1","2!","4","42","7","7$");
  assertMatch(false,"{[12]*,[45]*,[78]*}","3","6","9??");
  assertMatch(true,"}","}");
}
