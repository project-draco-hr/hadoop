{
  fileId=v;
  handle=new byte[HANDLE_LEN];
  handle[0]=(byte)(v >>> 56);
  handle[1]=(byte)(v >>> 48);
  handle[2]=(byte)(v >>> 40);
  handle[3]=(byte)(v >>> 32);
  handle[4]=(byte)(v >>> 24);
  handle[5]=(byte)(v >>> 16);
  handle[6]=(byte)(v >>> 8);
  handle[7]=(byte)(v >>> 0);
  for (int i=8; i < HANDLE_LEN; i++) {
    handle[i]=(byte)0;
  }
}
