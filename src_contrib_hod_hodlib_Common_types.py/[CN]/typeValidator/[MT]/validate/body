def validate(self, allowNone=False):
    'Validates configValidate object populating validatedInfo with\n           valueInfo dictionaries for each value added to the object.'
    for valItem in self.validateList:
        valueInfo = self.verify(valItem['type'], valItem['value'], allowNone)
        if valueInfo:
            valueInfo['name'] = valItem['name']
            self.validatedInfo.append(valueInfo)
        else:
            raise Exception(('\nMissing a return value: valueInfo\n%s' % self.verifyFunctions[valItem['type']](valItem['value'])))
