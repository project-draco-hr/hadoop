def __norm_size(self, file):
    norm = None
    if reSizeFormat.match(value):
        numberPart = int(reSizeFormat.sub('\\g<1>', value))
        factorPart = reSizeFormat.sub('\\g<2>', value)
        norm = normalize_size(numberPart, factorPart)
    else:
        norm = int(value)
    return norm
