{
  TaskAttemptUnsuccessfulCompletionEvent tauce=new TaskAttemptUnsuccessfulCompletionEvent(TypeConverter.fromYarn(taskAttempt.attemptId),TypeConverter.fromYarn(taskAttempt.attemptId.getTaskId().getTaskType()),attemptState.toString(),taskAttempt.finishTime,taskAttempt.nodeHostName == null ? "UNKNOWN" : taskAttempt.nodeHostName,StringUtils.join(LINE_SEPARATOR,taskAttempt.getDiagnostics()),taskAttempt.getProgressSplitBlock().burst());
  return tauce;
}
