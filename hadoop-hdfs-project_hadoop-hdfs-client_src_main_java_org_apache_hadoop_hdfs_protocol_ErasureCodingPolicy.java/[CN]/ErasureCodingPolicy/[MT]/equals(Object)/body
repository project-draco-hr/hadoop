{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  ErasureCodingPolicy that=(ErasureCodingPolicy)o;
  if (that.getName().equals(name) && that.getCellSize() == cellSize && that.getSchema().equals(schema)) {
    return true;
  }
  return false;
}
