{
  conf.set(DFS_FEDERATION_NAMESERVICE_ID,nameserviceId);
  NameNode nn=createNameNode(nnIndex,conf,numDataNodes,manageNameDfsDirs,format,operation,clusterId);
  conf.set(DFSUtil.getNameServiceIdKey(DFS_NAMENODE_RPC_ADDRESS_KEY,nameserviceId),NetUtils.getHostPortString(nn.getNameNodeAddress()));
  conf.set(DFSUtil.getNameServiceIdKey(DFS_NAMENODE_HTTP_ADDRESS_KEY,nameserviceId),NetUtils.getHostPortString(nn.getHttpAddress()));
  DFSUtil.setGenericConf(conf,nameserviceId,DFS_NAMENODE_HTTP_ADDRESS_KEY);
  nameNodes[nnIndex]=new NameNodeInfo(nn,new Configuration(conf));
}
