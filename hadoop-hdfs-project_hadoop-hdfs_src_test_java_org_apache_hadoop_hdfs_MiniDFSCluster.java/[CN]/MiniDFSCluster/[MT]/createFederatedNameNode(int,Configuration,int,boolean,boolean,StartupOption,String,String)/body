{
  conf.set(DFSConfigKeys.DFS_FEDERATION_NAMESERVICE_ID,nameserviceId);
  NameNode nn=createNameNode(nnIndex,conf,numDataNodes,manageNameDfsDirs,format,operation,clusterId);
  conf.set(DFSUtil.addKeySuffixes(DFSConfigKeys.DFS_NAMENODE_RPC_ADDRESS_KEY,nameserviceId),NameNode.getHostPortString(nn.getNameNodeAddress()));
  conf.set(DFSUtil.addKeySuffixes(DFSConfigKeys.DFS_NAMENODE_HTTP_ADDRESS_KEY,nameserviceId),NameNode.getHostPortString(nn.getHttpAddress()));
  DFSUtil.setGenericConf(conf,nameserviceId,DFSConfigKeys.DFS_NAMENODE_HTTP_ADDRESS_KEY);
  nameNodes[nnIndex]=new NameNodeInfo(nn,new Configuration(conf));
}
