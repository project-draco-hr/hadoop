{
  for (  String dn : statusMap.keySet()) {
    Map<String,String> nnStatus=statusMap.get(dn);
    String status=nnStatus.get(OVERALL_STATUS);
    if (status.equals(DecommissionStates.DECOMMISSIONED.toString())) {
      decommissioned++;
    }
 else     if (status.equals(DecommissionStates.DECOMMISSION_INPROGRESS.toString())) {
      decommissioning++;
    }
 else     if (status.equals(DecommissionStates.PARTIALLY_DECOMMISSIONED.toString())) {
      partial++;
    }
  }
}
