{
  return unprivilegedUser.doAs(new PrivilegedExceptionAction<PathBasedCacheDescriptor>(){
    @Override public PathBasedCacheDescriptor run() throws IOException {
      DistributedFileSystem myDfs=(DistributedFileSystem)FileSystem.get(conf);
      return myDfs.addPathBasedCacheDirective(directive);
    }
  }
);
}
