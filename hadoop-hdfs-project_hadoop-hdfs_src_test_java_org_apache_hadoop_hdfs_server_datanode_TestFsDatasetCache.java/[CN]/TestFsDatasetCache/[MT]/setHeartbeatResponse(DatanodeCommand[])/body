{
  NNHAStatusHeartbeat ha=new NNHAStatusHeartbeat(HAServiceState.ACTIVE,fsImage.getLastAppliedOrWrittenTxId());
  HeartbeatResponse response=new HeartbeatResponse(cmds,ha,null);
  doReturn(response).when(spyNN).sendHeartbeat((DatanodeRegistration)any(),(StorageReport[])any(),anyLong(),anyLong(),anyInt(),anyInt(),anyInt());
}
