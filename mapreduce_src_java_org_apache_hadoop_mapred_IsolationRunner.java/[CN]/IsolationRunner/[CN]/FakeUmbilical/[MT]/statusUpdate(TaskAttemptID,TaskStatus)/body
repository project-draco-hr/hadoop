{
  StringBuffer buf=new StringBuffer("Task ");
  buf.append(taskId);
  buf.append(" making progress to ");
  buf.append(taskStatus.getProgress());
  String state=taskStatus.getStateString();
  if (state != null) {
    buf.append(" and state of ");
    buf.append(state);
  }
  LOG.info(buf.toString());
  return true;
}
