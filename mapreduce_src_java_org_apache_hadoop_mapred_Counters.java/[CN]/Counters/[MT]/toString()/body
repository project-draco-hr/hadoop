{
  StringBuilder sb=new StringBuilder("Counters: " + size());
  for (  Group group : this) {
    sb.append("\n\t" + group.getDisplayName());
    for (    Counter counter : group) {
      sb.append("\n\t\t" + counter.getDisplayName() + "="+ counter.getCounter());
    }
  }
  return sb.toString();
}
