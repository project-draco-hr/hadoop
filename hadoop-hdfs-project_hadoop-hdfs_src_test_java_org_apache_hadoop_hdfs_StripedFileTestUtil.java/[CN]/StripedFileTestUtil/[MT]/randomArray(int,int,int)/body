{
  if (n > (max - min + 1) || max < min || min < 0 || max < 0) {
    return null;
  }
  int[] result=new int[n];
  for (int i=0; i < n; i++) {
    result[i]=-1;
  }
  int count=0;
  while (count < n) {
    int num=(int)(Math.random() * (max - min)) + min;
    boolean flag=true;
    for (int j=0; j < n; j++) {
      if (num == result[j]) {
        flag=false;
        break;
      }
    }
    if (flag) {
      result[count]=num;
      count++;
    }
  }
  return result;
}
