{
  FileInputStream fis=new FileInputStream(f);
  boolean success=false;
  try {
    Stat stat=NativeIO.POSIX.getFstat(fis.getFD());
    checkStat(f,stat.getOwner(),stat.getGroup(),expectedOwner,expectedGroup);
    success=true;
    return fis;
  }
  finally {
    if (!success) {
      fis.close();
    }
  }
}
