{
  if (snapshottables.isEmpty()) {
    return null;
  }
  SnapshottableDirectoryStatus[] status=new SnapshottableDirectoryStatus[snapshottables.size()];
  for (int i=0; i < snapshottables.size(); i++) {
    INodeDirectorySnapshottable dir=snapshottables.get(i);
    status[i]=new SnapshottableDirectoryStatus(dir.getModificationTime(),dir.getAccessTime(),dir.getFsPermission(),dir.getUserName(),dir.getGroupName(),dir.getLocalNameBytes(),dir.getNumSnapshots(),dir.getSnapshotQuota(),dir.getParent() == null ? new byte[0] : DFSUtil.string2Bytes(dir.getParent().getFullPathName()));
  }
  return status;
}
