def qdel(self, jobId, force=False):
    exitCode = 0
    qdel = self.__qdel
    if force:
        qdel = ('%s -p %s' % (qdel, jobId))
    else:
        qdel = ('%s %s' % (qdel, jobId))
    self.__log.debug(qdel)
    qdelProcess = simpleCommand('qdel', qdel, env=self.__env)
    qdelProcess.start()
    qdelProcess.wait()
    qdelProcess.join()
    exitCode = qdelProcess.exit_code()
    return exitCode
