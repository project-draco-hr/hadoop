{
  Job newJob=new JobImpl(jobId,appAttemptID,conf,dispatcher.getEventHandler(),taskAttemptListener,jobTokenSecretManager,fsTokens,clock,completedTasksFromPreviousRun,metrics,newApiCommitter,currentUser.getUserName(),appSubmitTime,amInfos,context,forcedState,diagnostic);
  ((RunningAppContext)context).jobs.put(newJob.getID(),newJob);
  dispatcher.register(JobFinishEvent.Type.class,createJobFinishEventHandler());
  return newJob;
}
