{
  boolean isSupported=false;
  JobContext _jobContext;
  if (committer != null) {
    if (newApiCommitter) {
      _jobContext=new JobContextImpl(getConfig(),TypeConverter.fromYarn(getJobId()));
    }
 else {
      _jobContext=new org.apache.hadoop.mapred.JobContextImpl(new JobConf(getConfig()),TypeConverter.fromYarn(getJobId()));
    }
    isSupported=committer.isRecoverySupported(_jobContext);
  }
  return isSupported;
}
