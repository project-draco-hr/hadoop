{
  boolean isSupported=false;
  Configuration conf=getConfig();
  if (committer != null) {
    final JobContext _jobContext;
    if (newApiCommitter) {
      _jobContext=new JobContextImpl(conf,TypeConverter.fromYarn(getJobId()));
    }
 else {
      _jobContext=new org.apache.hadoop.mapred.JobContextImpl(new JobConf(conf),TypeConverter.fromYarn(getJobId()));
    }
    isSupported=callWithJobClassLoader(conf,new ExceptionAction<Boolean>(){
      public Boolean call(      Configuration conf) throws IOException {
        return committer.isRecoverySupported(_jobContext);
      }
    }
);
  }
  return isSupported;
}
