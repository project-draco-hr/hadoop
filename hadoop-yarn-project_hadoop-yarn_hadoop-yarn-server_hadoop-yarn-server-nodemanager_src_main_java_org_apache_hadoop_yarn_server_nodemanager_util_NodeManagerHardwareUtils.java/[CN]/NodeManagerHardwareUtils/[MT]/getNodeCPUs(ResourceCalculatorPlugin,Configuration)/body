{
  int numProcessors=plugin.getNumProcessors();
  boolean countLogicalCores=conf.getBoolean(YarnConfiguration.NM_COUNT_LOGICAL_PROCESSORS_AS_CORES,YarnConfiguration.DEFAULT_NM_COUNT_LOGICAL_PROCESSORS_AS_CORES);
  if (!countLogicalCores) {
    numProcessors=plugin.getNumCores();
  }
  return numProcessors;
}
