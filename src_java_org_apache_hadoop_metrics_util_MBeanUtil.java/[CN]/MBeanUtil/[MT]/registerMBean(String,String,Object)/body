{
  final MBeanServer mbs=ManagementFactory.getPlatformMBeanServer();
  ObjectName name=getMBeanName(serviceName,nameName);
  try {
    mbs.registerMBean(theMbean,name);
    return name;
  }
 catch (  InstanceAlreadyExistsException ie) {
  }
catch (  Exception e) {
    e.printStackTrace();
  }
  return null;
}
