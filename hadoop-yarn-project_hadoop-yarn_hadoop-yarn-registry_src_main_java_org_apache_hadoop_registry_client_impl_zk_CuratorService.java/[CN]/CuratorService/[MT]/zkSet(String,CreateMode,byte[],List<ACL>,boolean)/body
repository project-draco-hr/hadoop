{
  Preconditions.checkArgument(data != null,"null data");
  checkServiceLive();
  if (!zkPathExists(path)) {
    zkCreate(path,mode,data,acl);
    return true;
  }
 else {
    if (overwrite) {
      zkUpdate(path,data);
      return false;
    }
 else {
      throw new FileAlreadyExistsException(path);
    }
  }
}
