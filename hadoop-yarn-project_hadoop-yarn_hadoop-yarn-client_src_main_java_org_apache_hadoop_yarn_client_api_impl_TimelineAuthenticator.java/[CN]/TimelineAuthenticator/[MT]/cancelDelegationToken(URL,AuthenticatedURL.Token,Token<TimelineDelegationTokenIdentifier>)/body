{
  Map<String,String> params=new HashMap<String,String>();
  params.put(TimelineAuthenticationConsts.OP_PARAM,TimelineDelegationTokenOperation.CANCELDELEGATIONTOKEN.toString());
  params.put(TimelineAuthenticationConsts.TOKEN_PARAM,dToken.encodeToUrlString());
  url=appendParams(url,params);
  AuthenticatedURL aUrl=new AuthenticatedURL(new TimelineAuthenticator());
  try {
    HttpURLConnection conn=aUrl.openConnection(url,token);
    conn.setRequestMethod(TimelineDelegationTokenOperation.CANCELDELEGATIONTOKEN.getHttpMethod());
    validateAndParseResponse(conn);
  }
 catch (  AuthenticationException ex) {
    throw new IOException(ex.toString(),ex);
  }
}
