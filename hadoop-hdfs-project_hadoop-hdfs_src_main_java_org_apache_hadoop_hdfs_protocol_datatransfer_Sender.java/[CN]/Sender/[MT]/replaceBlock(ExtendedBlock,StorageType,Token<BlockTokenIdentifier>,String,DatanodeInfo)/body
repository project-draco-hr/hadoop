{
  OpReplaceBlockProto proto=OpReplaceBlockProto.newBuilder().setHeader(DataTransferProtoUtil.buildBaseHeader(blk,blockToken)).setStorageType(PBHelper.convertStorageType(storageType)).setDelHint(delHint).setSource(PBHelper.convertDatanodeInfo(source)).build();
  send(out,Op.REPLACE_BLOCK,proto);
}
