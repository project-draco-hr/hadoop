{
  final DFSInputStream dfsis=dfs.open(getUriPath(f),bufferSize,verifyChecksum);
  final byte[] key=dfsis.getKey();
  final byte[] iv=dfsis.getIv();
  Preconditions.checkState(!(key == null ^ iv == null),"Only one of the Key and IV were found.");
  if (false && key != null) {
    final CryptoInputStream cbis=new CryptoInputStream(dfsis,factory,key,iv);
    return new HdfsDataInputStream(cbis);
  }
 else {
    return new HdfsDataInputStream(dfsis);
  }
}
