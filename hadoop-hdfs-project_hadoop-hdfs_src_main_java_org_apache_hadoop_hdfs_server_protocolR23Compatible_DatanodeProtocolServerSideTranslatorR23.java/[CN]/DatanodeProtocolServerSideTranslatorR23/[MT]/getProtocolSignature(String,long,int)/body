{
  if (!protocol.equals(RPC.getProtocolName(DatanodeWireProtocol.class))) {
    throw new IOException("Namenode Serverside implements " + RPC.getProtocolName(DatanodeWireProtocol.class) + ". The following requested protocol is unknown: "+ protocol);
  }
  return ProtocolSignature.getProtocolSignature(clientMethodsHash,DatanodeWireProtocol.versionID,DatanodeWireProtocol.class);
}
