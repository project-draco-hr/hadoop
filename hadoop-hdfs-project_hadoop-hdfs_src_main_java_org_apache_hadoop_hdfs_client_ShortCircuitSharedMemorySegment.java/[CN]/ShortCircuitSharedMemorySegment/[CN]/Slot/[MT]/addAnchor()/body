{
  long prev;
  do {
    prev=unsafe.getLongVolatile(null,this.slotAddress);
    if ((prev & 0x7fffffff) == 0x7fffffff) {
      return false;
    }
    if ((prev & ANCHORABLE_FLAG) == 0) {
      return false;
    }
  }
 while (!unsafe.compareAndSwapLong(null,this.slotAddress,prev,prev + 1));
  return true;
}
