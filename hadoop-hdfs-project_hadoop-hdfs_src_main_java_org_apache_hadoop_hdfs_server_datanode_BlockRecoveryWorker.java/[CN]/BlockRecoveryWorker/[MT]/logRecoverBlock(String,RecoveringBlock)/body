{
  ExtendedBlock block=rb.getBlock();
  DatanodeInfo[] targets=rb.getLocations();
  LOG.info(who + " calls recoverBlock(" + block+ ", targets=["+ Joiner.on(", ").join(targets)+ "]"+ ", newGenerationStamp="+ rb.getNewGenerationStamp()+ ", newBlock="+ rb.getNewBlock()+ ", isStriped="+ rb.isStriped()+ ")");
}
