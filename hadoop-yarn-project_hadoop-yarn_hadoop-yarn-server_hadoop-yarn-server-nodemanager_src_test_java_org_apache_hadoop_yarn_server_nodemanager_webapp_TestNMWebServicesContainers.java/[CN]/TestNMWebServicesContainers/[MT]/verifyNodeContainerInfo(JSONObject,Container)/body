{
  assertEquals("incorrect number of elements",10,info.length());
  verifyNodeContainerInfoGeneric(cont,info.getString("id"),info.getString("state"),info.getString("user"),info.getInt("exitCode"),info.getString("diagnostics"),info.getString("nodeId"),info.getInt("totalMemoryNeededMB"),info.getInt("totalVCoresNeeded"),info.getString("containerLogsLink"));
  assertTrue("containerLogFiles missing",info.getJSONArray("containerLogFiles") != null);
}
