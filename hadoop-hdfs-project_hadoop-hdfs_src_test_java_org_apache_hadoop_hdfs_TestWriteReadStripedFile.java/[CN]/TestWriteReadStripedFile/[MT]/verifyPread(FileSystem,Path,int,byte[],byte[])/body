{
  FSDataInputStream in=fs.open(srcPath);
  int[] startOffsets={0,1,cellSize - 102,cellSize,cellSize + 102,cellSize * (dataBlocks - 1),cellSize * (dataBlocks - 1) + 102,cellSize * dataBlocks,fileLength - 102,fileLength - 1};
  for (  int startOffset : startOffsets) {
    startOffset=Math.max(0,Math.min(startOffset,fileLength - 1));
    int remaining=fileLength - startOffset;
    in.readFully(startOffset,buf,0,remaining);
    for (int i=0; i < remaining; i++) {
      Assert.assertEquals("Byte at " + (startOffset + i) + " should be the "+ "same",expected[startOffset + i],buf[i]);
    }
  }
  in.close();
}
