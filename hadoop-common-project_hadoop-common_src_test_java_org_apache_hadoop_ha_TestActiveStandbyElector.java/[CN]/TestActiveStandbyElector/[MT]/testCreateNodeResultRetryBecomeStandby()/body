{
  elector.joinElection(data);
  elector.processResult(Code.CONNECTIONLOSS.intValue(),zkLockPathName,null,zkLockPathName);
  elector.processResult(Code.NODEEXISTS.intValue(),zkLockPathName,null,zkLockPathName);
  Mockito.verify(mockZK,Mockito.times(1)).exists(zkLockPathName,true,elector,null);
  Stat stat=new Stat();
  stat.setEphemeralOwner(0);
  Mockito.when(mockZK.getSessionId()).thenReturn(1L);
  elector.processResult(Code.OK.intValue(),zkLockPathName,null,stat);
  Mockito.verify(mockApp,Mockito.times(1)).becomeStandby();
  Mockito.verify(mockZK,Mockito.times(1)).exists(zkLockPathName,true,elector,null);
}
