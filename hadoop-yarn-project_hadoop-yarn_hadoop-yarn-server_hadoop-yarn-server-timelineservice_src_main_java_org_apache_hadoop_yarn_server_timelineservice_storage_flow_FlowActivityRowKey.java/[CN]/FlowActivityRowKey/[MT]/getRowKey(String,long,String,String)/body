{
  eventTs=TimelineStorageUtils.getTopOfTheDayTimestamp(eventTs);
  return Separator.QUALIFIERS.join(Bytes.toBytes(Separator.QUALIFIERS.encode(clusterId)),Bytes.toBytes(TimelineStorageUtils.invertLong(eventTs)),Bytes.toBytes(Separator.QUALIFIERS.encode(userId)),Bytes.toBytes(Separator.QUALIFIERS.encode(flowName)));
}
