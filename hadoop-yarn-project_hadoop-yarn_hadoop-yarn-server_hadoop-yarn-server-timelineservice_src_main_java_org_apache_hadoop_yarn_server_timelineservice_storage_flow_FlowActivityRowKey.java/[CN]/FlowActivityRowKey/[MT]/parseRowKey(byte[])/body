{
  byte[][] rowKeyComponents=Separator.QUALIFIERS.split(rowKey);
  if (rowKeyComponents.length < 4) {
    throw new IllegalArgumentException("the row key is not valid for " + "a flow activity");
  }
  String clusterId=Separator.QUALIFIERS.decode(Bytes.toString(rowKeyComponents[0]));
  long dayTs=TimelineStorageUtils.invertLong(Bytes.toLong(rowKeyComponents[1]));
  String userId=Separator.QUALIFIERS.decode(Bytes.toString(rowKeyComponents[2]));
  String flowName=Separator.QUALIFIERS.decode(Bytes.toString(rowKeyComponents[3]));
  return new FlowActivityRowKey(clusterId,dayTs,userId,flowName);
}
