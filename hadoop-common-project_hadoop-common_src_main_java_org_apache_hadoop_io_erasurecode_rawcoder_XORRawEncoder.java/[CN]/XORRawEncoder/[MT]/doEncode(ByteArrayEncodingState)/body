{
  int dataLen=encodingState.encodeLength;
  CoderUtil.resetOutputBuffers(encodingState.outputs,encodingState.outputOffsets,dataLen);
  byte[] output=encodingState.outputs[0];
  int iIdx, oIdx;
  for (iIdx=encodingState.inputOffsets[0], oIdx=encodingState.outputOffsets[0]; iIdx < encodingState.inputOffsets[0] + dataLen; iIdx++, oIdx++) {
    output[oIdx]=encodingState.inputs[0][iIdx];
  }
  for (int i=1; i < encodingState.inputs.length; i++) {
    for (iIdx=encodingState.inputOffsets[i], oIdx=encodingState.outputOffsets[0]; iIdx < encodingState.inputOffsets[i] + dataLen; iIdx++, oIdx++) {
      output[oIdx]^=encodingState.inputs[i][iIdx];
    }
  }
}
