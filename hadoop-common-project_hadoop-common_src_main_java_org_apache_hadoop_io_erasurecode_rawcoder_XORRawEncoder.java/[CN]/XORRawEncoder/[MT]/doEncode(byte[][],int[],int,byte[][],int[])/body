{
  byte[] output=outputs[0];
  resetBuffer(output,outputOffsets[0],dataLen);
  int iIdx, oIdx;
  for (iIdx=inputOffsets[0], oIdx=outputOffsets[0]; iIdx < inputOffsets[0] + dataLen; iIdx++, oIdx++) {
    output[oIdx]=inputs[0][iIdx];
  }
  for (int i=1; i < inputs.length; i++) {
    for (iIdx=inputOffsets[i], oIdx=outputOffsets[0]; iIdx < inputOffsets[i] + dataLen; iIdx++, oIdx++) {
      output[oIdx]^=inputs[i][iIdx];
    }
  }
}
