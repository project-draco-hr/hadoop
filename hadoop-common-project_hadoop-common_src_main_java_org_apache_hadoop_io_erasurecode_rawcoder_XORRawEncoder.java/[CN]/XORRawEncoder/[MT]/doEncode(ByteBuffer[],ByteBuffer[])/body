{
  ByteBuffer output=outputs[0];
  resetBuffer(output);
  int iIdx, oIdx;
  for (iIdx=inputs[0].position(), oIdx=output.position(); iIdx < inputs[0].limit(); iIdx++, oIdx++) {
    output.put(oIdx,inputs[0].get(iIdx));
  }
  for (int i=1; i < inputs.length; i++) {
    for (iIdx=inputs[i].position(), oIdx=output.position(); iIdx < inputs[i].limit(); iIdx++, oIdx++) {
      output.put(oIdx,(byte)(output.get(oIdx) ^ inputs[i].get(iIdx)));
    }
  }
}
