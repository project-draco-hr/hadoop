{
  TaskSchedulingContext t1=getTSC(q1.getQueueSchedulingContext());
  TaskSchedulingContext t2=getTSC(q2.getQueueSchedulingContext());
  double r1=(0 == t1.getCapacity()) ? 1.0f : (double)t1.getNumSlotsOccupied() / (double)t1.getCapacity();
  double r2=(0 == t2.getCapacity()) ? 1.0f : (double)t2.getNumSlotsOccupied() / (double)t2.getCapacity();
  if (r1 < r2)   return -1;
 else   if (r1 > r2)   return 1;
 else   return 0;
}
