    setLocation(other.getLocation());
  }
  if (other.hasHostName()) {
    setHostName(other.getHostName());
  }
  if (other.hasAdminState()) {
    setAdminState(other.getAdminState());
  }
  this.mergeUnknownFields(other.getUnknownFields());
  return this;
}
