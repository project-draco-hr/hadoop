{
  if (other == org.apache.hadoop.hdfs.protocol.proto.HdfsProtos.DatanodeInfoProto.getDefaultInstance())   return this;
  if (other.hasId()) {
    mergeId(other.getId());
  }
  if (other.hasCapacity()) {
    setCapacity(other.getCapacity());
  }
  if (other.hasDfsUsed()) {
    setDfsUsed(other.getDfsUsed());
  }
