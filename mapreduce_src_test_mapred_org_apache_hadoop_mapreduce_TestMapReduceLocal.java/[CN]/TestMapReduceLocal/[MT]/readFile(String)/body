{
  DataInputStream f=localFs.open(new Path(TEST_ROOT_DIR + "/" + name));
  BufferedReader b=new BufferedReader(new InputStreamReader(f));
  StringBuilder result=new StringBuilder();
  String line=b.readLine();
  while (line != null) {
    result.append(line);
    result.append('\n');
    line=b.readLine();
  }
  b.close();
  return result.toString();
}
