{
  if (rawEncoder == null) {
    rawEncoder=createRawEncoder(CommonConfigurationKeys.IO_ERASURECODE_CODEC_RS_RAWCODER_KEY,getNumDataUnits(),getNumParityUnits());
    if (rawEncoder == null) {
      rawEncoder=new RSRawEncoder(getNumDataUnits(),getNumParityUnits());
    }
  }
  return rawEncoder;
}
