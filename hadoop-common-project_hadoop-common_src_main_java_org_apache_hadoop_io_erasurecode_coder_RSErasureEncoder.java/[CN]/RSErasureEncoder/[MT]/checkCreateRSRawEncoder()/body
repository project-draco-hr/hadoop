{
  if (rawEncoder == null) {
    rawEncoder=createRawEncoder(CommonConfigurationKeys.IO_ERASURECODE_CODEC_RS_RAWCODER_KEY);
    if (rawEncoder == null) {
      rawEncoder=new JRSRawEncoder();
    }
    rawEncoder.initialize(getNumDataUnits(),getNumParityUnits(),getChunkSize());
  }
  return rawEncoder;
}
