{
  long effectiveBytesPerSec=scannedBytesSum / (SECONDS_PER_MINUTE * MINUTES_PER_HOUR);
  boolean shouldScan=effectiveBytesPerSec <= targetBytesPerSec;
  LOG.trace("calculateShouldScan: effectiveBytesPerSec = {}, and " + "targetBytesPerSec = {}.  shouldScan = {}",effectiveBytesPerSec,targetBytesPerSec,shouldScan);
  return shouldScan;
}
