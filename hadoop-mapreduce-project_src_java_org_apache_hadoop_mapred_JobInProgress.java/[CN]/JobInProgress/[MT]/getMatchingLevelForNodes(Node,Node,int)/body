{
  int count=0;
  int level1=n1.getLevel(), level2=n2.getLevel();
  while (n1 != null && level1 > level2) {
    n1=n1.getParent();
    level1--;
    count++;
  }
  while (n2 != null && level2 > level1) {
    n2=n2.getParent();
    level2--;
    count++;
  }
  do {
    if (n1.equals(n2) || count >= maxLevel) {
      return Math.min(count,maxLevel);
    }
    ++count;
    n1=n1.getParent();
    n2=n2.getParent();
  }
 while (n1 != null);
  return maxLevel;
}
