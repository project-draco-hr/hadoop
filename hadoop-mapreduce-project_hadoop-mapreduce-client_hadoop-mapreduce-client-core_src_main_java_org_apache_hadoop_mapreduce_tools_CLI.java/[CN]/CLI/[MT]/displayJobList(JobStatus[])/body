{
  System.out.println("Total jobs:" + jobs.length);
  System.out.println("JobId\tState\tStartTime\t" + "UserName\tQueue\tPriority\tMaps\tReduces\tUsedContainers\t" + "RsvdContainers\tUsedMem\tRsvdMem\tNeededMem\tAM info");
  for (  JobStatus job : jobs) {
    TaskReport[] mapReports=cluster.getJob(job.getJobID()).getTaskReports(TaskType.MAP);
    TaskReport[] reduceReports=cluster.getJob(job.getJobID()).getTaskReports(TaskType.REDUCE);
    System.out.printf("%s\t%s\t%d\t%s\t%s\t%s\t%d\t%d\t%d\t%d\t%dM\t%dM\t%dM\t%s\n",job.getJobID().toString(),job.getState(),job.getStartTime(),job.getUsername(),job.getQueue(),job.getPriority().name(),mapReports.length,reduceReports.length,job.getNumUsedSlots(),job.getNumReservedSlots(),job.getUsedMem(),job.getReservedMem(),job.getNeededMem(),job.getSchedulingInfo());
  }
}
