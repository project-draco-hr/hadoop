{
  writer.println("Total jobs:" + jobs.length);
  writer.printf(headerPattern,"JobId","State","StartTime","UserName","Queue","Priority","UsedContainers","RsvdContainers","UsedMem","RsvdMem","NeededMem","AM info");
  for (  JobStatus job : jobs) {
    writer.printf(dataPattern,job.getJobID().toString(),job.getState(),job.getStartTime(),job.getUsername(),job.getQueue(),job.getPriority().name(),job.getNumUsedSlots(),job.getNumReservedSlots(),job.getUsedMem(),job.getReservedMem(),job.getNeededMem(),job.getSchedulingInfo());
  }
  writer.flush();
}
