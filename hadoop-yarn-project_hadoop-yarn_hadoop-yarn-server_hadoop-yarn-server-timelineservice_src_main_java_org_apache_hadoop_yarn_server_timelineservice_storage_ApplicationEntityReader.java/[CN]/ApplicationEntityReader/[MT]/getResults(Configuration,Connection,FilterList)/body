{
  Scan scan=new Scan();
  if (flowRunId != null) {
    scan.setRowPrefixFilter(ApplicationRowKey.getRowKeyPrefix(clusterId,userId,flowName,flowRunId));
  }
 else {
    scan.setRowPrefixFilter(ApplicationRowKey.getRowKeyPrefix(clusterId,userId,flowName));
  }
  FilterList newList=new FilterList();
  newList.addFilter(new PageFilter(limit));
  if (filterList != null && !filterList.getFilters().isEmpty()) {
    newList.addFilter(filterList);
  }
  scan.setFilter(newList);
  return table.getResultScanner(hbaseConf,conn,scan);
}
