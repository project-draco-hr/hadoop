{
  Scan scan=new Scan();
  if (flowRunId != null) {
    scan.setRowPrefixFilter(ApplicationRowKey.getRowKeyPrefix(clusterId,userId,flowId,flowRunId));
  }
 else {
    scan.setRowPrefixFilter(ApplicationRowKey.getRowKeyPrefix(clusterId,userId,flowId));
  }
  scan.setFilter(new PageFilter(limit));
  return table.getResultScanner(hbaseConf,conn,scan);
}
