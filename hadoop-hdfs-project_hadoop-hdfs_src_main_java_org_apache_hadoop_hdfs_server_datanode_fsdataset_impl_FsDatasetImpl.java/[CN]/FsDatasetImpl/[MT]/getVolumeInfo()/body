{
  Collection<VolumeInfo> info=new ArrayList<VolumeInfo>();
  for (  FsVolumeImpl volume : getVolumes()) {
    long used=0;
    long free=0;
    try {
      used=volume.getDfsUsed();
      free=volume.getAvailable();
    }
 catch (    IOException e) {
      LOG.warn(e.getMessage());
      used=0;
      free=0;
    }
    info.add(new VolumeInfo(volume,used,free));
  }
  return info;
}
