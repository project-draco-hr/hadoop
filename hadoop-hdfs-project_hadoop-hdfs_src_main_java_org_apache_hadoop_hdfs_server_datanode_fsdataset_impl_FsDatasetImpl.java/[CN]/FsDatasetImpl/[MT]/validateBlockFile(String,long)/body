{
  final ReplicaInfo r;
  try (AutoCloseableLock lock=datasetLock.acquire()){
    r=volumeMap.get(bpid,blockId);
  }
   if (r != null) {
    if (r.blockDataExists()) {
      return r;
    }
    datanode.checkDiskErrorAsync();
  }
  if (LOG.isDebugEnabled()) {
    LOG.debug("blockId=" + blockId + ", replica="+ r);
  }
  return null;
}
