{
  final File f;
  try (AutoCloseableLock lock=datasetLock.acquire()){
    f=getFile(bpid,blockId,false);
  }
   if (f != null) {
    if (f.exists())     return f;
    datanode.checkDiskErrorAsync();
  }
  if (LOG.isDebugEnabled()) {
    LOG.debug("blockId=" + blockId + ", f="+ f);
  }
  return null;
}
