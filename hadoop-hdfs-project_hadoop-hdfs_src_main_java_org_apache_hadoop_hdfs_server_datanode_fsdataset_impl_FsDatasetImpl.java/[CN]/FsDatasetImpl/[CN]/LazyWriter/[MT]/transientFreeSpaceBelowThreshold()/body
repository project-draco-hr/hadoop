{
  long free=0;
  long capacity=0;
  for (  FsVolumeImpl v : volumes.volumes) {
    if (v.isTransientStorage()) {
      capacity+=v.getCapacity();
      free+=v.getAvailable();
    }
  }
  if (capacity == 0) {
    return false;
  }
  int percentFree=(int)(free * 100 / capacity);
  return percentFree < LOW_WATERMARK_FREE_SPACE_PERCENT || free < (estimateBlockSize * LOW_WATERMARK_FREE_SPACE_REPLICAS);
}
