{
  long free=0;
  long capacity=0;
  float percentFree=0.0f;
  for (  FsVolumeImpl v : volumes.volumes) {
    if (v.isTransientStorage()) {
      capacity+=v.getCapacity();
      free+=v.getAvailable();
    }
  }
  if (capacity == 0) {
    return false;
  }
  percentFree=(float)((double)free * 100 / capacity);
  return (percentFree < lowWatermarkFreeSpacePercentage) || (free < lowWatermarkFreeSpaceBytes);
}
