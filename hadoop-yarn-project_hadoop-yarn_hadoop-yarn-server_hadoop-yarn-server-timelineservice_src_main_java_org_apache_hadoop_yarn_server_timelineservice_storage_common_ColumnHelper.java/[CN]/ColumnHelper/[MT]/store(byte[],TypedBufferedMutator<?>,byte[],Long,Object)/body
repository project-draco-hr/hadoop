{
  if ((rowKey == null) || (columnQualifier == null) || (inputValue == null)) {
    return;
  }
  Put p=new Put(rowKey);
  if (timestamp == null) {
    p.addColumn(columnFamilyBytes,columnQualifier,GenericObjectMapper.write(inputValue));
  }
 else {
    p.addColumn(columnFamilyBytes,columnQualifier,timestamp,GenericObjectMapper.write(inputValue));
  }
  tableMutator.mutate(p);
}
