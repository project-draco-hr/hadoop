{
  AppLogAggregator aggregator=this.appLogAggregators.get(appId);
  if (aggregator == null) {
    LOG.warn("Log aggregation is not initialized for " + appId + ", did it fail to start?");
    this.dispatcher.getEventHandler().handle(new ApplicationEvent(appId,ApplicationEventType.APPLICATION_LOG_HANDLING_FAILED));
    return;
  }
  aggregator.finishLogAggregation();
}
