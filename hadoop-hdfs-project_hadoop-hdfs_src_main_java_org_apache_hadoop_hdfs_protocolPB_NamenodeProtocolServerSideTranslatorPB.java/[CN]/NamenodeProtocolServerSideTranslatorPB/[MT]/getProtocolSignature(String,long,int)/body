{
  if (!protocol.equals(RPC.getProtocolName(NamenodeProtocolPB.class))) {
    throw new IOException("Namenode Serverside implements " + RPC.getProtocolName(NamenodeProtocolPB.class) + ". The following requested protocol is unknown: "+ protocol);
  }
  return ProtocolSignature.getProtocolSignature(clientMethodsHash,RPC.getProtocolVersion(NamenodeProtocolPB.class),NamenodeProtocolPB.class);
}
