{
  for (  String queue : jobQueues.keySet()) {
    QueueInfo queueInfo=jobQueues.get(queue);
synchronized (queueInfo) {
      queueInfo.maxUsersAllowedToInitialize=getMaxUsersToInit(schedulerConf,queue);
      queueInfo.maxJobsPerUserToInitialize=schedulerConf.getMaxJobsPerUserToInitialize(queue);
    }
  }
}
