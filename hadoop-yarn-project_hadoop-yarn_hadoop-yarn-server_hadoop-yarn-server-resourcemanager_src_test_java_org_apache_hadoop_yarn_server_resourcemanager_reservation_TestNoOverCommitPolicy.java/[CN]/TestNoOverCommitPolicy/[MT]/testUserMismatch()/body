{
  int[] f=generateData(3600,(int)(0.5 * totCont));
  ReservationDefinition rDef=ReservationSystemTestUtil.createSimpleReservationDefinition(initTime,initTime + f.length + 1,f.length);
  ReservationId rid=ReservationSystemTestUtil.getNewReservationId();
  plan.addReservation(new InMemoryReservationAllocation(rid,rDef,"u1","dedicated",initTime,initTime + f.length,ReservationSystemTestUtil.generateAllocation(initTime,step,f),res,minAlloc),false);
  plan.updateReservation(new InMemoryReservationAllocation(rid,rDef,"u2","dedicated",initTime,initTime + f.length,ReservationSystemTestUtil.generateAllocation(initTime,step,f),res,minAlloc));
}
