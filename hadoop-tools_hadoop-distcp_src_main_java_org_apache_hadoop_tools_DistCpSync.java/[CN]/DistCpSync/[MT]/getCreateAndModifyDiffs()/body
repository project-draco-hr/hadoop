{
  List<DiffInfo> createDiff=diffMap.get(SnapshotDiffReport.DiffType.CREATE);
  List<DiffInfo> modifyDiff=diffMap.get(SnapshotDiffReport.DiffType.MODIFY);
  List<DiffInfo> diffs=new ArrayList<>(createDiff.size() + modifyDiff.size());
  diffs.addAll(createDiff);
  diffs.addAll(modifyDiff);
  return diffs.toArray(new DiffInfo[diffs.size()]);
}
