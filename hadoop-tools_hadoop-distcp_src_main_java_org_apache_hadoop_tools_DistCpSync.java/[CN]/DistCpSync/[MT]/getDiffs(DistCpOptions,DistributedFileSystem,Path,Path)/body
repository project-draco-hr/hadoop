{
  try {
    SnapshotDiffReport sourceDiff=fs.getSnapshotDiffReport(sourceDir,inputOptions.getFromSnapshot(),inputOptions.getToSnapshot());
    return DiffInfo.getDiffs(sourceDiff,targetDir);
  }
 catch (  IOException e) {
    DistCp.LOG.warn("Failed to compute snapshot diff on " + sourceDir,e);
  }
  return null;
}
