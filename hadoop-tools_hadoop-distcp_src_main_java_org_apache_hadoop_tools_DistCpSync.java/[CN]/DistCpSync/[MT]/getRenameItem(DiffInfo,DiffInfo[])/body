{
  for (  DiffInfo renameItem : renameDiffArray) {
    if (diff.source.equals(renameItem.source)) {
      if (diff.getType() == SnapshotDiffReport.DiffType.MODIFY) {
        return renameItem;
      }
    }
 else     if (isParentOf(renameItem.source,diff.source)) {
      return renameItem;
    }
  }
  return null;
}
