{
  for (  String key : isRelatedTo.keySet()) {
    byte[] columnName=TimelineWriterUtils.join(TimelineEntitySchemaConstants.ROW_KEY_SEPARATOR_BYTES,columnPrefix,Bytes.toBytes(key));
    byte[] value=infoValues.get(columnName);
    assertNotNull(value);
    String isRelatedToEntities=GenericObjectMapper.read(value).toString();
    assertNotNull(isRelatedToEntities);
    assertEquals(TimelineWriterUtils.getValueAsString(TimelineEntitySchemaConstants.ROW_KEY_SEPARATOR,isRelatedTo.get(key)),isRelatedToEntities);
  }
}
