{
  sb.append("{UCState=").append(blockUCState).append(", truncateBlock=").append(truncateBlock).append(", primaryNodeIndex=").append(primaryNodeIndex).append(", replicas=[");
  int i=0;
  for (  ReplicaUnderConstruction r : replicas) {
    r.appendStringTo(sb);
    if (++i < replicas.length) {
      sb.append(", ");
    }
  }
  sb.append("]}");
}
