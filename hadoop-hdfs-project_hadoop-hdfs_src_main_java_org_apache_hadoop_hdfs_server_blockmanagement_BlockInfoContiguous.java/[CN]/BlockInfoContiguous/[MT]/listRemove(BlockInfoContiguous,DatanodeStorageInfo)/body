{
  if (head == null)   return null;
  int dnIndex=this.findStorageInfo(storage);
  if (dnIndex < 0)   return head;
  BlockInfoContiguous next=this.getNext(dnIndex);
  BlockInfoContiguous prev=this.getPrevious(dnIndex);
  this.setNext(dnIndex,null);
  this.setPrevious(dnIndex,null);
  if (prev != null)   prev.setNext(prev.findStorageInfo(storage),next);
  if (next != null)   next.setPrevious(next.findStorageInfo(storage),prev);
  if (this == head)   head=next;
  return head;
}
