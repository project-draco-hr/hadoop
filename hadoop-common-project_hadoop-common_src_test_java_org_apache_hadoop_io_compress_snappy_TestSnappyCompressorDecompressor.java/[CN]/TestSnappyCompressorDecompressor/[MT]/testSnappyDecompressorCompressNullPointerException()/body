{
  try {
    SnappyDecompressor decompressor=new SnappyDecompressor();
    byte[] bytes=BytesGenerator.get(1024 * 6);
    decompressor.setInput(bytes,0,bytes.length);
    decompressor.decompress(null,0,0);
