{
  int numEdits=0;
  int logVersion=edits.getVersion();
  try {
    numEdits=loadEditRecords(logVersion,edits,false,expectedStartingTxId);
  }
  finally {
    fsNamesys.setBlockTotal();
    fsNamesys.notifyGenStampUpdate(maxGenStamp);
    if (closeOnExit) {
      edits.close();
    }
  }
  return numEdits;
}
