{
  fsNamesys.writeLock();
  try {
    long startTime=now();
    long numEdits=loadEditRecords(edits,false,expectedStartingTxId,recovery);
    FSImage.LOG.info("Edits file " + edits.getName() + " of size "+ edits.length()+ " edits # "+ numEdits+ " loaded in "+ (now() - startTime) / 1000 + " seconds");
    return numEdits;
  }
  finally {
    edits.close();
    fsNamesys.writeUnlock();
  }
}
