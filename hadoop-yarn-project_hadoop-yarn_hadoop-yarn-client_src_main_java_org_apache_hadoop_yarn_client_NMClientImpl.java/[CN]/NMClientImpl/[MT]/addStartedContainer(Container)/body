{
  if (startedContainers.containsKey(container.getId())) {
    throw RPCUtil.getRemoteException("Container " + container.getId() + " is already started");
  }
  StartedContainer startedContainer=new StartedContainer(container.getId(),container.getNodeId(),container.getContainerToken());
  startedContainers.put(startedContainer.getContainerId(),startedContainer);
  return startedContainer;
}
