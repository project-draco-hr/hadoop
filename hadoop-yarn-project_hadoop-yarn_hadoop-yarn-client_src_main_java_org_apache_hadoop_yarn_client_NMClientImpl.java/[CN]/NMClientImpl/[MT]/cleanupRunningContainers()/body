{
  for (  StartedContainer startedContainer : startedContainers.values()) {
    try {
      stopContainer(startedContainer.getContainerId(),startedContainer.getNodeId(),startedContainer.getContainerToken());
    }
 catch (    YarnRemoteException e) {
      LOG.error("Failed to stop Container " + startedContainer.getContainerId() + "when stopping NMClientImpl");
    }
catch (    IOException e) {
      LOG.error("Failed to stop Container " + startedContainer.getContainerId() + "when stopping NMClientImpl");
    }
  }
}
