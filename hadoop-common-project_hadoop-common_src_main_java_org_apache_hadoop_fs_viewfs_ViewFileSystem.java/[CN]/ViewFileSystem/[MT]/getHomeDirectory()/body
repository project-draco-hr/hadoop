{
  if (homeDir == null) {
    String base=fsState.getHomeDirPrefixValue();
    if (base == null) {
      base="/user";
    }
    homeDir=(base.equals("/") ? this.makeQualified(new Path(base + ugi.getShortUserName())) : this.makeQualified(new Path(base + "/" + ugi.getShortUserName())));
  }
  return homeDir;
}
