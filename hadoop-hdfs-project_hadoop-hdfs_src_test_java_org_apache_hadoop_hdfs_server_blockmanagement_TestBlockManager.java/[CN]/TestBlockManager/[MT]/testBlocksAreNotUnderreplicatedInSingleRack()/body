{
  List<DatanodeDescriptor> nodes=ImmutableList.of(DFSTestUtil.getDatanodeDescriptor("1.1.1.1","/rackA"),DFSTestUtil.getDatanodeDescriptor("2.2.2.2","/rackA"),DFSTestUtil.getDatanodeDescriptor("3.3.3.3","/rackA"),DFSTestUtil.getDatanodeDescriptor("4.4.4.4","/rackA"),DFSTestUtil.getDatanodeDescriptor("5.5.5.5","/rackA"),DFSTestUtil.getDatanodeDescriptor("6.6.6.6","/rackA"));
  addNodes(nodes);
  List<DatanodeDescriptor> origNodes=nodes.subList(0,3);
  ;
  for (int i=0; i < NUM_TEST_ITERS; i++) {
    doTestSingleRackClusterIsSufficientlyReplicated(i,origNodes);
  }
}
