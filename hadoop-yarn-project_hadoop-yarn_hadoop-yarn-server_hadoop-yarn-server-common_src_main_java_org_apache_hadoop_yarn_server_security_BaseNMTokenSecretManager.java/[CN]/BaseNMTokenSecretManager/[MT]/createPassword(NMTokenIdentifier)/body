{
  if (LOG.isDebugEnabled()) {
    LOG.debug("creating password for " + identifier.getApplicationAttemptId() + " for user "+ identifier.getApplicationSubmitter()+ " to run on NM "+ identifier.getNodeId());
  }
  readLock.lock();
  try {
    return createPassword(identifier.getBytes(),currentMasterKey.getSecretKey());
  }
  finally {
    readLock.unlock();
  }
}
