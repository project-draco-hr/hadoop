{
  HistoryFileInfo fileInfo=jobListCache.get(jobId);
  if (fileInfo != null) {
    return fileInfo;
  }
  scanIntermediateDirectory();
  fileInfo=jobListCache.get(jobId);
  if (fileInfo != null) {
    return fileInfo;
  }
  fileInfo=scanOldDirsForJob(jobId);
  if (fileInfo != null) {
    return fileInfo;
  }
  return null;
}
