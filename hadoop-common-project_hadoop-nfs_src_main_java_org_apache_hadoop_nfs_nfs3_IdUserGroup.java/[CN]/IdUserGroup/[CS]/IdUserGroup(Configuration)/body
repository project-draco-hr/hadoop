{
  long updateTime=conf.getLong(NFS_USERUPDATE_MILLY,TIMEOUT_DEFAULT);
  if (updateTime < TIMEOUT_MIN) {
    LOG.info("User configured user account update time is less" + " than 1 minute. Use 1 minute instead.");
    timeout=TIMEOUT_MIN;
  }
 else {
    timeout=updateTime;
  }
  String staticFilePath=conf.get(NFS_STATIC_MAPPING_FILE_KEY,NFS_STATIC_MAPPING_FILE_DEFAULT);
  staticMappingFile=new File(staticFilePath);
  updateMaps();
}
