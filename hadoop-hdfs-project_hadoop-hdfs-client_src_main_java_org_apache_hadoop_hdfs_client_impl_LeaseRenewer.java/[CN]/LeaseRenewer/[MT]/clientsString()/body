{
  if (dfsclients.isEmpty()) {
    return "[]";
  }
 else {
    final StringBuilder b=new StringBuilder("[").append(dfsclients.get(0).getClientName());
    for (int i=1; i < dfsclients.size(); i++) {
      b.append(", ").append(dfsclients.get(i).getClientName());
    }
    return b.append("]").toString();
  }
}
