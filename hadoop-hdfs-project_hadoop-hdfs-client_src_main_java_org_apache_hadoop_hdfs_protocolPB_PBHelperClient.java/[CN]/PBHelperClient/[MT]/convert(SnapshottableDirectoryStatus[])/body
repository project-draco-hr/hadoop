{
  if (status == null)   return null;
  SnapshottableDirectoryStatusProto[] protos=new SnapshottableDirectoryStatusProto[status.length];
  for (int i=0; i < status.length; i++) {
    protos[i]=convert(status[i]);
  }
  List<SnapshottableDirectoryStatusProto> protoList=Arrays.asList(protos);
  return SnapshottableDirectoryListingProto.newBuilder().addAllSnapshottableDirListing(protoList).build();
}
