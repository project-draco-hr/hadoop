{
  DataEncryptionKeyProto.Builder b=DataEncryptionKeyProto.newBuilder().setKeyId(bet.keyId).setBlockPoolId(bet.blockPoolId).setNonce(getByteString(bet.nonce)).setEncryptionKey(getByteString(bet.encryptionKey)).setExpiryDate(bet.expiryDate);
  if (bet.encryptionAlgorithm != null) {
    b.setEncryptionAlgorithm(bet.encryptionAlgorithm);
  }
  return b.build();
}
