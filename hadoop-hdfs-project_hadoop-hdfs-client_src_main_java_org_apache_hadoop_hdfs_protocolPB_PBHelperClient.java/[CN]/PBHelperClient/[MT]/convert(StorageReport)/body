{
  StorageReportProto.Builder builder=StorageReportProto.newBuilder().setBlockPoolUsed(r.getBlockPoolUsed()).setCapacity(r.getCapacity()).setDfsUsed(r.getDfsUsed()).setRemaining(r.getRemaining()).setStorageUuid(r.getStorage().getStorageID()).setStorage(convert(r.getStorage())).setNonDfsUsed(r.getNonDfsUsed());
  return builder.build();
}
