{
  List<ResourceRequest> askCopy=new ArrayList<ResourceRequest>();
  for (  ResourceRequest req : ask) {
    ResourceRequest reqCopy=ResourceRequest.newInstance(req.getPriority(),req.getResourceName(),req.getCapability(),req.getNumContainers(),req.getRelaxLocality());
    askCopy.add(reqCopy);
  }
  lastAsk=ask;
  lastRelease=release;
  lastIncrease=increaseRequests;
  lastDecrease=decreaseRequests;
  lastBlacklistAdditions=blacklistAdditions;
  lastBlacklistRemovals=blacklistRemovals;
  return super.allocate(applicationAttemptId,askCopy,release,blacklistAdditions,blacklistRemovals,increaseRequests,decreaseRequests);
}
