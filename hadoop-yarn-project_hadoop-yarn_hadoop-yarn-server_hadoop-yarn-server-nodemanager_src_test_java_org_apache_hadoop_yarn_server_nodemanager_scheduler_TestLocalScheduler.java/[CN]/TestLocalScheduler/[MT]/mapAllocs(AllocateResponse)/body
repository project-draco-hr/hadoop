{
  Map<NodeId,List<ContainerId>> allocs=new HashMap<>();
  for (  Container c : allocateResponse.getAllocatedContainers()) {
    List<ContainerId> cIds=allocs.get(c.getNodeId());
    if (cIds == null) {
      cIds=new ArrayList<>();
      allocs.put(c.getNodeId(),cIds);
    }
    cIds.add(c.getId());
  }
  return allocs;
}
