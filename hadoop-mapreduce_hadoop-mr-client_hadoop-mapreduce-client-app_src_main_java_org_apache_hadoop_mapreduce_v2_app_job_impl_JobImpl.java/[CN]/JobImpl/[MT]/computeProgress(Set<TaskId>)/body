{
  readLock.lock();
  try {
    float progress=0;
    for (    TaskId taskId : taskIds) {
      Task task=tasks.get(taskId);
      progress+=task.getProgress();
    }
    int taskIdsSize=taskIds.size();
    if (taskIdsSize != 0) {
      progress=progress / taskIdsSize;
    }
    return progress;
  }
  finally {
    readLock.unlock();
  }
}
