{
synchronized (pendingReplications) {
    out.println("Metasave: Blocks being replicated: " + pendingReplications.size());
    for (    Map.Entry<BlockInfo,PendingBlockInfo> entry : pendingReplications.entrySet()) {
      PendingBlockInfo pendingBlock=entry.getValue();
      Block block=entry.getKey();
      out.println(block + " StartTime: " + new Time(pendingBlock.timeStamp)+ " NumReplicaInProgress: "+ pendingBlock.getNumReplicas());
    }
  }
}
