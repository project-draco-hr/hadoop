{
  cleanupUserAppCache(user);
  cleanupUserFileCache(user);
  cleanupLogDirs(user);
  String[] files={"launch_container.sh","container_tokens","touch-file"};
  Path ws=new Path(workSpace.toURI());
  for (  String file : files) {
    File f=new File(workSpace,file);
    if (f.exists()) {
      exec.deleteAsUser(new DeletionAsUserContext.Builder().setUser(user).setSubDir(new Path(file)).setBasedirs(ws).build());
    }
  }
}
