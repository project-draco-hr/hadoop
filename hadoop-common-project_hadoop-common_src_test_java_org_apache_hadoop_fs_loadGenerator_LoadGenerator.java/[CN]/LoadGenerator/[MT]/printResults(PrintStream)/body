{
  out.println("Result of running LoadGenerator against fileSystem: " + FileContext.getFileContext().getDefaultFileSystem().getUri());
  if (numOfOps[OPEN] != 0) {
    out.println("Average open execution time: " + (double)executionTime[OPEN] / numOfOps[OPEN] + "ms");
  }
  if (numOfOps[LIST] != 0) {
    out.println("Average list execution time: " + (double)executionTime[LIST] / numOfOps[LIST] + "ms");
  }
  if (numOfOps[DELETE] != 0) {
    out.println("Average deletion execution time: " + (double)executionTime[DELETE] / numOfOps[DELETE] + "ms");
    out.println("Average create execution time: " + (double)executionTime[CREATE] / numOfOps[CREATE] + "ms");
    out.println("Average write_close execution time: " + (double)executionTime[WRITE_CLOSE] / numOfOps[WRITE_CLOSE] + "ms");
  }
  if (totalTime != 0) {
    out.println("Average operations per second: " + (double)totalOps / totalTime + "ops/s");
  }
  out.println();
}
