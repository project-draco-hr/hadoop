{
  if (priLevel >= 0 && priLevel < LEVEL && priorityQueues.get(priLevel).remove(block)) {
    NameNode.blockStateChangeLog.debug("BLOCK* NameSystem.UnderReplicationBlock.remove: Removing block {}" + " from priority queue {}",block,priLevel);
    return true;
  }
 else {
    for (int i=0; i < LEVEL; i++) {
      if (i != priLevel && priorityQueues.get(i).remove(block)) {
        NameNode.blockStateChangeLog.debug("BLOCK* NameSystem.UnderReplicationBlock.remove: Removing block" + " {} from priority queue {}",block,i);
        return true;
      }
    }
  }
  return false;
}
