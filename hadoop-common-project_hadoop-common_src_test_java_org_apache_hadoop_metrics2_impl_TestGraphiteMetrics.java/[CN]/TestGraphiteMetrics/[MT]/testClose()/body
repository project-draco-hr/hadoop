{
  GraphiteSink sink=new GraphiteSink();
  final GraphiteSink.Graphite mockGraphite=makeGraphite();
  Whitebox.setInternalState(sink,"graphite",mockGraphite);
  try {
    sink.close();
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
  try {
    verify(mockGraphite).close();
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
}
