{
  if (jc.flush(ivalue)) {
    WritableUtils.cloneInto(key,jc.key());
    WritableUtils.cloneInto(value,emit(ivalue));
    return true;
  }
  jc.clear();
  K iterkey=createKey();
  final PriorityQueue<ComposableRecordReader<K,?>> q=getRecordReaderQueue();
  while (!q.isEmpty()) {
    fillJoinCollector(iterkey);
    jc.reset(iterkey);
    if (jc.flush(ivalue)) {
      WritableUtils.cloneInto(key,jc.key());
      WritableUtils.cloneInto(value,emit(ivalue));
      return true;
    }
    jc.clear();
  }
  return false;
}
