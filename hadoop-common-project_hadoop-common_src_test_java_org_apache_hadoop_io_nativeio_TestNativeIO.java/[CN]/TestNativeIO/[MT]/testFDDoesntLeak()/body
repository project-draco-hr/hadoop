{
  assumeNotWindows();
  for (int i=0; i < 10000; i++) {
    FileDescriptor fd=NativeIO.POSIX.open(new File(TEST_DIR,"testNoFdLeak").getAbsolutePath(),O_WRONLY | O_CREAT,0700);
    assertNotNull(true);
    assertTrue(fd.valid());
    FileOutputStream fos=new FileOutputStream(fd);
    fos.write("foo".getBytes());
    fos.close();
  }
}
