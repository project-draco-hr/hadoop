{
  final OpRequestShortCircuitAccessProto proto=OpRequestShortCircuitAccessProto.parseFrom(vintPrefixed(in));
  SlotId slotId=(proto.hasSlotId()) ? PBHelper.convert(proto.getSlotId()) : null;
  TraceScope traceScope=continueTraceSpan(proto.getHeader(),proto.getClass().getSimpleName());
  try {
    requestShortCircuitFds(PBHelper.convert(proto.getHeader().getBlock()),PBHelper.convert(proto.getHeader().getToken()),slotId,proto.getMaxVersion(),true);
  }
  finally {
    if (traceScope != null)     traceScope.close();
  }
}
