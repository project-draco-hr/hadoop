{
  long elapsed=Times.elapsed(10,5,true);
  Assert.assertEquals("Elapsed time is not -1",-1,elapsed);
  elapsed=Times.elapsed(10,5,false);
  Assert.assertEquals("Elapsed time is not -1",-1,elapsed);
  elapsed=Times.elapsed(Long.MAX_VALUE,0,true);
  Assert.assertEquals("Elapsed time is not -1",-1,elapsed);
}
