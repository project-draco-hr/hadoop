{
  if (nsInfo.getNamespaceID() != getNamespaceID()) {
    throw new IOException("Incompatible namespaceID for journal " + this.sd + ": NameNode has nsId "+ nsInfo.getNamespaceID()+ " but storage has nsId "+ getNamespaceID());
  }
  if (!nsInfo.getClusterID().equals(getClusterID())) {
    throw new IOException("Incompatible clusterID for journal " + this.sd + ": NameNode has clusterId '"+ nsInfo.getClusterID()+ "' but storage has clusterId '"+ getClusterID()+ "'");
  }
}
