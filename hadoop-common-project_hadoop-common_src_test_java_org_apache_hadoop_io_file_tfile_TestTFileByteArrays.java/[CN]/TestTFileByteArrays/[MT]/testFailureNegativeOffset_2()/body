{
  if (skip)   return;
  closeOutput();
  Reader reader=new Reader(fs.open(path),fs.getFileStatus(path).getLen(),conf);
  Scanner scanner=reader.createScanner();
  try {
    scanner.lowerBound("keyX".getBytes(),-1,4);
    Assert.fail("Error on handling negative offset.");
  }
 catch (  Exception e) {
  }
 finally {
    reader.close();
    scanner.close();
  }
  closeOutput();
}
