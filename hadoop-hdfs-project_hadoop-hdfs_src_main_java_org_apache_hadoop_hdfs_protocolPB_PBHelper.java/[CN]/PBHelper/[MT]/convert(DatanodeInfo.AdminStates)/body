{
switch (inAs) {
case NORMAL:
    return DatanodeInfoProto.AdminState.NORMAL;
case DECOMMISSION_INPROGRESS:
  return DatanodeInfoProto.AdminState.DECOMMISSION_INPROGRESS;
case DECOMMISSIONED:
return DatanodeInfoProto.AdminState.DECOMMISSIONED;
default :
return DatanodeInfoProto.AdminState.NORMAL;
}
}
