{
  if (sdirStatusProto == null) {
    return null;
  }
  final HdfsFileStatusProto status=sdirStatusProto.getDirStatus();
  return new SnapshottableDirectoryStatus(status.getModificationTime(),status.getAccessTime(),PBHelper.convert(status.getPermission()),status.getOwner(),status.getGroup(),status.getPath().toByteArray(),status.getFileId(),sdirStatusProto.getSnapshotNumber(),sdirStatusProto.getSnapshotQuota(),sdirStatusProto.getParentFullpath().toByteArray());
}
