{
  return DatanodeInfoProto.newBuilder().setAdminState(PBHelper.convert(info.getAdminState())).setBlockPoolUsed(info.getBlockPoolUsed()).setCapacity(info.getCapacity()).setDfsUsed(info.getDfsUsed()).setHostName(info.getHostName()).setId(PBHelper.convert((DatanodeID)info)).setLastUpdate(info.getLastUpdate()).setLocation(info.getNetworkLocation()).setRemaining(info.getRemaining()).setXceiverCount(info.getXceiverCount()).build();
}
