{
  DatanodeInfoProto.Builder builder=DatanodeInfoProto.newBuilder();
  builder.setBlockPoolUsed(info.getBlockPoolUsed());
  builder.setAdminState(PBHelper.convert(info.getAdminState()));
  builder.setCapacity(info.getCapacity()).setDfsUsed(info.getDfsUsed()).setHostName(info.getHostName()).setId(PBHelper.convert((DatanodeID)info)).setLastUpdate(info.getLastUpdate()).setLocation(info.getNetworkLocation()).setRemaining(info.getRemaining()).setXceiverCount(info.getXceiverCount()).build();
  return builder.build();
}
