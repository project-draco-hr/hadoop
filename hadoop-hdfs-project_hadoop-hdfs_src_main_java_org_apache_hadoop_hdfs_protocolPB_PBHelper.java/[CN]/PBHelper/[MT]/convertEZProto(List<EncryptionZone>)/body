{
  final ArrayList<EncryptionZoneProto> ret=Lists.newArrayListWithCapacity(ezs.size());
  for (  EncryptionZone a : ezs) {
    final EncryptionZoneProto.Builder builder=EncryptionZoneProto.newBuilder();
    builder.setPath(a.getPath());
    builder.setKeyName(a.getKeyName());
    ret.add(builder.build());
  }
  return ret;
}
