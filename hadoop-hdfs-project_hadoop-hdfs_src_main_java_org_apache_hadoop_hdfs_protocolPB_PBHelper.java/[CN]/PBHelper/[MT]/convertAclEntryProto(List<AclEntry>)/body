{
  ArrayList<AclEntryProto> r=Lists.newArrayListWithCapacity(aclSpec.size());
  for (  AclEntry e : aclSpec) {
    r.add(AclEntryProto.newBuilder().setType(convert(e.getType())).setName(e.getName()).setPermissions(convert(e.getPermission())).setScope(convert(e.getScope())).build());
  }
  return r;
}
