{
  if (info == null) {
    return null;
  }
  return HdfsProtos.FileEncryptionInfoProto.newBuilder().setSuite(convert(info.getCipherSuite())).setKey(getByteString(info.getEncryptedDataEncryptionKey())).setIv(getByteString(info.getIV())).setEzKeyVersionName(info.getEzKeyVersionName()).setKeyName(info.getKeyName()).build();
}
