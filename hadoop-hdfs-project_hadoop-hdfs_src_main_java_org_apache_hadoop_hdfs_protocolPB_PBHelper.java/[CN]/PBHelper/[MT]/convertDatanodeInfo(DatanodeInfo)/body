{
  if (di == null)   return null;
  return DatanodeInfoProto.newBuilder().setId(PBHelper.convert((DatanodeID)di)).setLocation(di.getNetworkLocation()).setHostName(di.getHostName()).setCapacity(di.getCapacity()).setDfsUsed(di.getDfsUsed()).setRemaining(di.getRemaining()).setBlockPoolUsed(di.getBlockPoolUsed()).setLastUpdate(di.getLastUpdate()).setXceiverCount(di.getXceiverCount()).setAdminState(PBHelper.convert(di.getAdminState())).build();
}
