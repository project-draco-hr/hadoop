{
  if (fs == null)   return null;
  if (fs.hasLocations()) {
    return new HdfsLocatedFileStatus(fs.getLength(),fs.getFileType().equals(FileType.IS_DIR),fs.getBlockReplication(),fs.getBlocksize(),fs.getModificationTime(),fs.getAccessTime(),PBHelper.convert(fs.getPermission()),fs.getOwner(),fs.getGroup(),fs.getFileType().equals(FileType.IS_SYMLINK) ? fs.getSymlink().toByteArray() : null,fs.getPath().toByteArray(),PBHelper.convert(fs.hasLocations() ? fs.getLocations() : null));
  }
  return new HdfsFileStatus(fs.getLength(),fs.getFileType().equals(FileType.IS_DIR),fs.getBlockReplication(),fs.getBlocksize(),fs.getModificationTime(),fs.getAccessTime(),PBHelper.convert(fs.getPermission()),fs.getOwner(),fs.getGroup(),fs.getFileType().equals(FileType.IS_SYMLINK) ? fs.getSymlink().toByteArray() : null,fs.getPath().toByteArray());
}
