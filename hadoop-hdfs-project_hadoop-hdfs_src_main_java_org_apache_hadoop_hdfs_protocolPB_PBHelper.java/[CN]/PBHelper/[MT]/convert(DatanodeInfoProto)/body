{
  DatanodeIDProto dnId=info.getId();
  return new DatanodeInfo(dnId.getName(),dnId.getStorageID(),dnId.getInfoPort(),dnId.getIpcPort(),info.getCapacity(),info.getDfsUsed(),info.getRemaining(),info.getBlockPoolUsed(),info.getLastUpdate(),info.getXceiverCount(),info.getLocation(),info.getHostName(),convert(info.getAdminState()));
}
