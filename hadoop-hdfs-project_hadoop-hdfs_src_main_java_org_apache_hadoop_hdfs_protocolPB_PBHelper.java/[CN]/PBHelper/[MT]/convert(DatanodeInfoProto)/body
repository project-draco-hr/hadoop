{
  if (di == null)   return null;
  return new DatanodeInfo(PBHelper.convert(di.getId()),di.hasLocation() ? di.getLocation() : null,di.getCapacity(),di.getDfsUsed(),di.getRemaining(),di.getBlockPoolUsed(),di.getCacheCapacity(),di.getCacheUsed(),di.getLastUpdate(),di.getXceiverCount(),PBHelper.convert(di.getAdminState()));
}
