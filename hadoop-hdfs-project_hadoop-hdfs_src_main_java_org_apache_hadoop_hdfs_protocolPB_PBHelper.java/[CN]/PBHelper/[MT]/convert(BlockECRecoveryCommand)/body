{
  BlockECRecoveryCommandProto.Builder builder=BlockECRecoveryCommandProto.newBuilder();
  Collection<BlockECRecoveryInfo> blockECRecoveryInfos=blkECRecoveryCmd.getECTasks();
  for (  BlockECRecoveryInfo blkECRecoveryInfo : blockECRecoveryInfos) {
    builder.addBlockECRecoveryinfo(convertBlockECRecoveryInfo(blkECRecoveryInfo));
  }
  return builder.build();
}
