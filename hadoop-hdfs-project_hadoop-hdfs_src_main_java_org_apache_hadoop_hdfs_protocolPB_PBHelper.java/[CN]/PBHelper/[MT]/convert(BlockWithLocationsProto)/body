{
  final List<String> datanodeUuids=b.getDatanodeUuidsList();
  final List<String> storageUuids=b.getStorageUuidsList();
  final List<StorageTypeProto> storageTypes=b.getStorageTypesList();
  return new BlockWithLocations(PBHelperClient.convert(b.getBlock()),datanodeUuids.toArray(new String[datanodeUuids.size()]),storageUuids.toArray(new String[storageUuids.size()]),PBHelperClient.convertStorageTypes(storageTypes,storageUuids.size()));
}
