{
  try {
    FileSystem fs=FileSystem.getLocal(conf);
    BloomMapFile.Writer writer=new BloomMapFile.Writer(conf,TEST_FILE,MapFile.Writer.keyClass(IntWritable.class),MapFile.Writer.valueClass(Text.class));
