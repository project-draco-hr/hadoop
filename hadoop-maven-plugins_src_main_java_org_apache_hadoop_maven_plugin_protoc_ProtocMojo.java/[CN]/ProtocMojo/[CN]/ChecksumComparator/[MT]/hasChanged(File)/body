{
  if (!file.exists()) {
    throw new FileNotFoundException("Specified protoc include or source does not exist: " + file);
  }
  if (file.isDirectory()) {
    return hasDirectoryChanged(file);
  }
 else   if (file.isFile()) {
    return hasFileChanged(file);
  }
 else {
    throw new IOException("Not a file or directory: " + file);
  }
}
