{
  File[] listing=directory.listFiles();
  boolean changed=false;
  for (  File f : listing) {
    if (f.isDirectory()) {
      if (hasDirectoryChanged(f)) {
        changed=true;
      }
    }
 else     if (f.isFile()) {
      if (hasFileChanged(f)) {
        changed=true;
      }
    }
 else {
      getLog().debug("Skipping entry that is not a file or directory: " + f);
    }
  }
  return changed;
}
