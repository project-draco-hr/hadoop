{
  String prevKey=null;
  int sum=0;
  String key=readString();
  while (key != null) {
    if (prevKey != null && !key.equals(prevKey)) {
      System.out.println(prevKey + "\t" + sum);
      sum=0;
    }
    sum+=readInt();
    prevKey=key;
    key=readString();
  }
  System.out.println(prevKey + "\t" + sum);
  System.out.flush();
}
