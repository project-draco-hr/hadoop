{
  String anyPid="9999";
  int anySignal=9;
  String[] checkProcessAliveCommand=Shell.getSignalKillCommand(anySignal,anyPid);
  String[] expectedCommand;
  if (Shell.WINDOWS) {
    expectedCommand=new String[]{Shell.WINUTILS,"task","isAlive",anyPid};
  }
 else   if (Shell.isSetsidAvailable) {
    expectedCommand=new String[]{"bash","-c","kill -9 -- -" + anyPid};
  }
 else {
    expectedCommand=new String[]{"bash","-c","kill -9 " + anyPid};
  }
  Assert.assertArrayEquals(expectedCommand,checkProcessAliveCommand);
}
