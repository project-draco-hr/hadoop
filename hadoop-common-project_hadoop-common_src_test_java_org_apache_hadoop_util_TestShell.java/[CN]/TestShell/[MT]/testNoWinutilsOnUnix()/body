{
  Assume.assumeFalse(WINDOWS);
  try {
    getWinUtilsFile();
  }
 catch (  FileNotFoundException ex) {
    assertExContains(ex,E_NOT_A_WINDOWS_SYSTEM);
  }
  try {
    getWinUtilsPath();
  }
 catch (  RuntimeException ex) {
    assertExContains(ex,E_NOT_A_WINDOWS_SYSTEM);
    if (ex.getCause() == null || !(ex.getCause() instanceof FileNotFoundException)) {
      throw ex;
    }
  }
}
