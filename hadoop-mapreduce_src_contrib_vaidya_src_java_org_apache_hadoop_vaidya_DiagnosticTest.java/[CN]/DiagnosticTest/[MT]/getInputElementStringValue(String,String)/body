{
  Element inputElement=(Element)(this._testConfigElement.getElementsByTagName("InputElement").item(0));
  Element prs=null;
  String value;
  prs=(Element)inputElement.getElementsByTagName(elementName).item(0);
  if (prs != null) {
    value=prs.getFirstChild().getNodeValue().trim();
  }
 else {
    value=defaultValue;
  }
  return value;
}
