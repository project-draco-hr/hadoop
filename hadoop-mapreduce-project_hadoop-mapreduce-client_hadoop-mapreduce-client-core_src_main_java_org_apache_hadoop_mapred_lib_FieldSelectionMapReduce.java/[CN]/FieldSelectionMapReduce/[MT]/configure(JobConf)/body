{
  this.fieldSeparator=job.get(FieldSelectionHelper.DATA_FIELD_SEPERATOR,"\t");
  this.mapOutputKeyValueSpec=job.get(FieldSelectionHelper.MAP_OUTPUT_KEY_VALUE_SPEC,"0-:");
  this.ignoreInputKey=TextInputFormat.class.getCanonicalName().equals(job.getInputFormat().getClass().getCanonicalName());
  this.reduceOutputKeyValueSpec=job.get(FieldSelectionHelper.REDUCE_OUTPUT_KEY_VALUE_SPEC,"0-:");
  parseOutputKeyValueSpec();
  LOG.info(specToString());
}
