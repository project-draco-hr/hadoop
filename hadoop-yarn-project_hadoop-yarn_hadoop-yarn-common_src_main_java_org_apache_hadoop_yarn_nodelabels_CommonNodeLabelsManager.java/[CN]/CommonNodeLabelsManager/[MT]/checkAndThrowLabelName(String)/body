{
  if (label == null || label.isEmpty() || label.length() > MAX_LABEL_LENGTH) {
    throw new IOException("label added is empty or exceeds " + MAX_LABEL_LENGTH + " character(s)");
  }
  label=label.trim();
  boolean match=LABEL_PATTERN.matcher(label).matches();
  if (!match) {
    throw new IOException("label name should only contains " + "{0-9, a-z, A-Z, -, _} and should not started with {-,_}" + ", now it is="+ label);
  }
}
