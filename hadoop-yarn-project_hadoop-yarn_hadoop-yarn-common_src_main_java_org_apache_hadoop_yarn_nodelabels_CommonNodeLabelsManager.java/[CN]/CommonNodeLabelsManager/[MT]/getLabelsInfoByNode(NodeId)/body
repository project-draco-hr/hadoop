{
  try {
    readLock.lock();
    Set<String> labels=getLabelsByNode(nodeId,nodeCollections);
    if (labels.isEmpty()) {
      return EMPTY_NODELABEL_SET;
    }
    Set<NodeLabel> nodeLabels=createNodeLabelFromLabelNames(labels);
    return nodeLabels;
  }
  finally {
    readLock.unlock();
  }
}
