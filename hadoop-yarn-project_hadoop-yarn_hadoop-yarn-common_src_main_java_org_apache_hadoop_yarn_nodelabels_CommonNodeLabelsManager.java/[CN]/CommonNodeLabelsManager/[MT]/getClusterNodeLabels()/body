{
  try {
    readLock.lock();
    List<NodeLabel> nodeLabels=new ArrayList<>();
    for (    RMNodeLabel label : labelCollections.values()) {
      nodeLabels.add(NodeLabel.newInstance(label.getLabelName(),label.getIsExclusive()));
    }
    return nodeLabels;
  }
  finally {
    readLock.unlock();
  }
}
