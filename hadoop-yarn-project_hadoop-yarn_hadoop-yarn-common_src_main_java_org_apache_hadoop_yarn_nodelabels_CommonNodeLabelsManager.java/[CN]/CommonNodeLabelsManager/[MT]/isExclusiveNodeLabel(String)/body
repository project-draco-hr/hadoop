{
  try {
    readLock.lock();
    RMNodeLabel label=labelCollections.get(nodeLabel);
    if (label == null) {
      String message="Getting is-exclusive-node-label, node-label = " + nodeLabel + ", is not existed.";
      LOG.error(message);
      throw new IOException(message);
    }
    return label.getIsExclusive();
  }
  finally {
    readLock.unlock();
  }
}
