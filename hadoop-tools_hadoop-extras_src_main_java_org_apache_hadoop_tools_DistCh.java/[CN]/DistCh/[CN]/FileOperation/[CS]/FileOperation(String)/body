{
  try {
    String[] t=line.split(":",4);
    for (int i=0; i < t.length; i++) {
      if ("".equals(t[i])) {
        t[i]=null;
      }
    }
    src=new Path(t[0]);
    owner=t[1];
    group=t[2];
    permission=t[3] == null ? null : new FsPermission(Short.parseShort(t[3],8));
    checkState();
  }
 catch (  Exception e) {
    throw (IllegalArgumentException)new IllegalArgumentException("line=" + line).initCause(e);
  }
}
