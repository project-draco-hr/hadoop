{
  Map<Node,Integer> frequency=pickNodesAtRandom(100,"~" + "/d2");
  for (int j=0; j < dataNodes.length; j++) {
    int freq=frequency.get(dataNodes[j]);
    if (dataNodes[j].getNetworkLocation().startsWith("/d2")) {
      assertEquals(0,freq);
    }
 else {
      assertTrue(freq > 0);
    }
  }
}
