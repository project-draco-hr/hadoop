{
  try {
    String encodedKey=URIUtil.encodePath(key);
    URI uri=new URI(encodedKey);
    return uri;
  }
 catch (  URISyntaxException e) {
    throw new AssertionError("Failed to encode key: " + key);
  }
catch (  URIException e) {
    throw new AssertionError("Failed to encode key: " + key);
  }
}
