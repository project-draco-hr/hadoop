{
  try {
    Method setLastModified=properties.getClass().getDeclaredMethod("setLastModified",Date.class);
    setLastModified.setAccessible(true);
    setLastModified.invoke(this.properties,Calendar.getInstance(TimeZone.getTimeZone("UTC")).getTime());
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  return properties;
}
