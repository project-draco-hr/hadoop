{
  for (  String label : queueCapacities.getExistingNodeLabels()) {
    float capacity=queueCapacities.getCapacity(label);
    if (capacity > 0f) {
      queueCapacities.setAbsoluteCapacity(label,capacity * (parentQueueCapacities == null ? 1 : parentQueueCapacities.getAbsoluteCapacity(label)));
    }
    float maxCapacity=queueCapacities.getMaximumCapacity(label);
    if (maxCapacity > 0f) {
      queueCapacities.setAbsoluteMaximumCapacity(label,maxCapacity * (parentQueueCapacities == null ? 1 : parentQueueCapacities.getAbsoluteMaximumCapacity(label)));
    }
  }
}
