{
  String ret="resource:" + resource.toString();
  ret+="; type:" + type;
  ret+="; excessReservation:" + excessReservation;
  ret+="; applicationid:" + (application != null ? application.getApplicationId().toString() : "null");
  ret+="; skipped:" + skipped;
  ret+="; fulfilled reservation:" + fulfilledReservation;
  ret+="; allocations(count/resource):" + assignmentInformation.getNumAllocations() + "/"+ assignmentInformation.getAllocated().toString();
  ret+="; reservations(count/resource):" + assignmentInformation.getNumReservations() + "/"+ assignmentInformation.getReserved().toString();
  return ret;
}
