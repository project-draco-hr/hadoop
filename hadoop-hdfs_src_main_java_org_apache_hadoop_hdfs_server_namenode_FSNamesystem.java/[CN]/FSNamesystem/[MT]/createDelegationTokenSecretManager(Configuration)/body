{
  return new DelegationTokenSecretManager(conf.getLong(DFS_NAMENODE_DELEGATION_KEY_UPDATE_INTERVAL_KEY,DFS_NAMENODE_DELEGATION_KEY_UPDATE_INTERVAL_DEFAULT),conf.getLong(DFS_NAMENODE_DELEGATION_TOKEN_MAX_LIFETIME_KEY,DFS_NAMENODE_DELEGATION_TOKEN_MAX_LIFETIME_DEFAULT),conf.getLong(DFS_NAMENODE_DELEGATION_TOKEN_RENEW_INTERVAL_KEY,DFS_NAMENODE_DELEGATION_TOKEN_RENEW_INTERVAL_DEFAULT),DELEGATION_TOKEN_REMOVER_SCAN_INTERVAL,this);
}
