{
  SpanReceiverBuilder builder=new SpanReceiverBuilder(TraceUtils.wrapHadoopConf(confPrefix,config,extraConfig));
  SpanReceiver rcvr=builder.spanReceiverClass(className.trim()).build();
  if (rcvr == null) {
    throw new IOException("Failed to load SpanReceiver " + className);
  }
  return rcvr;
}
