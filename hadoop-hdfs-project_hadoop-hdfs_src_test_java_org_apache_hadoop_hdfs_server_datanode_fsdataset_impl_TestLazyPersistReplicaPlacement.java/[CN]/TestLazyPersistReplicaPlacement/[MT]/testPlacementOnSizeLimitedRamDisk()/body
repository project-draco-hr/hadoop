{
  startUpCluster(true,3);
  final String METHOD_NAME=GenericTestUtils.getMethodName();
  Path path1=new Path("/" + METHOD_NAME + ".01.dat");
  Path path2=new Path("/" + METHOD_NAME + ".02.dat");
