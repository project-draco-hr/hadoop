{
  JobID jobId=getJobID();
  TaskType t=TaskType.REDUCE;
  if (isMap) {
    t=TaskType.MAP;
  }
  if (!isSpeculative) {
    return new TaskAttemptID(jobId.getJtIdentifier(),jobId.getId(),t,(isMap) ? ++mapTaskCtr : ++redTaskCtr,0);
  }
 else {
    return new TaskAttemptID(jobId.getJtIdentifier(),jobId.getId(),t,(isMap) ? mapTaskCtr : redTaskCtr,1);
  }
}
