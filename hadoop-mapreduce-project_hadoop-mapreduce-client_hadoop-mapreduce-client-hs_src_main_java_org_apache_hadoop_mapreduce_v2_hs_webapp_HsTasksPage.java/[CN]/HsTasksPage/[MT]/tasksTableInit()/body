{
  TaskType type=null;
  String symbol=$(TASK_TYPE);
  if (!symbol.isEmpty()) {
    type=MRApps.taskType(symbol);
  }
  StringBuilder b=tableInit().append(", 'aaData': tasksTableData").append(", bDeferRender: true").append(", bProcessing: true").append("\n, aoColumnDefs: [\n").append("{'sType':'numeric', 'aTargets': [ 0 ]").append(", 'mRender': parseHadoopID }").append(", {'sType':'numeric', 'aTargets': [ 4").append(type == TaskType.REDUCE ? ", 9, 10, 11, 12" : ", 7").append(" ], 'mRender': renderHadoopElapsedTime }").append("\n, {'sType':'numeric', 'aTargets': [ 2, 3, 5").append(type == TaskType.REDUCE ? ", 6, 7, 8" : ", 6").append(" ], 'mRender': renderHadoopDate }]").append("\n, aaSorting: [[0, 'asc']]").append("}");
  return b.toString();
}
