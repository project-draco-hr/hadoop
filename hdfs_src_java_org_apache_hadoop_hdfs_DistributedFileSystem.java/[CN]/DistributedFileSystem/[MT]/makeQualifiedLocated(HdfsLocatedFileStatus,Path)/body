{
  return new LocatedFileStatus(f.getLen(),f.isDir(),f.getReplication(),f.getBlockSize(),f.getModificationTime(),f.getAccessTime(),f.getPermission(),f.getOwner(),f.getGroup(),null,(f.getFullPath(parent)).makeQualified(getUri(),getWorkingDirectory()),DFSUtil.locatedBlocks2Locations(f.getBlockLocations()));
}
