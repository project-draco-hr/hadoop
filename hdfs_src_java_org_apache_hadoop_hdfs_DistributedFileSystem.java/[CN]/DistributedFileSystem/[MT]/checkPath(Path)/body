{
  URI thisUri=this.getUri();
  URI thatUri=path.toUri();
  String thatAuthority=thatUri.getAuthority();
  if (thatUri.getScheme() != null && thatUri.getScheme().equalsIgnoreCase(thisUri.getScheme()) && thatUri.getPort() == NameNode.DEFAULT_PORT && (thisUri.getPort() == -1 || thisUri.getPort() == NameNode.DEFAULT_PORT) && thatAuthority.substring(0,thatAuthority.indexOf(":")).equalsIgnoreCase(thisUri.getAuthority()))   return;
  super.checkPath(path);
}
