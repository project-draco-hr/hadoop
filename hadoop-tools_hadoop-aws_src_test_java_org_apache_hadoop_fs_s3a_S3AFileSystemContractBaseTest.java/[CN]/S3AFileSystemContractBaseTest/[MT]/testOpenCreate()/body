{
  try {
    createAndReadFileTest(1024);
  }
 catch (  IOException e) {
    fail(e.getMessage());
  }
  try {
    createAndReadFileTest(5 * 1024 * 1024);
  }
 catch (  IOException e) {
    fail(e.getMessage());
  }
  try {
    createAndReadFileTest(20 * 1024 * 1024);
  }
 catch (  IOException e) {
    fail(e.getMessage());
  }
}
