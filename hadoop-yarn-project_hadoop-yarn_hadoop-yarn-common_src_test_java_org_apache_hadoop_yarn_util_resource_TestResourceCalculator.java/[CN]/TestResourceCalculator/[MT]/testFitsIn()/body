{
  Resource cluster=Resource.newInstance(1024,1);
  if (resourceCalculator instanceof DefaultResourceCalculator) {
    Assert.assertTrue(resourceCalculator.fitsIn(cluster,Resource.newInstance(1,2),Resource.newInstance(2,1)));
    Assert.assertTrue(resourceCalculator.fitsIn(cluster,Resource.newInstance(1,2),Resource.newInstance(2,2)));
    Assert.assertTrue(resourceCalculator.fitsIn(cluster,Resource.newInstance(1,2),Resource.newInstance(1,2)));
    Assert.assertTrue(resourceCalculator.fitsIn(cluster,Resource.newInstance(1,2),Resource.newInstance(1,1)));
    Assert.assertFalse(resourceCalculator.fitsIn(cluster,Resource.newInstance(2,1),Resource.newInstance(1,2)));
  }
 else   if (resourceCalculator instanceof DominantResourceCalculator) {
    Assert.assertFalse(resourceCalculator.fitsIn(cluster,Resource.newInstance(1,2),Resource.newInstance(2,1)));
    Assert.assertTrue(resourceCalculator.fitsIn(cluster,Resource.newInstance(1,2),Resource.newInstance(2,2)));
    Assert.assertTrue(resourceCalculator.fitsIn(cluster,Resource.newInstance(1,2),Resource.newInstance(1,2)));
    Assert.assertFalse(resourceCalculator.fitsIn(cluster,Resource.newInstance(1,2),Resource.newInstance(1,1)));
    Assert.assertFalse(resourceCalculator.fitsIn(cluster,Resource.newInstance(2,1),Resource.newInstance(1,2)));
  }
}
