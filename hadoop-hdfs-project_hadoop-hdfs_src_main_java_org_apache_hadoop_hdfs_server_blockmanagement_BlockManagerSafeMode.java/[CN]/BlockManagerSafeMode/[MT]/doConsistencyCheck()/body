{
  boolean assertsOn=false;
  assert assertsOn=true;
  if (!assertsOn) {
    return;
  }
  int activeBlocks=blockManager.getActiveBlockCount();
synchronized (this) {
    if (blockTotal != activeBlocks && !(blockSafe >= 0 && blockSafe <= blockTotal)) {
      LOG.warn("SafeMode is in inconsistent filesystem state. " + "BlockManagerSafeMode data: blockTotal={}, blockSafe={}; " + "BlockManager data: activeBlocks={}",blockTotal,blockSafe,activeBlocks);
    }
  }
}
