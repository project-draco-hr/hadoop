{
  if (args.length < 1) {
    System.err.println("Usage: FindFreePort < -random / <#port> >");
    System.exit(0);
  }
  int j=0;
  String cmd=args[j++];
  if ("-random".equals(cmd)) {
    System.out.println(findFreePortRandom());
  }
 else {
    System.out.println(findFreePort(Integer.parseInt(cmd)));
  }
}
