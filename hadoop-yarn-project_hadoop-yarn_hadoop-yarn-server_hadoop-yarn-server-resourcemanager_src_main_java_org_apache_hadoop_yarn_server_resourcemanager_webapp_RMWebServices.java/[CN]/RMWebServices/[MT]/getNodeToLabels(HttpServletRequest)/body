{
  init();
  NodeToLabelsInfo ntl=new NodeToLabelsInfo();
  HashMap<String,NodeLabelsInfo> ntlMap=ntl.getNodeToLabels();
  Map<NodeId,Set<String>> nodeIdToLabels=rm.getRMContext().getNodeLabelManager().getNodeLabels();
  for (  Map.Entry<NodeId,Set<String>> nitle : nodeIdToLabels.entrySet()) {
    ntlMap.put(nitle.getKey().toString(),new NodeLabelsInfo(nitle.getValue()));
  }
  return ntl;
}
