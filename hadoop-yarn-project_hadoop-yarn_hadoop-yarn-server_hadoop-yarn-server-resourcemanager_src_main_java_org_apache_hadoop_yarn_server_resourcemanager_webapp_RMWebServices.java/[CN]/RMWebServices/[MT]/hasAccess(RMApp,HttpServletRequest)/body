{
  UserGroupInformation callerUGI=getCallerUserGroupInformation(hsr,true);
  if (callerUGI != null && !(this.rm.getApplicationACLsManager().checkAccess(callerUGI,ApplicationAccessType.VIEW_APP,app.getUser(),app.getApplicationId()) || this.rm.getQueueACLsManager().checkAccess(callerUGI,QueueACL.ADMINISTER_QUEUE,app.getQueue(),app.getApplicationId(),app.getName()))) {
    return false;
  }
  return true;
}
