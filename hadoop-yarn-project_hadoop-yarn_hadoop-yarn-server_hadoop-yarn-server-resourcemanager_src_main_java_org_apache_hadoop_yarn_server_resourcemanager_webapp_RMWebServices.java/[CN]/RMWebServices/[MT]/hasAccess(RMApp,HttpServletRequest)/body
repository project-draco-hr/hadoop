{
  String remoteUser=hsr.getRemoteUser();
  UserGroupInformation callerUGI=null;
  if (remoteUser != null) {
    callerUGI=UserGroupInformation.createRemoteUser(remoteUser);
  }
  if (callerUGI != null && !(this.aclsManager.checkAccess(callerUGI,ApplicationAccessType.VIEW_APP,app.getUser(),app.getApplicationId()) || this.queueACLsManager.checkAccess(callerUGI,QueueACL.ADMINISTER_QUEUE,app.getQueue()))) {
    return false;
  }
  return true;
}
