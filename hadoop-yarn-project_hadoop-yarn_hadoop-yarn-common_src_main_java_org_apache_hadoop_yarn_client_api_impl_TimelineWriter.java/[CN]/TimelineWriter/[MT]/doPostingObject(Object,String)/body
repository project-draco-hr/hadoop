{
  WebResource webResource=client.resource(resURI);
  if (path == null) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("POST to " + resURI);
    }
    return webResource.accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON).post(ClientResponse.class,object);
  }
 else   if (path.equals("domain")) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("PUT to " + resURI + "/"+ path);
    }
    return webResource.path(path).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON).put(ClientResponse.class,object);
  }
 else {
    throw new YarnRuntimeException("Unknown resource type");
  }
}
