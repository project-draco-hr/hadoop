{
  long now=Time.now();
  for (Iterator<Map.Entry<Integer,DelegationKey>> it=allKeys.entrySet().iterator(); it.hasNext(); ) {
    Map.Entry<Integer,DelegationKey> e=it.next();
    if (e.getValue().getExpiryDate() < now) {
      it.remove();
      if (!e.getValue().equals(currentKey))       removeStoredMasterKey(e.getValue());
    }
  }
}
