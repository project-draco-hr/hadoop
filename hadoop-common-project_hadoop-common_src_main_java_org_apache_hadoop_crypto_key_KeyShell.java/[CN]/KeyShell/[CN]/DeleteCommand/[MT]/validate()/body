{
  provider=getKeyProvider();
  if (provider == null) {
    return false;
  }
  if (keyName == null) {
    out.println("There is no keyName specified. Please specify a " + "<keyname>. See the usage description with -help.");
    return false;
  }
  if (interactive) {
    try {
      cont=ToolRunner.confirmPrompt("You are about to DELETE all versions of " + " key " + keyName + " from KeyProvider "+ provider+ ". Continue? ");
      if (!cont) {
        out.println(keyName + " has not been deleted.");
      }
      return cont;
    }
 catch (    IOException e) {
      out.println(keyName + " will not be deleted.");
      e.printStackTrace(err);
    }
  }
  return true;
}
