{
  try {
    final List<String> keys=provider.getKeys();
    out.println("Listing keys for KeyProvider: " + provider);
    if (metadata) {
      final Metadata[] meta=provider.getKeysMetadata(keys.toArray(new String[keys.size()]));
      for (int i=0; i < meta.length; ++i) {
        out.println(keys.get(i) + " : " + meta[i]);
      }
    }
 else {
      for (      String keyName : keys) {
        out.println(keyName);
      }
    }
  }
 catch (  IOException e) {
    out.println("Cannot list keys for KeyProvider: " + provider + ": "+ e.getMessage());
    throw e;
  }
}
