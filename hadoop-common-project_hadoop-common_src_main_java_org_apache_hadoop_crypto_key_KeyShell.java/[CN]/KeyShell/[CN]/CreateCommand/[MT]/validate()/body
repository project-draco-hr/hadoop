{
  boolean rc=true;
  try {
    provider=getKeyProvider();
    if (provider == null) {
      rc=false;
    }
 else     if (provider.needsPassword()) {
      if (strict) {
        out.println(provider.noPasswordError());
        rc=false;
      }
 else {
        out.println(provider.noPasswordWarning());
      }
    }
  }
 catch (  IOException e) {
    e.printStackTrace(err);
  }
  if (keyName == null) {
    out.println("Please provide a <keyname>. See the usage description" + " with -help.");
    rc=false;
  }
  return rc;
}
