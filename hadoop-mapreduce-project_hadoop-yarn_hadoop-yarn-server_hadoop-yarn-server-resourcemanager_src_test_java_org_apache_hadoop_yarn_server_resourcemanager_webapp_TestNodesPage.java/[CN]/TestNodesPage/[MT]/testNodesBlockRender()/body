{
  int numberOfRacks=2;
  int numberOfNodesPerRack=2;
  int numberOfActualTableHeaders=7;
  PrintWriter writer=WebAppTests.testBlock(NodesBlock.class,RMContext.class,TestRMWebApp.mockRMContext(3,numberOfRacks,numberOfNodesPerRack,8 * TestRMWebApp.GiB)).getInstance(PrintWriter.class);
  Mockito.verify(writer,Mockito.times(numberOfActualTableHeaders)).print("<th");
  Mockito.verify(writer,Mockito.times(numberOfRacks * numberOfNodesPerRack * numberOfActualTableHeaders)).print("<td");
}
