{
  ApplicationId appId=localizerContext.getContainerId().getApplicationAttemptId().getApplicationId();
  Credentials systemCredentials=nmContext.getSystemCredentialsForApps().get(appId);
  if (systemCredentials == null) {
    return null;
  }
  LOG.info("Adding new framework tokens from RM for " + appId);
  for (  Token<?> token : systemCredentials.getAllTokens()) {
    LOG.info("Adding new application-token for localization: " + token);
  }
  return systemCredentials;
}
