{
  HAServiceProtocol proxy=null;
  try {
    proxy=svc.getProxy(conf,gracefulFenceTimeout);
    proxy.transitionToStandby();
    return true;
  }
 catch (  ServiceFailedException sfe) {
    LOG.warn("Unable to gracefully make " + svc + " standby ("+ sfe.getMessage()+ ")");
  }
catch (  IOException ioe) {
    LOG.warn("Unable to gracefully make " + svc + " standby (unable to connect)",ioe);
  }
 finally {
    if (proxy != null) {
      RPC.stopProxy(proxy);
    }
  }
  return false;
}
