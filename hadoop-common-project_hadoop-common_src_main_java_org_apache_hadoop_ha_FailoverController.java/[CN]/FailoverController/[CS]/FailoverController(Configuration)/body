{
  this.conf=conf;
  this.gracefulFenceTimeout=conf.getInt(CommonConfigurationKeys.HA_FC_GRACEFUL_FENCE_TIMEOUT_KEY,CommonConfigurationKeys.HA_FC_GRACEFUL_FENCE_TIMEOUT_DEFAULT);
  this.rpcTimeoutToNewActive=conf.getInt(CommonConfigurationKeys.HA_FC_NEW_ACTIVE_TIMEOUT_KEY,CommonConfigurationKeys.HA_FC_NEW_ACTIVE_TIMEOUT_DEFAULT);
}
