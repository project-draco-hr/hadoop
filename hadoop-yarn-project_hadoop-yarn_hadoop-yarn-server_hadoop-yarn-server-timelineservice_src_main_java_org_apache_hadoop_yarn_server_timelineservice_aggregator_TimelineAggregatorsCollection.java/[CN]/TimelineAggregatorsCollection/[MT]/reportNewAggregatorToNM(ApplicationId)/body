{
  this.nmAggregatorService=getNMAggregatorService();
  ReportNewAggregatorsInfoRequest request=ReportNewAggregatorsInfoRequest.newInstance(appId,this.timelineRestServerBindAddress);
  LOG.info("Report a new aggregator for application: " + appId + " to NM Aggregator Services.");
  nmAggregatorService.reportNewAggregatorInfo(request);
}
