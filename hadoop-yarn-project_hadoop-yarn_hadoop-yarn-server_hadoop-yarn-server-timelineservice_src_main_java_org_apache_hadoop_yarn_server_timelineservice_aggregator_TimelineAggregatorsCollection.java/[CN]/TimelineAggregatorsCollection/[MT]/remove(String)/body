{
synchronized (aggregators) {
    TimelineAggregator aggregator=aggregators.remove(id);
    if (aggregator == null) {
      String msg="the aggregator for " + id + " does not exist!";
      LOG.error(msg);
      return false;
    }
 else {
      aggregator.stop();
      LOG.info("the aggregator service for " + id + " was removed");
      return true;
    }
  }
}
