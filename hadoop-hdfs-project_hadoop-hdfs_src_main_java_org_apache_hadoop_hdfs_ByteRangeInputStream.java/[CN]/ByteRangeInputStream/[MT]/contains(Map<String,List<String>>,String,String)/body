{
  final List<String> values=headers.get(key);
  if (values != null) {
    for (    String v : values) {
      for (final StringTokenizer t=new StringTokenizer(v,","); t.hasMoreTokens(); ) {
        if (value.equalsIgnoreCase(t.nextToken())) {
          return true;
        }
      }
    }
  }
  return false;
}
