def __extractJobStatus(self, line):
    'This method parses an output line from the job status command and creates\n       the JobStatus object if there is a match'
    jobStatus = None
    line = line.strip()
    jsMatch = self.__jobStatusRegExp.match(line)
    if jsMatch:
        jobStatus = HadoopJobStatus(jsMatch.group(1), int(jsMatch.group(2)))
    return jobStatus
