{
  if (rmContext.getRMApps().get(applicationId).getFinalApplicationStatus() == FinalApplicationStatus.UNDEFINED) {
    List<AppAllocation> allocations=completedAppAllocations.get(applicationId);
    return new AppActivitiesInfo(allocations,applicationId);
  }
 else {
    return new AppActivitiesInfo("fail to get application activities after finished",applicationId.toString());
  }
}
