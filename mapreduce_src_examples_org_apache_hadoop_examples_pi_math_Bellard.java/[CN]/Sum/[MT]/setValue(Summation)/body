{
  if (s.getValue() == null)   throw new IllegalArgumentException("s.getValue()" + "\n  sigma=" + sigma + "\n  s    ="+ s);
  if (!s.contains(sigma) || !sigma.contains(s))   throw new IllegalArgumentException("!s.contains(sigma) || !sigma.contains(s)" + "\n  sigma=" + sigma + "\n  s    ="+ s);
  sigma.setValue(s.getValue());
}
