{
  PermissionStatus permStatus=PermissionStatus.createImmutable(owner,group,FsPermission.createImmutable(perm));
  INodeFile inodeFile=new INodeFile(INodeId.GRANDFATHER_INODE_ID,name.getBytes("UTF-8"),permStatus,0L,0L,null,REPLICATION,PREFERRED_BLOCK_SIZE,(byte)0);
  parent.addChild(inodeFile);
  return inodeFile;
}
