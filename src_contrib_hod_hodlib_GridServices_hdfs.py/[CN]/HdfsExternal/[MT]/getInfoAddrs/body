def getInfoAddrs(self):
    attrs = self.serviceDesc.getfinalAttrs()
    if (self.version < 16):
        addr = attrs['fs.default.name']
        (k, v) = addr.split(':')
        infoaddr = ((k + ':') + attrs['dfs.info.port'])
    else:
        infoaddr = attrs['dfs.http.address']
    return [infoaddr]
