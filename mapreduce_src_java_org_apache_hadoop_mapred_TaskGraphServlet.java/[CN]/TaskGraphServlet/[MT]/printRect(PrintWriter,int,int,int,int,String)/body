{
  if (height > 0) {
    out.print("<rect width=\"");
    out.print(width);
    out.print("\" height=\"");
    out.print(height);
    out.print("\" x=\"");
    out.print(x);
    out.print("\" y=\"");
    out.print(y);
    out.print("\" style=\"fill:");
    out.print(color);
    out.print("\"/>\n");
  }
}
