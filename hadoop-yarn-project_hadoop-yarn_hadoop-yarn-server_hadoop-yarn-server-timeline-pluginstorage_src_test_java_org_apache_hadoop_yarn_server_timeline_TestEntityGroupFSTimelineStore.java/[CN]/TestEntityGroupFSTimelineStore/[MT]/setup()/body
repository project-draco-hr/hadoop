{
  createTestFiles();
  store=new EntityGroupFSTimelineStore();
  if (currTestName.getMethodName().contains("Plugin")) {
    config.set(YarnConfiguration.TIMELINE_SERVICE_ENTITY_GROUP_PLUGIN_CLASSES,EntityGroupPlugInForTest.class.getName());
  }
  store.init(config);
  store.start();
  store.setFs(fs);
}
