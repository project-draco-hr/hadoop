{
  final String inputpath=conf.get(TestConstants.NATIVETASK_KVTEST_INPUTDIR) + "/LargeKV/" + keyclass.getName()+ "/"+ valueclass.getName();
  final String outputpath=conf.get(TestConstants.NATIVETASK_KVTEST_OUTPUTDIR) + "/LargeKV/" + keyclass.getName()+ "/"+ valueclass.getName();
  final FileSystem fs=FileSystem.get(conf);
  fs.delete(new Path(outputpath),true);
  fs.close();
  try {
    final KVJob keyJob=new KVJob(jobname,conf,keyclass,valueclass,inputpath,outputpath);
    assertTrue("job should complete successfully",keyJob.runJob());
  }
 catch (  final Exception e) {
    return "normal testcase run time error.";
  }
  return outputpath;
}
