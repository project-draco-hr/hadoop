{
  for (int i=0; i < dataNodes.length; i++) {
    cluster.remove(dataNodes[i]);
  }
  for (int i=0; i < dataNodes.length; i++) {
    assertFalse(cluster.contains(dataNodes[i]));
  }
  assertEquals(0,cluster.getNumOfLeaves());
  for (int i=0; i < dataNodes.length; i++) {
    cluster.add(dataNodes[i]);
  }
}
