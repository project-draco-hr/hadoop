{
  assert(raidPolicyPathPairList.size() > 0);
  Job job=createJob(getConf());
  createInputFile(job);
  try {
    job.submit();
    this.runningJob=job;
    LOG.info("Job Started: " + runningJob.getJobID());
    return true;
  }
 catch (  ClassNotFoundException e) {
    throw new IOException(e);
  }
catch (  InterruptedException e) {
    return false;
  }
}
