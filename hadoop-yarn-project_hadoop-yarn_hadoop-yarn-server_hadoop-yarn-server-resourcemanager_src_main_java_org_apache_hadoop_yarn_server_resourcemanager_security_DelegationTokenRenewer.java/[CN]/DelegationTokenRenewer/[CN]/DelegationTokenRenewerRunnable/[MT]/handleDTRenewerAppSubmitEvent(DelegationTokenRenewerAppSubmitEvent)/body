{
  try {
    DelegationTokenRenewer.this.handleAppSubmitEvent(event);
    rmContext.getDispatcher().getEventHandler().handle(new RMAppEvent(event.getApplicationId(),event.isApplicationRecovered() ? RMAppEventType.RECOVER : RMAppEventType.START));
  }
 catch (  Throwable t) {
    LOG.warn("Unable to add the application to the delegation token renewer.",t);
    rmContext.getDispatcher().getEventHandler().handle(new RMAppRejectedEvent(event.getApplicationId(),t.getMessage()));
  }
}
