{
  this.hasProxyUserPrivileges=conf.getBoolean(YarnConfiguration.RM_PROXY_USER_PRIVILEGES_ENABLED,YarnConfiguration.DEFAULT_RM_PROXY_USER_PRIVILEGES_ENABLED);
  this.tokenKeepAliveEnabled=conf.getBoolean(YarnConfiguration.LOG_AGGREGATION_ENABLED,YarnConfiguration.DEFAULT_LOG_AGGREGATION_ENABLED);
  this.tokenRemovalDelayMs=conf.getInt(YarnConfiguration.RM_NM_EXPIRY_INTERVAL_MS,YarnConfiguration.DEFAULT_RM_NM_EXPIRY_INTERVAL_MS);
  this.credentialsValidTimeRemaining=conf.getLong(RM_SYSTEM_CREDENTIALS_VALID_TIME_REMAINING,DEFAULT_RM_SYSTEM_CREDENTIALS_VALID_TIME_REMAINING);
  setLocalSecretManagerAndServiceAddr();
  renewerService=createNewThreadPoolService(conf);
  pendingEventQueue=new LinkedBlockingQueue<DelegationTokenRenewerEvent>();
  renewalTimer=new Timer(true);
  super.serviceInit(conf);
}
