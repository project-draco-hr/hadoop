{
  if (isServiceStarted) {
    for (    DelegationTokenToRenew dtr : dtrs) {
      renewToken(dtr);
    }
    for (    DelegationTokenToRenew dtr : dtrs) {
      addTokenToList(dtr);
      setTimerForTokenRenewal(dtr);
      if (LOG.isDebugEnabled()) {
        LOG.debug("Registering token for renewal for:" + " service = " + dtr.token.getService() + " for appId = "+ dtr.applicationId);
      }
    }
  }
 else {
    pendingTokenForRenewal.addAll(dtrs);
  }
}
