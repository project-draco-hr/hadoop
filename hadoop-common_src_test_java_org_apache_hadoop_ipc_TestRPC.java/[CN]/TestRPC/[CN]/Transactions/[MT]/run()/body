{
  int[] indata=new int[datasize];
  int[] outdata=null;
  int val=0;
  try {
    outdata=proxy.exchange(indata);
    val=proxy.add(1,2);
  }
 catch (  IOException e) {
    assertTrue("Exception from RPC exchange() " + e,false);
  }
  assertEquals(indata.length,outdata.length);
  assertEquals(val,3);
  for (int i=0; i < outdata.length; i++) {
    assertEquals(outdata[i],i);
  }
}
