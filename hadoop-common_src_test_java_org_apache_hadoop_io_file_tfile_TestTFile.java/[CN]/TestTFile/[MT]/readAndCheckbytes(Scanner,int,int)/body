{
  String value="value";
  for (int i=start; i < (start + n); i++) {
    byte[] key=readKey(scanner);
    byte[] val=readValue(scanner);
    String keyStr=String.format(localFormatter,i);
    String valStr=value + keyStr;
    assertTrue("btyes for keys do not match " + keyStr + " "+ new String(key),Arrays.equals(keyStr.getBytes(),key));
    assertTrue("bytes for vals do not match " + valStr + " "+ new String(val),Arrays.equals(valStr.getBytes(),val));
    assertTrue(scanner.advance());
    key=readKey(scanner);
    val=readValue(scanner);
    assertTrue("btyes for keys do not match",Arrays.equals(keyStr.getBytes(),key));
    assertTrue("bytes for vals do not match",Arrays.equals(valStr.getBytes(),val));
    assertTrue(scanner.advance());
  }
  return (start + n);
}
