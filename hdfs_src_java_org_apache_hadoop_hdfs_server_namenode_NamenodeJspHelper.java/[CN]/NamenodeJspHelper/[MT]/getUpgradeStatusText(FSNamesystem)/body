{
  String statusText="";
  try {
    UpgradeStatusReport status=fsn.distributedUpgradeProgress(UpgradeAction.GET_STATUS);
    statusText=(status == null ? "There are no upgrades in progress." : status.getStatusText(false));
  }
 catch (  IOException e) {
    statusText="Upgrade status unknown.";
  }
  return statusText;
}
