{
  File result=new File(outdir.getAbsolutePath() + File.separator + "part-00000");
  File success=new File(outdir.getAbsolutePath() + File.separator + "_SUCCESS");
  Assert.assertTrue(success.exists());
  FileInputStream fis=new FileInputStream(result);
  BufferedReader br=new BufferedReader(new InputStreamReader(fis,"UTF-8"));
  String line=br.readLine();
  Assert.assertTrue(("1 44" + TAB + "2").equals(line));
  line=br.readLine();
  Assert.assertTrue(("3 44" + TAB + "1").equals(line));
  line=br.readLine();
  Assert.assertTrue(("4 44" + TAB + "1").equals(line));
  br.close();
}
