{
  int checkMinutes=conf.getInt(YarnConfiguration.IN_MEMORY_CHECK_PERIOD,YarnConfiguration.DEFAULT_IN_MEMORY_CHECK_PERIOD);
  if (checkMinutes <= 0) {
    throw new HadoopIllegalArgumentException("Non-positive check period value: " + checkMinutes + ". The check period value must be greater than zero.");
  }
  return checkMinutes;
}
