{
  int initialMinutes=conf.getInt(YarnConfiguration.IN_MEMORY_INITIAL_DELAY,YarnConfiguration.DEFAULT_IN_MEMORY_INITIAL_DELAY);
  if (initialMinutes <= 0) {
    throw new HadoopIllegalArgumentException("Non-positive initial delay value: " + initialMinutes + ". The initial delay value must be greater than zero.");
  }
  return initialMinutes;
}
