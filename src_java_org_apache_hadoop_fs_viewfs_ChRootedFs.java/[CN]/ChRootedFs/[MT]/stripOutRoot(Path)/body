{
  try {
    checkPath(p);
  }
 catch (  IllegalArgumentException e) {
    throw new RuntimeException("Internal Error - path " + p + " should have been with URI"+ myUri);
  }
  String pathPart=p.toUri().getPath();
  return (pathPart.length() == chRootPathPartString.length()) ? "" : pathPart.substring(chRootPathPartString.length() + 1);
}
