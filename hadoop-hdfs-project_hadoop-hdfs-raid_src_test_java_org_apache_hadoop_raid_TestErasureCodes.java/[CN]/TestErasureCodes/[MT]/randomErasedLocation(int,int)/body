{
  int[] erasedLocations=new int[erasedLen];
  for (int i=0; i < erasedLen; i++) {
    Set<Integer> s=new HashSet<Integer>();
    while (s.size() != erasedLen) {
      s.add(RAND.nextInt(dataLen));
    }
    int t=0;
    for (    int erased : s) {
      erasedLocations[t++]=erased;
    }
  }
  return erasedLocations;
}
