{
  if (renewer == null) {
    this.renewer=new Text();
  }
 else {
    HadoopKerberosName renewerKrbName=new HadoopKerberosName(renewer.toString());
    try {
      this.renewer=new Text(renewerKrbName.getShortName());
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
  }
}
