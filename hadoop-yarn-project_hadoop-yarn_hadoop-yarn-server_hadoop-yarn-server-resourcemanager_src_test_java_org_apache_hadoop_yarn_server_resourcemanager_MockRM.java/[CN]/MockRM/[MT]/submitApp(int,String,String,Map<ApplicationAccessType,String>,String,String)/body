{
  Resource resource=Records.newRecord(Resource.class);
  resource.setMemorySize(masterMemory);
  Priority priority=Priority.newInstance(0);
  return submitApp(resource,name,user,acls,false,queue,super.getConfig().getInt(YarnConfiguration.RM_AM_MAX_ATTEMPTS,YarnConfiguration.DEFAULT_RM_AM_MAX_ATTEMPTS),null,null,true,false,false,null,0,null,true,priority,amLabel);
}
