{
  StringTokenizer itr=new StringTokenizer(inValue.toString());
  int left=0;
  int right=0;
  if (itr.hasMoreTokens()) {
    left=Integer.parseInt(itr.nextToken());
    if (itr.hasMoreTokens()) {
      right=Integer.parseInt(itr.nextToken());
    }
    key.set(left,right);
    value.set(right);
    context.write(key,value);
  }
}
