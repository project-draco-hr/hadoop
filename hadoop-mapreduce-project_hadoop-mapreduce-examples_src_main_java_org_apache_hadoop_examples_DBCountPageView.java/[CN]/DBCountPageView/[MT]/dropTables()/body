{
  String dropAccess="DROP TABLE HAccess";
  String dropPageview="DROP TABLE Pageview";
  Statement st=null;
  try {
    st=connection.createStatement();
    st.executeUpdate(dropAccess);
    st.executeUpdate(dropPageview);
    connection.commit();
    st.close();
  }
 catch (  SQLException ex) {
    try {
      if (st != null) {
        st.close();
      }
    }
 catch (    Exception e) {
    }
  }
}
