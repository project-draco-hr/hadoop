{
  YarnRPC rpc=YarnRPC.create(getConfig());
  InetSocketAddress rmAddress=NetUtils.createSocketAddr(this.rmAddress);
  Configuration rmClientConf=new Configuration(getConfig());
  rmClientConf.setClass(YarnConfiguration.YARN_SECURITY_INFO,RMNMSecurityInfoClass.class,SecurityInfo.class);
  return (ResourceTracker)rpc.getProxy(ResourceTracker.class,rmAddress,rmClientConf);
}
