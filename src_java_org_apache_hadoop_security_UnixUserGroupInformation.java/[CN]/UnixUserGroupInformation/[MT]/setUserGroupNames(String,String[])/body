{
  if (userName == null || userName.length() == 0) {
    throw new IllegalArgumentException("username should not be null nor empty");
  }
  if (groupNames == null) {
    throw new IllegalArgumentException("group names array should not be null");
  }
  for (int i=0; i < groupNames.length; i++) {
    if (groupNames[i] == null || groupNames[i].length() == 0) {
      throw new IllegalArgumentException("A null group name at index " + i);
    }
  }
  this.userName=userName;
  this.groupNames=groupNames;
}
