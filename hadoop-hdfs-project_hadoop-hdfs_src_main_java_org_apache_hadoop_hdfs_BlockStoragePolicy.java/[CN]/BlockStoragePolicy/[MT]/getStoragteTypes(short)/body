{
  final StorageType[] types=new StorageType[replication];
  int i=0;
  for (; i < types.length && i < storageTypes.length; i++) {
    types[i]=storageTypes[i];
  }
  final StorageType last=storageTypes[storageTypes.length - 1];
  for (; i < types.length; i++) {
    types[i]=last;
  }
  return types;
}
