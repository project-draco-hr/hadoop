{
  final byte id=Byte.parseByte(string);
  if (id < 1) {
    throw new IllegalArgumentException("Invalid block storage policy ID: id = " + id + " < 1");
  }
  if (id > 15) {
    throw new IllegalArgumentException("Invalid block storage policy ID: id = " + id + " > MAX = "+ ID_MAX);
  }
  return id;
}
