{
  final List<StorageType> types=new LinkedList<StorageType>();
  int i=0;
  for (; i < replication && i < storageTypes.length; i++) {
    types.add(storageTypes[i]);
  }
  final StorageType last=storageTypes[storageTypes.length - 1];
  for (; i < replication; i++) {
    types.add(last);
  }
  return types;
}
