{
  if (outprop != null && !System.out.checkError())   getProject().setNewProperty(outprop,out.toString());
  if (out != err && errprop != null && !System.err.checkError())   getProject().setNewProperty(errprop,err.toString());
  System.setErr(antErr);
  System.setOut(antOut);
  confloader.cleanup();
  confloader.setParent(null);
}
