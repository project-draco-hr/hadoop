{
  Pattern compile=Pattern.compile(regex);
  Matcher matcher=compile.matcher(viewFilePage);
  if (matcher.find()) {
    String u=matcher.group(1);
    String urlString=u.startsWith("///") ? ("http://" + u.substring(3)) : u;
    viewFilePage=StringEscapeUtils.unescapeHtml(DFSTestUtil.urlGet(new URL(urlString)));
    assertTrue("page should show preview of file contents",viewFilePage.contains(FILE_DATA));
  }
 else {
    fail(text + " hyperlink should be there in the page content : " + viewFilePage);
  }
}
