{
  try {
    readLock.lock();
    Map<String,PreemptableQueue> map=new HashMap<>();
    for (    Map.Entry<String,PreemptableQueue> entry : entities.entrySet()) {
      String key=entry.getKey();
      PreemptableQueue entity=entry.getValue();
      map.put(key,new PreemptableQueue(new HashMap<>(entity.getTotalKillableResources()),new HashMap<>(entity.getKillableContainers())));
    }
    return map;
  }
  finally {
    readLock.unlock();
  }
}
