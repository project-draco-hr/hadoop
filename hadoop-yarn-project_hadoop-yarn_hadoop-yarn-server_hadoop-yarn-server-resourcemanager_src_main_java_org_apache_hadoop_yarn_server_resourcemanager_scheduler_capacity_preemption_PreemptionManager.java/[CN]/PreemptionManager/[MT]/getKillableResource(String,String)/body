{
  try {
    readLock.lock();
    PreemptableQueue entity=entities.get(queueName);
    if (entity != null) {
      Resource res=entity.getTotalKillableResources().get(partition);
      if (res == null || res.equals(Resources.none())) {
        return Resources.none();
      }
      return Resources.clone(res);
    }
    return Resources.none();
  }
  finally {
    readLock.unlock();
  }
}
