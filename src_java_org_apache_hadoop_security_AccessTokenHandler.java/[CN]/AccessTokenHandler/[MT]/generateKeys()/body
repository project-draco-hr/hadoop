{
  keyGen=KeyGenerator.getInstance("HmacSHA1");
  serialNo++;
  currentKey=new AccessKey(serialNo,new Text(keyGen.generateKey().getEncoded()),System.currentTimeMillis() + 2 * keyUpdateInterval + tokenLifetime);
  serialNo++;
  nextKey=new AccessKey(serialNo,new Text(keyGen.generateKey().getEncoded()),System.currentTimeMillis() + 3 * keyUpdateInterval + tokenLifetime);
  allKeys.put(currentKey.getKeyID(),currentKey);
  allKeys.put(nextKey.getKeyID(),nextKey);
}
