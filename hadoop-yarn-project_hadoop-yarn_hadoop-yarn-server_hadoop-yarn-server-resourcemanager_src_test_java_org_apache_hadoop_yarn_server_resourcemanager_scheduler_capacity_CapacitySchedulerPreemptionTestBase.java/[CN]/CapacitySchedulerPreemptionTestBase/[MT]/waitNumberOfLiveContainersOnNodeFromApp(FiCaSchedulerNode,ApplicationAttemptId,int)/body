{
  int waitNum=0;
  while (waitNum < 500) {
    int total=0;
    for (    RMContainer c : node.getCopiedListOfRunningContainers()) {
      if (c.getApplicationAttemptId().equals(appId)) {
        total++;
      }
    }
    if (total == expected) {
      return;
    }
    Thread.sleep(10);
    waitNum++;
  }
  Assert.fail();
}
