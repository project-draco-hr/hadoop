{
  AddressMatcher matcher=new AddressMatcher(){
    @Override public boolean match(    InetSocketAddress s){
      return address.equals(s);
    }
  }
;
  for (  String key : keys) {
    String[] ids=getSuffixIDs(conf,key,matcher);
    if (ids != null && (ids[0] != null || ids[1] != null)) {
      return ids;
    }
  }
  return null;
}
