{
  if (array != null && array.length > 0) {
    final Random random=getRandom();
    for (int n=array.length; n > 1; ) {
      final int randomIndex=random.nextInt(n);
      n--;
      if (n != randomIndex) {
        final T tmp=array[randomIndex];
        array[randomIndex]=array[n];
        array[n]=tmp;
      }
    }
  }
  return array;
}
