{
  MetaInfo metaInfo=null;
  if (jobListCache.containsKey(jobId)) {
    metaInfo=jobListCache.get(jobId);
  }
  if (metaInfo != null) {
    return metaInfo;
  }
  metaInfo=scanIntermediateForJob(jobId);
  if (metaInfo != null) {
    return metaInfo;
  }
  metaInfo=scanOldDirsForJob(jobId);
  if (metaInfo != null) {
    return metaInfo;
  }
  return null;
}
