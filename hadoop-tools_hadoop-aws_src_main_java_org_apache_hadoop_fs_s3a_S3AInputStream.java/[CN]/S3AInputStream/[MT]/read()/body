{
  checkNotClosed();
  if (this.contentLength == 0 || (nextReadPos >= contentLength)) {
    return -1;
  }
  int byteRead;
  try {
    lazySeek(nextReadPos,1);
    byteRead=wrappedStream.read();
  }
 catch (  EOFException e) {
    return -1;
  }
catch (  IOException e) {
    onReadFailure(e,1);
    byteRead=wrappedStream.read();
  }
  if (byteRead >= 0) {
    pos++;
    nextReadPos++;
  }
  if (byteRead >= 0) {
    incrementBytesRead(1);
  }
  return byteRead;
}
