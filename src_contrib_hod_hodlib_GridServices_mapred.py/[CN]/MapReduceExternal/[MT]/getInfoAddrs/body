def getInfoAddrs(self):
    attrs = self.serviceDesc.getfinalAttrs()
    if (self.version < 16):
        addr = attrs['mapred.job.tracker']
        (k, v) = addr.split(':')
        infoaddr = ((k + ':') + attrs['mapred.job.tracker.info.port'])
    else:
        infoaddr = attrs['mapred.job.tracker.http.address']
    return [infoaddr]
