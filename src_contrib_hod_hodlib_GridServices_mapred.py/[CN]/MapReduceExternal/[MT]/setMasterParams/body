def setMasterParams(self, dict):
    self.serviceDesc['final-attrs']['mapred.job.tracker'] = ('%s:%s' % (dict['host'], dict['tracker_port']))
    if (self.version < 16):
        self.serviceDesc.dict['final-attrs']['mapred.job.tracker.info.port'] = str(self.serviceDesc.dict['info_port'])
    else:
        self.serviceDesc['final-attrs']['mapred.job.tracker.http.address'] = ('%s:%s' % (dict['host'], dict['info_port']))
