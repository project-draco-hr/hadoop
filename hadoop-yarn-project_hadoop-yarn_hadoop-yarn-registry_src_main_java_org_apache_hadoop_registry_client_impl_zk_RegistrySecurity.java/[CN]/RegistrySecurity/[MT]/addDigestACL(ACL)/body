{
  if (secureRegistry) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Added ACL {}",aclToString(acl));
    }
    digestACLs.add(acl);
    return true;
  }
 else {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Ignoring added ACL - registry is insecure{}",aclToString(acl));
    }
    return false;
  }
}
