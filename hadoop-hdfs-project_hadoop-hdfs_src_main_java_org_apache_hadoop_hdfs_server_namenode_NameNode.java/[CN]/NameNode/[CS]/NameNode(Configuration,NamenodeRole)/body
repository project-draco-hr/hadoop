{
  this.role=role;
  this.haEnabled=DFSUtil.isHAEnabled(conf);
  this.state=!haEnabled ? ACTIVE_STATE : STANDBY_STATE;
  try {
    initializeGenericKeys(conf,getNameServiceId(conf));
    initialize(conf);
  }
 catch (  IOException e) {
    this.stop();
    throw e;
  }
catch (  HadoopIllegalArgumentException e) {
    this.stop();
    throw e;
  }
}
