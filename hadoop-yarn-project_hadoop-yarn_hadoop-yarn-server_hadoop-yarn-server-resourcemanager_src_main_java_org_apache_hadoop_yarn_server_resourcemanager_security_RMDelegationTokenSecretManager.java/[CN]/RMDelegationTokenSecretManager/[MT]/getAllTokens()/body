{
  Map<RMDelegationTokenIdentifier,Long> allTokens=new HashMap<RMDelegationTokenIdentifier,Long>();
  for (  Map.Entry<RMDelegationTokenIdentifier,DelegationTokenInformation> entry : currentTokens.entrySet()) {
    allTokens.put(entry.getKey(),entry.getValue().getRenewDate());
  }
  return allTokens;
}
