{
  this.conf=conf;
  this.queueManager=queueManager;
  this.infoQueues=queueManager.getLeafQueueNames();
  this.store=ReflectionUtils.newInstance(conf.getClass(PrioritySchedulerOptions.DYNAMIC_SCHEDULER_STORE,FileAllocationStore.class,AllocationStore.class),conf);
  this.store.init(this.conf);
  this.store.load();
}
