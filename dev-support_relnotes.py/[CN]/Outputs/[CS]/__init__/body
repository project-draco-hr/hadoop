def __init__(self, base_file_name, file_name_pattern, keys, params={}):
    self.params = params
    self.base = open((base_file_name % params), 'w')
    self.others = {}
    for key in keys:
        both = dict(params)
        both['key'] = key
        self.others[key] = open((file_name_pattern % both), 'w')
