{
  final Block b=reportedBlock != null ? reportedBlock.getBlock() : null;
  String bStr=b != null ? (b + " with size=" + b.getNumBytes()+ " ") : " ";
  return bStr + "from " + source.getDisplayName()+ " to "+ target.getDisplayName()+ " through "+ (proxySource != null ? proxySource.datanode : "");
}
