{
  if (this == obj) {
    return true;
  }
 else   if (obj == null || !(obj instanceof StorageGroup)) {
    return false;
  }
 else {
    final StorageGroup that=(StorageGroup)obj;
    return this.getStorageType() == that.getStorageType() && this.getDatanodeInfo().equals(that.getDatanodeInfo());
  }
}
