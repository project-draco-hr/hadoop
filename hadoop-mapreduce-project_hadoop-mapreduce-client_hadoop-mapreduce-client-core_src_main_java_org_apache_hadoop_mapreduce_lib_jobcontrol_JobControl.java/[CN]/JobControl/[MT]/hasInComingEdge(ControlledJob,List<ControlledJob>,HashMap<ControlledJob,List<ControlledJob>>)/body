{
  boolean hasIncomingEdge=false;
  for (  ControlledJob k : controlledJobList) {
    if (k != controlledJob && k.getDependentJobs() != null && !processedMap.get(k).contains(controlledJob) && k.getDependentJobs().contains(controlledJob)) {
      hasIncomingEdge=true;
      break;
    }
  }
  return hasIncomingEdge;
}
