{
  assertEquals("shuffleFinishTime wrong",ta.getShuffleFinishTime(),shuffleFinishTime);
  assertEquals("mergeFinishTime wrong",ta.getSortFinishTime(),mergeFinishTime);
  assertEquals("elapsedShuffleTime wrong",ta.getShuffleFinishTime() - ta.getLaunchTime(),elapsedShuffleTime);
  assertEquals("elapsedMergeTime wrong",ta.getSortFinishTime() - ta.getShuffleFinishTime(),elapsedMergeTime);
  assertEquals("elapsedReduceTime wrong",ta.getFinishTime() - ta.getSortFinishTime(),elapsedReduceTime);
}
