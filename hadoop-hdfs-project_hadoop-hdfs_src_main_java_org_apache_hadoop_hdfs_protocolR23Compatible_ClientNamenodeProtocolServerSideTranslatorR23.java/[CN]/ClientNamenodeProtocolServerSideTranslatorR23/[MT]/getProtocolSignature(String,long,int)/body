{
  if (!protocol.equals(RPC.getProtocolName(ClientNamenodeWireProtocol.class))) {
    throw new IOException("Namenode Serverside implements " + RPC.getProtocolName(ClientNamenodeWireProtocol.class) + ". The following requested protocol is unknown: "+ protocol);
  }
  return ProtocolSignature.getProtocolSignature(clientMethodsHash,ClientNamenodeWireProtocol.versionID,ClientNamenodeWireProtocol.class);
}
