{
  if (conf.getBoolean(DFS_WEBHDFS_REST_CSRF_ENABLED_KEY,DFS_WEBHDFS_REST_CSRF_ENABLED_DEFAULT)) {
    this.restCsrfCustomHeader=conf.getTrimmed(DFS_WEBHDFS_REST_CSRF_CUSTOM_HEADER_KEY,DFS_WEBHDFS_REST_CSRF_CUSTOM_HEADER_DEFAULT);
    this.restCsrfMethodsToIgnore=new HashSet<>();
    this.restCsrfMethodsToIgnore.addAll(getTrimmedStringList(conf,DFS_WEBHDFS_REST_CSRF_METHODS_TO_IGNORE_KEY,DFS_WEBHDFS_REST_CSRF_METHODS_TO_IGNORE_DEFAULT));
  }
 else {
    this.restCsrfCustomHeader=null;
    this.restCsrfMethodsToIgnore=null;
  }
}
