{
  thisListing=new FsPathResponseRunner<DirectoryListing>(GetOpParam.Op.LISTSTATUS_BATCH,path,new StartAfterParam(new String(prevKey,Charsets.UTF_8))){
    @Override DirectoryListing decodeResponse(    Map<?,?> json) throws IOException {
      return JsonUtilClient.toDirectoryListing(json);
    }
  }
.run();
  i=0;
  prevKey=thisListing.getLastName();
}
