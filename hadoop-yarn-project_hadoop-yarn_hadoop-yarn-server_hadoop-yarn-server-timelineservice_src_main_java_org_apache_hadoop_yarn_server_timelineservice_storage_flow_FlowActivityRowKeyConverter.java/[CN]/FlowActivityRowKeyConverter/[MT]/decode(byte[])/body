{
  byte[][] rowKeyComponents=Separator.QUALIFIERS.split(rowKey,SEGMENT_SIZES);
  if (rowKeyComponents.length != 4) {
    throw new IllegalArgumentException("the row key is not valid for " + "a flow activity");
  }
  String clusterId=Separator.decode(Bytes.toString(rowKeyComponents[0]),Separator.QUALIFIERS,Separator.TAB,Separator.SPACE);
  Long dayTs=TimelineStorageUtils.invertLong(Bytes.toLong(rowKeyComponents[1]));
  String userId=Separator.decode(Bytes.toString(rowKeyComponents[2]),Separator.QUALIFIERS,Separator.TAB,Separator.SPACE);
  String flowName=Separator.decode(Bytes.toString(rowKeyComponents[3]),Separator.QUALIFIERS,Separator.TAB,Separator.SPACE);
  return new FlowActivityRowKey(clusterId,dayTs,userId,flowName);
}
