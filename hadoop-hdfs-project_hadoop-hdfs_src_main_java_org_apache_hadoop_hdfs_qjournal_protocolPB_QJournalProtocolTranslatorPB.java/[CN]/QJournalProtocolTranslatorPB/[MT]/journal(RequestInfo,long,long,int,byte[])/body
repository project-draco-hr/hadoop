{
  JournalRequestProto req=JournalRequestProto.newBuilder().setReqInfo(convert(reqInfo)).setSegmentTxnId(segmentTxId).setFirstTxnId(firstTxnId).setNumTxns(numTxns).setRecords(PBHelperClient.getByteString(records)).build();
  try {
    rpcProxy.journal(NULL_CONTROLLER,req);
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
