{
  try {
    NewEpochRequestProto req=NewEpochRequestProto.newBuilder().setJid(convertJournalId(jid)).setNsInfo(PBHelper.convert(nsInfo)).setEpoch(epoch).build();
    return rpcProxy.newEpoch(NULL_CONTROLLER,req);
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
