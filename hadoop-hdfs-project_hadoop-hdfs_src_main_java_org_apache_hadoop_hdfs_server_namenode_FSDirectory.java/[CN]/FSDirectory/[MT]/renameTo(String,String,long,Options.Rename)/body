{
  if (NameNode.stateChangeLog.isDebugEnabled()) {
    NameNode.stateChangeLog.debug("DIR* FSDirectory.renameTo: " + src + " to "+ dst);
  }
  waitForReady();
  writeLock();
  try {
    if (unprotectedRenameTo(src,dst,mtime,options)) {
      namesystem.incrDeletedFileCount(1);
    }
  }
  finally {
    writeUnlock();
  }
}
