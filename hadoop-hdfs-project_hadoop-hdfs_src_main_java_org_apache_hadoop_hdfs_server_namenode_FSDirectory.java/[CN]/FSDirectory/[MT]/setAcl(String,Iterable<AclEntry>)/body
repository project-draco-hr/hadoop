{
  writeLock();
  try {
    final INodeWithAdditionalFields node=resolveINodeWithAdditionalField(src);
    AclFeature f=node.getAclFeature();
    if (f == null) {
      f=new AclFeature();
      node.addAclFeature(f);
    }
    f.setEntries(Lists.newArrayList(aclSpec));
  }
  finally {
    writeUnlock();
  }
}
