{
  waitForReady();
  readLock();
  try {
    INode targetNode=rootDir.getNode(src,false);
    if (targetNode == null)     return null;
    if (targetNode.isDirectory())     return null;
    if (targetNode.isSymlink())     return null;
    return ((INodeFile)targetNode).getBlocks();
  }
  finally {
    readUnlock();
  }
}
