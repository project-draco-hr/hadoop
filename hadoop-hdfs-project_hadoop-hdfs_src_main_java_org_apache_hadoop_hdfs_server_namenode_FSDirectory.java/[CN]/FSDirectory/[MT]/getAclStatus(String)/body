{
  String srcs=normalizePath(src);
  readLock();
  try {
    if (srcs.endsWith(HdfsConstants.SEPARATOR_DOT_SNAPSHOT_DIR) && getINode4DotSnapshot(srcs) != null) {
      return new AclStatus.Builder().owner("").group("").build();
    }
    INodesInPath iip=getLastINodeInPath(srcs,true);
    INode inode=resolveLastINode(src,iip);
    int snapshotId=iip.getPathSnapshotId();
    List<AclEntry> acl=AclStorage.readINodeAcl(inode,snapshotId);
    return new AclStatus.Builder().owner(inode.getUserName()).group(inode.getGroupName()).stickyBit(inode.getFsPermission(snapshotId).getStickyBit()).addEntries(acl).build();
  }
  finally {
    readUnlock();
  }
}
