{
  readLock();
  try {
    INodesInPath iip=rootDir.getINodesInPath4Write(normalizePath(src),true);
    final INodeWithAdditionalFields inode=resolveINodeWithAdditionalFields(src,iip);
    int snapshotId=iip.getLatestSnapshotId();
    List<AclEntry> acl=AclStorage.readINodeAcl(inode,snapshotId);
    return new AclStatus.Builder().owner(inode.getUserName()).group(inode.getGroupName()).stickyBit(inode.getFsPermission(snapshotId).getStickyBit()).addEntries(acl).build();
  }
  finally {
    readUnlock();
  }
}
