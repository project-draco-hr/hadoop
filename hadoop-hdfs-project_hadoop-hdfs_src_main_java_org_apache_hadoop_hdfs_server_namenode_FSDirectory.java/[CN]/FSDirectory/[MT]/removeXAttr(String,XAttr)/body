{
  writeLock();
  try {
    XAttr removedXAttr=unprotectedRemoveXAttr(src,xAttr);
    if (removedXAttr != null) {
      fsImage.getEditLog().logRemoveXAttr(src,removedXAttr);
    }
 else {
      NameNode.stateChangeLog.info("DIR* FSDirectory.removeXAttr: XAttr " + XAttrHelper.getPrefixName(xAttr) + " does not exist on the path "+ src);
    }
  }
  finally {
    writeUnlock();
  }
}
