{
  INodesInPath iip;
  if (fileId == HdfsConstants.GRANDFATHER_INODE_ID) {
    iip=resolvePath(pc,src);
  }
 else {
    INode inode=getInode(fileId);
    if (inode == null) {
      iip=INodesInPath.fromComponents(INode.getPathComponents(src));
    }
 else {
      iip=INodesInPath.fromINode(inode);
    }
  }
  return iip;
}
