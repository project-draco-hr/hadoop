{
  int taskIndex=myAttemptID.getTaskId().getId();
  int attemptIndex=myAttemptID.getId();
  float result=200.0F;
switch (taskIndex % 4) {
case 0:
    if (taskIndex % 40 == 0 && attemptIndex == 0) {
      result=600.0F;
      break;
    }
  break;
case 2:
break;
case 1:
result=150.0F;
break;
case 3:
result=250.0F;
break;
}
return result;
}
