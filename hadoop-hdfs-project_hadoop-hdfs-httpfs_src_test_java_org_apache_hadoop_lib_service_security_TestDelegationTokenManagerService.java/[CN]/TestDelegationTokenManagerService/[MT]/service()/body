{
  String dir=TestDirHelper.getTestDir().getAbsolutePath();
  Configuration conf=new Configuration(false);
  conf.set("httpfs.services",StringUtils.join(",",Arrays.asList(InstrumentationService.class.getName(),SchedulerService.class.getName(),FileSystemAccessService.class.getName(),DelegationTokenManagerService.class.getName())));
  Server server=new HttpFSServerWebApp(dir,dir,dir,dir,conf);
  server.init();
  DelegationTokenManager tm=server.get(DelegationTokenManager.class);
  Assert.assertNotNull(tm);
  server.destroy();
}
