{
  String subDir=(isWorkDir) ? TaskTracker.getTaskWorkDir(task.getUser(),task.getJobID().toString(),task.getTaskID().toString(),task.isTaskCleanupTask()) : TaskTracker.getLocalTaskDir(task.getUser(),task.getJobID().toString(),task.getTaskID().toString(),task.isTaskCleanupTask());
  return subDir;
}
