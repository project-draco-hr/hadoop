{
  if (userAgent == null) {
    return false;
  }
  userAgent=userAgent.toLowerCase(Locale.ENGLISH);
  boolean isBrowser=true;
  for (  String nonBrowserUserAgent : nonBrowserUserAgents) {
    if (userAgent.contains(nonBrowserUserAgent)) {
      isBrowser=false;
      break;
    }
  }
  return isBrowser;
}
