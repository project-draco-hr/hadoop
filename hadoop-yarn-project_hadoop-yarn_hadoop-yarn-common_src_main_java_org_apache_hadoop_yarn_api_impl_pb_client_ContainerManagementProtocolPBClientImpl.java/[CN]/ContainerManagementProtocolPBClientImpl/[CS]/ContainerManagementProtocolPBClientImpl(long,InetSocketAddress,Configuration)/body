{
  RPC.setProtocolEngine(conf,ContainerManagementProtocolPB.class,ProtobufRpcEngine.class);
  UserGroupInformation ugi=UserGroupInformation.getCurrentUser();
  int expireIntvl=conf.getInt(NM_COMMAND_TIMEOUT,DEFAULT_COMMAND_TIMEOUT);
  proxy=(ContainerManagementProtocolPB)RPC.getProxy(ContainerManagementProtocolPB.class,clientVersion,addr,ugi,conf,NetUtils.getDefaultSocketFactory(conf),expireIntvl);
}
