{
  for (  MetricsTag t : tags) {
    Pattern pat=includeTagPatterns.get(t.name());
    if (pat != null && pat.matcher(t.value()).matches()) {
      return true;
    }
  }
  for (  MetricsTag t : tags) {
    Pattern pat=excludeTagPatterns.get(t.name());
    if (pat != null && pat.matcher(t.value()).matches()) {
      return false;
    }
  }
  if (!includeTagPatterns.isEmpty() && excludeTagPatterns.isEmpty()) {
    return false;
  }
  return true;
}
