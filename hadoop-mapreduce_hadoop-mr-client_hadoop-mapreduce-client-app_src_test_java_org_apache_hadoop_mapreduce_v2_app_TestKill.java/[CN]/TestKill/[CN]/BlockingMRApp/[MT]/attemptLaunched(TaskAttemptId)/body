{
  if (attemptID.getTaskId().getId() == 0 && attemptID.getId() == 0) {
    try {
      latch.await();
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
  }
 else {
    getContext().getEventHandler().handle(new TaskAttemptEvent(attemptID,TaskAttemptEventType.TA_DONE));
  }
}
