{
  final Future<byte[]> f=removeFirst();
  if (f != null) {
    printf("randomRecycler: ");
    try {
      recycle(f.get(10,TimeUnit.MILLISECONDS));
    }
 catch (    TimeoutException e) {
      recycle(new byte[maxArrayLength]);
      printf("timeout, new byte[%d]\n",maxArrayLength);
    }
  }
}
