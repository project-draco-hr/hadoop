{
  super(NodeHealthScriptRunner.class.getName());
  this.lastReportedTime=System.currentTimeMillis();
  this.isHealthy=true;
  this.healthReport="";
  this.nodeHealthScript=scriptName;
  this.intervalTime=chkInterval;
