{
  boolean test=false;
switch (flag) {
case 'e':
    test=true;
  break;
case 'd':
test=item.stat.isDirectory();
break;
case 'f':
test=item.stat.isFile();
break;
case 's':
test=(item.stat.getLen() > 0);
break;
case 'z':
test=(item.stat.getLen() == 0);
break;
default :
break;
}
if (!test) exitCode=1;
}
