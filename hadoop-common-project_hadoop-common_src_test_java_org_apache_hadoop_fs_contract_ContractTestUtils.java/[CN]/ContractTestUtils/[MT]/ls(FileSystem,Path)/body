{
  if (path == null) {
    return "/";
  }
  FileStatus[] stats;
  String pathtext="ls " + path;
  try {
    stats=fileSystem.listStatus(path);
  }
 catch (  FileNotFoundException e) {
    return pathtext + " -file not found";
  }
catch (  IOException e) {
    return pathtext + " -failed: " + e;
  }
  return dumpStats(pathtext,stats);
}
