{
  if (initCalled) {
    return;
  }
 else {
    initCalled=true;
  }
  Class<? extends DNSToSwitchMapping> dnsToSwitchMappingClass=conf.getClass(CommonConfigurationKeysPublic.NET_TOPOLOGY_NODE_SWITCH_MAPPING_IMPL_KEY,ScriptBasedMapping.class,DNSToSwitchMapping.class);
  try {
    Constructor<? extends DNSToSwitchMapping> dnsToSwitchMappingConstructor=dnsToSwitchMappingClass.getConstructor();
    dnsToSwitchMapping=dnsToSwitchMappingConstructor.newInstance();
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
