{
  if (properties == null) {
    getProps();
  }
  if (properties == null || updatingResource == null) {
    return null;
  }
 else {
    String source=updatingResource.get(name);
    if (source == null || source.equals(UNKNOWN_RESOURCE)) {
      return null;
    }
 else {
      return source;
    }
  }
}
