{
  Configuration conf=getConfig();
  String jaasContext=conf.getTrimmed(KEY_REGISTRY_ZKSERVICE_JAAS_CONTEXT);
  secureServer=StringUtils.isNotEmpty(jaasContext);
  if (secureServer) {
    RegistrySecurity.validateContext(jaasContext);
    RegistrySecurity.bindZKToServerJAASContext(jaasContext);
    System.setProperty(PROP_ZK_ALLOW_FAILED_SASL_CLIENTS,conf.get(KEY_ZKSERVICE_ALLOW_FAILED_SASL_CLIENTS,"true"));
    System.setProperty(RegistryInternalConstants.ZOOKEEPER_AUTH_PROVIDER + ".1",RegistryInternalConstants.SASLAUTHENTICATION_PROVIDER);
    String serverContext=System.getProperty(PROP_ZK_SERVER_SASL_CONTEXT);
    addDiagnostics("Server JAAS context s = %s",serverContext);
    return true;
  }
 else {
    return false;
  }
}
