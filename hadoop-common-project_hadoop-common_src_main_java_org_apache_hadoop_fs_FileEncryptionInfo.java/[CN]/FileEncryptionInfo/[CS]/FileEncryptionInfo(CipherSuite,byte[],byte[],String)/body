{
  checkNotNull(suite);
  checkNotNull(edek);
  checkNotNull(iv);
  checkNotNull(ezKeyVersionName);
  checkArgument(edek.length == suite.getAlgorithmBlockSize(),"Unexpected key length");
  checkArgument(iv.length == suite.getAlgorithmBlockSize(),"Unexpected IV length");
  this.cipherSuite=suite;
  this.edek=edek;
  this.iv=iv;
  this.ezKeyVersionName=ezKeyVersionName;
}
