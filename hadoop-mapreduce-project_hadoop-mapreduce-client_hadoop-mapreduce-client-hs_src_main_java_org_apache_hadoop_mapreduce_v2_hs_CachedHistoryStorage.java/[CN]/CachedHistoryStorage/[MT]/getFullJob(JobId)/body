{
  Job retVal=null;
  try {
    retVal=loadedJobCache.getUnchecked(jobId);
  }
 catch (  UncheckedExecutionException e) {
    if (e.getCause() instanceof HSFileRuntimeException) {
      LOG.error(e.getCause().getMessage());
      return null;
    }
 else {
      throw new YarnRuntimeException(e.getCause());
    }
  }
  return retVal;
}
