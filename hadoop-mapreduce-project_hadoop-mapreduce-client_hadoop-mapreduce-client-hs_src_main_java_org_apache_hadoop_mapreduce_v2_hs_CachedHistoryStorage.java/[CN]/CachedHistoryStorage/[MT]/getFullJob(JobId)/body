{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Looking for Job " + jobId);
  }
  try {
    Job result=loadedJobCache.get(jobId);
    if (result == null) {
      MetaInfo metaInfo=hsManager.getMetaInfo(jobId);
      if (metaInfo != null) {
        result=loadJob(metaInfo);
      }
    }
    return result;
  }
 catch (  IOException e) {
    throw new YarnException(e);
  }
}
