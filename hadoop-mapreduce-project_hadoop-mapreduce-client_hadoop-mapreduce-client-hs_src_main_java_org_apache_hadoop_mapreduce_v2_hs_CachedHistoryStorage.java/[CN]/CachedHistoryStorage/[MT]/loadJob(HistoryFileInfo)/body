{
  try {
    Job job=fileInfo.loadJob();
    if (LOG.isDebugEnabled()) {
      LOG.debug("Adding " + job.getID() + " to loaded job cache");
    }
    loadedJobCache.put(job.getID(),job);
    return job;
  }
 catch (  IOException e) {
    throw new YarnRuntimeException("Could not find/load job: " + fileInfo.getJobId(),e);
  }
}
