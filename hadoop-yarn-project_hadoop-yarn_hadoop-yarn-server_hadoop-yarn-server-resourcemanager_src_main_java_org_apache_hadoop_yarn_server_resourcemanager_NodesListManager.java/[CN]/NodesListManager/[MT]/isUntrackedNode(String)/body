{
  boolean untracked;
  String ip=resolver.resolve(hostName);
synchronized (hostsReader) {
    Set<String> hostsList=hostsReader.getHosts();
    Set<String> excludeList=hostsReader.getExcludedHosts();
    untracked=!hostsList.isEmpty() && !hostsList.contains(hostName) && !hostsList.contains(ip)&& !excludeList.contains(hostName)&& !excludeList.contains(ip);
  }
  return untracked;
}
