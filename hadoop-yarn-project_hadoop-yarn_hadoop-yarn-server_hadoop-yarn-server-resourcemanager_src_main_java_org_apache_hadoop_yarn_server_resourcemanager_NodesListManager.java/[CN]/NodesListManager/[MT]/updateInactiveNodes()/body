{
  long now=Time.monotonicNow();
  for (  Entry<NodeId,RMNode> entry : rmContext.getInactiveRMNodes().entrySet()) {
    NodeId nodeId=entry.getKey();
    RMNode rmNode=entry.getValue();
    if (isUntrackedNode(nodeId.getHost()) && rmNode.getUntrackedTimeStamp() == 0) {
      rmNode.setUntrackedTimeStamp(now);
    }
  }
}
