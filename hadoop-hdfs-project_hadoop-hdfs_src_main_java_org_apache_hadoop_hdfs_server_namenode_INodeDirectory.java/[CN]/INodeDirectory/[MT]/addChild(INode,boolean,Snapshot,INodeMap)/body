{
  final int low=searchChildren(node.getLocalNameBytes());
  if (low >= 0) {
    return false;
  }
  if (isInLatestSnapshot(latest)) {
    INodeDirectoryWithSnapshot sdir=replaceSelf4INodeDirectoryWithSnapshot(inodeMap);
    boolean added=sdir.addChild(node,setModTime,latest,inodeMap);
    return added;
  }
  addChild(node,low);
  if (setModTime) {
    updateModificationTime(node.getModificationTime(),latest,inodeMap);
  }
  return true;
}
