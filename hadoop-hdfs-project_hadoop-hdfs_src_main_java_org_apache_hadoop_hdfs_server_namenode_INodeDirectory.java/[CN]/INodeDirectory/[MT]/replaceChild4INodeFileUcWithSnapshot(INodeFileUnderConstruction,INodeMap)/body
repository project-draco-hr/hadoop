{
  Preconditions.checkArgument(!(child instanceof INodeFileUnderConstructionWithSnapshot),"Child file is already an INodeFileUnderConstructionWithSnapshot, child=" + child);
  final INodeFileUnderConstructionWithSnapshot newChild=new INodeFileUnderConstructionWithSnapshot(child,null);
  replaceChildFile(child,newChild,inodeMap);
  return newChild;
}
