{
  assertChildrenNonNull();
  Preconditions.checkArgument(!(child instanceof INodeFileWithSnapshot),"Child file is already an INodeFileWithLink, child=" + child);
  final INodeFileWithSnapshot newChild=new INodeFileWithSnapshot(child);
  final int i=searchChildrenForExistingINode(newChild);
  children.set(i,newChild);
  return newChild;
}
