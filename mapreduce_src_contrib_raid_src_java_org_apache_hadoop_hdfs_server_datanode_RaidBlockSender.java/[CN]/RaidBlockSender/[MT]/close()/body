{
  IOException ioe=null;
  if (checksumIn != null) {
    try {
      checksumIn.close();
    }
 catch (    IOException e) {
      ioe=e;
    }
    checksumIn=null;
  }
  if (blockIn != null) {
    try {
      blockIn.close();
    }
 catch (    IOException e) {
      ioe=e;
    }
    blockIn=null;
  }
  if (ioe != null) {
    throw ioe;
  }
}
