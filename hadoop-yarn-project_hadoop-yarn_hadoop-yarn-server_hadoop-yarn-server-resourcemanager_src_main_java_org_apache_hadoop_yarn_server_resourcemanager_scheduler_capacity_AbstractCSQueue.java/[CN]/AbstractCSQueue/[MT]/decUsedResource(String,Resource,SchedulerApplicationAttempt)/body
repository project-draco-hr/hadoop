{
  if (nodeLabel == null) {
    nodeLabel=RMNodeLabelsManager.NO_LABEL;
  }
  queueUsage.decUsed(nodeLabel,resourceToDec);
  CSQueueUtils.updateUsedCapacity(resourceCalculator,labelManager.getResourceByLabel(nodeLabel,Resources.none()),minimumAllocation,queueUsage,queueCapacities,nodeLabel);
  if (null != parent) {
    parent.decUsedResource(nodeLabel,resourceToDec,null);
  }
}
