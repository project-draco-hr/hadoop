{
  try {
    writeLock.lock();
    queueUsage.incUsed(nodePartition,resource);
    if (!changeContainerResource) {
      ++numContainers;
    }
    CSQueueUtils.updateQueueStatistics(resourceCalculator,clusterResource,minimumAllocation,this,labelManager,nodePartition);
  }
  finally {
    writeLock.unlock();
  }
}
