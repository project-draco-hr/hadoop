{
  if (timelineEntity.getPrimaryFilters() != null && timelineEntity.getPrimaryFilters().containsKey(TimelineStore.SystemFilter.ENTITY_OWNER.toString())) {
    throw new YarnException("User should not use the timeline system filter key: " + TimelineStore.SystemFilter.ENTITY_OWNER);
  }
  timelineEntity.addPrimaryFilter(TimelineStore.SystemFilter.ENTITY_OWNER.toString(),owner);
}
