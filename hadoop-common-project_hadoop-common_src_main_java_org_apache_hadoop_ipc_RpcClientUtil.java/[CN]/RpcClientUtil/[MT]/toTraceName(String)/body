{
  int lastPeriod=fullName.lastIndexOf('.');
  if (lastPeriod < 0) {
    return fullName;
  }
  int secondLastPeriod=fullName.lastIndexOf('.',lastPeriod - 1);
  if (secondLastPeriod < 0) {
    return fullName;
  }
  return fullName.substring(secondLastPeriod + 1,lastPeriod) + "#" + fullName.substring(lastPeriod + 1);
}
