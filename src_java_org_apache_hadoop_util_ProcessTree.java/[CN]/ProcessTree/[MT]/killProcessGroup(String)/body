{
  if (!ProcessTree.isProcessGroupAlive(pgrpId)) {
    return;
  }
  String[] args={"kill","-9","-" + pgrpId};
  ShellCommandExecutor shexec=new ShellCommandExecutor(args);
  try {
    shexec.execute();
  }
 catch (  IOException e) {
    LOG.warn("Error sending SIGKILL to process group " + pgrpId + " ."+ StringUtils.stringifyException(e));
  }
 finally {
    LOG.info("Killing process group" + pgrpId + " with SIGKILL. Exit code "+ shexec.getExitCode());
  }
}
