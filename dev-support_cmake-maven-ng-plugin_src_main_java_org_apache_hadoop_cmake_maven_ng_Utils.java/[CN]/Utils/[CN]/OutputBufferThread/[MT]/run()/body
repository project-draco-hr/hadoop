{
  try {
    char[] arr=new char[8192];
    while (true) {
      int amt=reader.read(arr);
      if (amt < 0)       return;
      char[] arr2=new char[amt];
      for (int i=0; i < amt; i++) {
        arr2[i]=arr[i];
      }
      bufs.add(arr2);
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
 finally {
    try {
      reader.close();
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
}
