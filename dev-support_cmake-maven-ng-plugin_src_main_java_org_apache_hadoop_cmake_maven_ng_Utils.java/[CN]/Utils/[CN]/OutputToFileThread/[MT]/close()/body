{
  if (is != null) {
    try {
      is.close();
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
 finally {
      is=null;
    }
  }
  if (out != null) {
    try {
      out.close();
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
 finally {
      out=null;
    }
  }
}
