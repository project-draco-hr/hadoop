{
  if ((prefix == null) || (prefix.equals(""))) {
    return;
  }
  FileOutputStream fos=new FileOutputStream(outFile,false);
  BufferedWriter wr=null;
  try {
    wr=new BufferedWriter(new OutputStreamWriter(fos,"UTF8"));
    wr.write(prefix);
  }
  finally {
    if (wr != null) {
      wr.close();
    }
 else {
      fos.close();
    }
  }
}
