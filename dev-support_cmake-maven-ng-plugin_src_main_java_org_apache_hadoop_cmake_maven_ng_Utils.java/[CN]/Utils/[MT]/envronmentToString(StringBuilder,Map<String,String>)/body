{
  if ((env == null) || (env.isEmpty())) {
    return;
  }
  bld.append("ENV: ");
  for (  Map.Entry<String,String> entry : env.entrySet()) {
    String val=entry.getValue();
    if (val == null) {
      val="";
    }
    bld.append(entry.getKey()).append(" = ").append(val).append("\n");
  }
  bld.append("=======================================" + "========================================\n");
}
