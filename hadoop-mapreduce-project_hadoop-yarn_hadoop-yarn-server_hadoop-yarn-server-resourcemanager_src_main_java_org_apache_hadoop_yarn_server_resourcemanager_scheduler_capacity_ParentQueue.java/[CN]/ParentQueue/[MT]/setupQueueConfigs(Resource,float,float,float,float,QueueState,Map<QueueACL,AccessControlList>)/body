{
  CSQueueUtils.checkMaxCapacity(getQueueName(),capacity,maximumCapacity);
  this.capacity=capacity;
  this.absoluteCapacity=absoluteCapacity;
  this.maximumCapacity=maximumCapacity;
  this.absoluteMaxCapacity=absoluteMaxCapacity;
  this.state=state;
  this.acls=acls;
  this.queueInfo.setCapacity(this.capacity);
  this.queueInfo.setMaximumCapacity(this.maximumCapacity);
  this.queueInfo.setQueueState(this.state);
  StringBuilder aclsString=new StringBuilder();
  for (  Map.Entry<QueueACL,AccessControlList> e : acls.entrySet()) {
    aclsString.append(e.getKey() + ":" + e.getValue().getAclString());
  }
  CSQueueUtils.updateQueueStatistics(this,parent,clusterResource,minimumAllocation);
  LOG.info(queueName + ", capacity=" + capacity+ ", asboluteCapacity="+ absoluteCapacity+ ", maxCapacity="+ maximumCapacity+ ", asboluteMaxCapacity="+ absoluteMaxCapacity+ ", state="+ state+ ", acls="+ aclsString);
}
