{
  float queueLimit=clusterResource.getMemory() * absoluteCapacity;
  float parentAbsoluteCapacity=(rootQueue) ? 1.0f : parent.getAbsoluteCapacity();
  setUtilization(usedResources.getMemory() / queueLimit);
  setUsedCapacity(usedResources.getMemory() / (clusterResource.getMemory() * parentAbsoluteCapacity));
  Resource resourceLimit=Resources.createResource((int)queueLimit);
  metrics.setAvailableResourcesToQueue(Resources.subtractFrom(resourceLimit,usedResources));
}
