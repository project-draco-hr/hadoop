{
  BufferedReader reader=null;
  String strLine;
  long count=0;
  try {
    reader=new BufferedReader(new FileReader(entityFile));
    while ((strLine=reader.readLine()) != null) {
      if (strLine.trim().length() > 0)       count++;
    }
  }
  finally {
    reader.close();
  }
  return count;
}
