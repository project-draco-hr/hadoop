{
  String outputDirApp=testRootDir.getAbsolutePath() + "/" + FileSystemTimelineWriterImpl.ENTITIES_DIR+ "/"+ YarnConfiguration.DEFAULT_RM_CLUSTER_ID+ "/"+ app.getUser()+ "/"+ app.getName()+ "/"+ TimelineUtils.DEFAULT_FLOW_VERSION+ "/"+ app.getStartTime()+ "/"+ app.getApplicationId();
  return outputDirApp;
}
