{
  String outputDirApp=testRootDir.getAbsolutePath() + "/" + FileSystemTimelineWriterImpl.ENTITIES_DIR+ "/"+ YarnConfiguration.DEFAULT_RM_CLUSTER_ID+ "/"+ app.getUser()+ "/"+ TimelineUtils.generateDefaultFlowNameBasedOnAppId(app.getApplicationId())+ "/"+ DEFAULT_FLOW_VERSION+ "/"+ DEFAULT_FLOW_RUN+ "/"+ app.getApplicationId();
  return outputDirApp;
}
