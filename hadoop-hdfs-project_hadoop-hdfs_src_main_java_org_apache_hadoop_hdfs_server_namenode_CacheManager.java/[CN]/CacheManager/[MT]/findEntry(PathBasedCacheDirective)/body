{
  List<PathBasedCacheEntry> existing=entriesByPath.get(directive.getPath().toUri().getPath());
  if (existing == null) {
    return null;
  }
  for (  PathBasedCacheEntry entry : existing) {
    if (entry.getPool().getPoolName().equals(directive.getPool())) {
      return entry;
    }
  }
  return null;
}
