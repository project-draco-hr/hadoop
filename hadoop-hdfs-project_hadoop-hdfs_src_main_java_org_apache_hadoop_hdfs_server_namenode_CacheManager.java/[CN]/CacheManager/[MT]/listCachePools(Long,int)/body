{
  final int MAX_PREALLOCATED_REPLIES=16;
  ArrayList<CachePool> results=new ArrayList<CachePool>(Math.min(MAX_PREALLOCATED_REPLIES,maxRepliesPerRequest));
  SortedMap<Long,CachePool> tailMap=cachePoolsById.tailMap(prevKey,false);
  for (  CachePool pool : tailMap.values()) {
    results.add(pool);
  }
  return results;
}
