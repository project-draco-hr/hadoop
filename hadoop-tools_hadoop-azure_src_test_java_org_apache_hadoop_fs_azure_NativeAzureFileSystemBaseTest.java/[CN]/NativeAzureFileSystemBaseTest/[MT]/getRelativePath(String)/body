{
  int slashCount=0;
  int i;
  for (i=0; i < path.length(); i++) {
    if (path.charAt(i) == '/') {
      slashCount++;
      if (slashCount == 3) {
        return path.substring(i + 1,path.length());
      }
    }
  }
  throw new RuntimeException("Incorrect path prefix -- expected wasb://.../...");
}
