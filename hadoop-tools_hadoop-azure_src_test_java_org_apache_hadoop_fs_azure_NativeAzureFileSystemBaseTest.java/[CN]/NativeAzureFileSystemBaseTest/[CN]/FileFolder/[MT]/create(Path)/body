{
  if (isFolder()) {
    if (present) {
      assertTrue(fs.mkdirs(makePath(prefix,name)));
    }
    create(makePath(prefix,name),members);
  }
 else   if (isLeaf()) {
    if (present) {
      assertTrue(fs.createNewFile(makePath(prefix,name)));
    }
  }
 else {
    assertTrue("The object must be a (leaf) file or a folder.",false);
  }
}
