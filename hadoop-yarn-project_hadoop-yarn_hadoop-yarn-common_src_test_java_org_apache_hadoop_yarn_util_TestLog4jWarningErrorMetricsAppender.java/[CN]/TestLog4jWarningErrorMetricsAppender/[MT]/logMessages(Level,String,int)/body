{
  for (int i=0; i < count; ++i) {
switch (level.toInt()) {
case Level.FATAL_INT:
      logger.fatal(message);
    break;
case Level.ERROR_INT:
  logger.error(message);
break;
case Level.WARN_INT:
logger.warn(message);
break;
case Level.INFO_INT:
logger.info(message);
break;
case Level.DEBUG_INT:
logger.debug(message);
break;
case Level.TRACE_INT:
logger.trace(message);
break;
}
}
}
