def _createWorkDirs(self):
    for dir in self.workdirs:
        if os.path.exists(dir):
            if (not os.access(dir, (((os.F_OK | os.R_OK) | os.W_OK) | os.X_OK))):
                raise ValueError, ('Workdir %s does not allow rwx permission.' % dir)
            continue
        try:
            os.makedirs(dir)
        except:
            pass
