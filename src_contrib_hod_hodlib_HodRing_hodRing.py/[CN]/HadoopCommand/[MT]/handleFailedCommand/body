def handleFailedCommand(self):
    self.log.error(('hadoop error: %s' % self.__hadoopThread.exit_status_string()))
    if os.path.exists(self.err):
        f = None
        try:
            f = open(self.err)
            lines = f.readlines()
            for line in lines:
                self.stdErrContents = ('%s%s' % (self.stdErrContents, line))
        finally:
            if (f is not None):
                f.close()
    self.log.error(((('See %s.out and/or %s.err for details. They are ' % (self.name, self.name)) + 'located at subdirectories under either ') + 'hodring.work-dirs or hodring.log-destination-uri.'))
