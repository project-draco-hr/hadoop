{
  LOG.info(rmId + "is elected leader, transitioning to active");
  try {
    rmContext.getRMAdminService().transitionToActive(new HAServiceProtocol.StateChangeRequestInfo(HAServiceProtocol.RequestSource.REQUEST_BY_ZKFC));
  }
 catch (  Exception e) {
    LOG.info(rmId + " failed to transition to active, giving up leadership",e);
    notLeader();
    reJoinElection();
  }
}
