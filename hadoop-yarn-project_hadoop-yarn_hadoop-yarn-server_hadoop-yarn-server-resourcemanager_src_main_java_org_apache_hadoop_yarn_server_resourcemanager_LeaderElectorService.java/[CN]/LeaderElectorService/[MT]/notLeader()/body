{
  LOG.info(rmId + " relinquish leadership");
  try {
    rmContext.getRMAdminService().transitionToStandby(new HAServiceProtocol.StateChangeRequestInfo(HAServiceProtocol.RequestSource.REQUEST_BY_ZKFC));
  }
 catch (  Exception e) {
    LOG.info(rmId + " did not transition to standby successfully.");
  }
}
