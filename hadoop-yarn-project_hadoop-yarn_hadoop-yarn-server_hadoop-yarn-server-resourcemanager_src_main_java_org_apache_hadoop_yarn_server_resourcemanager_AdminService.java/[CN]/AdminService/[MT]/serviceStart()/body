{
  Configuration conf=getConfig();
  YarnRPC rpc=YarnRPC.create(conf);
  this.server=rpc.getServer(ResourceManagerAdministrationProtocol.class,this,masterServiceAddress,conf,null,conf.getInt(YarnConfiguration.RM_ADMIN_CLIENT_THREAD_COUNT,YarnConfiguration.DEFAULT_RM_ADMIN_CLIENT_THREAD_COUNT));
  if (conf.getBoolean(CommonConfigurationKeysPublic.HADOOP_SECURITY_AUTHORIZATION,false)) {
    refreshServiceAcls(conf,new RMPolicyProvider());
  }
  this.server.start();
  conf.updateConnectAddr(YarnConfiguration.RM_ADMIN_ADDRESS,server.getListenerAddress());
  super.serviceStart();
}
