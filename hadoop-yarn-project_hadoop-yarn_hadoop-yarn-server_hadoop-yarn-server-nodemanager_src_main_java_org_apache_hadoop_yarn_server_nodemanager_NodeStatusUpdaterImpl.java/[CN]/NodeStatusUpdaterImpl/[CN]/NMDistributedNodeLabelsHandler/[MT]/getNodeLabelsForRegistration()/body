{
  Set<NodeLabel> nodeLabels=nodeLabelsProvider.getNodeLabels();
  nodeLabels=(null == nodeLabels) ? CommonNodeLabelsManager.EMPTY_NODELABEL_SET : nodeLabels;
  previousNodeLabels=nodeLabels;
  try {
    validateNodeLabels(nodeLabels);
  }
 catch (  IOException e) {
    nodeLabels=null;
  }
  return nodeLabels;
}
