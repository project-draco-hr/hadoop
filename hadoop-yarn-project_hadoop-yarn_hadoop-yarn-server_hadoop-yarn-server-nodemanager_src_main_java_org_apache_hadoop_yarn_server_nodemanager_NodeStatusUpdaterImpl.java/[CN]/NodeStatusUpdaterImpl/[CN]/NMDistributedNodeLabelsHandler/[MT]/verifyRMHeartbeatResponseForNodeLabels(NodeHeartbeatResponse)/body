{
  if (updatedLabelsSentToRM) {
    if (response.getAreNodeLabelsAcceptedByRM()) {
      lastNodeLabelSendFailMills=0L;
      LOG.info("Node Labels {" + StringUtils.join(",",previousNodeLabels) + "} were Accepted by RM ");
    }
 else {
      lastNodeLabelSendFailMills=System.currentTimeMillis();
      LOG.error("NM node labels {" + StringUtils.join(",",previousNodeLabels) + "} were not accepted by RM and message from RM : "+ response.getDiagnosticsMessage());
    }
  }
}
