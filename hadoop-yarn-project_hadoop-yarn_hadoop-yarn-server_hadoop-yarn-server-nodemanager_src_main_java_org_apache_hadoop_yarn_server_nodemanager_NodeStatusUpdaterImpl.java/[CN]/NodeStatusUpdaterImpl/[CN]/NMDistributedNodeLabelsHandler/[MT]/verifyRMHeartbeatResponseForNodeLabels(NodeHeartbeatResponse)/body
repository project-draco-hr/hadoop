{
  if (areLabelsSentToRM) {
    if (response.getAreNodeLabelsAcceptedByRM() && LOG.isDebugEnabled()) {
      LOG.debug("Node Labels {" + StringUtils.join(",",previousNodeLabels) + "} were Accepted by RM ");
    }
 else {
      LOG.error("NM node labels {" + StringUtils.join(",",previousNodeLabels) + "} were not accepted by RM and message from RM : "+ response.getDiagnosticsMessage());
    }
  }
}
