{
  this.nodeId=this.context.getNodeId();
  InetSocketAddress httpBindAddress=getConfig().getSocketAddr(YarnConfiguration.NM_WEBAPP_ADDRESS,YarnConfiguration.DEFAULT_NM_WEBAPP_ADDRESS,YarnConfiguration.DEFAULT_NM_WEBAPP_PORT);
  try {
    this.httpPort=httpBindAddress.getPort();
    registerWithRM();
    super.start();
    startStatusUpdater();
  }
 catch (  Exception e) {
    throw new AvroRuntimeException(e);
  }
}
