{
synchronized (recentlyStoppedContainers) {
    long currentTime=System.currentTimeMillis();
    Iterator<ContainerId> i=recentlyStoppedContainers.keySet().iterator();
    while (i.hasNext()) {
      if (recentlyStoppedContainers.get(i.next()) < currentTime) {
        i.remove();
      }
 else {
        break;
      }
    }
  }
}
