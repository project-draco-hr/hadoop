{
  RecordFactory recordFactory=RecordFactoryPBImpl.get();
  UnRegisterNodeManagerRequest request=recordFactory.newRecordInstance(UnRegisterNodeManagerRequest.class);
  request.setNodeId(this.nodeId);
  try {
    resourceTracker.unRegisterNodeManager(request);
    LOG.info("Successfully Unregistered the Node " + this.nodeId + " with ResourceManager.");
  }
 catch (  Exception e) {
    LOG.warn("Unregistration of the Node " + this.nodeId + " failed.",e);
  }
}
