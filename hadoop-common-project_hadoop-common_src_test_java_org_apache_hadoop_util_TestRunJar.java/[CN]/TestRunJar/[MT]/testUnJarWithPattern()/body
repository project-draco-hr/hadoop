{
  File unjarDir=new File(TEST_ROOT_DIR,"unjar-pattern");
  assertFalse("unjar dir shouldn't exist at test start",new File(unjarDir,"foobar.txt").exists());
  RunJar.unJar(new File(TEST_ROOT_DIR,TEST_JAR_NAME),unjarDir,Pattern.compile(".*baz.*"));
  assertFalse("foobar not unpacked",new File(unjarDir,"foobar.txt").exists());
  assertTrue("foobaz unpacked",new File(unjarDir,"foobaz.txt").exists());
}
