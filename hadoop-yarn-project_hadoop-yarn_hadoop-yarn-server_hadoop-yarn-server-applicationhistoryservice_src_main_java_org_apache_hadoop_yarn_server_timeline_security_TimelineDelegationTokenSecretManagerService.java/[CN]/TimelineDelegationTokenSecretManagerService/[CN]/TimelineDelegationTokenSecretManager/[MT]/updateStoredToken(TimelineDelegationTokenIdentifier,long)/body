{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Updating token " + tokenId.getSequenceNumber());
  }
  try {
    if (stateStore != null) {
      stateStore.updateToken(tokenId,renewDate);
    }
  }
 catch (  IOException e) {
    LOG.error("Unable to update token " + tokenId.getSequenceNumber(),e);
  }
}
