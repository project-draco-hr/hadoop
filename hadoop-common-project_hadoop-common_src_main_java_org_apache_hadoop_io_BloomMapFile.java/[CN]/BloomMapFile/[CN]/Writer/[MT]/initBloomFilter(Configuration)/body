{
  numKeys=conf.getInt(IO_MAPFILE_BLOOM_SIZE_KEY,IO_MAPFILE_BLOOM_SIZE_DEFAULT);
  float errorRate=conf.getFloat(IO_MAPFILE_BLOOM_ERROR_RATE_KEY,IO_MAPFILE_BLOOM_ERROR_RATE_DEFAULT);
  vectorSize=(int)Math.ceil((double)(-HASH_COUNT * numKeys) / Math.log(1.0 - Math.pow(errorRate,1.0 / HASH_COUNT)));
  bloomFilter=new DynamicBloomFilter(vectorSize,HASH_COUNT,Hash.getHashType(conf),numKeys);
}
