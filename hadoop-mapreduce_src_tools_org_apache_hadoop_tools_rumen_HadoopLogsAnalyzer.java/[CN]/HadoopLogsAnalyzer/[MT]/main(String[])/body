{
  try {
    HadoopLogsAnalyzer analyzer=new HadoopLogsAnalyzer();
    int result=ToolRunner.run(analyzer,args);
    if (result == 0) {
      return;
    }
    System.exit(result);
  }
 catch (  FileNotFoundException e) {
    LOG.error("",e);
    e.printStackTrace(staticDebugOutput);
    System.exit(1);
  }
catch (  IOException e) {
    LOG.error("",e);
    e.printStackTrace(staticDebugOutput);
    System.exit(2);
  }
catch (  Exception e) {
    LOG.error("",e);
    e.printStackTrace(staticDebugOutput);
    System.exit(3);
  }
}
