{
  ExtendedAclSection s=ExtendedAclSection.parseDelimitedFrom(in);
  ctx.extendedAclTable=new ImmutableList[s.getNumEntry() + 1];
  for (int i=0; i < s.getNumEntry(); ++i) {
    ExtendedAclSection.Entry e=ExtendedAclSection.Entry.parseDelimitedFrom(in);
    ctx.extendedAclTable[e.getId()]=ImmutableList.copyOf(PBHelper.convertAclEntry(e.getAcl().getEntriesList()));
  }
}
