{
  final TableListing listing=getOptionDescriptionListing();
  listing.addRow("<path>","The path of the encryption zone to create. " + "It must be an empty directory.");
  listing.addRow("<keyName>","Name of the key to use for the " + "encryption zone. A new key will be generated if unspecified.");
  return getShortUsage() + "\n" + "Create a new encryption zone.\n\n"+ listing.toString();
}
