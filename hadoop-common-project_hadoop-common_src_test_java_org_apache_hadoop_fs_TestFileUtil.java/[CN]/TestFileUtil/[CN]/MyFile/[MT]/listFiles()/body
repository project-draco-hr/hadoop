{
  final File[] files=super.listFiles();
  if (files == null) {
    return null;
  }
  List<File> filesList=Arrays.asList(files);
  Collections.sort(filesList);
  File[] myFiles=new MyFile[files.length];
  int i=0;
  for (  File f : filesList) {
    myFiles[i++]=new MyFile(f);
  }
  return myFiles;
}
