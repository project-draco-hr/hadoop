def load_url_data(url):
    if onward30:
        ourl = urllib.request.urlopen(url)
        codec = ourl.info().get_param('charset')
        content = ourl.read().decode(codec)
        data = simplejson.loads(content)
    else:
        ourl = urllib2.urlopen(url)
        data = simplejson.load(ourl)
    return data
