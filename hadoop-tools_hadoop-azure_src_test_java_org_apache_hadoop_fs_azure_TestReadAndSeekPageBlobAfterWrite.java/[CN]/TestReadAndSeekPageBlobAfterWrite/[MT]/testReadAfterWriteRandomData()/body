{
  final int PDS=PAGE_DATA_SIZE;
  int[] dataSizes={0,1,2,3,PDS - 1,PDS,PDS + 1,PDS + 2,PDS + 3,(2 * PDS) - 1,(2 * PDS),(2 * PDS) + 1,(2 * PDS) + 2,(2 * PDS) + 3,(10 * PDS) - 1,(10 * PDS),(10 * PDS) + 1,(10 * PDS) + 2,(10 * PDS) + 3,MAX_BYTES};
  for (  int i : dataSizes) {
    testReadAfterWriteRandomData(i);
  }
}
