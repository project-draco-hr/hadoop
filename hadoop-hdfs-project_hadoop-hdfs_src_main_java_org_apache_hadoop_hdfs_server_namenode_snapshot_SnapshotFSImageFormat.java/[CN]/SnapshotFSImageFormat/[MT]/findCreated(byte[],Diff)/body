{
  INode c=diff.searchCreated(createdNodeName);
  INode d=diff.searchDeleted(createdNodeName);
  if (c == null && d != null) {
    return d;
  }
 else   if (c != null && d != null) {
    if (c.isFile() && ((INodeFile)c).getBlocks() == ((INodeFile)d).getBlocks()) {
      return c;
    }
 else {
      return d;
    }
  }
  return null;
}
