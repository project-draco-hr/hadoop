{
  for (  DirectoryDiff postDiff : parent.getDiffs()) {
    final INode d=postDiff.getChildrenDiff().search(ListType.DELETED,createdNodeName);
    if (d != null) {
      return d;
    }
  }
  INode currentChild=parent.getChild(createdNodeName,Snapshot.CURRENT_STATE_ID);
  if (currentChild == null) {
    throw new IOException("Cannot find an INode associated with the INode " + DFSUtil.bytes2String(createdNodeName) + " in created list while loading FSImage.");
  }
  return currentChild;
}
