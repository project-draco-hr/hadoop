{
  String confLabelString=conf.get(YarnConfiguration.NM_PROVIDER_CONFIGURED_NODE_LABELS,null);
  String[] nodeLabelsFromConfiguration=(confLabelString == null || confLabelString.isEmpty()) ? new String[]{} : StringUtils.getStrings(confLabelString);
  setNodeLabels(convertToNodeLabelSet(new HashSet<String>(Arrays.asList(nodeLabelsFromConfiguration))));
}
