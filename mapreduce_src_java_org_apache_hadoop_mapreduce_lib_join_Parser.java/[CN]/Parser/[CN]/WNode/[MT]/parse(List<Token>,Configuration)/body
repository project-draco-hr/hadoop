{
  StringBuilder sb=new StringBuilder();
  Iterator<Token> i=ll.iterator();
  while (i.hasNext()) {
    Token t=i.next();
    if (TType.COMMA.equals(t.getType())) {
      try {
        inf=(InputFormat<?,?>)ReflectionUtils.newInstance(conf.getClassByName(sb.toString()),conf);
      }
 catch (      ClassNotFoundException e) {
        throw new IOException(e);
      }
catch (      IllegalArgumentException e) {
        throw new IOException(e);
      }
      break;
    }
    sb.append(t.getStr());
  }
  if (!i.hasNext()) {
    throw new IOException("Parse error");
  }
  Token t=i.next();
  if (!TType.QUOT.equals(t.getType())) {
    throw new IOException("Expected quoted string");
  }
  indir=t.getStr();
}
