{
  this.conf=conf;
  this.maxSize=maxSize == 0 ? MAX_SIZE_DEFAULT : maxSize;
  this.steps=steps == 0 ? INTERVAL_DEFAULT : steps;
  this.formatOutput=formatOutput;
  this.out=out;
  long numIntervals=this.maxSize / this.steps;
  Preconditions.checkState(numIntervals <= MAX_INTERVALS,"Too many distribution intervals (maxSize/step): " + numIntervals + ", should be less than "+ (MAX_INTERVALS + 1)+ ".");
  this.distribution=new int[1 + (int)(numIntervals)];
}
