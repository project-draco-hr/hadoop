{
  StringBuilder sb=new StringBuilder();
  boolean first=true;
  for (  String part : args) {
    if (first) {
      first=false;
      if (part.startsWith("#") || isAbsUrl(part)) {
        sb.append(part);
      }
 else {
        uappend(sb,pathPrefix);
        uappend(sb,part);
      }
    }
 else {
      uappend(sb,part);
    }
  }
  return sb.toString();
}
