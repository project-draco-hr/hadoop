{
  this.bpid=bpid;
  this.block=blk;
  this.volume=volume;
  this.blockIn=blockIn;
  this.metaIn=metaIn;
  this.blockChannel=blockIn.getChannel();
  this.metaChannel=metaIn.getChannel();
  this.blockSize=blockChannel.size();
  this.isMapped=false;
  this.isLocked=false;
  this.isChecksummed=false;
}
