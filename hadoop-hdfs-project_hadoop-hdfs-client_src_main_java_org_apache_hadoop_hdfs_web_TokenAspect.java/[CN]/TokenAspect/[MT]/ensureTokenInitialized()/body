{
  if (!hasInitedToken || (action != null && !action.isValid())) {
    Token<?> token=fs.getDelegationToken(null);
    if (token != null) {
      fs.setDelegationToken(token);
      addRenewAction(fs);
      if (LOG.isDebugEnabled()) {
        LOG.debug("Created new DT for {}",token.getService());
      }
    }
    hasInitedToken=true;
  }
}
