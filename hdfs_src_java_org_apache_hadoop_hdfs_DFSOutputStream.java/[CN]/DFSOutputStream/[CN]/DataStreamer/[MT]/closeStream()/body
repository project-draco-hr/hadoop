{
  if (blockStream != null) {
    try {
      blockStream.close();
    }
 catch (    IOException e) {
    }
 finally {
      blockStream=null;
    }
  }
  if (blockReplyStream != null) {
    try {
      blockReplyStream.close();
    }
 catch (    IOException e) {
    }
 finally {
      blockReplyStream=null;
    }
  }
}
