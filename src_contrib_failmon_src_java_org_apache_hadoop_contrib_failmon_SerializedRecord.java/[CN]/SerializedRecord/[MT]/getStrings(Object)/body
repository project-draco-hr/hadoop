{
  ArrayList<String> retval=new ArrayList<String>();
  retval.clear();
  if (o == null)   retval.add("null");
 else   if (o instanceof String)   retval.add((String)o);
 else   if (o instanceof Calendar)   retval.add(dateFormatter.format(((Calendar)o).getTime()));
 else   if (o instanceof InetAddress[])   for (  InetAddress ip : ((InetAddress[])o))   retval.add(ip.getHostAddress());
 else   if (o instanceof String[])   for (  String s : (String[])o)   retval.add(s);
 else   retval.add(o.toString());
  return retval;
}
