{
  String retval="";
  ArrayList<String> keys=new ArrayList<String>(fields.keySet());
  arrangeKeys(keys);
  for (int i=0; i < keys.size(); i++) {
    String value=fields.get(keys.get(i));
    if (value == null)     retval+=keys.get(i) + ":\tnull\n";
 else     retval+=keys.get(i) + ":\t" + value+ "\n";
  }
  return retval;
}
