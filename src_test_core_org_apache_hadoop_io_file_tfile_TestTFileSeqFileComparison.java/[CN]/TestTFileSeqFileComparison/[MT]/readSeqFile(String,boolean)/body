{
  System.out.println("=== SeqFile: Reading (" + parameters + ") === ");
  Path path=new Path(options.rootDir,"SeqFile.Performance");
  KVReadable readable=new SeqFileReadable(fs,path,options.osInputBufferSize);
  timeRead(path,readable);
  if (delFile) {
    if (fs.exists(path)) {
      fs.delete(path,true);
    }
  }
}
