{
  RpcScheduler sched=mock(RpcScheduler.class);
  when(sched.getPriorityLevel(Matchers.<Schedulable>any())).thenReturn(0,0,0,0,0,1,0);
  fcq.setScheduler(sched);
  for (int i=0; i < 5; i++) {
    assertTrue(fcq.offer(mockCall("c")));
  }
  assertTrue(fcq.offer(mockCall("c")));
  assertEquals(6,fcq.size());
}
