{
  JobID jobId=new JobID("1234",0);
  for (  TaskType type : TaskType.values()) {
    TaskID taskId=new TaskID(jobId,type,0);
    assertEquals("TaskID incorrectly reported its type",type,taskId.getTaskType());
  }
  TaskID taskId=new TaskID();
  assertEquals("TaskID of default type incorrectly reported its type",TaskType.REDUCE,taskId.getTaskType());
}
