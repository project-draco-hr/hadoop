{
  Text.writeString(out,inputQuery);
  if (segmentParams != null && segmentParams.size() > 0) {
    out.writeLong(segmentParams.size());
    VerticaRecord record=new VerticaRecord(segmentParams,true);
    record.write(out);
  }
 else   out.writeLong(0);
  out.writeLong(start);
  out.writeLong(end);
}
