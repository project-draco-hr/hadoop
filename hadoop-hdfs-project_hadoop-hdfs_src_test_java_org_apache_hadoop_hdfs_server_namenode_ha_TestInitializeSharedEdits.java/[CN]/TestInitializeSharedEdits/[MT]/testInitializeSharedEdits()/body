{
  assertCannotStartNameNodes();
  assertFalse(NameNode.initializeSharedEdits(cluster.getConfiguration(0)));
  assertCanStartHaNameNodes("1");
  shutdownClusterAndRemoveSharedEditsDir();
  assertCannotStartNameNodes();
  assertFalse(NameNode.initializeSharedEdits(cluster.getConfiguration(0)));
  assertCanStartHaNameNodes("2");
}
