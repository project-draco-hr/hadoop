{
  if (!hasFileLength()) {
    return false;
  }
  if (!hasUnderConstruction()) {
    return false;
  }
  if (!hasIsLastBlockComplete()) {
    return false;
  }
  for (int i=0; i < getBlocksCount(); i++) {
    if (!getBlocks(i).isInitialized()) {
      return false;
    }
  }
  if (hasLastBlock()) {
    if (!getLastBlock().isInitialized()) {
      return false;
    }
  }
  return true;
}
