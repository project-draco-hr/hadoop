{
  while (shouldRun) {
    try {
      try {
        doTailEdits();
      }
 catch (      IOException e) {
        LOG.info("Got error, will try again.",e);
      }
    }
 catch (    Throwable t) {
      LOG.error("Edit log tailer received throwable",t);
    }
    try {
      Thread.sleep(sleepTime);
    }
 catch (    InterruptedException e) {
      LOG.warn("Edit log tailer interrupted",e);
    }
  }
}
