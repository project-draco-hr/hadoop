{
  super.init();
  if (SERVER != null) {
    throw new RuntimeException("HttpFSServer server already initialized");
  }
  SERVER=this;
  adminGroup=getConfig().get(getPrefixedName(CONF_ADMIN_GROUP),"admin");
  LOG.info("Connects to Namenode [{}]",get().get(FileSystemAccess.class).getDefaultConfiguration().get("fs.default.name"));
}
