{
  cluster=MRCluster.createCluster(new Configuration());
  cluster.setUp();
  client=cluster.getJTClient().getClient();
  dfs=client.getFs();
  dfs.delete(URIPATH,true);
  Collection<TTClient> tts=cluster.getTTClients();
  for (  TTClient tt : tts) {
    tt.kill();
  }
  for (  TTClient tt : tts) {
    tt.start();
  }
  Thread.sleep(5000);
}
