{
  InetSocketAddress addr=target.getAddress();
  return ActiveNodeInfo.newBuilder().setHostname(addr.getHostName()).setPort(addr.getPort()).setZkfcPort(target.getZKFCAddress().getPort()).setNameserviceId(localTarget.getNameServiceId()).setNamenodeId(localTarget.getNameNodeId()).build().toByteArray();
}
