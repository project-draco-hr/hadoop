{
  InetSocketAddress addr=target.getAddress();
  return ActiveNodeInfo.newBuilder().setHostname(addr.getHostName()).setPort(addr.getPort()).setZkfcPort(target.getZKFCAddress().getPort()).setNameserviceId(localNNTarget.getNameServiceId()).setNamenodeId(localNNTarget.getNameNodeId()).build().toByteArray();
}
