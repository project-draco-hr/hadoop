{
  UserGroupInformation ugi=UserGroupInformation.createProxyUser("u2",UserGroupInformation.getLoginUser());
  ugi.doAs(new PrivilegedExceptionAction<Void>(){
    public Void run() throws Exception {
      try {
        mrRun();
