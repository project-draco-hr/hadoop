{
  dfsc.removeFileBeingWritten(inodeId);
synchronized (this) {
    if (dfsc.isFilesBeingWrittenEmpty()) {
      dfsclients.remove(dfsc);
    }
    if (emptyTime == Long.MAX_VALUE) {
      for (      DFSClient c : dfsclients) {
        if (!c.isFilesBeingWrittenEmpty()) {
          return;
        }
      }
      emptyTime=Time.now();
    }
  }
}
