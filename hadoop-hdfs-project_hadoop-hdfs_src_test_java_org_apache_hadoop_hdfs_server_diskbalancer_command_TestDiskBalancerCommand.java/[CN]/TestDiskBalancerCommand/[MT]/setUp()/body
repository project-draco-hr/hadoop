{
  Configuration conf=new HdfsConfiguration();
  conf.setBoolean(DFSConfigKeys.DFS_DISK_BALANCER_ENABLED,true);
  cluster=new MiniDFSCluster.Builder(conf).numDataNodes(3).storagesPerDatanode(2).build();
  cluster.waitActive();
  clusterJson=getClass().getResource("/diskBalancer/data-cluster-64node-3disk.json").toURI();
}
