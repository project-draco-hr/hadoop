{
  final String cmdLine=String.format("hdfs diskbalancer -uri %s -report -top 128",clusterJson.toString());
  final List<String> outputs=runCommand(cmdLine);
  assertThat(outputs.get(0),containsString("Processing report command"));
  assertThat(outputs.get(1),is(allOf(containsString("Reporting top"),containsString("64"),containsString("DataNode(s) benefiting from running DiskBalancer"))));
  assertThat(outputs.get(31),is(allOf(containsString("30/64 null[null:0]"),containsString("a87654a9-54c7-4693-8dd9-c9c7021dc340"),containsString("9 volumes with node data density 1.97"))));
}
