{
  if (curId <= 0) {
    return basePath;
  }
  String name="";
switch (type) {
case FILE:
    name=FILE_PREFIX + new Integer(curId % limitPerDir).toString();
  break;
case DIRECTORY:
name=DIR_PREFIX + new Integer(curId % limitPerDir).toString();
break;
}
Path base=getPath((curId / limitPerDir),limitPerDir,Type.DIRECTORY);
return new Path(base,name);
}
