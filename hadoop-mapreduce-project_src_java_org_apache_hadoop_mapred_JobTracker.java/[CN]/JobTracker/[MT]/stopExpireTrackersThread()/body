{
  if (this.expireTrackersThread != null && this.expireTrackersThread.isAlive()) {
    LOG.info("Stopping expireTrackers");
    this.expireTrackersThread.interrupt();
    try {
      this.expireTrackersThread.join();
    }
 catch (    InterruptedException ex) {
      ex.printStackTrace();
    }
  }
}
