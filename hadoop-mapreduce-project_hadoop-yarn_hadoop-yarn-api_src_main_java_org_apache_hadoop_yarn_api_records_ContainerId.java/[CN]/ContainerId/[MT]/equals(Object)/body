{
  if (other == null) {
    return false;
  }
  if (other.getClass().isAssignableFrom(this.getClass())) {
    ContainerId otherCId=(ContainerId)other;
    if (this.getApplicationAttemptId().equals(otherCId.getApplicationAttemptId())) {
      return this.getId() == otherCId.getId();
    }
  }
  return false;
}
