{
  if (!isACLsEnable) {
    return true;
  }
  if (scheduler instanceof CapacityScheduler) {
    return authorizer.checkPermission(new AccessRequest(((CapacityScheduler)scheduler).getQueue(queueName).getPrivilegedEntity(),callerUGI,SchedulerUtils.toAccessType(acl),appId.toString(),appName));
  }
 else {
    return scheduler.checkAccess(callerUGI,acl,queueName);
  }
}
