def testSuccess(self):
    self.__hadoopConfig.gen_site_conf(confDir=self.confDir, tempDir=self.tempDir, numNodes=self.numNodes, hdfsAddr=self.hdfsAddr, mrSysDir=self.mySysDir, mapredAddr=self.mapredAddr, clientParams=self.clientParams, serverParams=self.serverParams, finalServerParams=self.finalServerParams, clusterFactor=self.clusterFactor)
    xmldoc = minidom.parse(self.hadoopSite)
    xmldoc = xmldoc.childNodes[0]
    properties = xmldoc.childNodes
    keyvals = {}
    for prop in properties:
        if (not isinstance(prop, minidom.Comment)):
            name = prop.getElementsByTagName('name')[0].childNodes[0].data
            value = prop.getElementsByTagName('value')[0].childNodes[0].data
            keyvals[name] = value
    assert keyvals['fs.default.name'].startswith('hdfs://')
    assert (keyvals['hadoop.tmp.dir'] == self.tempDir)
    pass
