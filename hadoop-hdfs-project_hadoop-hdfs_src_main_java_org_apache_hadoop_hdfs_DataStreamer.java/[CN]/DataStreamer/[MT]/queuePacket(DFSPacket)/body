{
synchronized (dataQueue) {
    if (packet == null)     return;
    packet.addTraceParent(Trace.currentSpan());
    dataQueue.addLast(packet);
    lastQueuedSeqno=packet.getSeqno();
    if (LOG.isDebugEnabled()) {
      LOG.debug("Queued packet " + packet.getSeqno());
    }
    dataQueue.notifyAll();
  }
}
