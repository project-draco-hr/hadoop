{
  if (blockStream != null) {
    try {
      blockStream.close();
    }
 catch (    IOException e) {
      setLastException(e);
    }
 finally {
      blockStream=null;
    }
  }
  if (blockReplyStream != null) {
    try {
      blockReplyStream.close();
    }
 catch (    IOException e) {
      setLastException(e);
    }
 finally {
      blockReplyStream=null;
    }
  }
  if (null != s) {
    try {
      s.close();
    }
 catch (    IOException e) {
      setLastException(e);
    }
 finally {
      s=null;
    }
  }
}
