{
  List<NameValuePair> list=URLEncodedUtils.parse(request.getQueryString(),UTF8_CHARSET);
  if (list != null) {
    for (    NameValuePair nv : list) {
      if (DelegationTokenAuthenticatedURL.DO_AS.equalsIgnoreCase(nv.getName())) {
        return nv.getValue();
      }
    }
  }
  return null;
}
