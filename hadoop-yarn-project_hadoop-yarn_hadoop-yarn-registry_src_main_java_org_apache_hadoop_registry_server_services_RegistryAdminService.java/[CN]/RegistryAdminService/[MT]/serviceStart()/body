{
  super.serviceStart();
  try {
    createRootRegistryPaths();
  }
 catch (  NoPathPermissionsException e) {
    String message=String.format(Locale.ENGLISH,"Failed to create root paths {%s};" + "\ndiagnostics={%s}" + "\ncurrent registry is:"+ "\n{%s}",e,bindingDiagnosticDetails(),dumpRegistryRobustly(true));
    LOG.error(" Failure {}",e,e);
    LOG.error(message);
    throw new NoPathPermissionsException(e.getPath().toString(),message,e);
  }
}
