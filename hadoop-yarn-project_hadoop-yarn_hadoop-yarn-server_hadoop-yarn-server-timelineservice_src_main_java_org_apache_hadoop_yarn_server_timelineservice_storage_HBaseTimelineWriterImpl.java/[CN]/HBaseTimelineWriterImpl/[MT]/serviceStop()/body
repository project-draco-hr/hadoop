{
  if (entityTable != null) {
    LOG.info("closing entity table");
    entityTable.close();
  }
  if (appToFlowTable != null) {
    LOG.info("closing app_flow table");
    appToFlowTable.close();
  }
  if (conn != null) {
    LOG.info("closing the hbase Connection");
    conn.close();
  }
  super.serviceStop();
}
