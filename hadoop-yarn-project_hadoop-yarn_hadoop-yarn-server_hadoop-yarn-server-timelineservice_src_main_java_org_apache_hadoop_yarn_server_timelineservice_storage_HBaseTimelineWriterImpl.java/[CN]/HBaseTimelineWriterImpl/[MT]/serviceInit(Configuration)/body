{
  super.serviceInit(conf);
  Configuration hbaseConf=HBaseConfiguration.create(conf);
  conn=ConnectionFactory.createConnection(hbaseConf);
  TableName entityTableName=TableName.valueOf(hbaseConf.get(TimelineEntitySchemaConstants.ENTITY_TABLE_NAME,TimelineEntitySchemaConstants.DEFAULT_ENTITY_TABLE_NAME));
  entityTable=conn.getBufferedMutator(entityTableName);
}
