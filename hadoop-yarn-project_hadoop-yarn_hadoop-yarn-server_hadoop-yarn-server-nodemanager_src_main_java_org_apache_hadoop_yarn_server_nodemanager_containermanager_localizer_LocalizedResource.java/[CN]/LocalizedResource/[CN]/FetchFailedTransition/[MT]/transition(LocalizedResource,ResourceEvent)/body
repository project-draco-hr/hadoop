{
  ResourceFailedLocalizationEvent failedEvent=(ResourceFailedLocalizationEvent)event;
  Queue<ContainerId> containers=rsrc.ref;
  Throwable failureCause=failedEvent.getCause();
  for (  ContainerId container : containers) {
    rsrc.dispatcher.getEventHandler().handle(new ContainerResourceFailedEvent(container,failedEvent.getLocalResourceRequest(),failureCause));
  }
}
