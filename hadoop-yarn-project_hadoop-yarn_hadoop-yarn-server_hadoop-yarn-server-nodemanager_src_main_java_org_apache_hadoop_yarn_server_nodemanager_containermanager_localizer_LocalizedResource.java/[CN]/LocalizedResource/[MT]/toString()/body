{
  StringBuilder sb=new StringBuilder();
  sb.append("{ ").append(rsrc.toString()).append(",").append(getState() == ResourceState.LOCALIZED ? getLocalPath() + "," + getSize() : "pending").append(",[");
  for (  ContainerId c : ref) {
    sb.append("(").append(c.toString()).append(")");
  }
  sb.append("],").append(getTimestamp()).append(",").append(getState()).append("}");
  return sb.toString();
}
