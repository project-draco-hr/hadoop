{
  K internal=cache.get(name);
  if (internal != null) {
    lookups++;
    return internal;
  }
  if (!initialized) {
    UseCount useCount=transientMap.get(name);
    if (useCount != null) {
      useCount.increment();
      if (useCount.get() >= useThreshold) {
        promote(name);
      }
      return useCount.value;
    }
    useCount=new UseCount(name);
    transientMap.put(name,useCount);
  }
  return null;
}
