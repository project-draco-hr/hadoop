{
  if (rsRawDecoder == null) {
    rsRawDecoder=createRawDecoder(CommonConfigurationKeys.IO_ERASURECODE_CODEC_RS_RAWCODER_KEY,getNumDataUnits(),getNumParityUnits());
    if (rsRawDecoder == null) {
      rsRawDecoder=new RSRawDecoder(getNumDataUnits(),getNumParityUnits());
    }
  }
  return rsRawDecoder;
}
