{
  setCapacity(capacity);
  setRemaining(remaining);
  setBlockPoolUsed(blockPoolUsed);
  setDfsUsed(dfsUsed);
  setCacheCapacity(cacheCapacity);
  setCacheUsed(cacheUsed);
  setXceiverCount(xceiverCount);
