{
  Comparator<Path> comp=new Comparator<Path>(){
    public int compare(    Path p1,    Path p2){
      if (isXorParityFile(p2) || isRsParityFile(p2)) {
        return -1;
      }
      if (isXorParityFile(p1) || isRsParityFile(p1)) {
        return 1;
      }
      return 0;
    }
  }
;
  Collections.sort(files,comp);
}
