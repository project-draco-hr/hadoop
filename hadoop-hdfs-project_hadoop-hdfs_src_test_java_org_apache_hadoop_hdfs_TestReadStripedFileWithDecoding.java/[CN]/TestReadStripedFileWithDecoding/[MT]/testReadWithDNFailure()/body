{
  for (  int fileLength : fileLengths) {
    for (    int dnFailureNum : dnFailureNums) {
      try {
        setup();
        testReadWithDNFailure(fileLength,dnFailureNum);
      }
 catch (      IOException ioe) {
        String fileType=fileLength < (blockSize * dataBlocks) ? "smallFile" : "largeFile";
        LOG.error("Failed to read file with DN failure:" + " fileType = " + fileType + ", dnFailureNum = "+ dnFailureNum);
      }
 finally {
        tearDown();
      }
    }
  }
}
