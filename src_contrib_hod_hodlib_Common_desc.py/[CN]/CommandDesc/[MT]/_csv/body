def _csv(row, delim=','):
    'return a string in csv format'
    import cStringIO
    import csv
    queue = cStringIO.StringIO()
    writer = csv.writer(queue, delimiter=delim, escapechar='\\', quoting=csv.QUOTE_NONE, doublequote=False, lineterminator='\n')
    writer.writerow(row)
    return queue.getvalue().rstrip('\n')
