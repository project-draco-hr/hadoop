{
  try {
    Configuration config=new Configuration(conf);
    config.setInt(IPC_CLIENT_CONNECT_MAX_RETRIES_KEY,connectRetries);
    config.setInt(IPC_CLIENT_CONNECT_MAX_RETRIES_ON_SOCKET_TIMEOUTS_KEY,connectRetries);
    waitStarted=true;
    TestProtocol proxy=RPC.waitForProxy(TestProtocol.class,TestProtocol.versionID,new InetSocketAddress(ADDRESS,20),config,15000L);
    proxy.echo("");
  }
 catch (  Throwable throwable) {
    caught=throwable;
  }
}
