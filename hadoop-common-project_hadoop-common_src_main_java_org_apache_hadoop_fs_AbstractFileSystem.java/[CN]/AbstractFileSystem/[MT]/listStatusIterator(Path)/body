{
  return new RemoteIterator<FileStatus>(){
    private int i=0;
    private FileStatus[] statusList=listStatus(f);
    @Override public boolean hasNext(){
      return i < statusList.length;
    }
    @Override public FileStatus next(){
      if (!hasNext()) {
        throw new NoSuchElementException();
      }
      return statusList[i++];
    }
  }
;
}
