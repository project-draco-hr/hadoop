{
  long num=0;
  for (  JournalAndStream jas : journals) {
    if (jas.isDisabled()) {
      LOG.info("Skipping jas " + jas + " since it's disabled");
      continue;
    }
 else {
      long newNum=jas.getManager().getNumberOfTransactions(fromTxnId,inProgressOk);
      if (newNum > num) {
        num=newNum;
      }
    }
  }
  return num;
}
