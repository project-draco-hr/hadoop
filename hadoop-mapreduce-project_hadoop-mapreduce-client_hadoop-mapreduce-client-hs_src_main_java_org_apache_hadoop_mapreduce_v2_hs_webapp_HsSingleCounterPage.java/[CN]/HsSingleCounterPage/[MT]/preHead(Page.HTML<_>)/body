{
  commonPreHead(html);
  String tid=$(TASK_ID);
  String activeNav="2";
  if (tid == null || tid.isEmpty()) {
    activeNav="1";
  }
  set(initID(ACCORDION,"nav"),"{autoHeight:false, active:" + activeNav + "}");
  set(DATATABLES_ID,"singleCounter");
  set(initID(DATATABLES,"singleCounter"),counterTableInit());
  setTableStyles(html,"singleCounter");
}
