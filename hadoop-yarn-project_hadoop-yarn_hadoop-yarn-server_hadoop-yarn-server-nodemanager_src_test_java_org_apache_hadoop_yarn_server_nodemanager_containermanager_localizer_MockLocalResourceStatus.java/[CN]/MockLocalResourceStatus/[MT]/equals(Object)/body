{
  if (!(o instanceof MockLocalResourceStatus)) {
    return false;
  }
  MockLocalResourceStatus other=(MockLocalResourceStatus)o;
  return getResource().equals(other.getResource()) && getStatus().equals(other.getStatus()) && (null != getLocalPath() && getLocalPath().equals(other.getLocalPath()))&& (null != getException() && getException().equals(other.getException()));
}
