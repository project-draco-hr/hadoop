{
  if (info != null) {
    return info;
  }
  info=new JobInfo();
  int eventCtr=0;
  HistoryEvent event;
  try {
    while ((event=reader.getNextEvent()) != null) {
      handleEvent(event);
      ++eventCtr;
    }
  }
 catch (  IOException ioe) {
    LOG.info("Caught exception parsing history file after " + eventCtr + " events",ioe);
    parseException=ioe;
  }
 finally {
    in.close();
  }
  return info;
}
