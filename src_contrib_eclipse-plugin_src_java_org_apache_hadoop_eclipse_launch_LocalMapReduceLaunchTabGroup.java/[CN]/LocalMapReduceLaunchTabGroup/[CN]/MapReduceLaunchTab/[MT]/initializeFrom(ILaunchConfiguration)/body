{
  try {
    mapperClass.setText(configuration.getAttribute("org.apache.hadoop.eclipse.launch.mapper",""));
    reducerClass.setText(configuration.getAttribute("org.apache.hadoop.eclipse.launch.reducer",""));
    combinerClass.setText(configuration.getAttribute("org.apache.hadoop.eclipse.launch.combiner",""));
  }
 catch (  CoreException e) {
    e.printStackTrace();
    setErrorMessage(e.getMessage());
  }
}
