{
  Map<ApplicationAccessType,String> appAcls=app.getContainers().values().iterator().next().getLaunchContext().getApplicationACLs();
  app.dispatcher.getEventHandler().handle(new LogAggregatorAppStartedEvent(app.appId,app.user,app.credentials,ContainerLogsRetentionPolicy.ALL_CONTAINERS,appAcls));
  for (  Container container : app.containers.values()) {
    app.dispatcher.getEventHandler().handle(new ContainerInitEvent(container.getContainerID()));
  }
}
