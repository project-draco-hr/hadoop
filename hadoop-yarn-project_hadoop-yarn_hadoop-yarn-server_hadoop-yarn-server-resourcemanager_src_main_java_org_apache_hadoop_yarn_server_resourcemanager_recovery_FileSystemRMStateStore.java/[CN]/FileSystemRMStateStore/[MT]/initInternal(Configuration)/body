{
  fsWorkingPath=new Path(conf.get(YarnConfiguration.FS_RM_STATE_STORE_URI));
  rootDirPath=new Path(fsWorkingPath,ROOT_DIR_NAME);
  rmDTSecretManagerRoot=new Path(rootDirPath,RM_DT_SECRET_MANAGER_ROOT);
  rmAppRoot=new Path(rootDirPath,RM_APP_ROOT);
  amrmTokenSecretManagerRoot=new Path(rootDirPath,AMRMTOKEN_SECRET_MANAGER_ROOT);
  fsNumRetries=conf.getInt(YarnConfiguration.FS_RM_STATE_STORE_NUM_RETRIES,YarnConfiguration.DEFAULT_FS_RM_STATE_STORE_NUM_RETRIES);
  fsRetryInterval=conf.getLong(YarnConfiguration.FS_RM_STATE_STORE_RETRY_INTERVAL_MS,YarnConfiguration.DEFAULT_FS_RM_STATE_STORE_RETRY_INTERVAL_MS);
}
