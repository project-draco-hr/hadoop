{
  writeSomeData();
  final FSDataInputStream in=fc.open(src1);
  for (int seekInc=1; seekInc < 1024; seekInc+=32) {
    long seekTo=data.length - 1;
    while (seekTo >= 0) {
      in.seek(seekTo);
      int b=in.read();
      byte expected=data[(int)seekTo];
      assertTrue("seek(" + seekTo + ") Expected: "+ expected+ ", but got: "+ b,b == expected);
      seekTo-=seekInc;
    }
  }
  in.close();
}
