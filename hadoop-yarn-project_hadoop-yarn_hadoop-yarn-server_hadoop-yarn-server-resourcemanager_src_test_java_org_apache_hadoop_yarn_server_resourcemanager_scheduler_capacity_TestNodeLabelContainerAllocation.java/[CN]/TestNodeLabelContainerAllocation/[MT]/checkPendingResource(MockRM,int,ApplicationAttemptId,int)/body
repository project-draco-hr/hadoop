{
  CapacityScheduler cs=(CapacityScheduler)rm.getRMContext().getScheduler();
  FiCaSchedulerApp app=cs.getApplicationAttempt(attemptId);
  ResourceRequest rr=app.getAppSchedulingInfo().getResourceRequest(Priority.newInstance(priority),"*");
  Assert.assertEquals(memory,rr.getCapability().getMemorySize() * rr.getNumContainers());
}
