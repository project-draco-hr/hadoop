{
  if (!(container.nmClientAsync.getCallbackHandler() instanceof AbstractCallbackHandler)) {
    LOG.error("Callback handler does not implement container resource " + "increase callback methods");
    return;
  }
  AbstractCallbackHandler handler=(AbstractCallbackHandler)container.nmClientAsync.getCallbackHandler();
  try {
    if (!(event instanceof IncreaseContainerResourceEvent)) {
      throw new AssertionError("Unexpected event type. Expecting:" + "IncreaseContainerResourceEvent. Got:" + event);
    }
    IncreaseContainerResourceEvent increaseEvent=(IncreaseContainerResourceEvent)event;
    container.nmClientAsync.getClient().increaseContainerResource(increaseEvent.getContainer());
    try {
      handler.onContainerResourceIncreased(increaseEvent.getContainerId(),increaseEvent.getContainer().getResource());
    }
 catch (    Throwable thr) {
      LOG.info("Unchecked exception is thrown from " + "onContainerResourceIncreased for Container " + event.getContainerId(),thr);
    }
  }
 catch (  Exception e) {
    try {
      handler.onIncreaseContainerResourceError(event.getContainerId(),e);
    }
 catch (    Throwable thr) {
      LOG.info("Unchecked exception is thrown from " + "onIncreaseContainerResourceError for Container " + event.getContainerId(),thr);
    }
  }
}
