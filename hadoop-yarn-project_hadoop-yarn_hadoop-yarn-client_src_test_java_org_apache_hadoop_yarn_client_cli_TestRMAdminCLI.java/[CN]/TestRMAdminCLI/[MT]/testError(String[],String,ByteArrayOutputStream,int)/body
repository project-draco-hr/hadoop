{
  int actualResultCode=rmAdminCLI.run(args);
  assertEquals("Expected result code: " + resultCode + ", actual result code is: "+ actualResultCode,resultCode,actualResultCode);
  assertTrue(String.format("Expected error message: %n" + template + " is not included in messages: %n"+ data.toString()),data.toString().contains(template));
  data.reset();
}
