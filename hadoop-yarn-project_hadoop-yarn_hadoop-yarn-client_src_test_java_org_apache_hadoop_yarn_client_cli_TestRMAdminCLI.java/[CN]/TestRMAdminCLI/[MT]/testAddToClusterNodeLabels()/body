{
  String[] args={"-addToClusterNodeLabels","x","-directlyAccessNodeLabelStore"};
  assertEquals(0,rmAdminCLI.run(args));
  assertTrue(dummyNodeLabelsManager.getClusterNodeLabels().containsAll(ImmutableSet.of("x")));
  args=new String[]{"-addToClusterNodeLabels"};
  assertTrue(0 != rmAdminCLI.run(args));
  args=new String[]{"-addToClusterNodeLabels","-directlyAccessNodeLabelStore"};
  assertTrue(0 != rmAdminCLI.run(args));
}
