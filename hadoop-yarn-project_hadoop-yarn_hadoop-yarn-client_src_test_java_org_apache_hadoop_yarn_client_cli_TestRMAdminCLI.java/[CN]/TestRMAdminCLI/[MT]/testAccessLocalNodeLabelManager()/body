{
  assertFalse(dummyNodeLabelsManager.getServiceState() == STATE.STOPPED);
  String[] args={"-addToClusterNodeLabels","x,y","-directlyAccessNodeLabelStore"};
  assertEquals(0,rmAdminCLI.run(args));
  assertTrue(dummyNodeLabelsManager.getClusterNodeLabels().containsAll(ImmutableSet.of("x","y")));
  dummyNodeLabelsManager.removeFromClusterNodeLabels(ImmutableSet.of("x","y"));
  args=new String[]{"-addToClusterNodeLabels","-directlyAccessNodeLabelStore","x,y"};
  assertEquals(0,rmAdminCLI.run(args));
  assertTrue(dummyNodeLabelsManager.getClusterNodeLabels().containsAll(ImmutableSet.of("x","y")));
  assertTrue(dummyNodeLabelsManager.getServiceState() == STATE.STOPPED);
}
