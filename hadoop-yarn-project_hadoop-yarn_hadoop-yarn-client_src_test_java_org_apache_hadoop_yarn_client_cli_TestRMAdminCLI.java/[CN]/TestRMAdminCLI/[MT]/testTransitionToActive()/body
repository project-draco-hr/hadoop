{
  String[] args={"-transitionToActive","rm1"};
  assertEquals(-1,rmAdminCLI.run(args));
  verify(haadmin,never()).transitionToActive(any(HAServiceProtocol.StateChangeRequestInfo.class));
  assertEquals(0,rmAdminCLIWithHAEnabled.run(args));
  verify(haadmin).transitionToActive(any(HAServiceProtocol.StateChangeRequestInfo.class));
}
