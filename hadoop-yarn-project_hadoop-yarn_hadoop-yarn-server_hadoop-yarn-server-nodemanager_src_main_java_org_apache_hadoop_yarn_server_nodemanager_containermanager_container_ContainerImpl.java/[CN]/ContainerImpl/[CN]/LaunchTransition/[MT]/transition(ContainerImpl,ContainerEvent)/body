{
  container.sendContainerMonitorStartEvent();
  container.metrics.runningContainer();
  container.wasLaunched=true;
  container.setIsReInitializing(false);
  if (container.reInitContext != null && !container.reInitContext.canRollback()) {
    container.reInitContext=null;
  }
  if (container.recoveredAsKilled) {
    LOG.info("Killing " + container.containerId + " due to recovered as killed");
    container.addDiagnostics("Container recovered as killed.\n");
    container.dispatcher.getEventHandler().handle(new ContainersLauncherEvent(container,ContainersLauncherEventType.CLEANUP_CONTAINER));
  }
}
