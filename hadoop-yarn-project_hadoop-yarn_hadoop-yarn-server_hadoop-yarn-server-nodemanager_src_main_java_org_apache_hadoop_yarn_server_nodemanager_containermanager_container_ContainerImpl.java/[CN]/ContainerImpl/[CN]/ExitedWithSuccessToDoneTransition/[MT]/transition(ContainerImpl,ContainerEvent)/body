{
  if (container.wasLaunched) {
    container.metrics.endRunningContainer();
  }
 else {
    LOG.warn("Container exited with success despite being killed and not" + "actually running");
  }
  container.metrics.completedContainer();
  NMAuditLogger.logSuccess(container.user,AuditConstants.FINISH_SUCCESS_CONTAINER,"ContainerImpl",container.containerId.getApplicationAttemptId().getApplicationId(),container.containerId);
  super.transition(container,event);
}
