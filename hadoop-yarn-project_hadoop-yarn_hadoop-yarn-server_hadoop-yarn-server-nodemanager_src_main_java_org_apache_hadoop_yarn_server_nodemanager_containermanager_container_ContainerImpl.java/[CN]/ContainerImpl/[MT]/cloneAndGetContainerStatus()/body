{
  this.readLock.lock();
  try {
    ContainerStatus status=BuilderUtils.newContainerStatus(this.containerId,getCurrentState(),diagnostics.toString(),exitCode,getResource(),this.containerTokenIdentifier.getExecutionType());
    status.setIPs(ips == null ? null : Arrays.asList(ips.split(",")));
    status.setHost(host);
    return status;
  }
  finally {
    this.readLock.unlock();
  }
}
