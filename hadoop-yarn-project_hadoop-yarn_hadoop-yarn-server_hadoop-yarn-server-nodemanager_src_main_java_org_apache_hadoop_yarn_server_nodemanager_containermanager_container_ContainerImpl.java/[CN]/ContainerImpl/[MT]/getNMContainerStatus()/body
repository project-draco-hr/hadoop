{
  this.readLock.lock();
  try {
    return NMContainerStatus.newInstance(this.containerId,this.version,getCurrentState(),getResource(),diagnostics.toString(),exitCode,containerTokenIdentifier.getPriority(),containerTokenIdentifier.getCreationTime(),containerTokenIdentifier.getNodeLabelExpression());
  }
  finally {
    this.readLock.unlock();
  }
}
