{
  long pmemBytes=getResource().getMemory() * 1024 * 1024L;
  float pmemRatio=daemonConf.getFloat(YarnConfiguration.NM_VMEM_PMEM_RATIO,YarnConfiguration.DEFAULT_NM_VMEM_PMEM_RATIO);
  long vmemBytes=(long)(pmemRatio * pmemBytes);
  int cpuVcores=getResource().getVirtualCores();
  dispatcher.getEventHandler().handle(new ContainerStartMonitoringEvent(containerId,vmemBytes,pmemBytes,cpuVcores));
}
