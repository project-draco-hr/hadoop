{
  this(conf,dispatcher,launchContext,creds,metrics,containerTokenIdentifier,context);
  this.recoveredStatus=recoveredStatus;
  this.exitCode=exitCode;
  this.recoveredAsKilled=wasKilled;
  this.diagnostics.append(diagnostics);
  if (recoveredCapability != null && !this.resource.equals(recoveredCapability)) {
    this.resource=Resource.newInstance(recoveredCapability.getMemory(),recoveredCapability.getVirtualCores());
  }
}
