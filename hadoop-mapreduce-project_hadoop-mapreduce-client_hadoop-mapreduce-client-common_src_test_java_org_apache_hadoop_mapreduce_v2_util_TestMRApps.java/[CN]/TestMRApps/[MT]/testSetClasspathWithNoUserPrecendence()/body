{
  Configuration conf=new Configuration();
  conf.setBoolean(MRJobConfig.MAPREDUCE_JOB_USER_CLASSPATH_FIRST,false);
  Map<String,String> env=new HashMap<String,String>();
  try {
    MRApps.setClasspath(env,conf);
  }
 catch (  Exception e) {
    fail("Got exception while setting classpath");
  }
  String env_str=env.get("CLASSPATH");
  int index=env_str.indexOf("job.jar/job.jar:job.jar/classes/:job.jar/lib/*:$PWD/*");
  assertNotSame("MAPREDUCE_JOB_USER_CLASSPATH_FIRST false, and job.jar is not" + " in the classpath!",index,-1);
  assertNotSame("MAPREDUCE_JOB_USER_CLASSPATH_FIRST false, but taking effect!",index,0);
}
