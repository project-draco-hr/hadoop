{
  byte[] data=null;
  try {
    data=db.get(LATEST_SEQUENCE_NUMBER_KEY);
  }
 catch (  DBException e) {
    throw new IOException(e);
  }
  if (data != null) {
    DataInputStream in=new DataInputStream(new ByteArrayInputStream(data));
    try {
      state.latestSequenceNumber=in.readInt();
    }
  finally {
      IOUtils.cleanup(LOG,in);
    }
  }
}
