{
  String host=cache.getHost();
  if (host == null) {
    host=cache.getScheme();
  }
  if (host == null) {
    URI defaultUri=FileSystem.get(conf).getUri();
    host=defaultUri.getHost();
    if (host == null) {
      host=defaultUri.getScheme();
    }
  }
  String path=host + cache.getPath();
  path=path.replace(":/","/");
  return path;
}
