{
  Vector<TaskInProgress> results=new Vector<TaskInProgress>();
  TaskInProgress tips[]=null;
  if (shouldBeMap) {
    tips=maps;
  }
 else {
    tips=reduces;
  }
  for (int i=0; i < tips.length; i++) {
    if (tips[i].isComplete() == shouldBeComplete) {
      results.add(tips[i]);
    }
  }
  return results;
}
