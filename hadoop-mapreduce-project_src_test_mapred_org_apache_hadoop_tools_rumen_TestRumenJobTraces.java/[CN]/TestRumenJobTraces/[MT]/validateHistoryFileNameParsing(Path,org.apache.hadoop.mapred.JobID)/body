{
  JobID extractedJID=JobID.forName(JobHistoryUtils.extractJobID(jhFileName.getName()));
  assertEquals("TraceBuilder failed to parse the current JH filename" + jhFileName,jid,extractedJID);
  jhFileName=jhFileName.suffix(JobHistory.getOldFileSuffix("123"));
  extractedJID=JobID.forName(JobHistoryUtils.extractJobID(jhFileName.getName()));
  assertEquals("TraceBuilder failed to parse the current JH filename" + "(old-suffix):" + jhFileName,jid,extractedJID);
}
