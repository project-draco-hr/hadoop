{
  this.attemptDirCacheSize=cacheSize;
  this.attemptDirCache=new LinkedHashMap<ApplicationAttemptId,Path>(attemptDirCacheSize,0.75f,true){
    private static final long serialVersionUID=1L;
    @Override protected boolean removeEldestEntry(    Map.Entry<ApplicationAttemptId,Path> eldest){
      return size() > attemptDirCacheSize;
    }
  }
;
  this.fs=fs;
  this.activePath=activePath;
}
