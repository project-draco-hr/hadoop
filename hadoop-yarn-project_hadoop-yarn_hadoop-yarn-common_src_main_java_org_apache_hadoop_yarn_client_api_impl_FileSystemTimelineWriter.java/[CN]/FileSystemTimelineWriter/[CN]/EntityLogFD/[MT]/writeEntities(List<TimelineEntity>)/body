{
  if (writerClosed()) {
    prepareForWrite();
  }
  if (LOG.isDebugEnabled()) {
    LOG.debug("Writing entity list of size " + entities.size());
  }
  for (  TimelineEntity entity : entities) {
    getObjectMapper().writeValue(getJsonGenerator(),entity);
  }
  updateLastModifiedTime(Time.monotonicNow());
}
