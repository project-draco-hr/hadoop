{
  if (!logFDs.isEmpty()) {
    for (    Entry<ApplicationAttemptId,HashMap<TimelineEntityGroupId,EntityLogFD>> logFDMapEntry : logFDs.entrySet()) {
      HashMap<TimelineEntityGroupId,EntityLogFD> logFDMap=logFDMapEntry.getValue();
      for (      Entry<TimelineEntityGroupId,EntityLogFD> logFDEntry : logFDMap.entrySet()) {
        EntityLogFD logFD=logFDEntry.getValue();
        try {
          logFD.lock();
          if (currentTimeStamp - logFD.getLastModifiedTime() >= ttl) {
            logFD.close();
          }
        }
  finally {
          logFD.unlock();
        }
      }
    }
  }
}
