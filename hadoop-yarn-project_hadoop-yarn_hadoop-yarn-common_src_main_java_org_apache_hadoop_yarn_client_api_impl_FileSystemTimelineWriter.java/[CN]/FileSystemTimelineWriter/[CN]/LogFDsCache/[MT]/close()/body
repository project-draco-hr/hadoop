{
  serviceStopped=true;
  flushTimer.cancel();
  cleanInActiveFDsTimer.cancel();
  try {
    this.domainFDLocker.lock();
    if (domainLogFD != null) {
      domainLogFD.close();
      domainLogFD=null;
    }
  }
  finally {
    this.domainFDLocker.unlock();
  }
  closeSummaryFDs(summanyLogFDs);
  closeEntityFDs(entityLogFDs);
}
