{
  if (!protocol.equals(RPC.getProtocolName(ClientDatanodeWireProtocol.class))) {
    throw new IOException("Datanode Serverside implements " + ClientDatanodeWireProtocol.class + ". The following requested protocol is unknown: "+ protocol);
  }
  return ProtocolSignature.getProtocolSignature(clientMethodsHash,ClientDatanodeWireProtocol.versionID,ClientDatanodeWireProtocol.class);
}
