{
  IOException ioe;
  while (true) {
    try {
      return readerStrategy.doRead(blockReader,off,len);
    }
 catch (    ChecksumException ce) {
      DFSClient.LOG.warn("Found Checksum error for " + getCurrentBlock() + " from "+ currentNode+ " at "+ ce.getPos());
      throw ce;
    }
catch (    IOException e) {
      ioe=e;
    }
    boolean sourceFound=seekToBlockSource(pos);
    if (!sourceFound) {
      throw ioe;
    }
  }
}
