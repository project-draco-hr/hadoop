{
  final Span parentSpan=Trace.currentSpan();
  return new Callable<Void>(){
    @Override public Void call() throws Exception {
      TraceScope scope=Trace.startSpan("Parallel reading " + hedgedReadId,parentSpan);
      try {
        actualGetFromOneDataNode(datanode,blockStartOffset,start,end,buf,offsets,lengths,corruptedBlockMap);
      }
  finally {
        scope.close();
      }
      return null;
    }
  }
;
}
