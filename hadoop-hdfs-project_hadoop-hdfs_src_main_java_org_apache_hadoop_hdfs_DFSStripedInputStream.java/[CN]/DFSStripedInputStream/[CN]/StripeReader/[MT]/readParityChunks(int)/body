{
  for (int i=dataBlkNum, j=0; i < dataBlkNum + parityBlkNum && j < num; i++) {
    if (alignedStripe.chunks[i] == null) {
      if (prepareParityChunk(i) && readChunk(targetBlocks[i],i)) {
        j++;
      }
 else {
        alignedStripe.missingChunksNum++;
      }
    }
  }
  checkMissingBlocks();
}
