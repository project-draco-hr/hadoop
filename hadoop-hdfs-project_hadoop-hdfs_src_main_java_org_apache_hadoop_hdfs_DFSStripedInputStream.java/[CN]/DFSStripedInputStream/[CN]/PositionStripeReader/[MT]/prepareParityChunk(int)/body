{
  Preconditions.checkState(index >= dataBlkNum && alignedStripe.chunks[index] == null);
  final int decodeIndex=convertIndex4Decode(index,dataBlkNum,parityBlkNum);
  alignedStripe.chunks[index]=new StripingChunk(decodeInputs[decodeIndex]);
  alignedStripe.chunks[index].addByteArraySlice(0,(int)alignedStripe.getSpanInBlock());
  return true;
}
