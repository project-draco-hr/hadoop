{
  if (lengthIndices[0] >= k.beginFieldIdx) {
    int position=0;
    for (int i=1; i < k.beginFieldIdx; i++) {
      position+=lengthIndices[i] + keyFieldSeparator.length;
    }
    if (position + k.beginChar <= (end - start)) {
      return start + position + k.beginChar - 1;
    }
  }
  return -1;
}
