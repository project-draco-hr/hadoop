{
  preHead(html);
  html.title($(TITLE)).link(root_url("static","yarn.css")).style("#layout { height: 100%; }","#layout thead td { height: 3em; }","#layout #navcell { width: 11em; padding: 0 1em; }","#layout td.content { padding-top: 0 }","#layout tbody { vertical-align: top; }","#layout tfoot td { height: 4em; }")._(JQueryUI.class);
  postHead(html);
  JQueryUI.jsnotice(html);
  html.table("#layout.ui-widget-content").thead().tr().td().$colspan(2)._(header())._()._()._().tfoot().tr().td().$colspan(2)._(footer())._()._()._().tbody().tr().td().$id("navcell")._(nav())._().td().$class("content")._(content())._()._()._()._()._();
}
