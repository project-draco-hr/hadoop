{
  appAttempt.startTime=System.currentTimeMillis();
  appAttempt.masterService.registerAppAttempt(appAttempt.applicationAttemptId);
  appAttempt.eventHandler.handle(new AppAddedSchedulerEvent(appAttempt.applicationAttemptId,appAttempt.submissionContext.getQueue(),appAttempt.submissionContext.getUser()));
}
