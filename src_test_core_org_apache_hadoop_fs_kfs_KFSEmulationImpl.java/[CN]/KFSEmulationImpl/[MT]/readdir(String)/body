  FileStatus[] p=localFS.listStatus(new Path(path));
  try {
    p=localFS.listStatus(new Path(path));
  }
 catch (  FileNotFoundException fnfe) {
    return null;
  }
  String[] entries=null;
  entries=new String[p.length];
  for (int i=0; i < p.length; i++)   entries[i]=p[i].getPath().toString();
  return entries;
}
