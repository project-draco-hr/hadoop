{
  String dir="/testNamenodeRetryCache/testDelete";
  newCall();
  nnRpc.mkdirs(dir,perm,true);
  newCall();
  Assert.assertTrue(nnRpc.delete(dir,false));
  Assert.assertTrue(nnRpc.delete(dir,false));
  Assert.assertTrue(nnRpc.delete(dir,false));
  newCall();
  Assert.assertFalse(nnRpc.delete(dir,false));
}
