{
  String dir="/testNamenodeRetryCache/testDelete";
  newCall();
  namesystem.mkdirs(dir,perm,true);
  newCall();
  Assert.assertTrue(namesystem.delete(dir,false));
  Assert.assertTrue(namesystem.delete(dir,false));
  Assert.assertTrue(namesystem.delete(dir,false));
  newCall();
  Assert.assertFalse(namesystem.delete(dir,false));
}
