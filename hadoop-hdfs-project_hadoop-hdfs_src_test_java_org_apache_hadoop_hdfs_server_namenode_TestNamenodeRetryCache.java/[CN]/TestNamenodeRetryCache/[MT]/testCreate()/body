{
  String src="/testNamenodeRetryCache/testCreate/file";
  newCall();
  HdfsFileStatus status=namesystem.startFile(src,perm,"holder","clientmachine",EnumSet.of(CreateFlag.CREATE),true,(short)1,512);
  Assert.assertEquals(status,namesystem.startFile(src,perm,"holder","clientmachine",EnumSet.of(CreateFlag.CREATE),true,(short)1,512));
  Assert.assertEquals(status,namesystem.startFile(src,perm,"holder","clientmachine",EnumSet.of(CreateFlag.CREATE),true,(short)1,512));
  newCall();
  try {
    namesystem.startFile(src,perm,"holder","clientmachine",EnumSet.of(CreateFlag.CREATE),true,(short)1,512);
    Assert.fail("testCreate - expected exception is not thrown");
  }
 catch (  IOException e) {
  }
}
