{
  String target="/testNamenodeRetryCache/testCreateSymlink/target";
  newCall();
  namesystem.createSymlink(target,"/a/b",perm,true);
  namesystem.createSymlink(target,"/a/b",perm,true);
  namesystem.createSymlink(target,"/a/b",perm,true);
  newCall();
