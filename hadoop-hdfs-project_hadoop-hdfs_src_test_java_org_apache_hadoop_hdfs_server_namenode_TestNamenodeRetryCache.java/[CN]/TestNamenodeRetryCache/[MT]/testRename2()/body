{
  String src="/testNamenodeRetryCache/testRename2/src";
  String target="/testNamenodeRetryCache/testRename2/target";
  resetCall();
  namesystem.mkdirs(src,perm,true);
  newCall();
  namesystem.renameTo(src,target,Rename.NONE);
  namesystem.renameTo(src,target,Rename.NONE);
  namesystem.renameTo(src,target,Rename.NONE);
  newCall();
