{
  NodeId nodeId=BuilderUtils.newNodeId("localhost",0);
  Resource capability=Resource.newInstance(4096,4);
  RMNodeImpl node=new RMNodeImpl(nodeId,rmContext,null,0,0,null,ResourceOption.newInstance(capability,RMNode.OVER_COMMIT_TIMEOUT_MILLIS_DEFAULT),null);
  node.handle(new RMNodeEvent(node.getNodeID(),RMNodeEventType.STARTED));
  Assert.assertEquals(NodeState.RUNNING,node.getState());
  return node;
}
