{
  if (baseDirs == null || baseDirs.length == 0) {
    LOG.info("Deleting absolute path : " + subDir);
    lfs.delete(subDir,true);
    return;
  }
  for (  Path baseDir : baseDirs) {
    Path del=subDir == null ? baseDir : new Path(baseDir,subDir);
    LOG.info("Deleting path : " + del);
    lfs.delete(del,true);
  }
}
