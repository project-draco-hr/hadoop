{
  Key key=new Key(blockId,bpid);
  Value prevValue=mappableBlockMap.get(key);
  if (prevValue != null) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Block with id " + blockId + ", pool "+ bpid+ " already exists in the FsDatasetCache with state "+ prevValue.state);
    }
    numBlocksFailedToCache.incrementAndGet();
    return;
  }
  mappableBlockMap.put(key,new Value(null,State.CACHING));
  volumeExecutor.execute(new CachingTask(key,blockFileName,length,genstamp));
  if (LOG.isDebugEnabled()) {
    LOG.debug("Initiating caching for Block with id " + blockId + ", pool "+ bpid);
  }
}
