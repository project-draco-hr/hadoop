{
  Value value;
synchronized (FsDatasetCache.this) {
    value=mappableBlockMap.get(key);
  }
  Preconditions.checkNotNull(value);
  Preconditions.checkArgument(value.state == State.UNCACHING);
  IOUtils.closeQuietly(value.mappableBlock);
synchronized (FsDatasetCache.this) {
    mappableBlockMap.remove(key);
  }
  long newUsedBytes=usedBytesCount.release(value.mappableBlock.getLength());
  if (LOG.isDebugEnabled()) {
    LOG.debug("Uncaching of block " + key.id + " in "+ key.bpid+ " completed.  usedBytes = "+ newUsedBytes);
  }
}
