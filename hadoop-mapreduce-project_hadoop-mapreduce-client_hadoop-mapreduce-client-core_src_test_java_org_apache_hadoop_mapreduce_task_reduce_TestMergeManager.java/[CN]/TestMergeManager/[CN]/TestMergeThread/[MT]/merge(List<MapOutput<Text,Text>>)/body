{
synchronized (this) {
    numMerges.incrementAndGet();
    for (    MapOutput<Text,Text> input : inputs) {
      manager.unreserve(input.getSize());
    }
  }
  try {
    mergeStart.await();
    mergeComplete.await();
  }
 catch (  InterruptedException e) {
  }
catch (  BrokenBarrierException e) {
  }
}
