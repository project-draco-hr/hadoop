{
  for (  CounterGroup g : counters) {
    String groupName=g.getName();
    for (    Counter c : g) {
      String name=groupName + ":" + c.getName();
      TimelineMetric metric=new TimelineMetric();
      metric.setId(name);
      metric.addValue(System.currentTimeMillis(),c.getValue());
      entity.addMetric(metric);
    }
  }
}
