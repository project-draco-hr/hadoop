{
  String cluster=getClusterID(clusterId,getConfig());
  return reader.getEntities(userId,cluster,flowId,flowRunId,appId,entityType,limit,createdTimeBegin,createdTimeEnd,modifiedTimeBegin,modifiedTimeEnd,relatesTo,isRelatedTo,infoFilters,configFilters,metricFilters,eventFilters,null,null,fieldsToRetrieve);
}
