{
  String cluster=getClusterID(clusterId,getConfig());
  Set<TimelineEntity> entities=reader.getEntities(userId,cluster,flowName,flowRunId,appId,entityType,limit,createdTimeBegin,createdTimeEnd,relatesTo,isRelatedTo,infoFilters,configFilters,metricFilters,eventFilters,null,null,fieldsToRetrieve);
  if (entities != null) {
    TimelineEntityType type=getTimelineEntityType(entityType);
    TimelineReaderContext context=new TimelineReaderContext(cluster,userId,flowName,flowRunId,appId,entityType,null);
    for (    TimelineEntity entity : entities) {
      fillUID(type,entity,context);
    }
  }
  return entities;
}
