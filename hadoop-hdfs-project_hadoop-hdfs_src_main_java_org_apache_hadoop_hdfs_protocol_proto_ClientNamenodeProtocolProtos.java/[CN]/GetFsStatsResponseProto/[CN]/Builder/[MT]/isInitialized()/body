{
  if (!hasCapacity()) {
    return false;
  }
  if (!hasUsed()) {
    return false;
  }
  if (!hasRemaining()) {
    return false;
  }
  if (!hasUnderReplicated()) {
    return false;
  }
  if (!hasCorruptBlocks()) {
    return false;
  }
  if (!hasMissingBlocks()) {
    return false;
  }
  return true;
}
