{
  if (getChildren().isEmpty()) {
    return false;
  }
  Node firstChild=children.get(0);
  if (firstChild instanceof InnerNode) {
    Node firstGrandChild=(((InnerNode)firstChild).children).get(0);
    if (firstGrandChild instanceof InnerNode) {
      return false;
    }
 else {
      return true;
    }
  }
  return false;
}
