{
  StackTraceElement[] stack=Thread.currentThread().getStackTrace();
  for (  StackTraceElement ste : stack) {
switch (ste.getMethodName()) {
case "getDfsUsed":
case "getBlockPoolUsed":
case "getAvailable":
case "getVolumeMap":
      return;
default :
    break;
}
}
FsDatasetImpl.LOG.trace("Reference count: " + op + " "+ this+ ": "+ this.reference.getReferenceCount());
FsDatasetImpl.LOG.trace(Joiner.on("\n").join(Thread.currentThread().getStackTrace()));
}
