{
  ObjectMapper mapper=new ObjectMapper();
  File file=getSaveFile();
  this.state=mapper.reader(BlockIteratorState.class).readValue(file);
  LOG.trace("load({}, {}): loaded iterator {} from {}: {}",storageID,bpid,name,file.getAbsoluteFile(),mapper.writerWithDefaultPrettyPrinter().writeValueAsString(state));
}
