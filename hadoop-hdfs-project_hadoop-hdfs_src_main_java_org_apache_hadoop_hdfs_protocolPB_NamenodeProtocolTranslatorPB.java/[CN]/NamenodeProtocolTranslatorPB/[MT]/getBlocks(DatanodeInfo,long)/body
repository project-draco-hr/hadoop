{
  GetBlocksRequestProto req=GetBlocksRequestProto.newBuilder().setDatanode(PBHelperClient.convert((DatanodeID)datanode)).setSize(size).build();
  try {
    return PBHelper.convert(rpcProxy.getBlocks(NULL_CONTROLLER,req).getBlocks());
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
