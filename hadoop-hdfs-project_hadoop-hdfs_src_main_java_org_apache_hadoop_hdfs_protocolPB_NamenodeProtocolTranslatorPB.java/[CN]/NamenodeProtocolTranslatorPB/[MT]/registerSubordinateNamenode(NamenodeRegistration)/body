{
  RegisterRequestProto req=RegisterRequestProto.newBuilder().setRegistration(PBHelper.convert(registration)).build();
  try {
    return PBHelper.convert(rpcProxy.registerSubordinateNamenode(NULL_CONTROLLER,req).getRegistration());
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
