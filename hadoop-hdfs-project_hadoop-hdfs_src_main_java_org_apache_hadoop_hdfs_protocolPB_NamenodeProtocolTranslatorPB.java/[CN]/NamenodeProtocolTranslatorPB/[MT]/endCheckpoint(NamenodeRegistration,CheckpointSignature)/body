{
  EndCheckpointRequestProto req=EndCheckpointRequestProto.newBuilder().setRegistration(PBHelper.convert(registration)).setSignature(PBHelper.convert(sig)).build();
  try {
    rpcProxy.endCheckpoint(NULL_CONTROLLER,req);
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
