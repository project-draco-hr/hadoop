{
  ErrorReportRequestProto req=ErrorReportRequestProto.newBuilder().setErrorCode(errorCode).setMsg(msg).setRegistartion(PBHelper.convert(registration)).build();
  try {
    rpcProxy.errorReport(NULL_CONTROLLER,req);
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
