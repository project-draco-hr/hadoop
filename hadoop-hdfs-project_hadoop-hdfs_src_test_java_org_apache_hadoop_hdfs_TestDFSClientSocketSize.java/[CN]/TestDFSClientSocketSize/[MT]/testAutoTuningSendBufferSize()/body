{
  conf.setInt(DFS_CLIENT_SOCKET_SEND_BUFFER_SIZE_KEY,0);
  socket=createSocket();
  LOG.info("The auto tuned send buffer size is: {}",socket.getSendBufferSize());
  assertTrue("Send buffer size should be non-negative value which is " + "determined by system (kernel).",socket.getSendBufferSize() > 0);
}
