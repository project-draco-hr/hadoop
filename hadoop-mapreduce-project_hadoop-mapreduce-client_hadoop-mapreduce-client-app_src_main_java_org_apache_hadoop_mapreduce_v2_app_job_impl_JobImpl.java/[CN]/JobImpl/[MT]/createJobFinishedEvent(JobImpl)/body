{
  JobFinishedEvent jfe=new JobFinishedEvent(job.oldJobId,job.finishTime,job.succeededMapTaskCount,job.succeededReduceTaskCount,job.failedMapTaskCount,job.failedReduceTaskCount,TypeConverter.fromYarn(job.getMapCounters()),TypeConverter.fromYarn(job.getReduceCounters()),TypeConverter.fromYarn(job.getCounters()));
  return jfe;
}
