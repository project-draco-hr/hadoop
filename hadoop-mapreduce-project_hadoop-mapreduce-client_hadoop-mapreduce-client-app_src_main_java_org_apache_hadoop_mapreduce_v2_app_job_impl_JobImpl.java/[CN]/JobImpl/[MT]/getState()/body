{
  readLock.lock();
  try {
    JobState state=getExternalState(getInternalState());
    if (!appContext.safeToReportTerminationToUser() && (state == JobState.SUCCEEDED || state == JobState.FAILED || state == JobState.KILLED || state == JobState.ERROR)) {
      return lastNonFinalState;
    }
 else {
      return state;
    }
  }
  finally {
    readLock.unlock();
  }
}
