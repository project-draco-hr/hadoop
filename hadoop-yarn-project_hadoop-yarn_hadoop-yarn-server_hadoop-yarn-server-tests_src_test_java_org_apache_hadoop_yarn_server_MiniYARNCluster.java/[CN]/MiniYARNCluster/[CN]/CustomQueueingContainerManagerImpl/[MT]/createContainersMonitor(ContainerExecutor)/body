{
  return new ContainersMonitorImpl(exec,dispatcher,this.context){
    @Override public void increaseContainersAllocation(    ProcessTreeInfo pti){
    }
    @Override public void decreaseContainersAllocation(    ProcessTreeInfo pti){
    }
    @Override public boolean hasResourcesAvailable(    ContainersMonitorImpl.ProcessTreeInfo pti){
      return true;
    }
  }
;
}
