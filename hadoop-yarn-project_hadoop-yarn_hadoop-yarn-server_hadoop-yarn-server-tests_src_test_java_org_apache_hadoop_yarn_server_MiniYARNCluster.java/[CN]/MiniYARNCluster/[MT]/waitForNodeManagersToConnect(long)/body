{
  ResourceManager rm=getResourceManager();
  GetClusterMetricsRequest req=GetClusterMetricsRequest.newInstance();
  for (int i=0; i < timeout / 100; i++) {
    if (nodeManagers.length == rm.getClientRMService().getClusterMetrics(req).getClusterMetrics().getNumNodeManagers()) {
      return true;
    }
    Thread.sleep(100);
  }
  return false;
}
