{
  this.datum=(TaskFailed)odatum;
  this.id=TaskID.forName(datum.taskid.toString());
  this.taskType=TaskType.valueOf(datum.taskType.toString());
  this.finishTime=datum.finishTime;
  this.error=datum.error.toString();
  this.failedDueToAttempt=datum.failedDueToAttempt == null ? null : TaskAttemptID.forName(datum.failedDueToAttempt.toString());
  this.status=datum.status.toString();
  this.counters=EventReader.fromAvro(datum.counters);
}
