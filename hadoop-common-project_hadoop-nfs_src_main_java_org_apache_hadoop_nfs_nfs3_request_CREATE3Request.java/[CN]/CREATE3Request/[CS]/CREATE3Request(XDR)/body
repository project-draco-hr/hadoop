{
  super(xdr);
  name=xdr.readString();
  mode=xdr.readInt();
  objAttr=new SetAttr3();
  if ((mode == Nfs3Constant.CREATE_UNCHECKED) || (mode == Nfs3Constant.CREATE_GUARDED)) {
    objAttr.deserialize(xdr);
  }
 else   if (mode == Nfs3Constant.CREATE_EXCLUSIVE) {
    verf=xdr.readHyper();
  }
 else {
    throw new IOException("Wrong create mode:" + mode);
  }
}
