{
  if (attemptId == 1) {
    appsSubmitted.incr();
  }
 else {
    appsFailed.decr();
  }
  appsPending.incr();
  QueueMetrics userMetrics=getUserMetrics(user);
  if (userMetrics != null) {
    userMetrics.submitApp(user,attemptId);
  }
  if (parent != null) {
    parent.submitApp(user,attemptId);
  }
}
