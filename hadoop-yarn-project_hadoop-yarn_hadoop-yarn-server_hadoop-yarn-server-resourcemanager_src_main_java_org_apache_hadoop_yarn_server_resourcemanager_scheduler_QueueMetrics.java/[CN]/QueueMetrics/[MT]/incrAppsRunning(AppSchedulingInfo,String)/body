{
  runBuckets.add(app.getApplicationId(),System.currentTimeMillis());
  appsRunning.incr();
  appsPending.decr();
  QueueMetrics userMetrics=getUserMetrics(user);
  if (userMetrics != null) {
    userMetrics.incrAppsRunning(app,user);
  }
  if (parent != null) {
    parent.incrAppsRunning(app,user);
  }
}
