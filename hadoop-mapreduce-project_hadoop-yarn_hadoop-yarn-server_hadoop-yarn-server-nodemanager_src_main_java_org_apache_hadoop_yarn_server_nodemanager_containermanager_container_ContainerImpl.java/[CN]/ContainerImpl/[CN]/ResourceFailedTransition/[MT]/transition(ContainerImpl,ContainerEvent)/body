{
  ContainerResourceFailedEvent rsrcFailedEvent=(ContainerResourceFailedEvent)event;
  container.diagnostics.append(StringUtils.stringifyException(rsrcFailedEvent.getCause())).append("\n");
  container.dispatcher.getEventHandler().handle(new ContainerLocalizationEvent(LocalizationEventType.CLEANUP_CONTAINER_RESOURCES,container));
  container.metrics.endInitingContainer();
}
