{
  Path file=createFile(userfs,"testSuperCanChangeOwnerGroup");
  nnfs.setOwner(file,NOUSER,NOGROUP);
  FileStatus status=nnfs.getFileStatus(file);
  assertThat("A super user can change owner",status.getOwner(),is(NOUSER));
  assertThat("A super user can change group",status.getGroup(),is(NOGROUP));
}
