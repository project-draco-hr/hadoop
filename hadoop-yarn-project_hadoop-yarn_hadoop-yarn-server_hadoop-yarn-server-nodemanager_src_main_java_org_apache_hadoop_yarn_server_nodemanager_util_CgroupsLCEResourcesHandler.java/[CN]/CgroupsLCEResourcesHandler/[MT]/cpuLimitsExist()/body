{
  String path=pathForCgroup(CONTROLLER_CPU,"");
  File quotaFile=new File(path,CONTROLLER_CPU + "." + CPU_QUOTA_US);
  if (quotaFile.exists()) {
    String contents=FileUtils.readFileToString(quotaFile,"UTF-8");
    int quotaUS=Integer.parseInt(contents.trim());
    if (quotaUS != -1) {
      return true;
    }
  }
  return false;
}
