{
  RecoveringBlock b=PBHelper.convert(request.getBlock());
  ReplicaRecoveryInfo r;
  try {
    r=impl.initReplicaRecovery(b);
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
  return InitReplicaRecoveryResponseProto.newBuilder().setBlock(PBHelper.convert(r)).setState(PBHelper.convert(r.getOriginalReplicaState())).build();
}
