{
  final String storageID;
  try {
    storageID=impl.updateReplicaUnderRecovery(PBHelper.convert(request.getBlock()),request.getRecoveryId(),request.getNewBlockId(),request.getNewLength());
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
  return UpdateReplicaUnderRecoveryResponseProto.newBuilder().setStorageUuid(storageID).build();
}
