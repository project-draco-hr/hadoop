{
  ExtendedBlock b;
  try {
    b=impl.updateReplicaUnderRecovery(PBHelper.convert(request.getBlock()),request.getRecoveryId(),request.getNewLength());
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
  return UpdateReplicaUnderRecoveryResponseProto.newBuilder().setBlock(PBHelper.convert(b)).build();
}
