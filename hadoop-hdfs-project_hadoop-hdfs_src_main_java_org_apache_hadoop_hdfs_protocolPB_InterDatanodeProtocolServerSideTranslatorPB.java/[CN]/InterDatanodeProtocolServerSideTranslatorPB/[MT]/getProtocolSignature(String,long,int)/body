{
  if (!protocol.equals(RPC.getProtocolName(InterDatanodeProtocol.class))) {
    throw new IOException("Namenode Serverside implements " + RPC.getProtocolName(InterDatanodeProtocol.class) + ". The following requested protocol is unknown: "+ protocol);
  }
  return ProtocolSignature.getProtocolSignature(clientMethodsHash,RPC.getProtocolVersion(InterDatanodeProtocolPB.class),InterDatanodeProtocol.class);
}
