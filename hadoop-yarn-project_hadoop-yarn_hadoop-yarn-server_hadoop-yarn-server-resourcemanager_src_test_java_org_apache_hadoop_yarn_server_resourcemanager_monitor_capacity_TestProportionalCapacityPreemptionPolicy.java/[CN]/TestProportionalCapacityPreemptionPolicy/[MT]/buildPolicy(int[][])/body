{
  ProportionalCapacityPreemptionPolicy policy=new ProportionalCapacityPreemptionPolicy(conf,mDisp,mCS,mClock);
  ParentQueue mRoot=buildMockRootQueue(rand,qData);
  when(mCS.getRootQueue()).thenReturn(mRoot);
  Resource clusterResources=Resource.newInstance(leafAbsCapacities(qData[0],qData[7]),0);
  when(mCS.getClusterResources()).thenReturn(clusterResources);
  return policy;
}
