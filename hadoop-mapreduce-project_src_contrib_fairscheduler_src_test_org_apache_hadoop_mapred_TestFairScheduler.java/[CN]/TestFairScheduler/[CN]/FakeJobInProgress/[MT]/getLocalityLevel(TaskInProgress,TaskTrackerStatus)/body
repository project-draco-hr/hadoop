{
  FakeTaskInProgress ftip=(FakeTaskInProgress)tip;
  if (ftip.inputLocations != null) {
    for (    String location : ftip.inputLocations) {
      if (location.equals(tts.host)) {
        return 0;
      }
    }
    for (    String location : ftip.inputLocations) {
      if (getRack(location).equals(getRack(tts.host))) {
        return 1;
      }
    }
    return 2;
  }
 else {
    return -1;
  }
}
