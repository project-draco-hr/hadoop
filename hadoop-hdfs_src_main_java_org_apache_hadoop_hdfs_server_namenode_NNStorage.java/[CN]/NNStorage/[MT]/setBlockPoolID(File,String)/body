{
  if (bpid == null || bpid.equals("")) {
    throw new InconsistentFSStateException(storage,"file " + Storage.STORAGE_FILE_VERSION + " has no block pool Id.");
  }
  if (!blockpoolID.equals("") && !blockpoolID.equals(bpid)) {
    throw new InconsistentFSStateException(storage,"Unexepcted blockpoolID " + bpid + " . Expected "+ blockpoolID);
  }
  setBlockPoolID(bpid);
}
