{
  readLock.lock();
  try {
    if (cumulativeCapacity.isEmpty()) {
      return true;
    }
    if (cumulativeCapacity.size() == 2) {
      return cumulativeCapacity.firstEntry().getValue().equals(ZERO_RESOURCE) && cumulativeCapacity.lastEntry().getValue() == null;
    }
    return false;
  }
  finally {
    readLock.unlock();
  }
}
