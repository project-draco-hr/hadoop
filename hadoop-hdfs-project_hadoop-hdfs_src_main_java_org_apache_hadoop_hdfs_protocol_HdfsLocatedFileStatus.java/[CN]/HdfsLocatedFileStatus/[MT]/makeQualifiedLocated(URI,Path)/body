{
  return new LocatedFileStatus(getLen(),isDir(),getReplication(),getBlockSize(),getModificationTime(),getAccessTime(),getPermission(),getOwner(),getGroup(),isSymlink() ? new Path(getSymlink()) : null,(getFullPath(path)).makeQualified(defaultUri,null),DFSUtilClient.locatedBlocks2Locations(getBlockLocations()));
}
