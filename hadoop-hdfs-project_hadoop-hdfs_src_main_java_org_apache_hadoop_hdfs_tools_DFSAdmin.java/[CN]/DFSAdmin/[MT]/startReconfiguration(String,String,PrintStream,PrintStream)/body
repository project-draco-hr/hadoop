{
  String outMsg=null;
  String errMsg=null;
  int ret=0;
  try {
    ret=startReconfigurationDispatch(nodeType,address,out,err);
    outMsg=String.format("Started reconfiguration task on node [%s].",address);
  }
 catch (  IOException e) {
    errMsg=String.format("Node [%s] reconfiguring: %s.",address,e.toString());
  }
  if (errMsg != null) {
    err.println(errMsg);
    return 1;
  }
 else {
    out.println(outMsg);
    return ret;
  }
}
