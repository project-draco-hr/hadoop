{
  return new Runnable(){
    @Override public void run(){
      while (!stopped && !Thread.currentThread().isInterrupted()) {
        Event event;
        try {
          event=eventQueue.take();
        }
 catch (        InterruptedException ie) {
          LOG.info("AsyncDispatcher thread interrupted",ie);
          return;
        }
        if (event != null) {
          dispatch(event);
        }
      }
    }
  }
;
}
