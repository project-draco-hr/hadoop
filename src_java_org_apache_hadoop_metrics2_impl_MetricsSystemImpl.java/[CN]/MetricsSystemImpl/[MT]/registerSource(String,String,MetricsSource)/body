{
  checkNotNull(config,"config");
  MetricsConfig conf=sourceConfigs.get(name);
  MetricsSourceAdapter sa=conf != null ? new MetricsSourceAdapter(prefix,name,desc,source,injectedTags,period,conf) : new MetricsSourceAdapter(prefix,name,desc,source,injectedTags,period,config.subset(SOURCE_KEY));
  sources.put(name,sa);
  sa.start();
  LOG.info("Registered source " + name);
}
