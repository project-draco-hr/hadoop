{
synchronized (services) {
    AppLevelAggregatorService service=services.remove(appId);
    if (service == null) {
      String msg="the application aggregator service for " + appId + " does not exist!";
      LOG.error(msg);
      return false;
    }
 else {
      service.stop();
      LOG.info("the application aggregator service for " + appId + " was removed");
      return true;
    }
  }
}
