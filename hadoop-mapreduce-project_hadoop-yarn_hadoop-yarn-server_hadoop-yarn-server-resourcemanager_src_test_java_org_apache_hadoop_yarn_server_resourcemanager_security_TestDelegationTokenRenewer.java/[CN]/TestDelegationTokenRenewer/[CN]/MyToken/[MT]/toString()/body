{
  StringBuilder sb=new StringBuilder(1024);
  sb.append("id=");
  String id=StringUtils.byteToHexString(this.getIdentifier());
  int idLen=id.length();
  sb.append(id.substring(idLen - 6));
  sb.append(";k=");
  sb.append(this.getKind());
  sb.append(";s=");
  sb.append(this.getService());
  return sb.toString();
}
