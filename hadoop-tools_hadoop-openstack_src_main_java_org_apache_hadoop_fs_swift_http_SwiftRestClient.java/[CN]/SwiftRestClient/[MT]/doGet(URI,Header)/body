{
  return perform("",uri,new GetMethodProcessor<HttpBodyContent>(){
    @Override public HttpBodyContent extractResult(    GetMethod method) throws IOException {
      return new HttpBodyContent(new HttpInputStreamWithRelease(uri,method),method.getResponseContentLength());
    }
    @Override protected void setup(    GetMethod method) throws SwiftInternalStateException {
      setHeaders(method,requestHeaders);
    }
  }
);
}
