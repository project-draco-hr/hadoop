{
  if (!isFlowRunRegion) {
    return;
  }
  Scan scan=new Scan(get);
  scan.setMaxVersions();
  RegionScanner scanner=null;
  try {
    scanner=new FlowScanner(e.getEnvironment(),scan.getBatch(),region.getScanner(scan),FlowScannerOperation.READ);
    scanner.next(results);
    e.bypass();
  }
  finally {
    if (scanner != null) {
      scanner.close();
    }
  }
}
