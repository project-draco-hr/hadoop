{
  conf.set(CommonConfigurationKeysPublic.FS_DEFAULT_NAME_KEY,"hdfs://" + NSID);
  conf.set(DFSConfigKeys.DFS_NAMESERVICES,NSID);
  conf.set(DFSConfigKeys.DFS_NAMESERVICE_ID,NSID);
  conf.set(DFSUtil.addKeySuffixes(DFSConfigKeys.DFS_HA_NAMENODES_KEY_PREFIX,NSID),"nn1,nn2");
  conf.set(DFSConfigKeys.DFS_HA_NAMENODE_ID_KEY,"nn1");
  conf.set(DFSUtil.addKeySuffixes(DFSConfigKeys.DFS_NAMENODE_RPC_ADDRESS_KEY,NSID,"nn1"),nn1Addr);
  conf.set(DFSUtil.addKeySuffixes(DFSConfigKeys.DFS_NAMENODE_RPC_ADDRESS_KEY,NSID,"nn2"),nn2Addr);
}
