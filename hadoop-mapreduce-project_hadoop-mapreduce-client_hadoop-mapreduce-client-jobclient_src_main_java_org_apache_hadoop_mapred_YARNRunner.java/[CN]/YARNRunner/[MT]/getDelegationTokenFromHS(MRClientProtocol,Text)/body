{
  GetDelegationTokenRequest request=recordFactory.newRecordInstance(GetDelegationTokenRequest.class);
  request.setRenewer(renewer.toString());
  DelegationToken mrDelegationToken=hsProxy.getDelegationToken(request).getDelegationToken();
  return new Token<MRDelegationTokenIdentifier>(mrDelegationToken.getIdentifier().array(),mrDelegationToken.getPassword().array(),new Text(mrDelegationToken.getKind()),new Text(mrDelegationToken.getService()));
}
