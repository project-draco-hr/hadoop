{
  info("\'" + sinfo.getQueueName() + "\' Queue Status")._("Queue State:",sinfo.getState())._("Minimum Queue Memory Capacity:",Integer.toString(sinfo.getMinQueueMemoryCapacity()))._("Maximum Queue Memory Capacity:",Integer.toString(sinfo.getMaxQueueMemoryCapacity()))._("Number of Nodes:",Integer.toString(sinfo.getNumNodes()))._("Used Node Capacity:",Integer.toString(sinfo.getUsedNodeCapacity()))._("Available Node Capacity:",Integer.toString(sinfo.getAvailNodeCapacity()))._("Total Node Capacity:",Integer.toString(sinfo.getTotalNodeCapacity()))._("Number of Node Containers:",Integer.toString(sinfo.getNumContainers()));
  html._(InfoBlock.class);
}
