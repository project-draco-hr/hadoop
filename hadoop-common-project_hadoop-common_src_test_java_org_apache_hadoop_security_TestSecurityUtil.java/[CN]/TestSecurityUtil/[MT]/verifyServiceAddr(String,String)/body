{
  InetSocketAddress addr;
  int port=123;
  addr=NetUtils.createSocketAddrForHost(host,port);
  verifyAddress(addr,host,ip,port);
  addr=NetUtils.createSocketAddr(host + ":" + port);
  verifyAddress(addr,host,ip,port);
  addr=NetUtils.createSocketAddr(host + ":" + port,port + 1);
  verifyAddress(addr,host,ip,port);
  addr=NetUtils.createSocketAddr(host,port);
  verifyAddress(addr,host,ip,port);
}
