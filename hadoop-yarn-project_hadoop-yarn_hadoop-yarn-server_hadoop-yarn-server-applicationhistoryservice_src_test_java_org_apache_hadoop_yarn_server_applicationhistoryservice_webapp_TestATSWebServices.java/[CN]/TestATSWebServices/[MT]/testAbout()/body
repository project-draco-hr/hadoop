{
  WebResource r=resource();
  ClientResponse response=r.path("ws").path("v1").path("apptimeline").accept(MediaType.APPLICATION_JSON).get(ClientResponse.class);
  assertEquals(MediaType.APPLICATION_JSON_TYPE,response.getType());
  ATSWebServices.AboutInfo about=response.getEntity(ATSWebServices.AboutInfo.class);
  Assert.assertNotNull(about);
  Assert.assertEquals("Application Timeline API",about.getAbout());
}
