{
  LinkedList<String> args=new LinkedList<String>(Arrays.asList(argv));
  try {
    if (isDeprecated()) {
      displayWarning("DEPRECATED: Please use '" + getReplacementCommand() + "' instead.");
    }
    processOptions(args);
    processRawArguments(args);
  }
 catch (  IOException e) {
    displayError(e);
  }
  return (numErrors == 0) ? exitCode : exitCodeForError();
}
