{
  if (task == null) {
    return null;
  }
  if (task.getTaskType() == null) {
    LOG.warn("Task " + task.getTaskID() + " has nulll TaskType");
    return null;
  }
  if (task.getTaskStatus() == null) {
    LOG.warn("Task " + task.getTaskID() + " has nulll TaskStatus");
    return null;
  }
  return task;
}
