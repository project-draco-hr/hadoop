{
  maybeInitBuilder();
  if (tags == null || tags.isEmpty()) {
    builder.clearApplicationTags();
    this.applicationTags=null;
    return;
  }
  checkTags(tags);
  this.applicationTags=new HashSet<String>();
  for (  String tag : tags) {
    this.applicationTags.add(tag.toLowerCase(Locale.ENGLISH));
  }
}
