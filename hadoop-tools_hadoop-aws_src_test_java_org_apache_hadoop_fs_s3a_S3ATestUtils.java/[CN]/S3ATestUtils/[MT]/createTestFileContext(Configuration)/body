{
  String fsname=conf.getTrimmed(TestS3AFileSystemContract.TEST_FS_S3A_NAME,"");
  boolean liveTest=!StringUtils.isEmpty(fsname);
  URI testURI=null;
  if (liveTest) {
    testURI=URI.create(fsname);
    liveTest=testURI.getScheme().equals(Constants.FS_S3A);
  }
  if (!liveTest) {
    throw new AssumptionViolatedException("No test filesystem in " + TestS3AFileSystemContract.TEST_FS_S3A_NAME);
  }
  FileContext fc=FileContext.getFileContext(testURI,conf);
  return fc;
}
