{
  DBConfiguration dbConf=getDBConf();
  @SuppressWarnings("unchecked") Class<T> inputClass=(Class<T>)(dbConf.getInputClass());
  try {
    return new OracleDataDrivenDBRecordReader<T>(split,inputClass,conf,getConnection(),dbConf,dbConf.getInputConditions(),dbConf.getInputFieldNames(),dbConf.getInputTableName());
  }
 catch (  SQLException ex) {
    throw new IOException(ex.getMessage());
  }
}
