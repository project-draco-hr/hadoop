{
  FileInputStreamCache cache=cacheRef.get();
  if (cache == null)   return;
synchronized (cache) {
    if (cache.closed)     return;
    long curTime=Time.monotonicNow();
    for (Iterator<Entry<Key,Value>> iter=cache.map.entries().iterator(); iter.hasNext(); iter=cache.map.entries().iterator()) {
      Entry<Key,Value> entry=iter.next();
      if (entry.getValue().getTime() + cache.expiryTimeMs >= curTime) {
        break;
      }
      entry.getValue().close();
      iter.remove();
    }
  }
}
