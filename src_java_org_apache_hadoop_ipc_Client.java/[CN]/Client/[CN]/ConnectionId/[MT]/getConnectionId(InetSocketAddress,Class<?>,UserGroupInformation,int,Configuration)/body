{
  String remotePrincipal=getRemotePrincipal(conf,addr,protocol);
  return new ConnectionId(addr,protocol,ticket,rpcTimeout,remotePrincipal,conf.getInt("ipc.client.connection.maxidletime",10000),conf.getInt("ipc.client.connect.max.retries",10),conf.getBoolean("ipc.client.tcpnodelay",false),conf.getBoolean("ipc.client.ping",true),Client.getPingInterval(conf));
}
