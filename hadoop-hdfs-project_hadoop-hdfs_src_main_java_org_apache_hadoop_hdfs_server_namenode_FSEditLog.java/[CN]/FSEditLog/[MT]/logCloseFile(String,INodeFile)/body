{
  CloseOp op=CloseOp.getInstance(cache.get()).setPath(path).setReplication(newNode.getBlockReplication()).setModificationTime(newNode.getModificationTime()).setAccessTime(newNode.getAccessTime()).setBlockSize(newNode.getPreferredBlockSize()).setBlocks(newNode.getBlocks()).setPermissionStatus(newNode.getPermissionStatus());
  logEdit(op);
}
