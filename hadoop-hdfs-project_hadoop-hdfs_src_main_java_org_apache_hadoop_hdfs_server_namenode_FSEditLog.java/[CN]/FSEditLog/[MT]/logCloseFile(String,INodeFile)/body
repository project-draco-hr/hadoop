{
  CloseOp op=CloseOp.getInstance(cache.get()).setPath(path).setReplication(newNode.getFileReplication()).setModificationTime(newNode.getModificationTime()).setAccessTime(newNode.getAccessTime()).setBlockSize(newNode.getPreferredBlockSize()).setLazyPersistFlag(newNode.getLazyPersistFlag()).setBlocks(newNode.getBlocks()).setPermissionStatus(newNode.getPermissionStatus());
  logEdit(op);
}
