{
  AddOp op=AddOp.getInstance(cache.get()).setInodeId(newNode.getId()).setPath(path).setReplication(newNode.getBlockReplication()).setModificationTime(newNode.getModificationTime()).setAccessTime(newNode.getAccessTime()).setBlockSize(newNode.getPreferredBlockSize()).setBlocks(newNode.getBlocks()).setPermissionStatus(newNode.getPermissionStatus()).setClientName(newNode.getClientName()).setClientMachine(newNode.getClientMachine());
  logEdit(op);
}
