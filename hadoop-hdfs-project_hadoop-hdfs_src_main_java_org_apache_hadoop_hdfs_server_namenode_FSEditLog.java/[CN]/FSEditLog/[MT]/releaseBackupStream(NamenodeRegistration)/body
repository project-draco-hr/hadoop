{
  for (Iterator<JournalAndStream> iter=journals.iterator(); iter.hasNext(); ) {
    JournalAndStream jas=iter.next();
    if (jas.manager instanceof BackupJournalManager && ((BackupJournalManager)jas.manager).matchesRegistration(registration)) {
      jas.abort();
      LOG.info("Removing backup journal " + jas);
      iter.remove();
    }
  }
}
