{
  PermissionStatus permissions=newNode.getPermissionStatus();
  MkdirOp op=MkdirOp.getInstance(cache.get()).setInodeId(newNode.getId()).setPath(path).setTimestamp(newNode.getModificationTime()).setPermissionStatus(permissions).setAclEntries(permissions.getPermission().getAclBit() ? AclStorage.readINodeLogicalAcl(newNode) : null);
  logEdit(op);
}
