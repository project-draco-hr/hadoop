{
  int i=start;
  while (i <= end) {
    if (a[i] != ZERO) {
      if (a[i] != DECIMAL && isdigit(a[i])) {
        return false;
      }
      break;
    }
    i++;
  }
  if (i != (end + 1) && a[i++] == DECIMAL) {
    while (i <= end) {
      if (a[i] != ZERO) {
        if (isdigit(a[i])) {
          return false;
        }
        break;
      }
      i++;
    }
  }
  return true;
}
