def __repr__(self):
    'Returns a string representation of a config object including all\n           normalizations.'
    print_string = ''
    for section in self._mySections:
        print_string = ('%s[%s]\n' % (print_string, section))
        options = self._dict[section].keys()
        for option in options:
            print_string = ('%s%s = %s\n' % (print_string, option, self._dict[section][option]))
        print_string = ('%s\n' % print_string)
    print_string = re.sub('\n\n$', '', print_string)
    return print_string
