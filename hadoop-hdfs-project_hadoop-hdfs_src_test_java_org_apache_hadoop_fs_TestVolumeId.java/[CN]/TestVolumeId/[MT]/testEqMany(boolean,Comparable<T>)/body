{
  Comparable<T> vidNext;
  int sum=0;
  for (int i=0; i < volumeIds.length; i++) {
    if (i == volumeIds.length - 1) {
      vidNext=volumeIds[0];
    }
 else {
      vidNext=volumeIds[i + 1];
    }
    testEq(eq,volumeIds[i],vidNext);
    sum+=sign(volumeIds[i].compareTo((T)vidNext));
  }
  assertTrue(sum < volumeIds.length);
}
