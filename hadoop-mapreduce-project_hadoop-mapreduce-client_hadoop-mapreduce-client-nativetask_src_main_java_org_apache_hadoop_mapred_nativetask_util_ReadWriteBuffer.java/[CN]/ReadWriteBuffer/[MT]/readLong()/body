{
  final long result=((_buff[_readPoint + 0] & 255) << 0) + ((_buff[_readPoint + 1] & 255) << 8) + ((_buff[_readPoint + 2] & 255) << 16)+ ((long)(_buff[_readPoint + 3] & 255) << 24)+ ((long)(_buff[_readPoint + 4] & 255) << 32)+ ((long)(_buff[_readPoint + 5] & 255) << 40)+ ((long)(_buff[_readPoint + 6] & 255) << 48)+ (((long)_buff[_readPoint + 7] << 56));
