{
  if (!doneWithRecovery)   return;
  ContainerPBImpl containerPBImpl=(ContainerPBImpl)container;
  try {
    zkClient.setData(APPS + ConverterUtils.toString(container.getId().getApplicationAttemptId().getApplicationId()) + ZK_PATH_SEPARATOR+ APP_MASTER_CONTAINER,containerPBImpl.getProto().toByteArray(),-1);
  }
 catch (  InterruptedException ie) {
    LOG.info("Interrupted",ie);
    throw new InterruptedIOException(ie.getMessage());
  }
catch (  KeeperException ke) {
    LOG.info("Keeper exception",ke);
    throw convertToIOException(ke);
  }
}
