{
  FSAppAttempt app=scheduler.getSchedulerApp(attId);
  FSLeafQueue queue=app.getQueue();
  Collection<FSAppAttempt> runnableApps=queue.getRunnableAppSchedulables();
  Collection<FSAppAttempt> nonRunnableApps=queue.getNonRunnableAppSchedulables();
  assertEquals(runnable,runnableApps.contains(app));
  assertEquals(!runnable,nonRunnableApps.contains(app));
}
