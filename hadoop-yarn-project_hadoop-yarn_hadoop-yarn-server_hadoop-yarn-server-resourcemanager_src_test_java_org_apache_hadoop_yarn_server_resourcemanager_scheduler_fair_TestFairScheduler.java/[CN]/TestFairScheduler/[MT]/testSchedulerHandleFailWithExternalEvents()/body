{
  Configuration conf=createConfiguration();
  scheduler.reinitialize(conf,resourceManager.getRMContext());
  ImmutableSet<? extends SchedulerEvent> externalEvents=ImmutableSet.of(new ExternalAppAddedSchedulerEvent(),new ExternalNodeRemovedSchedulerEvent(),new ExternalNodeUpdateSchedulerEvent(),new ExternalNodeAddedSchedulerEvent(),new ExternalAppRemovedSchedulerEvent(),new ExternalContainerExpiredSchedulerEvent());
  UnmodifiableIterator<? extends SchedulerEvent> iter=externalEvents.iterator();
  while (iter.hasNext())   handleExternalEvent(iter.next());
}
