{
  setConf(conf);
  if (!this.initialized) {
    this.containerTokenSecretManager=containerTokenSecretManager;
    this.rmContext=rmContext;
    this.minimumAllocation=Resources.createResource(conf.getInt(MINIMUM_ALLOCATION,MINIMUM_MEMORY));
    this.maximumAllocation=Resources.createResource(conf.getInt(MAXIMUM_ALLOCATION,MAXIMUM_MEMORY));
    this.metrics=QueueMetrics.forQueue(DEFAULT_QUEUE_NAME,null,false,conf);
    this.activeUsersManager=new ActiveUsersManager(metrics);
    this.initialized=true;
  }
}
