{
  if (b == null) {
    throw new NullPointerException();
  }
  if (b.length == 0) {
    return 0;
  }
  if (currentPos >= length) {
    return -1;
  }
  int bytesRead=(int)Math.min(b.length,length - currentPos);
  if (data != null) {
    System.arraycopy(data,currentPos,b,0,bytesRead);
  }
 else {
    for (    int i : b) {
      b[i]=theRepeatedData;
    }
  }
  currentPos+=bytesRead;
  return bytesRead;
}
