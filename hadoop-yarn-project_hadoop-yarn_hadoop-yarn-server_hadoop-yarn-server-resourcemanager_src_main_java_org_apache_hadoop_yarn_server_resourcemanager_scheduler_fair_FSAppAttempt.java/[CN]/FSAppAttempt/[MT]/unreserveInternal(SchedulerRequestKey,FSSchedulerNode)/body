{
  Map<NodeId,RMContainer> reservedContainers=this.reservedContainers.get(schedulerKey);
  RMContainer reservedContainer=reservedContainers.remove(node.getNodeID());
  if (reservedContainers.isEmpty()) {
    this.reservedContainers.remove(schedulerKey);
  }
  resetReReservations(schedulerKey);
  Resource resource=reservedContainer.getContainer().getResource();
  this.attemptResourceUsage.decReserved(resource);
  LOG.info("Application " + getApplicationId() + " unreserved "+ " on node "+ node+ ", currently has "+ reservedContainers.size()+ " at priority "+ schedulerKey.getPriority()+ "; currentReservation "+ this.attemptResourceUsage.getReserved());
}
