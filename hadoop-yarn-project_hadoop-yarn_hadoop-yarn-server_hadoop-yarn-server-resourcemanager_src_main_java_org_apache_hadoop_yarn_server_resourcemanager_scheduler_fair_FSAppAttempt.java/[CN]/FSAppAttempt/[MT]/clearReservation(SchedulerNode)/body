{
  String rackName=node.getRackName() == null ? "NULL" : node.getRackName();
  try {
    writeLock.lock();
    Set<String> rackReservations=reservations.get(rackName);
    if (rackReservations != null) {
      rackReservations.remove(node.getNodeName());
    }
  }
  finally {
    writeLock.unlock();
  }
}
