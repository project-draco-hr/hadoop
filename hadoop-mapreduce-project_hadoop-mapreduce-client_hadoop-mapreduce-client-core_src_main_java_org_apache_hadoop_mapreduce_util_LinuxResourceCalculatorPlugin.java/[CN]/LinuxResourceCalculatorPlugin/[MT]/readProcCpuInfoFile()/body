{
  if (readCpuInfoFile) {
    return;
  }
  BufferedReader in=null;
  InputStreamReader fReader=null;
  try {
    fReader=new InputStreamReader(new FileInputStream(procfsCpuFile),Charsets.UTF_8);
    in=new BufferedReader(fReader);
  }
 catch (  FileNotFoundException f) {
    return;
  }
  Matcher mat=null;
  try {
    numProcessors=0;
    String str=in.readLine();
    while (str != null) {
      mat=PROCESSOR_FORMAT.matcher(str);
      if (mat.find()) {
        numProcessors++;
      }
      mat=FREQUENCY_FORMAT.matcher(str);
      if (mat.find()) {
        cpuFrequency=(long)(Double.parseDouble(mat.group(1)) * 1000);
      }
      str=in.readLine();
    }
  }
 catch (  IOException io) {
    LOG.warn("Error reading the stream " + io);
  }
 finally {
    try {
      fReader.close();
      try {
        in.close();
      }
 catch (      IOException i) {
        LOG.warn("Error closing the stream " + in);
      }
    }
 catch (    IOException i) {
      LOG.warn("Error closing the stream " + fReader);
    }
  }
  readCpuInfoFile=true;
}
