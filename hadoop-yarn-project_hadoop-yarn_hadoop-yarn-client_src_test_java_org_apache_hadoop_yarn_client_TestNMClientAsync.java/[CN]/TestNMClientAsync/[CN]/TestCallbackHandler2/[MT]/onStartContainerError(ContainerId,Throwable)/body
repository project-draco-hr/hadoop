{
  if (!t.getMessage().equals(NMClientAsync.StatefulContainer.OutOfOrderTransition.STOP_BEFORE_START_ERROR_MSG)) {
    exceptionOccurred.set(true);
    return;
  }
  try {
    barrierC.await();
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
catch (  BrokenBarrierException e) {
    e.printStackTrace();
  }
}
