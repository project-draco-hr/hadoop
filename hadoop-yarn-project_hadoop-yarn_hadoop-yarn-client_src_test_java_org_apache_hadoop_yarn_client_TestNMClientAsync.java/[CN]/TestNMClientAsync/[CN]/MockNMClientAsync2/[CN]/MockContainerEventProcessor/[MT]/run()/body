{
  try {
    if (event.getType() == ContainerEventType.START_CONTAINER) {
      barrierA.await();
      barrierB.await();
    }
    super.run();
    if (event.getType() == ContainerEventType.STOP_CONTAINER) {
      barrierB.await();
    }
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
catch (  BrokenBarrierException e) {
    e.printStackTrace();
  }
}
