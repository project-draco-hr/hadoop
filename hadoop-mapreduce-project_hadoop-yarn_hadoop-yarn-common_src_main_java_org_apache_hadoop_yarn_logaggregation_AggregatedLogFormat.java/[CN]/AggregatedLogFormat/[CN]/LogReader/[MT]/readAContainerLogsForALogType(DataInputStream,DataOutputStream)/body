{
  byte[] buf=new byte[65535];
  String fileType=valueStream.readUTF();
  String fileLengthStr=valueStream.readUTF();
  long fileLength=Long.parseLong(fileLengthStr);
  out.writeUTF("\nLogType:");
  out.writeUTF(fileType);
  out.writeUTF("\nLogLength:");
  out.writeUTF(fileLengthStr);
  out.writeUTF("\nLog Contents:\n");
  int curRead=0;
  long pendingRead=fileLength - curRead;
  int toRead=pendingRead > buf.length ? buf.length : (int)pendingRead;
  int len=valueStream.read(buf,0,toRead);
  while (len != -1 && curRead < fileLength) {
    out.write(buf,0,len);
    curRead+=len;
    pendingRead=fileLength - curRead;
    toRead=pendingRead > buf.length ? buf.length : (int)pendingRead;
    len=valueStream.read(buf,0,toRead);
  }
}
