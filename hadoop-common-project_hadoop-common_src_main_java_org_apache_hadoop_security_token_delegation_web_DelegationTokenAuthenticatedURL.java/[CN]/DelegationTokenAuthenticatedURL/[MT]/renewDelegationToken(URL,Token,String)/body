{
  Preconditions.checkNotNull(url,"url");
  Preconditions.checkNotNull(token,"token");
  Preconditions.checkNotNull(token.delegationToken,"No delegation token available");
  try {
    return ((KerberosDelegationTokenAuthenticator)getAuthenticator()).renewDelegationToken(url,token,token.delegationToken,doAsUser);
  }
 catch (  IOException ex) {
    token.delegationToken=null;
    throw ex;
  }
}
