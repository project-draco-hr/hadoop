{
  if (publishSystemMetrics) {
    ContainerId container=(appAttempt.getMasterContainer() == null) ? null : appAttempt.getMasterContainer().getId();
    dispatcher.getEventHandler().handle(new AppAttemptRegisteredEvent(appAttempt.getAppAttemptId(),appAttempt.getHost(),appAttempt.getRpcPort(),appAttempt.getTrackingUrl(),appAttempt.getOriginalTrackingUrl(),container,registeredTime));
  }
}
