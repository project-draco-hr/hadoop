{
  if (publishSystemMetrics) {
    ApplicationSubmissionContext appSubmissionContext=app.getApplicationSubmissionContext();
    dispatcher.getEventHandler().handle(new ApplicationCreatedEvent(app.getApplicationId(),app.getName(),app.getApplicationType(),app.getUser(),app.getQueue(),app.getSubmitTime(),createdTime,app.getApplicationTags(),appSubmissionContext.getUnmanagedAM(),appSubmissionContext.getPriority(),app.getAppNodeLabelExpression(),app.getAmNodeLabelExpression()));
  }
}
