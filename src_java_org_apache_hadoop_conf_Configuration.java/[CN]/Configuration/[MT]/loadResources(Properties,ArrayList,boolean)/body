{
  if (loadDefaults) {
    for (    String resource : defaultResources) {
      loadResource(properties,resource,quiet);
    }
    if (getResource("hadoop-site.xml") != null) {
      loadResource(properties,"hadoop-site.xml",quiet);
    }
  }
  for (  Object resource : resources) {
    loadResource(properties,resource,quiet);
  }
  processDeprecation();
}
