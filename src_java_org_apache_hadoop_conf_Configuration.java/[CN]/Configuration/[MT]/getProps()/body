{
  if (properties == null) {
    properties=new Properties();
    loadResources(properties,resources,quietmode);
    if (overlay != null) {
      properties.putAll(overlay);
      if (storeResource) {
        for (        Map.Entry<Object,Object> item : overlay.entrySet()) {
          updatingResource.put((String)item.getKey(),"Unknown");
        }
      }
    }
  }
  return properties;
}
