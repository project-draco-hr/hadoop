{
  disposeSasl();
  data=null;
  dataLengthBuffer=null;
  if (!channel.isOpen())   return;
  try {
    socket.shutdownOutput();
  }
 catch (  Exception e) {
    LOG.warn("Ignoring socket shutdown exception");
  }
  if (channel.isOpen()) {
    try {
      channel.close();
    }
 catch (    Exception e) {
    }
  }
  try {
    socket.close();
  }
 catch (  Exception e) {
  }
}
