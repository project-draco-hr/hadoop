{
  String rmPrincipal=conf.get(YarnConfiguration.RM_PRINCIPAL);
  String renewer=null;
  if (rmPrincipal != null && rmPrincipal.length() > 0) {
    String rmHost=conf.getSocketAddr(YarnConfiguration.RM_ADDRESS,YarnConfiguration.DEFAULT_RM_ADDRESS,YarnConfiguration.DEFAULT_RM_PORT).getHostName();
    renewer=SecurityUtil.getServerPrincipal(rmPrincipal,rmHost);
  }
  return renewer;
}
