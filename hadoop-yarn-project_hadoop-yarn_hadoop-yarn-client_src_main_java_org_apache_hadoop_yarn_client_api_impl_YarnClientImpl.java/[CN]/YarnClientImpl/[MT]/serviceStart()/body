{
  try {
    rmClient=ClientRMProxy.createRMProxy(getConfig(),ApplicationClientProtocol.class);
    if (historyServiceEnabled) {
      historyClient.start();
    }
    if (timelineServiceEnabled) {
      timelineClient.start();
    }
  }
 catch (  IOException e) {
    throw new YarnRuntimeException(e);
  }
  super.serviceStart();
}
