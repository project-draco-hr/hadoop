{
  org.apache.hadoop.security.token.Token<TimelineDelegationTokenIdentifier> timelineDelegationToken=timelineClient.getDelegationToken(UserGroupInformation.getCurrentUser().getUserName());
  if (timelineDelegationToken == null) {
    return;
  }
  Credentials credentials=new Credentials();
  DataInputByteBuffer dibb=new DataInputByteBuffer();
  ByteBuffer tokens=clc.getTokens();
  if (tokens != null) {
    dibb.reset(tokens);
    credentials.readTokenStorageStream(dibb);
    tokens.rewind();
  }
  for (  org.apache.hadoop.security.token.Token<? extends TokenIdentifier> token : credentials.getAllTokens()) {
    TokenIdentifier tokenIdentifier=token.decodeIdentifier();
    if (tokenIdentifier instanceof TimelineDelegationTokenIdentifier) {
      return;
    }
  }
  credentials.addToken(timelineService,timelineDelegationToken);
  if (LOG.isDebugEnabled()) {
    LOG.debug("Add timline delegation token into credentials: " + timelineDelegationToken);
  }
  DataOutputBuffer dob=new DataOutputBuffer();
  credentials.writeTokenStorageToStream(dob);
  tokens=ByteBuffer.wrap(dob.getData(),0,dob.getLength());
  clc.setTokens(tokens);
}
