{
  StringBuilder sb=new StringBuilder();
  sb.append(useraction.SYMBOL);
  sb.append(groupaction.SYMBOL);
  sb.append(otheraction.SYMBOL);
  if (stickyBit) {
    sb.replace(sb.length() - 1,sb.length(),otheraction.implies(FsAction.EXECUTE) ? "t" : "T");
  }
  if (aclBit) {
    sb.append('+');
  }
  return sb.toString();
}
