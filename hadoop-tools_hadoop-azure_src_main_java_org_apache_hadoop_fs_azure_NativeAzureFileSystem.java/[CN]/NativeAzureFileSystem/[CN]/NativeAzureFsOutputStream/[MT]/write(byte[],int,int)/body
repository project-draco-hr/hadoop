{
  try {
    out.write(b,off,len);
  }
 catch (  IOException e) {
    if (e.getCause() instanceof StorageException) {
      StorageException storageExcp=(StorageException)e.getCause();
      LOG.error("Encountered Storage Exception for write on Blob : {}" + " Exception details: {} Error Code : {}",key,e.getMessage(),storageExcp.getErrorCode());
    }
    throw e;
  }
}
