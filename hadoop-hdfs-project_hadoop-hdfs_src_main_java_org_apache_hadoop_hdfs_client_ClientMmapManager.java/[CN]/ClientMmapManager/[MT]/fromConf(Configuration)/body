{
  return new ClientMmapManager(conf.getInt(DFS_CLIENT_MMAP_CACHE_SIZE,DFS_CLIENT_MMAP_CACHE_SIZE_DEFAULT),conf.getLong(DFS_CLIENT_MMAP_CACHE_TIMEOUT_MS,DFS_CLIENT_MMAP_CACHE_TIMEOUT_MS_DEFAULT),conf.getInt(DFS_CLIENT_MMAP_CACHE_THREAD_RUNS_PER_TIMEOUT,DFS_CLIENT_MMAP_CACHE_THREAD_RUNS_PER_TIMEOUT_DEFAULT));
}
