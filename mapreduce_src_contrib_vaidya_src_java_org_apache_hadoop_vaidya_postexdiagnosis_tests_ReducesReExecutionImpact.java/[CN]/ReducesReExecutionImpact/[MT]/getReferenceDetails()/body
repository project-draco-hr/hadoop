{
  String ref="* Total Reduce Tasks: " + this._job.getLongValue(JobKeys.TOTAL_REDUCES) + "\n"+ "* Launched Reduce Tasks: "+ this._job.getLongValue(JobKeys.LAUNCHED_REDUCES)+ "\n"+ "* Percent Reduce Tasks ReExecuted: "+ this._percentReducesReExecuted+ "\n"+ "* Impact: "+ truncate(this._impact);
  return ref;
}
