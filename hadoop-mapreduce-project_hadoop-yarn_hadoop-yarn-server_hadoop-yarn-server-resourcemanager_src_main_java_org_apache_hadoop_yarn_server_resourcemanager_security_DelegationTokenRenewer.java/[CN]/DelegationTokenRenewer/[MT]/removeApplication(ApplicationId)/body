{
synchronized (delegationTokens) {
    Iterator<DelegationTokenToRenew> it=delegationTokens.iterator();
    while (it.hasNext()) {
      DelegationTokenToRenew dttr=it.next();
      if (dttr.applicationId.equals(applicationId)) {
        if (LOG.isDebugEnabled()) {
          LOG.debug("Removing delegation token for appId=" + applicationId + "; token="+ dttr.token.getService());
        }
        if (dttr.timerTask != null)         dttr.timerTask.cancel();
        cancelToken(dttr);
        it.remove();
      }
    }
  }
}
