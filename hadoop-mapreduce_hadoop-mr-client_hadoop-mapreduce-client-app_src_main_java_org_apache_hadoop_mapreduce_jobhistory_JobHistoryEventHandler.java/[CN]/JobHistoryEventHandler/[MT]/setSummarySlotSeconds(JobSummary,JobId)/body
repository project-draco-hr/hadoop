{
  Counter slotMillisMapCounter=context.getJob(jobId).getCounters().getCounter(JobCounter.SLOTS_MILLIS_MAPS);
  if (slotMillisMapCounter != null) {
    summary.setMapSlotSeconds(slotMillisMapCounter.getValue());
  }
  Counter slotMillisReduceCounter=context.getJob(jobId).getCounters().getCounter(JobCounter.SLOTS_MILLIS_REDUCES);
  if (slotMillisReduceCounter != null) {
    summary.setMapSlotSeconds(slotMillisReduceCounter.getValue());
  }
}
