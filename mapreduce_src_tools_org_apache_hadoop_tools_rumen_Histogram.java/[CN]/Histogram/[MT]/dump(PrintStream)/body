{
  stream.print("dumping Histogram " + name + ":\n");
  Iterator<Map.Entry<Long,Long>> iter=iterator();
  while (iter.hasNext()) {
    Map.Entry<Long,Long> ent=iter.next();
    stream.print("val/count pair: " + (long)ent.getKey() + ", "+ (long)ent.getValue()+ "\n");
  }
  stream.print("*** end *** \n");
}
