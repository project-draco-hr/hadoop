{
  int y=1, iIdx, oIdx;
  for (int i=0; i < p.length; i++) {
    ByteBuffer pi=p[i];
    for (iIdx=pi.position(), oIdx=q.position(); iIdx < pi.limit(); iIdx++, oIdx++) {
      int pij=pi.get(iIdx) & 0x000000FF;
      q.put(oIdx,(byte)(q.get(oIdx) ^ mulTable[pij][y]));
    }
    y=mulTable[x][y];
  }
}
