{
  int y=1, iIdx, oIdx;
  for (int i=0; i < p.length; i++) {
    byte[] pi=p[i];
    for (iIdx=offsets[i], oIdx=offset; iIdx < offsets[i] + len; iIdx++, oIdx++) {
      int pij=pi != null ? pi[iIdx] & 0x000000FF : 0;
      q[oIdx]=(byte)(q[oIdx] ^ mulTable[pij][y]);
    }
    y=mulTable[x][y];
  }
}
