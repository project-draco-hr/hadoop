{
  List<String> m=new ArrayList<String>();
synchronized (nameToRackMap) {
    for (    String name : names) {
      String rackId;
      if ((rackId=nameToRackMap.get(name)) != null) {
        m.add(rackId);
      }
 else {
        throw new UnknownHostException(name);
      }
    }
    return m;
  }
}
