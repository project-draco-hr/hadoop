{
  if (bytesPerChecksum <= 0) {
    return null;
  }
switch (type) {
case CHECKSUM_NULL:
    return new DataChecksum(CHECKSUM_NULL,new ChecksumNull(),CHECKSUM_NULL_SIZE,bytesPerChecksum);
case CHECKSUM_CRC32:
  return new DataChecksum(CHECKSUM_CRC32,new PureJavaCrc32(),CHECKSUM_CRC32_SIZE,bytesPerChecksum);
default :
return null;
}
}
