{
  if (blockIds == null) {
    System.err.println("Error: No block ids specified");
  }
 else {
    String valStr=value.toString();
    for (    String blockId : blockIds) {
      if (valStr.indexOf(blockId) != -1) {
        blockIdText.set(blockId);
        valText.set(valStr);
        context.write(blockIdText,valText);
        break;
      }
    }
  }
}
