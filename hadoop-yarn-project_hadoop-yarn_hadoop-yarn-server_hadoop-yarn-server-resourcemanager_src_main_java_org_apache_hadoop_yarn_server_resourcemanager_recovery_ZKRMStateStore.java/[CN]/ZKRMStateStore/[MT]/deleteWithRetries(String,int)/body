{
  new ZKAction<Void>(){
    @Override public Void run() throws KeeperException, InterruptedException {
      if (zkClient.exists(path,true) == null) {
        LOG.error("Trying to delete a path (" + path + ") that doesn't exist.");
      }
      zkClient.delete(path,version);
      return null;
    }
  }
.runWithRetries();
}
