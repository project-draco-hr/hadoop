{
  ArrayList<Op> opList=new ArrayList<Op>();
  String nodeRemovePath=getNodePath(delegationTokensRootPath,DELEGATION_TOKEN_PREFIX + rmDTIdentifier.getSequenceNumber());
  if (LOG.isDebugEnabled()) {
    LOG.debug("Removing RMDelegationToken_" + rmDTIdentifier.getSequenceNumber());
  }
  if (existsWithRetries(nodeRemovePath,false) != null) {
    opList.add(Op.delete(nodeRemovePath,-1));
  }
 else {
    LOG.debug("Attempted to delete a non-existing znode " + nodeRemovePath);
  }
  doMultiWithRetries(opList);
}
