{
  List<ReservationAgent> listAlg=new LinkedList<ReservationAgent>();
  ReservationAgent algAligned=new IterativePlanner(new StageEarliestStartByDemand(),new StageAllocatorLowCostAligned(smoothnessFactor));
  listAlg.add(algAligned);
  ReservationAgent algGreedy=new IterativePlanner(new StageEarliestStartByJobArrival(),new StageAllocatorGreedy());
  listAlg.add(algGreedy);
  planner=new TryManyReservationAgents(listAlg);
}
