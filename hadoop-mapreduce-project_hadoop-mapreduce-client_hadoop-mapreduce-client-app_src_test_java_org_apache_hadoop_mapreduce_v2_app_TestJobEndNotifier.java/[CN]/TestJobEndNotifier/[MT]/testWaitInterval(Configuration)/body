{
  conf.set(MRJobConfig.MR_JOB_END_NOTIFICATION_MAX_RETRY_INTERVAL,"5");
  conf.set(MRJobConfig.MR_JOB_END_RETRY_INTERVAL,"1");
  setConf(conf);
  Assert.assertTrue("Expected waitInterval to be 1, but was " + waitInterval,waitInterval == 1);
  conf.set(MRJobConfig.MR_JOB_END_RETRY_INTERVAL,"10");
  setConf(conf);
  Assert.assertTrue("Expected waitInterval to be 5, but was " + waitInterval,waitInterval == 5);
  conf.set(MRJobConfig.MR_JOB_END_RETRY_INTERVAL,"-10");
  setConf(conf);
  Assert.assertTrue("Expected waitInterval to be 5, but was " + waitInterval,waitInterval == 5);
}
