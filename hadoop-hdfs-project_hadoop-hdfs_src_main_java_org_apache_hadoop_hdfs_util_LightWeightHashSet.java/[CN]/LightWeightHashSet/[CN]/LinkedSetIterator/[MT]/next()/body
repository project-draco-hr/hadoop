{
  if (modification != startModification) {
    throw new ConcurrentModificationException("modification=" + modification + " != startModification = "+ startModification);
  }
  if (next == null) {
    throw new NoSuchElementException();
  }
  final T e=next.element;
  final LinkedElement<T> n=next.next;
  next=n != null ? n : nextNonemptyEntry();
  return e;
}
