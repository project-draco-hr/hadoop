{
  if (current == null) {
    throw new NoSuchElementException();
  }
  if (modification != expectedModification) {
    throw new ConcurrentModificationException("modification=" + modification + " != expectedModification = "+ expectedModification);
  }
  LightWeightHashSet.this.removeElem(current.element);
  current=null;
  expectedModification=modification;
}
