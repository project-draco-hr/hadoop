{
  ExtendedBlock block=rb.getBlock();
  DatanodeInfo[] targets=rb.getLocations();
  LOG.info(who + " calls recoverBlock(" + block+ ", targets=["+ Joiner.on(", ").join(targets)+ "]"+ ((rb.getNewBlock() == null) ? ", newGenerationStamp=" + rb.getNewGenerationStamp() : ", newBlock=" + rb.getNewBlock())+ ")");
}
