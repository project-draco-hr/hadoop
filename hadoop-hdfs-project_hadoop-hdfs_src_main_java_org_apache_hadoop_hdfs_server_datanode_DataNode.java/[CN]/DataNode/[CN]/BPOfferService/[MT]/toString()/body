{
  if (bpNSInfo == null) {
    String storageId=dn.getStorageId();
    if (storageId == null || "".equals(storageId)) {
      storageId="unknown";
    }
    return "Block pool <registering> (storage id " + storageId + ") connecting to "+ nnAddr;
  }
 else {
    return "Block pool " + getBlockPoolId() + " (storage id "+ dn.getStorageId()+ ") registered with "+ nnAddr;
  }
}
