{
  if (n1 == null) {
    return n2;
  }
 else   if (n2 == null) {
    return n1;
  }
  if (n1 instanceof Integer || n1 instanceof Long) {
    return n1.longValue() + n2.longValue();
  }
  if (n1 instanceof Float || n1 instanceof Double) {
    return n1.doubleValue() + n2.doubleValue();
  }
  return null;
}
