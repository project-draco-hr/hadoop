{
  List<Socket> socklist=multimap.get(remote);
  if (socklist == null) {
    return null;
  }
  Iterator<Socket> iter=socklist.iterator();
  while (iter.hasNext()) {
    Socket candidate=iter.next();
    iter.remove();
    if (!candidate.isClosed()) {
      return candidate;
    }
  }
  return null;
}
