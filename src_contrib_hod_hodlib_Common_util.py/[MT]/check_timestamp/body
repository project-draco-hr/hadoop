def check_timestamp(timeStamp):
    ' Checks the validity of a timeStamp.\n\n        timeStamp - (YYYY-MM-DD HH:MM:SS in UTC)\n\n        returns True or False\n    '
    isValid = True
    try:
        timeStruct = time.strptime(timeStamp, '%Y-%m-%d %H:%M:%S')
    except:
        isValid = False
    return isValid
