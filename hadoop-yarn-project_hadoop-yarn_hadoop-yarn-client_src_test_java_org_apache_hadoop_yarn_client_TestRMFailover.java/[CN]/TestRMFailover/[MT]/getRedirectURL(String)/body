{
  String redirectUrl=null;
  try {
    HttpURLConnection conn=(HttpURLConnection)new URL(url).openConnection();
    conn.setInstanceFollowRedirects(false);
    if (conn.getResponseCode() == HttpServletResponse.SC_TEMPORARY_REDIRECT)     redirectUrl=conn.getHeaderField("Location");
  }
 catch (  Exception e) {
  }
  return redirectUrl;
}
