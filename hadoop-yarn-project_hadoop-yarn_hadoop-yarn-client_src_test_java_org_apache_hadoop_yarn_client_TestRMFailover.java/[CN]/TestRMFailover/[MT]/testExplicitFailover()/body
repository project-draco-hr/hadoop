{
  conf.setBoolean(YarnConfiguration.AUTO_FAILOVER_ENABLED,false);
  cluster.init(conf);
  cluster.start();
  getAdminService(0).transitionToActive(req);
  assertFalse("RM never turned active",-1 == cluster.getActiveRMIndex());
  verifyConnections();
  explicitFailover();
  verifyConnections();
  explicitFailover();
  verifyConnections();
}
