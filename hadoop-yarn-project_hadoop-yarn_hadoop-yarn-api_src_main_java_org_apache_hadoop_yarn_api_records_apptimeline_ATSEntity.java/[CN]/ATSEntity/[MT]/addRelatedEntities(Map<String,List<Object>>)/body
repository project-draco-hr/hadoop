{
  for (  Map.Entry<String,List<Object>> relatedEntity : relatedEntities.entrySet()) {
    List<Object> thisRelatedEntity=this.relatedEntities.get(relatedEntity.getKey());
    if (thisRelatedEntity == null) {
      this.relatedEntities.put(relatedEntity.getKey(),relatedEntity.getValue());
    }
 else {
      thisRelatedEntity.addAll(relatedEntity.getValue());
    }
  }
}
