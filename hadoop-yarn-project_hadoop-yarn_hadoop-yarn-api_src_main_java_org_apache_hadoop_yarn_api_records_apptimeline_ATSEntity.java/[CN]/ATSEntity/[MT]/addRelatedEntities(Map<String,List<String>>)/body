{
  for (  Entry<String,List<String>> relatedEntity : relatedEntities.entrySet()) {
    List<String> thisRelatedEntity=this.relatedEntities.get(relatedEntity.getKey());
    if (thisRelatedEntity == null) {
      this.relatedEntities.put(relatedEntity.getKey(),relatedEntity.getValue());
    }
 else {
      thisRelatedEntity.addAll(relatedEntity.getValue());
    }
  }
}
