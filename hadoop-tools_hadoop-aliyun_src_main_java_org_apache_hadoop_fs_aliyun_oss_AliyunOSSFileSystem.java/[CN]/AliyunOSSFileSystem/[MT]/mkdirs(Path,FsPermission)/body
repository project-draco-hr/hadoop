{
  try {
    FileStatus fileStatus=getFileStatus(path);
    if (fileStatus.isDirectory()) {
      return true;
    }
 else {
      throw new FileAlreadyExistsException("Path is a file: " + path);
    }
  }
 catch (  FileNotFoundException e) {
    validatePath(path);
    String key=pathToKey(path);
    return mkdir(key);
  }
}
