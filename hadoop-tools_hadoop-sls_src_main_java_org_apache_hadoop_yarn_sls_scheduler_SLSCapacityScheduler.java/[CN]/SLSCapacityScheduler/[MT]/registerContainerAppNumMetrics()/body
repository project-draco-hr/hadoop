{
  metrics.register("variable.running.application",new Gauge<Integer>(){
    @Override public Integer getValue(){
      if (getRootQueueMetrics() == null) {
        return 0;
      }
 else {
        return getRootQueueMetrics().getAppsRunning();
      }
    }
  }
);
  metrics.register("variable.running.container",new Gauge<Integer>(){
    @Override public Integer getValue(){
      if (getRootQueueMetrics() == null) {
        return 0;
      }
 else {
        return getRootQueueMetrics().getAllocatedContainers();
      }
    }
  }
);
}
