{
  return new String[]{"-input",inputPath.toString(),"-output",outputPath.toString(),"-mapper",map,"-reducer",StreamJob.REDUCE_NONE,"-jobconf","mapred.job.tracker=" + "localhost:" + mr.getJobTrackerPort(),"-jobconf","fs.default.name=" + fs.getUri().toString(),"-jobconf","mapred.map.tasks=1","-jobconf","keep.failed.task.files=true","-jobconf","mapreduce.task.userlog.limit.kb=" + USERLOG_LIMIT_KB,"-jobconf","stream.tmpdir=" + System.getProperty("test.build.data","/tmp")};
