{
  return new ContainerLauncherImpl(context){
    @Override protected ContainerManager getCMProxy(    ContainerId containerID,    String containerManagerBindAddr,    ContainerToken containerToken) throws IOException {
      try {
synchronized (this) {
          wait();
        }
      }
 catch (      InterruptedException e) {
        LOG.info(e);
        if (!MRAppWithSlowNM.this.swallowInterrupts) {
          throw new IOException(e);
        }
        Thread.currentThread().interrupt();
      }
      return null;
    }
  }
;
}
