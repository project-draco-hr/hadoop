{
  int timeOut=20;
  while (expectedNumEvents != containerLauncher.numEventsProcessed || timeOut++ < 20) {
    LOG.info("Waiting for number of events to become " + expectedNumEvents + ". It is now "+ containerLauncher.numEventsProcessed);
    Thread.sleep(1000);
  }
  Assert.assertEquals(expectedNumEvents,containerLauncher.numEventsProcessed);
}
