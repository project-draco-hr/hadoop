{
  if (level == 0) {
    return;
  }
  parent.leftChild=new Node(new Path(parent.nodePath,"leftChild"),height - level,parent,fs);
  parent.rightChild=new Node(new Path(parent.nodePath,"rightChild"),height - level,parent,fs);
  addDirNode(parent.leftChild,parent.leftChild.level);
  addDirNode(parent.rightChild,parent.rightChild.level);
  genChildren(parent.leftChild,level - 1);
  genChildren(parent.rightChild,level - 1);
}
