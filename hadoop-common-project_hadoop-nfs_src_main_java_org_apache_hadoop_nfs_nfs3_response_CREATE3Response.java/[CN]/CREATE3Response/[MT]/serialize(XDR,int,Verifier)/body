{
  super.serialize(out,xid,verifier);
  if (getStatus() == Nfs3Status.NFS3_OK) {
    out.writeBoolean(true);
    objHandle.serialize(out);
    out.writeBoolean(true);
    postOpObjAttr.serialize(out);
  }
  if (dirWcc == null) {
    dirWcc=new WccData(null,null);
  }
  dirWcc.serialize(out);
  return out;
}
