{
  if (chunk.useByteBuffer()) {
    ByteBufferStrategy strategy=new ByteBufferStrategy(chunk.getByteBuffer(),dfsStripedInputStream.getReadStatistics(),dfsStripedInputStream.getDFSClient());
    return new ByteBufferStrategy[]{strategy};
  }
  ByteBufferStrategy[] strategies=new ByteBufferStrategy[chunk.getChunkBuffer().getSlices().size()];
  for (int i=0; i < strategies.length; i++) {
    ByteBuffer buffer=chunk.getChunkBuffer().getSlice(i);
    strategies[i]=new ByteBufferStrategy(buffer,dfsStripedInputStream.getReadStatistics(),dfsStripedInputStream.getDFSClient());
  }
  return strategies;
}
