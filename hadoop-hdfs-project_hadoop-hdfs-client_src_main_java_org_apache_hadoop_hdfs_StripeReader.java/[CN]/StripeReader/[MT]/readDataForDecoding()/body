{
  prepareDecodeInputs();
  for (int i=0; i < dataBlkNum; i++) {
    Preconditions.checkNotNull(alignedStripe.chunks[i]);
    if (alignedStripe.chunks[i].state == StripingChunk.REQUESTED) {
      if (!readChunk(targetBlocks[i],i)) {
        alignedStripe.missingChunksNum++;
      }
    }
  }
  checkMissingBlocks();
}
