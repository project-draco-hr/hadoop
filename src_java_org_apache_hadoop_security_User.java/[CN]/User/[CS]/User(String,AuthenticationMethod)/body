{
  fullName=name;
  int atIdx=name.indexOf('@');
  if (atIdx == -1) {
    shortName=name;
  }
 else {
    int slashIdx=name.indexOf('/');
    if (slashIdx == -1 || atIdx < slashIdx) {
      shortName=name.substring(0,atIdx);
    }
 else {
      shortName=name.substring(0,slashIdx);
    }
  }
  this.authMethod=authMethod;
}
