{
  Container container=rmContainer.getContainer();
  deductAvailableResource(container.getResource());
  ++numContainers;
  launchedContainers.put(container.getId(),rmContainer);
  LOG.info("Assigned container " + container.getId() + " of capacity "+ container.getResource()+ " on host "+ rmNode.getNodeAddress()+ ", which currently has "+ numContainers+ " containers, "+ getUsedResource()+ " used and "+ getAvailableResource()+ " available");
}
