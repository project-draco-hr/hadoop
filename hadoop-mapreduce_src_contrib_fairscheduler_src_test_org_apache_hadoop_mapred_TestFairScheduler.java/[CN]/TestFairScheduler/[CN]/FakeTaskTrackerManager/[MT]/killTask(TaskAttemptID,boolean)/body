{
  String attemptIdStr=attemptId.toString();
  FakeTaskInProgress tip=tips.get(attemptIdStr);
  if (tip.isMapTask()) {
    maps--;
  }
 else {
    reduces--;
  }
  tip.killAttempt();
  TaskStatus status=statuses.get(attemptIdStr);
  trackerForTip.get(attemptIdStr).getTaskReports().remove(status);
  return true;
}
