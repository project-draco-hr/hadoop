{
  assertEquals(sched.getRunningTasks(),metrics.getMetric("runningTasks"));
  assertEquals(sched.getDemand(),metrics.getMetric("demand"));
  assertEquals(sched.getFairShare(),metrics.getMetric("fairShare").doubleValue(),.001);
  assertEquals(sched.getWeight(),metrics.getMetric("weight").doubleValue(),.001);
}
