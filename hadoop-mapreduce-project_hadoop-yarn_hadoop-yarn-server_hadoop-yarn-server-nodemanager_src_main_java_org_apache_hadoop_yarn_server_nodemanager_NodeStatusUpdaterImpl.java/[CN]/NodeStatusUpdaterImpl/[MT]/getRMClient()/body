{
  Configuration conf=getConfig();
  YarnRPC rpc=YarnRPC.create(conf);
  InetSocketAddress rmAddress=NetUtils.createSocketAddr(this.rmAddress,YarnConfiguration.DEFAULT_RM_RESOURCE_TRACKER_PORT,YarnConfiguration.RM_RESOURCE_TRACKER_ADDRESS);
  return (ResourceTracker)rpc.getProxy(ResourceTracker.class,rmAddress,conf);
}
