{
  this.nodeId=this.context.getNodeId();
  String httpBindAddressStr=getConfig().get(YarnConfiguration.NM_WEBAPP_ADDRESS,YarnConfiguration.DEFAULT_NM_WEBAPP_ADDRESS);
  InetSocketAddress httpBindAddress=NetUtils.createSocketAddr(httpBindAddressStr);
  try {
    this.httpPort=httpBindAddress.getPort();
    registerWithRM();
    super.start();
    startStatusUpdater();
  }
 catch (  Exception e) {
    throw new AvroRuntimeException(e);
  }
}
