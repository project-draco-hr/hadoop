{
  StringBuilder sb=new StringBuilder();
  boolean first=true;
  for (  Map.Entry<K,? extends Message> e : map.entrySet()) {
    if (!first) {
      sb.append("\n");
    }
    first=false;
    sb.append(e.getKey()).append(": ").append(TextFormat.shortDebugString(e.getValue()));
  }
  return sb.toString();
}
