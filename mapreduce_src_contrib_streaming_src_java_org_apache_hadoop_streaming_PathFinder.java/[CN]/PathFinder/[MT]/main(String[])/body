{
  if (args.length < 1) {
    System.out.println("Usage: java PathFinder <filename>");
    System.exit(1);
  }
  PathFinder finder=new PathFinder("PATH");
  File file=finder.getAbsolutePath(args[0]);
  if (file != null) {
    System.out.println("Full path name = " + file.getCanonicalPath());
  }
}
