{
  byte[][] rowKeyComponents=Separator.QUALIFIERS.split(rowKey);
  if (rowKeyComponents.length < 4) {
    throw new IllegalArgumentException("the row key is not valid for " + "a flow run");
  }
  String clusterId=Separator.QUALIFIERS.decode(Bytes.toString(rowKeyComponents[0]));
  String userId=Separator.QUALIFIERS.decode(Bytes.toString(rowKeyComponents[1]));
  String flowId=Separator.QUALIFIERS.decode(Bytes.toString(rowKeyComponents[2]));
  long flowRunId=TimelineStorageUtils.invertLong(Bytes.toLong(rowKeyComponents[3]));
  return new FlowRunRowKey(clusterId,userId,flowId,flowRunId);
}
