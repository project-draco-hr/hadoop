{
  if (pathInfo == null || pathInfo.length() == 0) {
    LOG.info("Can't get file path from the request");
    return false;
  }
  for (  Path ldapPathVar : ldapPaths) {
    String ldapPath=ldapPathVar.toString();
    if (isPathQualified(ldapPath) && isPathAuthroized(ldapPath)) {
      String allowedPath=extractPath(ldapPath);
      if (pathInfo.startsWith(allowedPath))       return true;
    }
 else {
      if (pathInfo.startsWith(ldapPath))       return true;
    }
  }
  return false;
}
