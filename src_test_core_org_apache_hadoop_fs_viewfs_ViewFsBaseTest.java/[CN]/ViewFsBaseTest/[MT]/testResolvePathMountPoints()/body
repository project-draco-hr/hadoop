{
  Assert.assertEquals(new Path(targetTestRoot,"user"),fcView.getDefaultFileSystem().resolvePath(new Path("/user")));
  Assert.assertEquals(new Path(targetTestRoot,"data"),fcView.getDefaultFileSystem().resolvePath(new Path("/data")));
  Assert.assertEquals(new Path(targetTestRoot,"dir2"),fcView.getDefaultFileSystem().resolvePath(new Path("/internalDir/linkToDir2")));
  Assert.assertEquals(new Path(targetTestRoot,"dir3"),fcView.getDefaultFileSystem().resolvePath(new Path("/internalDir/internalDir2/linkToDir3")));
}
