{
  for (Iterator<StorageDirectory> it=fsimage.getStorage().dirIterator(NameNodeDirType.EDITS); it.hasNext(); ) {
    File editFile=fsimage.getStorage().getStorageFile(it.next(),NameNodeFile.EDITS);
    System.out.println("Verifying file: " + editFile);
    int numEdits=new FSEditLogLoader(namesystem).loadFSEdits(new EditLogFileInputStream(editFile));
    System.out.println("Number of edits: " + numEdits);
  }
}
