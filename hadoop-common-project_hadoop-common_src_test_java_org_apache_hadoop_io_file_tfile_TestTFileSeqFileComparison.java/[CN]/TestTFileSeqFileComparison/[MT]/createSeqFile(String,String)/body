{
  System.out.println("=== SeqFile: Creation (" + parameters + ") === ");
  Path path=new Path(options.rootDir,"SeqFile.Performance");
  KVAppendable appendable=new SeqFileAppendable(fs,path,options.osOutputBufferSize,compress,options.minBlockSize);
