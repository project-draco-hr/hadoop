{
  System.out.println("=== TFile: Reading (" + parameters + ") === ");
{
    Path path=new Path(options.rootDir,"TFile.Performance");
    KVReadable readable=new TFileReadable(fs,path,options.osInputBufferSize,conf);
    timeRead(path,readable);
    if (delFile) {
      if (fs.exists(path)) {
        fs.delete(path,true);
      }
    }
  }
}
