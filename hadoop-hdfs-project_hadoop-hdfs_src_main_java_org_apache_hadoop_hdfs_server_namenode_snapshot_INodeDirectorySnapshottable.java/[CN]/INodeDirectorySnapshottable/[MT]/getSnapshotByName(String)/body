{
  Snapshot s=null;
  if (snapshotName != null && !snapshotName.isEmpty()) {
    final int index=searchSnapshot(DFSUtil.string2Bytes(snapshotName));
    if (index < 0) {
      throw new SnapshotException("Cannot find the snapshot of directory " + this.getFullPathName() + " with name "+ snapshotName);
    }
    s=snapshotsByNames.get(index);
  }
  return s;
}
