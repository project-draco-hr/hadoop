{
  List<DiffReportEntry> diffList=new ArrayList<DiffReportEntry>();
  for (  Map.Entry<INodeDirectoryWithSnapshot,ChildrenDiff> entry : diffMap.entrySet()) {
    diffList.add(new DiffReportEntry(DiffType.MODIFY,entry.getKey().getFullPathName()));
    List<DiffReportEntry> subList=entry.getValue().generateReport(entry.getKey(),isFromEarlier());
    diffList.addAll(subList);
  }
  return new SnapshotDiffReport(snapshotRoot.getFullPathName(),getSnapshotName(from),getSnapshotName(to),diffList);
}
