{
  return new ReconfigurableBase(oldConf){
    @Override protected Configuration getNewConf(){
      return newConf;
    }
    @Override public Collection<String> getReconfigurableProperties(){
      return reconfigurableProperties;
    }
    @Override protected String reconfigurePropertyImpl(    String property,    String newVal) throws ReconfigurationException {
      return newVal;
    }
  }
;
}
