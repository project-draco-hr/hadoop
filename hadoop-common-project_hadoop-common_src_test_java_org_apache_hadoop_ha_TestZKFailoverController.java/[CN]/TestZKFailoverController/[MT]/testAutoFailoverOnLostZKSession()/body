{
  try {
    cluster.start();
    cluster.expireAndVerifyFailover(0,1);
    cluster.expireAndVerifyFailover(1,0);
    LOG.info("======= Running test cases second time to test " + "re-establishment =========");
    cluster.expireAndVerifyFailover(0,1);
    cluster.expireAndVerifyFailover(1,0);
  }
  finally {
    cluster.stop();
  }
}
