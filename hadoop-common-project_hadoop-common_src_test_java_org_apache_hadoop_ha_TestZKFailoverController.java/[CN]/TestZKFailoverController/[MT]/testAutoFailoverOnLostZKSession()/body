{
  try {
    setupFCs();
    expireAndVerifyFailover(thr1,thr2);
    expireAndVerifyFailover(thr2,thr1);
    LOG.info("======= Running test cases second time to test " + "re-establishment =========");
    expireAndVerifyFailover(thr1,thr2);
    expireAndVerifyFailover(thr2,thr1);
  }
  finally {
    stopFCs();
  }
}
