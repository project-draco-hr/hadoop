    jobs=MockHistoryJobs.newHistoryJobs(appID,numJobs,numTasks,numAttempts);
  }
 catch (  IOException e) {
    throw new YarnException(e);
  }
  partialJobs=jobs.partial;
  fullJobs=jobs.full;
}
