{
  fakeProgress.setProgress(progress);
  cpuPlugin.emulate();
  assertEquals("Emulation interval test for cpu usage failed " + info + "!",expectedTotalCpuUsage,fakeCore.getCpuUsage(),0L);
  assertEquals("Emulation interval test for num calls failed " + info + "!",expectedTotalNumCalls,fakeCore.getNumCalls(),0L);
}
