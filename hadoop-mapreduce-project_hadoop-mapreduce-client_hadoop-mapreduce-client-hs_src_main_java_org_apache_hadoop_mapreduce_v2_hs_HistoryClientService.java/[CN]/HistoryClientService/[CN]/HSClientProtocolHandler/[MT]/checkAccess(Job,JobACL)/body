{
  UserGroupInformation callerUGI;
  try {
    callerUGI=UserGroupInformation.getCurrentUser();
  }
 catch (  IOException e) {
    throw RPCUtil.getRemoteException(e);
  }
  if (!job.checkAccess(callerUGI,jobOperation)) {
    throw RPCUtil.getRemoteException(new AccessControlException("User " + callerUGI.getShortUserName() + " cannot perform operation "+ jobOperation.name()+ " on "+ job.getID()));
  }
}
