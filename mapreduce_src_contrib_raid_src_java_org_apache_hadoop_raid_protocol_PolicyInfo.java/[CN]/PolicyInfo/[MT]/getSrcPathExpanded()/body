{
  FileSystem fs=srcPath.getFileSystem(conf);
  FileStatus[] gpaths=fs.globStatus(srcPath);
  if (gpaths == null) {
    return null;
  }
  Path[] values=new Path[gpaths.length];
  for (int i=0; i < gpaths.length; i++) {
    Path p=gpaths[i].getPath();
    values[i]=p.makeQualified(fs);
  }
  return values;
}
