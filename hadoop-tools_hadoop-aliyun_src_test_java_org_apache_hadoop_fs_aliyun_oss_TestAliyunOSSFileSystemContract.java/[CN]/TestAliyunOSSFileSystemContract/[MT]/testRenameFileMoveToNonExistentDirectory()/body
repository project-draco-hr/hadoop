{
  if (this.renameSupported()) {
    Path src=this.path("/test/hadoop/file");
    this.createFile(src);
    Path dst=this.path("/test/new/newfile");
    try {
      super.rename(src,dst,false,true,false);
      fail("Should throw FileNotFoundException!");
    }
 catch (    FileNotFoundException e) {
    }
  }
}
