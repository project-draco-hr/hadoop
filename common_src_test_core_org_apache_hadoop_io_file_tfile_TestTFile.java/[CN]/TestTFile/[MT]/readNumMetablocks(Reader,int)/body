{
  int len=("something to test" + 0).getBytes().length;
  for (int i=0; i < n; i++) {
    DataInputStream din=reader.getMetaBlock("TfileMeta" + i);
    byte b[]=new byte[len];
    din.readFully(b);
    assertTrue("faield to match metadata",Arrays.equals(("something to test" + i).getBytes(),b));
    din.close();
  }
}
