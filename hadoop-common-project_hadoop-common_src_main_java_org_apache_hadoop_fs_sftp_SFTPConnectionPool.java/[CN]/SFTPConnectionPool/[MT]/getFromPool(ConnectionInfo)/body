{
  Set<ChannelSftp> cons=idleConnections.get(info);
  ChannelSftp channel;
  if (cons != null && cons.size() > 0) {
    Iterator<ChannelSftp> it=cons.iterator();
    if (it.hasNext()) {
      channel=it.next();
      idleConnections.remove(info);
      return channel;
    }
 else {
      throw new IOException("Connection pool error.");
    }
  }
  return null;
}
