{
  super(getApplicationAttemptId(applicationId,startCount),getContainerId(applicationId,startCount),"testhost",3333,System.currentTimeMillis());
  this.testWorkDir=new File("target",testName);
  testAbsPath=new Path(testWorkDir.getAbsolutePath());
  LOG.info("PathUsed: " + testAbsPath);
  if (cleanOnStart) {
    testAbsPath=new Path(testWorkDir.getAbsolutePath());
    try {
      FileContext.getLocalFSFileContext().delete(testAbsPath,true);
    }
 catch (    Exception e) {
      LOG.warn("COULD NOT CLEANUP: " + testAbsPath,e);
      throw new YarnException("could not cleanup test dir",e);
    }
  }
  this.maps=maps;
  this.reduces=reduces;
  this.autoComplete=autoComplete;
}
