{
  String[] dirs={"dir"};
  DirectoryCollection dc=new DirectoryCollection(dirs,0.0F,0.0F,100);
  float testValue=57.5F;
  float delta=0.1F;
  dc.setDiskUtilizationPercentageCutoff(testValue,50.0F);
  Assert.assertEquals(testValue,dc.getDiskUtilizationPercentageCutoffHigh(),delta);
  Assert.assertEquals(50.0F,dc.getDiskUtilizationPercentageCutoffLow(),delta);
  testValue=-57.5F;
  dc.setDiskUtilizationPercentageCutoff(testValue,testValue);
  Assert.assertEquals(0.0F,dc.getDiskUtilizationPercentageCutoffHigh(),delta);
  Assert.assertEquals(0.0F,dc.getDiskUtilizationPercentageCutoffLow(),delta);
  testValue=157.5F;
  dc.setDiskUtilizationPercentageCutoff(testValue,testValue);
  Assert.assertEquals(100.0F,dc.getDiskUtilizationPercentageCutoffHigh(),delta);
  Assert.assertEquals(100.0F,dc.getDiskUtilizationPercentageCutoffLow(),delta);
  long spaceValue=57;
  dc.setDiskUtilizationSpaceCutoff(spaceValue);
  Assert.assertEquals(spaceValue,dc.getDiskUtilizationSpaceCutoff());
  spaceValue=-57;
  dc.setDiskUtilizationSpaceCutoff(spaceValue);
  Assert.assertEquals(0,dc.getDiskUtilizationSpaceCutoff());
}
