{
  String oldStyleUgi=conf.get(DEPRECATED_UGI_KEY);
  if (oldStyleUgi != null) {
    LOG.warn(DEPRECATED_UGI_KEY + " should not be used. Instead, use " + USERNAME_KEY+ ".");
    String[] parts=oldStyleUgi.split(",");
    return parts[0];
  }
 else {
    return conf.get(USERNAME_KEY,USERNAME_DEFAULT);
  }
}
