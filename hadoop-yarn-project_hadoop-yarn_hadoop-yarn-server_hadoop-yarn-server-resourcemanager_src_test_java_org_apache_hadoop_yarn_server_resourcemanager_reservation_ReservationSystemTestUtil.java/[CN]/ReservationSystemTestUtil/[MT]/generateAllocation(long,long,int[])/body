{
  Map<ReservationInterval,ReservationRequest> req=new TreeMap<ReservationInterval,ReservationRequest>();
  for (int i=0; i < alloc.length; i++) {
    req.put(new ReservationInterval(startTime + i * step,startTime + (i + 1) * step),ReservationRequest.newInstance(Resource.newInstance(1024,1),alloc[i]));
  }
  return req;
}
