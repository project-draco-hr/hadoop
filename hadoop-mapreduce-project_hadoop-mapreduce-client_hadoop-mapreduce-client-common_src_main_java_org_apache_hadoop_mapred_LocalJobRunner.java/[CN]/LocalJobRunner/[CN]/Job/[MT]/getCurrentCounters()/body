{
  if (null == mapCounters) {
    return new Counters();
  }
  Counters current=new Counters();
  for (  Counters c : mapCounters) {
    current=Counters.sum(current,c);
  }
  if (null != reduceCounters && reduceCounters.length > 0) {
    for (    Counters c : reduceCounters) {
      current=Counters.sum(current,c);
    }
  }
  return current;
}
