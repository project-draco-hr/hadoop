{
  if (data_fail != null) {
    FileUtil.setWritable(data_fail,true);
  }
  if (failedDir != null) {
    FileUtil.setWritable(failedDir,true);
  }
  if (cluster != null) {
    cluster.shutdown();
  }
  for (int i=0; i < 3; i++) {
    FileUtil.setExecutable(new File(dataDir,"data" + (2 * i + 1)),true);
    FileUtil.setExecutable(new File(dataDir,"data" + (2 * i + 2)),true);
  }
}
