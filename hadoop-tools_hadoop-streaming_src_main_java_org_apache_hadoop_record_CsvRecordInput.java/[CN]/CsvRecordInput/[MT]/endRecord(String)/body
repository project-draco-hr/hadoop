{
  char c=(char)stream.read();
  if (tag == null || tag.isEmpty()) {
    if (c != '\n' && c != '\r') {
      throw new IOException("Error deserializing record.");
    }
 else {
      return;
    }
  }
  if (c != '}') {
    throw new IOException("Error deserializing " + tag);
  }
  c=(char)stream.read();
  if (c != ',') {
    stream.unread(c);
  }
  return;
}
