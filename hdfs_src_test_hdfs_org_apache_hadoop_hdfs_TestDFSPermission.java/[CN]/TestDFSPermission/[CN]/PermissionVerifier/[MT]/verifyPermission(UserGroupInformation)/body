{
  if (this.ugi != ugi) {
    setRequiredPermissions(ugi);
    this.ugi=ugi;
  }
  try {
    try {
      call();
      assertFalse(expectPermissionDeny());
    }
 catch (    AccessControlException e) {
      assertTrue(expectPermissionDeny());
    }
  }
 catch (  AssertionFailedError ae) {
    logPermissions();
    throw ae;
  }
}
