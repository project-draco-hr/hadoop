{
  if (primaryFilter == null) {
    return getEntityByTime(EMPTY_BYTES,entityType,limit,windowStart,windowEnd,fromId,fromTs,secondaryFilters,fields,checkAcl,false);
  }
 else {
    byte[] base=KeyBuilder.newInstance().add(primaryFilter.getName()).add(fstConf.asByteArray(primaryFilter.getValue()),true).getBytesForLookup();
    return getEntityByTime(base,entityType,limit,windowStart,windowEnd,fromId,fromTs,secondaryFilters,fields,checkAcl,true);
  }
}
