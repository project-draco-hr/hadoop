{
  long timeout;
  try {
    timeout=Long.parseLong(strTimeout);
  }
 catch (  NumberFormatException ex) {
    throw new IllegalArgumentException(INVALID_TIMEOUT_ERR_MSG + strTimeout);
  }
  if (timeout < -1) {
    throw new IllegalArgumentException(INVALID_TIMEOUT_ERR_MSG + timeout);
  }
  return timeout;
}
