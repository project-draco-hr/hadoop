{
  Set<String> labels=null;
  if (directlyAccessNodeLabelStore) {
    labels=getNodeLabelManagerInstance(getConf()).getClusterNodeLabels();
  }
 else {
    ResourceManagerAdministrationProtocol adminProto=createAdminProtocol();
    labels=adminProto.getClusterNodeLabels(GetClusterNodeLabelsRequest.newInstance()).getNodeLabels();
  }
  System.out.println(String.format("Node-labels=%s",StringUtils.join(sortStrSet(labels).iterator(),",")));
  return 0;
}
