{
  File timeFile=new File(currDir,NameNodeFile.TIME.getName());
  long timeStamp=0L;
  if (timeFile.exists() && timeFile.canRead()) {
    DataInputStream in=new DataInputStream(new FileInputStream(timeFile));
    try {
      timeStamp=in.readLong();
    }
  finally {
      in.close();
    }
  }
  return timeStamp;
}
