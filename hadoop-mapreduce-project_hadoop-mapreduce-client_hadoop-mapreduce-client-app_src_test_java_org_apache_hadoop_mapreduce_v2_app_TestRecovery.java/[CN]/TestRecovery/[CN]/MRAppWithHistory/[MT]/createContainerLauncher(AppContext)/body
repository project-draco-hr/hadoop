{
  MockContainerLauncher launcher=new MockContainerLauncher(){
    @Override public void handle(    ContainerLauncherEvent event){
      TaskAttemptId taskAttemptID=event.getTaskAttemptID();
      if (taskAttemptID.getId() != 1 || taskAttemptID.getTaskId().getId() != 0) {
        super.handle(event);
      }
    }
  }
;
  launcher.shufflePort=5467;
  return launcher;
}
