{
  StringBuilder builder=new StringBuilder();
  for (int i=0; i < numRows + 1; i++) {
    String prefix="";
    for (int j=0; j < columns.length; j++) {
      builder.append(prefix);
      prefix=" ";
      builder.append(columns[j].getRow(i));
    }
    builder.append("\n");
  }
  return builder.toString();
}
