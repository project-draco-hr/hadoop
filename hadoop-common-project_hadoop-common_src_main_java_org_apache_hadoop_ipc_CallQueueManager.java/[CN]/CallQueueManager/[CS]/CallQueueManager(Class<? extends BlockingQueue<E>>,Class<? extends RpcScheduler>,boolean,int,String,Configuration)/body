{
  int priorityLevels=parseNumLevels(namespace,conf);
  this.scheduler=createScheduler(schedulerClass,priorityLevels,namespace,conf);
  BlockingQueue<E> bq=createCallQueueInstance(backingClass,priorityLevels,maxQueueSize,namespace,conf);
  this.clientBackOffEnabled=clientBackOffEnabled;
  this.putRef=new AtomicReference<BlockingQueue<E>>(bq);
  this.takeRef=new AtomicReference<BlockingQueue<E>>(bq);
  LOG.info("Using callQueue: " + backingClass + " queueCapacity: "+ maxQueueSize+ " scheduler: "+ schedulerClass);
}
