{
  if (capacity <= 0) {
    return null;
  }
  List<SocketAndStreams> socklist=multimap.get(remote);
  if (socklist == null) {
    return null;
  }
  Iterator<SocketAndStreams> iter=socklist.iterator();
  while (iter.hasNext()) {
    SocketAndStreams candidate=iter.next();
    iter.remove();
    if (!candidate.sock.isClosed()) {
      return candidate;
    }
  }
  return null;
}
