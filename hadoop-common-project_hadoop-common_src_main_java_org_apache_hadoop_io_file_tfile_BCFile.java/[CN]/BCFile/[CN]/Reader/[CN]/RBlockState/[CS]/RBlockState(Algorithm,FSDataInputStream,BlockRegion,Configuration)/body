{
  this.compressAlgo=compressionAlgo;
  this.region=region;
  this.decompressor=compressionAlgo.getDecompressor();
  try {
    this.in=compressAlgo.createDecompressionStream(new BoundedRangeFileInputStream(fsin,this.region.getOffset(),this.region.getCompressedSize()),decompressor,TFile.getFSInputBufferSize(conf));
  }
 catch (  IOException e) {
    compressAlgo.returnDecompressor(decompressor);
    throw e;
  }
}
