{
  this.user=user;
  this.recordFactory=RecordFactoryProvider.getRecordFactory(conf);
  this.id=BuilderUtils.newContainerId(recordFactory,appId,appAttemptId,uniqId);
  this.launchContext=recordFactory.newRecordInstance(ContainerLaunchContext.class);
  ContainerToken containerToken=BuilderUtils.newContainerToken(id,"127.0.0.1",1234,user,BuilderUtils.newResource(1024,1),System.currentTimeMillis() + 10000,123,"password".getBytes());
  this.state=ContainerState.NEW;
  mockContainer=mock(org.apache.hadoop.yarn.api.records.Container.class);
  when(mockContainer.getContainerToken()).thenReturn(containerToken);
  when(mockContainer.getId()).thenReturn(id);
}
