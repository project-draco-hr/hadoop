{
synchronized (jobsMap) {
    if (jobsMap.containsKey(jobName)) {
      List<DistRaid> list=jobsMap.get(jobName);
synchronized (list) {
        for (Iterator<DistRaid> it=list.iterator(); it.hasNext(); ) {
          DistRaid val=it.next();
          if (val == job) {
            it.remove();
          }
        }
        if (list.size() == 0) {
          jobsMap.remove(jobName);
        }
      }
    }
  }
}
