{
  String id=super.getId();
  if (id == null) {
    StringBuilder sb=new StringBuilder();
    sb.append(getCluster());
    sb.append('/');
    sb.append(getDate().getTime());
    sb.append('/');
    sb.append(getUser());
    sb.append('@');
    sb.append(getFlowName());
    id=sb.toString();
    setId(id);
  }
  return id;
}
