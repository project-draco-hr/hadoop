{
  try {
    NameNode.initializeGenericKeys(conf,DFSUtil.getSecondaryNameServiceId(conf));
    initialize(conf,commandLineOpts);
  }
 catch (  IOException e) {
    shutdown();
    LOG.fatal("Failed to start secondary namenode. ",e);
    throw e;
  }
catch (  HadoopIllegalArgumentException e) {
    shutdown();
    LOG.fatal("Failed to start secondary namenode. ",e);
    throw e;
  }
}
