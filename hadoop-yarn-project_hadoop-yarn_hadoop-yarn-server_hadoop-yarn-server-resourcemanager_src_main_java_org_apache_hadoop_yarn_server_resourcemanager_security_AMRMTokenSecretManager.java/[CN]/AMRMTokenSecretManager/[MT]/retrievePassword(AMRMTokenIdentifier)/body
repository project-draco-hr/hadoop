{
  ApplicationAttemptId applicationAttemptId=identifier.getApplicationAttemptId();
  if (LOG.isDebugEnabled()) {
    LOG.debug("Trying to retrieve password for " + applicationAttemptId);
  }
  byte[] password=this.passwords.get(applicationAttemptId);
  if (password == null) {
    throw new InvalidToken("Password not found for ApplicationAttempt " + applicationAttemptId);
  }
  return password;
}
