{
  if (blockPoolScannerMap.get(blockPoolId) != null) {
    return;
  }
  BlockPoolSliceScanner bpScanner=new BlockPoolSliceScanner(blockPoolId,datanode,dataset,conf);
  blockPoolScannerMap.put(blockPoolId,bpScanner);
  LOG.info("Added bpid=" + blockPoolId + " to blockPoolScannerMap, new size="+ blockPoolScannerMap.size());
}
