{
  final FileStatus targetFileStatus=targetFS.getFileStatus(target);
  return syncFolders && (targetFileStatus.getLen() != source.getLen() || (!skipCrc && !DistCpUtils.checksumsAreEqual(sourceFS,source.getPath(),null,targetFS,target)) || (source.getBlockSize() != targetFileStatus.getBlockSize() && preserve.contains(FileAttribute.BLOCKSIZE)));
}
