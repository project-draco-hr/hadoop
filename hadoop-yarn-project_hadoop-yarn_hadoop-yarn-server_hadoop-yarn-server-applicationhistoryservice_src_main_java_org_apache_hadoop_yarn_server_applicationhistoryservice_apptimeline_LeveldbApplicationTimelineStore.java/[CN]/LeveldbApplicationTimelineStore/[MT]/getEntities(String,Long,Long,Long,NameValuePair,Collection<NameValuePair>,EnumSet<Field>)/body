{
  if (primaryFilter == null) {
    return getEntityByTime(ENTITY_ENTRY_PREFIX,entityType,limit,windowStart,windowEnd,secondaryFilters,fields);
  }
 else {
    byte[] base=KeyBuilder.newInstance().add(INDEXED_ENTRY_PREFIX).add(primaryFilter.getName()).add(GenericObjectMapper.write(primaryFilter.getValue()),true).add(ENTITY_ENTRY_PREFIX).getBytesForLookup();
    return getEntityByTime(base,entityType,limit,windowStart,windowEnd,secondaryFilters,fields);
  }
}
