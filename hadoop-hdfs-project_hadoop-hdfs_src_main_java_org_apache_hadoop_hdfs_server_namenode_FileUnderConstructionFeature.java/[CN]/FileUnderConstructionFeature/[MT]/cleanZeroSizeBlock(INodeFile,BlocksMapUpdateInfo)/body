{
  final BlockInfoContiguous[] blocks=f.getBlocks();
  if (blocks != null && blocks.length > 0 && blocks[blocks.length - 1] instanceof BlockInfoContiguousUnderConstruction) {
    BlockInfoContiguousUnderConstruction lastUC=(BlockInfoContiguousUnderConstruction)blocks[blocks.length - 1];
    if (lastUC.getNumBytes() == 0) {
      collectedBlocks.addDeleteBlock(lastUC);
      f.removeLastBlock(lastUC);
    }
  }
}
