{
  Path filePath=new Path(testFileName);
  FSDataInputStream istream=dfs.open(filePath,10240);
  ByteBuffer buf=ByteBuffer.allocate(10240);
  int count=0;
  try {
    while (istream.read(buf) > 0) {
      count+=1;
      buf.clear();
      istream.seek(istream.getPos() + 5);
    }
  }
 catch (  IOException ioe) {
  }
 finally {
    istream.close();
  }
}
