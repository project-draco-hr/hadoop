{
  Path file=new Path("withoutTraceWriteTest.dat");
  FSDataOutputStream stream=dfs.create(file);
  for (int i=0; i < 10; i++) {
    byte[] data=RandomStringUtils.randomAlphabetic(102400).getBytes();
    stream.write(data);
  }
  stream.hflush();
  stream.close();
  Assert.assertTrue(SetSpanReceiver.SetHolder.size() == 0);
}
