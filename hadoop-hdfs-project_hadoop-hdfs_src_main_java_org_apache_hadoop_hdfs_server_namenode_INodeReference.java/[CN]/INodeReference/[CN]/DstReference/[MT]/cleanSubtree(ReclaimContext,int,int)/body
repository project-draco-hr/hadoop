{
  if (snapshot == Snapshot.CURRENT_STATE_ID && prior == Snapshot.NO_SNAPSHOT_ID) {
    destroyAndCollectBlocks(reclaimContext);
  }
 else {
    if (prior == Snapshot.NO_SNAPSHOT_ID) {
      prior=getPriorSnapshot(this);
    }
    if (snapshot != Snapshot.CURRENT_STATE_ID && prior != Snapshot.NO_SNAPSHOT_ID && Snapshot.ID_INTEGER_COMPARATOR.compare(snapshot,prior) <= 0) {
      return;
    }
    getReferredINode().cleanSubtree(reclaimContext,snapshot,prior);
  }
}
