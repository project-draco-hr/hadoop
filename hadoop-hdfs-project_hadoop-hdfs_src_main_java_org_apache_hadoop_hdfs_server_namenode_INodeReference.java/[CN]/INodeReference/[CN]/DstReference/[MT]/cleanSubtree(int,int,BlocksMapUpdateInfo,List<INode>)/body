{
  if (snapshot == Snapshot.CURRENT_STATE_ID && prior == Snapshot.NO_SNAPSHOT_ID) {
    Quota.Counts counts=Quota.Counts.newInstance();
    this.computeQuotaUsage(counts,true);
    destroyAndCollectBlocks(collectedBlocks,removedINodes);
    return counts;
  }
 else {
    if (prior == Snapshot.NO_SNAPSHOT_ID) {
      prior=getPriorSnapshot(this);
    }
    if (snapshot != Snapshot.CURRENT_STATE_ID && prior != Snapshot.NO_SNAPSHOT_ID && Snapshot.ID_INTEGER_COMPARATOR.compare(snapshot,prior) <= 0) {
      return Quota.Counts.newInstance();
    }
    return getReferredINode().cleanSubtree(snapshot,prior,collectedBlocks,removedINodes);
  }
}
