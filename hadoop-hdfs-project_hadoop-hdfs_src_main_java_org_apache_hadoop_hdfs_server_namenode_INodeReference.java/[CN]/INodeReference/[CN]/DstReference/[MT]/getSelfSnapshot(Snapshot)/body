{
  WithCount wc=(WithCount)getReferredINode().asReference();
  INode referred=wc.getReferredINode();
  Snapshot lastSnapshot=null;
  if (referred instanceof INodeFileWithSnapshot) {
    lastSnapshot=((INodeFileWithSnapshot)referred).getDiffs().getLastSnapshot();
  }
 else   if (referred instanceof INodeDirectoryWithSnapshot) {
    lastSnapshot=((INodeDirectoryWithSnapshot)referred).getLastSnapshot();
  }
  if (lastSnapshot != null && !lastSnapshot.equals(prior)) {
    return lastSnapshot;
  }
 else {
    return null;
  }
}
