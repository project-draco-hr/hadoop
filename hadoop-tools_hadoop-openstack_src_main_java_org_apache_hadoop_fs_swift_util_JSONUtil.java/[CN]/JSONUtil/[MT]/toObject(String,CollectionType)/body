{
  try {
    return (T)jsonMapper.readValue(value,collectionType);
  }
 catch (  JsonGenerationException e) {
    throw new SwiftJsonMarshallingException(e.toString() + " source: " + value,e);
  }
catch (  JsonMappingException e) {
    throw new SwiftJsonMarshallingException(e.toString() + " source: " + value,e);
  }
}
