{
  maybeInitBuilder();
  builder.clearLogAggregationReportsForApps();
  for (  Entry<ApplicationId,LogAggregationReport> entry : logAggregationReportsForApps.entrySet()) {
    builder.addLogAggregationReportsForApps(LogAggregationReportsForAppsProto.newBuilder().setAppId(convertToProtoFormat(entry.getKey())).setLogAggregationReport(convertToProtoFormat(entry.getValue())));
  }
}
