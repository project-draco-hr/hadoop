{
  maybeInitBuilder();
  builder.clearRegisteredAggregators();
  for (  Map.Entry<ApplicationId,String> entry : registeredAggregators.entrySet()) {
    builder.addRegisteredAggregators(AppAggregatorsMapProto.newBuilder().setAppId(convertToProtoFormat(entry.getKey())).setAppAggregatorAddr(entry.getValue()));
  }
}
