{
  if (fs == null)   return null;
  return new org.apache.hadoop.hdfs.protocol.HdfsLocatedFileStatus(fs.getLen(),fs.isDir(),fs.getReplication(),fs.getBlockSize(),fs.getModificationTime(),fs.getAccessTime(),FsPermissionWritable.convertPermission(fs.getPermission()),fs.getOwner(),fs.getGroup(),fs.getSymlinkInBytes(),fs.getLocalNameInBytes(),LocatedBlocksWritable.convertLocatedBlocks(fs.getBlockLocations()));
}
