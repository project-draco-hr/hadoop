{
  String jobHistoryFile=String.format(JOB_HISTORY_FILE_FORMATTER,JOB_ID,SUBMIT_TIME,USER_NAME,JOB_NAME_WITH_DELIMITER_ESCAPE,FINISH_TIME,NUM_MAPS,NUM_REDUCES,JOB_STATUS,QUEUE_NAME);
  JobIndexInfo info=FileNameIndexUtils.getIndexInfo(jobHistoryFile);
  Assert.assertEquals("Job name doesn't match",JOB_NAME_WITH_DELIMITER,info.getJobName());
}
