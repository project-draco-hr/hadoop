def __init__(self, config, log=None):
    hodBaseService.__init__(self, 'serviceRegistry', config)
    self.__serviceDict = {}
    self.__failCount = {}
    self.__released = {}
    self.__locked = {}
    self.__serviceDictLock = threading.Lock()
    self.RMErrorMsgs = None
    self.log = log
    if (self.log is None):
        self.log = logging.getLogger()
