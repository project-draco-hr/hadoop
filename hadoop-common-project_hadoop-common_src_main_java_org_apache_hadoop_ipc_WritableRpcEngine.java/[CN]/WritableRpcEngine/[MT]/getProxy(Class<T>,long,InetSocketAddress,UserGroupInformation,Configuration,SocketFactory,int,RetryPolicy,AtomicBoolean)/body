{
  if (connectionRetryPolicy != null) {
    throw new UnsupportedOperationException("Not supported: connectionRetryPolicy=" + connectionRetryPolicy);
  }
  T proxy=(T)Proxy.newProxyInstance(protocol.getClassLoader(),new Class[]{protocol},new Invoker(protocol,addr,ticket,conf,factory,rpcTimeout,fallbackToSimpleAuth));
  return new ProtocolProxy<T>(protocol,proxy,true);
}
