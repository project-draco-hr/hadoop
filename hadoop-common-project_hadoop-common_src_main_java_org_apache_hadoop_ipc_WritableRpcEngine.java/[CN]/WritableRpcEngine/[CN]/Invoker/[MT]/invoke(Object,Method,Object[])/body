{
  long startTime=0;
  if (LOG.isDebugEnabled()) {
    startTime=Time.now();
  }
  ObjectWritable value=(ObjectWritable)client.call(RPC.RpcKind.RPC_WRITABLE,new Invocation(method,args),remoteId);
  if (LOG.isDebugEnabled()) {
    long callTime=Time.now() - startTime;
    LOG.debug("Call: " + method.getName() + " "+ callTime);
  }
  return value.get();
}
