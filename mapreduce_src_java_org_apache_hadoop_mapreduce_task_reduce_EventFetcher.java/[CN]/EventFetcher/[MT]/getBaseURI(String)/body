{
  StringBuffer baseUrl=new StringBuffer(url);
  if (!url.endsWith("/")) {
    baseUrl.append("/");
  }
  baseUrl.append("mapOutput?job=");
  baseUrl.append(reduce.getJobID());
  baseUrl.append("&reduce=");
  baseUrl.append(reduce.getTaskID().getId());
  baseUrl.append("&map=");
  URI u=URI.create(baseUrl.toString());
  return u;
}
