{
  if (resourceTypes.contains(SchedulerResourceTypes.CPU)) {
    return Math.min(calculateRatioOrMaxValue(available.getMemory(),required.getMemory()),calculateRatioOrMaxValue(available.getVirtualCores(),required.getVirtualCores()));
  }
  return calculateRatioOrMaxValue(available.getMemory(),required.getMemory());
}
