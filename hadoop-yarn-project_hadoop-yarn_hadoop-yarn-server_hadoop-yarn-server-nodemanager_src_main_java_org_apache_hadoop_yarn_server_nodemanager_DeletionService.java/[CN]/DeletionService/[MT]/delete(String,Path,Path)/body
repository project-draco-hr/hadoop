{
  if (debugDelay != -1) {
    if (baseDirs == null || baseDirs.length == 0) {
      sched.schedule(new FileDeletionTask(this,user,subDir,null),debugDelay,TimeUnit.SECONDS);
    }
 else {
      sched.schedule(new FileDeletionTask(this,user,subDir,Arrays.asList(baseDirs)),debugDelay,TimeUnit.SECONDS);
    }
  }
}
