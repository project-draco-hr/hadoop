{
  binaryPath=conf.get(MULTI_USER_BINARY_PATH_KEY);
  if (binaryPath == null || binaryPath.trim().isEmpty()) {
    throw new IllegalArgumentException("Binary path for multi-user path is not present. Please set " + MULTI_USER_BINARY_PATH_KEY + " correctly");
  }
  File binaryFile=new File(binaryPath);
  if (!binaryFile.exists() || !binaryFile.canExecute()) {
    throw new IllegalArgumentException("Binary file path is not configured correctly. Please set " + MULTI_USER_BINARY_PATH_KEY + " to properly configured binary file.");
  }
  mangingUser=conf.get(MULTI_USER_MANAGING_USER + daemonName);
  if (mangingUser == null || mangingUser.trim().isEmpty()) {
    throw new IllegalArgumentException("Manging user for daemon not present please set : " + MULTI_USER_MANAGING_USER + daemonName+ " to correct value.");
  }
}
