{
  String[] args1={"create","key1","--cipher","AES/CBC/pkcs5Padding","--provider","jceks://file" + tmpDir + "/keystore.jceks"};
  int rc=0;
  KeyShell ks=new KeyShell();
  ks.setConf(new Configuration());
  rc=ks.run(args1);
  assertEquals(0,rc);
  assertTrue(outContent.toString().contains("key1 has been successfully " + "created."));
  outContent.reset();
  String[] args2={"delete","key1","--provider","jceks://file" + tmpDir + "/keystore.jceks"};
  rc=ks.run(args2);
  assertEquals(0,rc);
  assertTrue(outContent.toString().contains("key1 has been successfully " + "deleted."));
}
