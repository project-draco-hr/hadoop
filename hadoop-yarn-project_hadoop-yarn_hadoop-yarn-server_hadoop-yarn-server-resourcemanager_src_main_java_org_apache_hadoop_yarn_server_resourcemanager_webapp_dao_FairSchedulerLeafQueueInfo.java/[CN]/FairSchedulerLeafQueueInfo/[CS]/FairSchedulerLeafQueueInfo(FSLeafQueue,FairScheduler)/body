{
  super(queue,scheduler);
  Collection<AppSchedulable> apps=queue.getRunnableAppSchedulables();
  for (  AppSchedulable app : apps) {
    if (app.getApp().isPending()) {
      numPendingApps++;
    }
 else {
      numActiveApps++;
    }
  }
  numPendingApps+=queue.getNonRunnableAppSchedulables().size();
}
