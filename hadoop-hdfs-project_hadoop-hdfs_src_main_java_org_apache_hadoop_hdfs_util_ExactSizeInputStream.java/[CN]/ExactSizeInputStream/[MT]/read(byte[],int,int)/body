{
  if (remaining <= 0) {
    return -1;
  }
  len=Math.min(len,remaining);
  final int result=super.read(b,off,len);
  if (result >= 0) {
    remaining-=result;
  }
 else   if (remaining > 0) {
    throw new EOFException("Premature EOF. Expected " + remaining + "more bytes");
  }
  return result;
}
