{
  startTCPServer();
  if (register) {
    ShutdownHookManager.get().addShutdownHook(new NfsShutdownHook(),SHUTDOWN_HOOK_PRIORITY);
    try {
      rpcProgram.register(PortmapMapping.TRANSPORT_TCP,nfsBoundPort);
    }
 catch (    Throwable e) {
      LOG.fatal("Failed to start the server. Cause:",e);
      terminate(1,e);
    }
  }
}
