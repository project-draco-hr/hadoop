{
  if (blocks == null || blocks.length == 0) {
    return false;
  }
  int newSize=blocks.length - 1;
  if (!blocks[newSize].equals(oldblock)) {
    return false;
  }
  BlockInfoStriped[] newlist=new BlockInfoStriped[newSize];
  System.arraycopy(blocks,0,newlist,0,newSize);
  setBlocks(newlist);
  return true;
}
