{
  TimelineEntity e1=reader.getEntity("user1","cluster1","some_flow_name",1002345678919L,"application_1111111111_2222",TimelineEntityType.YARN_APPLICATION.toString(),null,null,null,EnumSet.of(Field.ALL));
