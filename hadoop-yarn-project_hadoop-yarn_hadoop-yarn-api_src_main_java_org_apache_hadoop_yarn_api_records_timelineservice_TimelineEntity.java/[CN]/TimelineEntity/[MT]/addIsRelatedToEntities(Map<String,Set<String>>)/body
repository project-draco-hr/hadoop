{
  for (  Map.Entry<String,Set<String>> entry : isRelatedToEntities.entrySet()) {
    Set<String> ids=this.isRelatedToEntities.get(entry.getKey());
    if (ids == null) {
      ids=new HashSet<>();
      this.isRelatedToEntities.put(entry.getKey(),ids);
    }
    ids.addAll(entry.getValue());
  }
}
