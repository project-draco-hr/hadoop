{
  Pattern ipat=includeTagPatterns.get(tag.name());
  if (ipat != null && ipat.matcher(tag.value()).matches()) {
    return true;
  }
  Pattern epat=excludeTagPatterns.get(tag.name());
  if (epat != null && epat.matcher(tag.value()).matches()) {
    return false;
  }
  if (ipat != null && epat == null) {
    return false;
  }
  return true;
}
