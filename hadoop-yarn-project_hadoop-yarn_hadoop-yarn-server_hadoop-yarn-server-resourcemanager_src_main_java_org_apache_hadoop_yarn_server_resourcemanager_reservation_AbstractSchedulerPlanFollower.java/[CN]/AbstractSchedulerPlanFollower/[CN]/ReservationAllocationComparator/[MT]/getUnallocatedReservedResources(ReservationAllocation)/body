{
  Resource resResource;
  Resource reservationResource=planFollower.getReservationQueueResourceIfExists(plan,reservation.getReservationId());
  if (reservationResource != null) {
    resResource=Resources.subtract(reservation.getResourcesAtTime(now),reservationResource);
  }
 else {
    resResource=reservation.getResourcesAtTime(now);
  }
  return resResource;
}
