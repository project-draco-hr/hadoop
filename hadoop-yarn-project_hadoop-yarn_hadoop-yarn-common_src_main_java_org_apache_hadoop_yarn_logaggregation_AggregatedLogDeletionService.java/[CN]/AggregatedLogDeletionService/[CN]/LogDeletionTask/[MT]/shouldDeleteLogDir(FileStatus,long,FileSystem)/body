{
  boolean shouldDelete=true;
  try {
    for (    FileStatus node : fs.listStatus(dir.getPath())) {
      if (node.getModificationTime() >= cutoffMillis) {
        shouldDelete=false;
        break;
      }
    }
  }
 catch (  IOException e) {
    logIOException("Error reading the contents of " + dir.getPath(),e);
    shouldDelete=false;
  }
  return shouldDelete;
}
