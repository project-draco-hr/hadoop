{
  RMServerUtils.checkAndNormalizeContainerChangeRequest(rmContext,request,increase);
  ContainerId containerId=request.getContainerId();
  RMContainer rmContainer=getRMContainer(containerId);
  SchedulerNode schedulerNode=getSchedulerNode(rmContainer.getAllocatedNode());
  return new SchedContainerChangeRequest(schedulerNode,rmContainer,request.getCapability());
}
