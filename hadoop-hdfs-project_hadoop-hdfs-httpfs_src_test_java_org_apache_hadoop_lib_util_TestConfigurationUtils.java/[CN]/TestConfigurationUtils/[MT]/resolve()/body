{
  Configuration conf=new Configuration(false);
  conf.set("a","A");
  conf.set("b","${a}");
  Assert.assertEquals(conf.getRaw("a"),"A");
  Assert.assertEquals(conf.getRaw("b"),"${a}");
  conf=ConfigurationUtils.resolve(conf);
  Assert.assertEquals(conf.getRaw("a"),"A");
  Assert.assertEquals(conf.getRaw("b"),"A");
}
