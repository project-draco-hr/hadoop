{
  AccessControlList acl=new AccessControlList("drwho tardis");
  Throwable th=null;
  try {
    acl.addUser(" * ");
  }
 catch (  Throwable t) {
    th=t;
  }
  assertNotNull(th);
  assertTrue(th instanceof IllegalArgumentException);
  th=null;
  try {
    acl.addGroup(" * ");
  }
 catch (  Throwable t) {
    th=t;
  }
  assertNotNull(th);
  assertTrue(th instanceof IllegalArgumentException);
  th=null;
  try {
    acl.removeUser(" * ");
  }
 catch (  Throwable t) {
    th=t;
  }
  assertNotNull(th);
  assertTrue(th instanceof IllegalArgumentException);
  th=null;
  try {
    acl.removeGroup(" * ");
  }
 catch (  Throwable t) {
    th=t;
  }
  assertNotNull(th);
  assertTrue(th instanceof IllegalArgumentException);
}
