{
  AccessControlList acl;
  acl=new AccessControlList("*");
  assertTrue(acl.isAllAllowed());
  acl=new AccessControlList("  * ");
  assertTrue(acl.isAllAllowed());
  acl=new AccessControlList(" *");
  assertTrue(acl.isAllAllowed());
  acl=new AccessControlList("*  ");
  assertTrue(acl.isAllAllowed());
}
