{
  JsonSerializer<?> defaultSerializer=new DefaultRumenSerializer();
  testSerializer(new FileName("hdfs://mynn:123/home/user/test.json"),"hdfs://mynn:123/home/user/test.json",defaultSerializer);
  testSerializer(new FileName("file:///home/user/test.json"),"file:///home/user/test.json",defaultSerializer);
  testSerializer(new FileName("hdfs://mynn:123/home/user/"),"hdfs://mynn:123/home/user/",defaultSerializer);
  testSerializer(new FileName("file:///home/user/"),"file:///home/user/",defaultSerializer);
  testSerializer(new FileName("/home/user/test.json"),"/home/user/test.json",defaultSerializer);
  testSerializer(new FileName("home/user/test.json"),"home/user/test.json",defaultSerializer);
  testSerializer(new FileName("/home/user/"),"/home/user/",defaultSerializer);
  testSerializer(new FileName("home/user/"),"home/user/",defaultSerializer);
  testSerializer(new FileName("home"),"home",defaultSerializer);
  testSerializer(new FileName("home/"),"home/",defaultSerializer);
  testSerializer(new FileName("/home"),"/home",defaultSerializer);
  testSerializer(new FileName("/home/"),"/home/",defaultSerializer);
  testSerializer(new FileName("./tmp"),"./tmp",defaultSerializer);
  testSerializer(new FileName("./tmp/"),"./tmp/",defaultSerializer);
  testSerializer(new FileName("../tmp"),"../tmp",defaultSerializer);
  FileName fileName=new FileName("hdfs://testnn:123/user/test1,file:///user/test2," + "/user/test3");
  testSerializer(fileName,"hdfs://testnn:123/user/test1,file:///user/test2,/user/test3",defaultSerializer);
}
