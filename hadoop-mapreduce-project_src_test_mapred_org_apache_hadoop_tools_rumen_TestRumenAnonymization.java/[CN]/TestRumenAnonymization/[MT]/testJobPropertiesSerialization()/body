{
  JsonSerializer<?> defaultSerializer=new DefaultRumenSerializer();
  Properties properties=new Properties();
  properties.put("test-key","test-value");
  properties.put(MRJobConfig.USER_NAME,"bob");
  properties.put(JobConf.MAPRED_TASK_JAVA_OPTS,"-Xmx1G");
  JobProperties jp=new JobProperties(properties);
  testSerializer(jp,"{test-key:test-value," + "mapreduce.job.user.name:bob," + "mapred.child.java.opts:-Xmx1G}",defaultSerializer);
}
