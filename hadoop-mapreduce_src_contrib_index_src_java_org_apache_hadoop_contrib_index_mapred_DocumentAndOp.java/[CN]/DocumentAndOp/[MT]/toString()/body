{
  StringBuilder buffer=new StringBuilder();
  buffer.append(this.getClass().getName());
  buffer.append("[op=");
  buffer.append(op);
  buffer.append(", doc=");
  if (doc != null) {
    buffer.append(doc);
  }
 else {
    buffer.append("null");
  }
  buffer.append(", term=");
  if (term != null) {
    buffer.append(term);
  }
 else {
    buffer.append("null");
  }
  buffer.append("]");
  return buffer.toString();
}
