{
  StringBuilder sb=new StringBuilder("Permission denied: ").append("user=").append(user).append(", ").append("access=").append(access).append(", ").append("inode=\"").append(inode.getFullPathName()).append("\":").append(inode.getUserName(snapshotId)).append(':').append(inode.getGroupName(snapshotId)).append(':').append(inode.isDirectory() ? 'd' : '-').append(mode);
  return sb.toString();
}
