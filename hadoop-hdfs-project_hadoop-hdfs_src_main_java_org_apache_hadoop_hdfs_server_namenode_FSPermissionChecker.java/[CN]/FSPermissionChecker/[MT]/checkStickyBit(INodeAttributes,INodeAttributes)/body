{
  if (!parent.getFsPermission().getStickyBit()) {
    return;
  }
  if (parent.getUserName().equals(getUser())) {
    return;
  }
  if (inode.getUserName().equals(getUser())) {
    return;
  }
  throw new AccessControlException("Permission denied by sticky bit setting:" + " user=" + getUser() + ", inode="+ inode);
}
