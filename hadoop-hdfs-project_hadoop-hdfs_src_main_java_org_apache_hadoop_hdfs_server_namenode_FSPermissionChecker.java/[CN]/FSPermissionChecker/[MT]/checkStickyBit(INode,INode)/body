{
  if (!parent.getFsPermission().getStickyBit()) {
    return;
  }
  if (parent.getUserName().equals(user)) {
    return;
  }
  if (inode.getUserName().equals(user)) {
    return;
  }
  throw new AccessControlException("Permission denied by sticky bit setting:" + " user=" + user + ", inode="+ inode);
}
