{
  for (int i=0; i <= last; i++) {
    INodeAttributes inode=inodeAttrs[i];
    if (!inode.isDirectory()) {
      throw new AccessControlException(constructPath(inodeAttrs,i) + " (is not a directory)");
    }
    if (!hasPermission(inode,FsAction.EXECUTE)) {
      throw new AccessControlException(toAccessControlString(inode,constructPath(inodeAttrs,i),FsAction.EXECUTE));
    }
  }
}
