{
  Configuration newConf;
  if (conf == null) {
    newConf=new Configuration(false);
  }
 else {
    newConf=new Configuration(conf);
  }
  newConf.set(KMSConfiguration.KEY_PROVIDER_URI,"jceks://file@" + new Path(keyStoreDir.getAbsolutePath(),"kms.keystore").toUri());
  newConf.set("hadoop.kms.authentication.type","simple");
  return newConf;
}
