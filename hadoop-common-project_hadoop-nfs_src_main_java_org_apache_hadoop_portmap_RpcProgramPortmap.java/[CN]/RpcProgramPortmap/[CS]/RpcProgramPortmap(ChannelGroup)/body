{
  this.allChannels=allChannels;
  PortmapMapping m=new PortmapMapping(PROGRAM,VERSION,PortmapMapping.TRANSPORT_TCP,RpcProgram.RPCB_PORT);
  PortmapMapping m1=new PortmapMapping(PROGRAM,VERSION,PortmapMapping.TRANSPORT_UDP,RpcProgram.RPCB_PORT);
  map.put(PortmapMapping.key(m),m);
  map.put(PortmapMapping.key(m1),m1);
}
