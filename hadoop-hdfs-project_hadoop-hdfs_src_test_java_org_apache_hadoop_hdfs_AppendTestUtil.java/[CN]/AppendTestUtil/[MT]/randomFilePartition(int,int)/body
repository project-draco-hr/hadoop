{
  int[] p=new int[parts];
  for (int i=0; i < p.length; i++) {
    p[i]=nextInt(n - i - 1) + 1;
  }
  Arrays.sort(p);
  for (int i=1; i < p.length; i++) {
    if (p[i] <= p[i - 1]) {
      p[i]=p[i - 1] + 1;
    }
  }
  LOG.info("partition=" + Arrays.toString(p));
  assertTrue("i=0",p[0] > 0 && p[0] < n);
  for (int i=1; i < p.length; i++) {
    assertTrue("i=" + i,p[i] > p[i - 1] && p[i] < n);
  }
  return p;
}
