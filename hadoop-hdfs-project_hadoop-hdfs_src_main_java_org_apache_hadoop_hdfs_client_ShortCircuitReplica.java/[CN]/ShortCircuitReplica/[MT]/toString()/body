{
  return new StringBuilder().append("ShortCircuitReplica{").append("key=").append(key).append(", metaHeader.version=").append(metaHeader.getVersion()).append(", metaHeader.checksum=").append(metaHeader.getChecksum()).append(", ident=").append("0x").append(Integer.toHexString(System.identityHashCode(this))).append(", creationTimeMs=").append(creationTimeMs).append("}").toString();
}
