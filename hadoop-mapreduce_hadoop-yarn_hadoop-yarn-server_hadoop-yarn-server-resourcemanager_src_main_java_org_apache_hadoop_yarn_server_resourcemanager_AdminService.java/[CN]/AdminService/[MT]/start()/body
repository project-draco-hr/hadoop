{
  YarnRPC rpc=YarnRPC.create(getConfig());
  Configuration serverConf=new Configuration(getConfig());
  serverConf.setClass(YarnConfiguration.YARN_SECURITY_INFO,SchedulerSecurityInfo.class,SecurityInfo.class);
  this.server=rpc.getServer(RMAdminProtocol.class,this,masterServiceAddress,serverConf,null,serverConf.getInt(RMConfig.RM_ADMIN_THREADS,RMConfig.DEFAULT_RM_ADMIN_THREADS));
  this.server.start();
  super.start();
}
