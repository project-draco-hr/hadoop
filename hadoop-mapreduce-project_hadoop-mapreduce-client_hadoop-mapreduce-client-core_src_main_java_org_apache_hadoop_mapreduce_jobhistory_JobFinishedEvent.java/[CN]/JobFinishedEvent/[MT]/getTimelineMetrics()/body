{
  Set<TimelineMetric> jobMetrics=JobHistoryEventUtils.countersToTimelineMetric(getMapCounters(),finishTime);
  jobMetrics.addAll(JobHistoryEventUtils.countersToTimelineMetric(getReduceCounters(),finishTime));
  jobMetrics.addAll(JobHistoryEventUtils.countersToTimelineMetric(getTotalCounters(),finishTime));
  return jobMetrics;
}
