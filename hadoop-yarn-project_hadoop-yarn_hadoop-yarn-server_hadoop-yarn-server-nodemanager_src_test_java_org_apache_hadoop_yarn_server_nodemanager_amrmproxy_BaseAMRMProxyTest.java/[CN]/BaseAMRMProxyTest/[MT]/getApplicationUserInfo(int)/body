{
  final ApplicationAttemptId attemptId=getApplicationAttemptId(testAppId);
  UserGroupInformation ugi=UserGroupInformation.createRemoteUser(attemptId.toString());
  AMRMTokenIdentifier token=new AMRMTokenIdentifier(attemptId,1);
  ugi.addTokenIdentifier(token);
  return new ApplicationUserInfo(ugi,attemptId);
}
