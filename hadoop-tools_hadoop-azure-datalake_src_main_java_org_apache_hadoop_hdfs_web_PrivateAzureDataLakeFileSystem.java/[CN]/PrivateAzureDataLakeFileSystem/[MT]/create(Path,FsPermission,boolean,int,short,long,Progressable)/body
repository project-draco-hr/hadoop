{
  statistics.incrementWriteOps(1);
  return new FSDataOutputStream(new BatchAppendOutputStream(f,bufferSize,new PermissionParam(applyUMask(permission)),new OverwriteParam(overwrite),new BufferSizeParam(bufferSize),new ReplicationParam(replication),new BlockSizeParam(blockSize),new ADLVersionInfo(VersionInfo.getVersion())),statistics){
  }
;
}
