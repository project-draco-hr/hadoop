{
  FileStatus[] fileStatuses=super.listStatus(f);
  for (int i=0; i < fileStatuses.length; i++) {
    if (overrideOwner) {
      fileStatuses[i]=new FileStatus(fileStatuses[i].getLen(),fileStatuses[i].isDirectory(),fileStatuses[i].getReplication(),fileStatuses[i].getBlockSize(),fileStatuses[i].getModificationTime(),fileStatuses[i].getAccessTime(),fileStatuses[i].getPermission(),userName,"hdfs",fileStatuses[i].getPath());
    }
  }
  return fileStatuses;
}
