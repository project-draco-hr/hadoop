{
  statistics.incrementReadOps(1);
  final HttpOpParam.Op op=GetOpParam.Op.OPEN;
  FsPathConnectionRunner runner=null;
  if (featureConcurrentReadWithReadAhead) {
    URL url=this.toUrl(op,f,new BufferSizeParam(buffersize),new ReadADLNoRedirectParam(true),new ADLVersionInfo(VersionInfo.getVersion()));
    BatchByteArrayInputStream bb=new BatchByteArrayInputStream(url,f,maxBufferSize,maxConcurrentConnection);
    FSDataInputStream fin=new FSDataInputStream(bb);
    return fin;
  }
 else {
    if (featureRedirectOff) {
      runner=new FsPathConnectionRunner(ADLGetOpParam.Op.OPEN,f,new BufferSizeParam(buffersize),new ReadADLNoRedirectParam(true),new ADLVersionInfo(VersionInfo.getVersion()));
    }
 else {
      runner=new FsPathConnectionRunner(op,f,new BufferSizeParam(buffersize));
    }
    return new FSDataInputStream(new OffsetUrlInputStream(new UnresolvedUrlOpener(runner),new OffsetUrlOpener(null)));
  }
}
