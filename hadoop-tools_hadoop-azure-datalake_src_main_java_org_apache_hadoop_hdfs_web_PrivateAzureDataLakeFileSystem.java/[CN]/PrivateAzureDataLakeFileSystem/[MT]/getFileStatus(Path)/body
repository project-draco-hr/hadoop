{
  statistics.incrementReadOps(1);
  FileStatus status=super.getFileStatus(f);
  if (overrideOwner) {
    FileStatus proxiedStatus=new FileStatus(status.getLen(),status.isDirectory(),status.getReplication(),status.getBlockSize(),status.getModificationTime(),status.getAccessTime(),status.getPermission(),userName,"hdfs",status.getPath());
    return proxiedStatus;
  }
 else {
    return status;
  }
}
