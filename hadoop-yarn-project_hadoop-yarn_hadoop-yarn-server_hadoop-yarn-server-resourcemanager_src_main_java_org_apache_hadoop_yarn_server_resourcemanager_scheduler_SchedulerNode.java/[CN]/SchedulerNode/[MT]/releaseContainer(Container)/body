{
  if (!isValidContainer(container.getId())) {
    LOG.error("Invalid container released " + container);
    return;
  }
  if (null != launchedContainers.remove(container.getId())) {
    updateResource(container);
  }
  LOG.info("Released container " + container.getId() + " of capacity "+ container.getResource()+ " on host "+ rmNode.getNodeAddress()+ ", which currently has "+ numContainers+ " containers, "+ getAllocatedResource()+ " used and "+ getUnallocatedResource()+ " available"+ ", release resources="+ true);
}
