{
  for (  GridmixRecord ignored : values) {
    acc+=ratio;
    while (acc >= 1.0 && factory.next(null,val)) {
      context.write(NullWritable.get(),val);
      acc-=1.0;
      try {
        matcher.match();
      }
 catch (      Exception e) {
        LOG.debug("Error in resource usage emulation! Message: ",e);
      }
    }
  }
}
