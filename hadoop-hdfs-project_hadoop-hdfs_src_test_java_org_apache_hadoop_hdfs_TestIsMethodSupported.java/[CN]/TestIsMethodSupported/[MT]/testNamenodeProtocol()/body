{
  NamenodeProtocolTranslatorPB translator=(NamenodeProtocolTranslatorPB)NameNodeProxies.createNonHAProxy(conf,nnAddress,NamenodeProtocol.class,UserGroupInformation.getCurrentUser(),true).getProxy();
  boolean exists=translator.isMethodSupported("rollEditLog");
  Assert.assertTrue(exists);
  exists=translator.isMethodSupported("bogusMethod");
  Assert.assertFalse(exists);
}
