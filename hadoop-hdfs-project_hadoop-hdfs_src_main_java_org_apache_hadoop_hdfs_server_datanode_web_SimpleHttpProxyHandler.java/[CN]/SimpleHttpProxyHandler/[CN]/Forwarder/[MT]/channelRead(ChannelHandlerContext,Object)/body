{
  client.writeAndFlush(msg).addListener(new ChannelFutureListener(){
    @Override public void operationComplete(    ChannelFuture future){
      if (future.isSuccess()) {
        ctx.channel().read();
      }
 else {
        LOG.debug("Proxy failed. Cause: ",future.cause());
        future.channel().close();
      }
    }
  }
);
}
