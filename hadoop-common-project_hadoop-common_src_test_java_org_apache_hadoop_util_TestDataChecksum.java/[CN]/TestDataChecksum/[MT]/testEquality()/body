{
  assertEquals(DataChecksum.newDataChecksum(DataChecksum.CHECKSUM_CRC32,512),DataChecksum.newDataChecksum(DataChecksum.CHECKSUM_CRC32,512));
  assertFalse(DataChecksum.newDataChecksum(DataChecksum.CHECKSUM_CRC32,512).equals(DataChecksum.newDataChecksum(DataChecksum.CHECKSUM_CRC32,1024)));
  assertFalse(DataChecksum.newDataChecksum(DataChecksum.CHECKSUM_CRC32,512).equals(DataChecksum.newDataChecksum(DataChecksum.CHECKSUM_CRC32C,512)));
}
