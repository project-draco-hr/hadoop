{
  final long inputSizeInMB=cSize * 300;
  String tracePath=getTraceFile("highram_mr_jobs_case2");
  Assert.assertNotNull("Trace file has not found.",tracePath);
  String[] runtimeValues={"LOADJOB",RoundRobinUserResolver.class.getName(),"REPLAY",inputSizeInMB + "m","file://" + UtilsForGridmix.getProxyUsersFile(conf),tracePath};
  String[] otherArgs={"-D",GridMixConfig.GRIDMIX_DISTCACHE_ENABLE + "=false","-D",GridMixConfig.GRIDMIX_COMPRESSION_ENABLE + "=false","-D",GridmixJob.GRIDMIX_HIGHRAM_EMULATION_ENABLE + "=true"};
  validateTaskMemoryParamters(tracePath,true);
  runGridmixAndVerify(runtimeValues,otherArgs,tracePath);
}
