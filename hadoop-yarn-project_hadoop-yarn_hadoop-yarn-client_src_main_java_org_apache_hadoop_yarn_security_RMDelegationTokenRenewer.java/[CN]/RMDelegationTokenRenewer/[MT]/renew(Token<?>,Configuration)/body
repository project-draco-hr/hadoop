{
  YarnClientImpl yarnClient=getYarnClient(conf,SecurityUtil.getTokenServiceAddr(token));
  try {
    DelegationToken dToken=BuilderUtils.newDelegationToken(token.getIdentifier(),token.getKind().toString(),token.getPassword(),token.getService().toString());
    return yarnClient.renewRMDelegationToken(dToken);
  }
  finally {
    yarnClient.stop();
  }
}
