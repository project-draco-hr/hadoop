{
  BaseHeaderProto.Builder builder=BaseHeaderProto.newBuilder().setBlock(PBHelper.convert(blk)).setToken(PBHelper.convert(blockToken));
  if (Trace.isTracing()) {
    Span s=Trace.currentSpan();
    builder.setTraceInfo(DataTransferTraceInfoProto.newBuilder().setTraceId(s.getTraceId()).setParentId(s.getSpanId()));
  }
  return builder.build();
}
