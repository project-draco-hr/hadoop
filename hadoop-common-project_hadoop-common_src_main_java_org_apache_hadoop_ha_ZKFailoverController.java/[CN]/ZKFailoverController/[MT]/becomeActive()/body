{
  LOG.info("Trying to make " + localTarget + " active...");
  try {
    HAServiceProtocolHelper.transitionToActive(localTarget.getProxy(conf,FailoverController.getRpcTimeoutToNewActive(conf)));
    LOG.info("Successfully transitioned " + localTarget + " to active state");
  }
 catch (  Throwable t) {
    LOG.fatal("Couldn't make " + localTarget + " active",t);
    elector.quitElection(true);
  }
}
