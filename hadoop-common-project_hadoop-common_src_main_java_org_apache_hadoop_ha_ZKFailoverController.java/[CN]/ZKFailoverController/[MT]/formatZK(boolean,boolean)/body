{
  if (elector.parentZNodeExists()) {
    if (!force && (!interactive || !confirmFormat())) {
      return ERR_CODE_FORMAT_DENIED;
    }
    try {
      elector.clearParentZNode();
    }
 catch (    IOException e) {
      LOG.error("Unable to clear zk parent znode",e);
      return 1;
    }
  }
  elector.ensureParentZNode();
  return 0;
}
