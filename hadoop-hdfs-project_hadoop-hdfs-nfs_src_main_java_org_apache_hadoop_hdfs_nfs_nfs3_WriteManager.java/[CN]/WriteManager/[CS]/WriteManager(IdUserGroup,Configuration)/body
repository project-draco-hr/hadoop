{
  this.iug=iug;
  streamTimeout=config.getLong("dfs.nfs3.stream.timeout",DEFAULT_STREAM_TIMEOUT);
  LOG.info("Stream timeout is " + streamTimeout + "ms.");
  if (streamTimeout < MINIMIUM_STREAM_TIMEOUT) {
    LOG.info("Reset stream timeout to minimum value " + MINIMIUM_STREAM_TIMEOUT + "ms.");
    streamTimeout=MINIMIUM_STREAM_TIMEOUT;
  }
  this.streamMonitor=new StreamMonitor();
}
