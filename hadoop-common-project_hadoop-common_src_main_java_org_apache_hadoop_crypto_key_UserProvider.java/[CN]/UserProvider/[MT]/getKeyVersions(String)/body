{
  List<KeyVersion> list=new ArrayList<KeyVersion>();
  Metadata km=getMetadata(name);
  if (km != null) {
    int latestVersion=km.getVersions();
    for (int i=0; i < latestVersion; i++) {
      KeyVersion v=getKeyVersion(buildVersionName(name,i));
      if (v != null) {
        list.add(v);
      }
    }
  }
  return list;
}
