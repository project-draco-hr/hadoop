{
  UnreliableInterface unreliable=(UnreliableInterface)RetryProxy.create(UnreliableInterface.class,unreliableImpl,TRY_ONCE_DONT_FAIL);
  unreliable.alwaysSucceeds();
  unreliable.failsOnceThenSucceeds();
  try {
    unreliable.failsOnceThenSucceedsWithReturnValue();
    fail("Should fail");
  }
 catch (  UnreliableException e) {
  }
}
