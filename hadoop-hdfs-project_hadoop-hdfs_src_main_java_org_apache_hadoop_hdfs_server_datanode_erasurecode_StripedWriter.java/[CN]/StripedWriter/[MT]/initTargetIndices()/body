{
  BitSet bitset=reconstructor.getLiveBitSet();
  int m=0;
  int k=0;
  hasValidTargets=false;
  for (int i=0; i < dataBlkNum + parityBlkNum; i++) {
    if (!bitset.get(i)) {
      if (reconstructor.getBlockLen(i) > 0) {
        if (m < targets.length) {
          targetIndices[m++]=(short)i;
          hasValidTargets=true;
        }
      }
    }
  }
}
