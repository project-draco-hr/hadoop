{
  ArrayList<Status> statusList=Lists.newArrayList();
  ArrayList<Integer> flagList=Lists.newArrayList();
  for (  int r : replies) {
    statusList.add(StatusFormat.getStatus(r));
    flagList.add(r);
  }
  proto=PipelineAckProto.newBuilder().setSeqno(seqno).addAllReply(statusList).addAllFlag(flagList).setDownstreamAckTimeNanos(downstreamAckTimeNanos).build();
}
