{
  Thread[] th=new Thread[32];
  for (int i=0; i < th.length; i++) {
    th[i]=new Thread(){
      @Override public void run(){
        try {
          doTestCache();
        }
 catch (        Throwable t) {
          failure=t;
        }
      }
    }
;
    th[i].start();
  }
  for (int i=0; i < th.length; i++) {
    th[i].join();
  }
  if (failure != null) {
    failure.printStackTrace();
    fail(failure.getMessage());
  }
}
