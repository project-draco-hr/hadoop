{
  System.out.printf("JobId\tState\tStartTime\tUserName\tQueue\tPriority\tSchedulingInfo\n");
  for (  JobStatus job : jobs) {
    System.out.printf("%s\t%d\t%d\t%s\t%s\t%s\t%s\n",job.getJobID(),job.getRunState(),job.getStartTime(),job.getUsername(),job.getQueue(),job.getJobPriority().name(),job.getSchedulingInfo());
  }
}
