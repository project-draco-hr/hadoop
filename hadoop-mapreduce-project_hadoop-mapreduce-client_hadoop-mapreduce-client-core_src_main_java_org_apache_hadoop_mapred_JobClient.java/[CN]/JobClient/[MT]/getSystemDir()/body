{
  try {
    return clientUgi.doAs(new PrivilegedExceptionAction<Path>(){
      @Override public Path run() throws IOException, InterruptedException {
        return cluster.getSystemDir();
      }
    }
);
  }
 catch (  IOException ioe) {
    return null;
  }
catch (  InterruptedException ie) {
    return null;
  }
}
