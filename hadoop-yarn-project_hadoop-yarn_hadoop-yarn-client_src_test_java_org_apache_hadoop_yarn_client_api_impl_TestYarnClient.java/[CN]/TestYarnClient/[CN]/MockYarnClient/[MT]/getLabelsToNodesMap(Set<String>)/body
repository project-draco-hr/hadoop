{
  Map<NodeLabel,Set<NodeId>> map=new HashMap<NodeLabel,Set<NodeId>>();
  Set<NodeId> setNodeIds=new HashSet<NodeId>(Arrays.asList(NodeId.newInstance("host1",0),NodeId.newInstance("host2",0)));
  for (  String label : labels) {
    map.put(NodeLabel.newInstance(label),setNodeIds);
  }
  return map;
}
