{
  super.serviceStop();
  if (pauseMonitor != null) {
    pauseMonitor.stop();
  }
  DefaultMetricsSystem.shutdown();
  if (rmContext != null) {
    RMStateStore store=rmContext.getStateStore();
    try {
      if (null != store) {
        store.close();
      }
    }
 catch (    Exception e) {
      LOG.error("Error closing store.",e);
    }
  }
}
