{
  if (webApp != null) {
    webApp.stop();
  }
  if (fetcher != null) {
    fetcher.stop();
  }
  if (configurationProvider != null) {
    configurationProvider.close();
  }
  super.serviceStop();
  if (curator != null) {
    curator.close();
  }
  transitionToStandby(false);
  rmContext.setHAServiceState(HAServiceState.STOPPING);
}
