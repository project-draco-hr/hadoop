{
  if (webApp != null) {
    webApp.stop();
  }
  rmDTSecretManager.stopThreads();
  this.appTokenSecretManager.stop();
  this.containerTokenSecretManager.stop();
  DefaultMetricsSystem.shutdown();
  RMStateStore store=rmContext.getStateStore();
  try {
    store.close();
  }
 catch (  Exception e) {
    LOG.error("Error closing store.",e);
  }
  super.stop();
}
