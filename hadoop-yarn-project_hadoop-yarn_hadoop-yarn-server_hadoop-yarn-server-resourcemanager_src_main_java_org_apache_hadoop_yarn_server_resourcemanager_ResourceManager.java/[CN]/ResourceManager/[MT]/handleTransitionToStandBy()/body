{
  if (rmContext.isHAEnabled()) {
    try {
      LOG.info("Transitioning RM to Standby mode");
      transitionToStandby(true);
      adminService.resetLeaderElection();
      return;
    }
 catch (    Exception e) {
      LOG.fatal("Failed to transition RM to Standby mode.");
      ExitUtil.terminate(1,e);
    }
  }
}
