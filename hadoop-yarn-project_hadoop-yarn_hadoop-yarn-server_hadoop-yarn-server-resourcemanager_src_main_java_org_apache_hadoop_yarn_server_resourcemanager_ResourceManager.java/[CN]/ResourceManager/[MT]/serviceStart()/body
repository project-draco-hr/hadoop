{
  if (this.rmContext.isHAEnabled()) {
    transitionToStandby(true);
  }
 else {
    transitionToActive();
  }
  startWepApp();
  if (getConfig().getBoolean(YarnConfiguration.IS_MINI_YARN_CLUSTER,false)) {
    int port=webApp.port();
    WebAppUtils.setRMWebAppPort(conf,port);
  }
  super.serviceStart();
}
