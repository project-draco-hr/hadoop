{
  byte[] first=Bytes.toBytes(Separator.QUALIFIERS.joinEncoded(userId,clusterId,flowId));
  byte[] second=Bytes.toBytes(EntityRowKey.invert(flowRunId));
  byte[] third=Bytes.toBytes(Separator.QUALIFIERS.joinEncoded(appId,te.getType(),te.getId()));
  return Separator.QUALIFIERS.join(first,second,third);
}
