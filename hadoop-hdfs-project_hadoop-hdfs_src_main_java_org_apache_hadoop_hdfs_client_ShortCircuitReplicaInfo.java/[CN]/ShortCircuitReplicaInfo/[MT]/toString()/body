{
  StringBuilder builder=new StringBuilder();
  String prefix="";
  builder.append("ShortCircuitReplicaInfo{");
  if (replica != null) {
    builder.append(prefix).append(replica);
    prefix=", ";
  }
  if (exc != null) {
    builder.append(prefix).append(exc);
    prefix=", ";
  }
  builder.append("}");
  return builder.toString();
}
