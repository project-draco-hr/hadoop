{
  if ((rowKey == null) || (table == null) || (columnFamily == null)|| (columnQualifier == null)|| (inputValue == null)) {
    return;
  }
  Put p=null;
  if (cellTimeStamp == null) {
    if (columnPrefix != null) {
      p=new Put(rowKey);
      p.addColumn(columnFamily,join(TimelineEntitySchemaConstants.ROW_KEY_SEPARATOR_BYTES,columnPrefix,columnQualifier),GenericObjectMapper.write(inputValue));
    }
 else {
      p=new Put(rowKey);
      p.addColumn(columnFamily,columnQualifier,GenericObjectMapper.write(inputValue));
    }
  }
 else {
    Cell cell=CellUtil.createCell(rowKey,columnFamily,columnQualifier,cellTimeStamp,TimelineEntitySchemaConstants.ZERO_BYTES,GenericObjectMapper.write(inputValue));
    p=new Put(rowKey);
    p.add(cell);
  }
  if (p != null) {
    table.mutate(p);
  }
}
