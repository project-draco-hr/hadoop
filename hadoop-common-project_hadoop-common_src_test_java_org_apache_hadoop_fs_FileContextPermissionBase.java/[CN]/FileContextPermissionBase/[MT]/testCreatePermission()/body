{
  if (Path.WINDOWS) {
    System.out.println("Cannot run test for Windows");
    return;
  }
  String filename="foo";
  Path f=getTestRootPath(fc,filename);
  createFile(fc,filename);
  doFilePermissionCheck(FileContext.DEFAULT_PERM.applyUMask(fc.getUMask()),fc.getFileStatus(f).getPermission());
}
