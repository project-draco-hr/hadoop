{
  LOG.info("Job " + job.getJobID() + " submitted to queue "+ job.getProfile().getQueueName());
  JobQueue qi=getJobQueue(job.getProfile().getQueueName());
  if (null == qi) {
    LOG.warn("Invalid queue " + job.getProfile().getQueueName() + " specified for job "+ job.getProfile().getJobID()+ ". Ignoring job.");
    return;
  }
  qi.jobAdded(job);
}
