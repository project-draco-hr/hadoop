{
  this.allocateLeft=allocateLeft;
  int startingIndex;
  if (allocateLeft) {
    startingIndex=0;
  }
 else {
    startingIndex=reservation.getReservationRequests().getReservationResources().size();
  }
  li=reservation.getReservationRequests().getReservationResources().listIterator(startingIndex);
}
