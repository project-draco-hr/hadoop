{
  BufferedInputStream bin=new BufferedInputStream(edits);
  DataInputStream in=new DataInputStream(bin);
  int numEdits=0;
  try {
    LogHeader header=LogHeader.read(in);
    numEdits=loadEditRecords(header.logVersion,in,header.checksum,false,expectedStartingTxId);
  }
  finally {
    if (closeOnExit)     in.close();
  }
  return numEdits;
}
