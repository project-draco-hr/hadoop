{
  Preconditions.checkNotNull(array);
  Preconditions.checkArgument(array.length == byteArrayLength);
  LOG.debug(", {}",this);
  notify();
  numAllocated--;
  if (numAllocated < 0) {
    numAllocated=0;
  }
  if (freeQueue.size() < maxAllocated - numAllocated) {
    LOG.debug(", freeQueue.offer");
    freeQueue.offer(array);
  }
  return freeQueue.size();
}
