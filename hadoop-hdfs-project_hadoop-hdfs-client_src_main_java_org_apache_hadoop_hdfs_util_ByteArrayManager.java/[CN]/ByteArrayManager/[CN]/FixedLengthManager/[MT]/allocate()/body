{
  LOG.debug(", {}",this);
  for (; numAllocated >= maxAllocated; ) {
    LOG.debug(": wait ...");
    wait();
    LOG.debug("wake up: {}",this);
  }
  numAllocated++;
  final byte[] array=freeQueue.poll();
  LOG.debug(", recycled? {}",array != null);
  return array != null ? array : new byte[byteArrayLength];
}
