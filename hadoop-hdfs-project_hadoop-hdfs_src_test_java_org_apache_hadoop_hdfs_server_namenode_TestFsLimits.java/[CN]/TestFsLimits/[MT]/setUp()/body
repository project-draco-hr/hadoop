{
  conf=new Configuration();
  conf.set(DFSConfigKeys.DFS_NAMENODE_NAME_DIR_KEY,fileAsURI(new File(MiniDFSCluster.getBaseDirectory(),"namenode")).toString());
  rootInode=new INodeDirectoryWithQuota(getMockNamesystem().allocateNewInodeId(),INodeDirectory.ROOT_NAME,perms);
  inodes=new INode[]{rootInode,null};
  fs=null;
  fsIsReady=true;
}
