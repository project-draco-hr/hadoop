{
  conf.setInt(DFSConfigKeys.DFS_NAMENODE_MAX_COMPONENT_LENGTH_KEY,2);
  mkdirs("/5",null);
  rename("/5","/555",PathComponentTooLongException.class);
  rename("/5","/55",null);
  mkdirs("/6",null);
  deprecatedRename("/6","/666",PathComponentTooLongException.class);
  deprecatedRename("/6","/66",null);
}
