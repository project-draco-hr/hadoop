{
  final Configuration conf=new Configuration();
  checkInvalidPath("har://hdfs-/foo.har",conf);
  checkInvalidPath("har://hdfs/foo.har",conf);
  checkInvalidPath("har://-hdfs/foo.har",conf);
  checkInvalidPath("har://-/foo.har",conf);
  checkInvalidPath("har://127.0.0.1-/foo.har",conf);
  checkInvalidPath("har://127.0.0.1/foo.har",conf);
}
