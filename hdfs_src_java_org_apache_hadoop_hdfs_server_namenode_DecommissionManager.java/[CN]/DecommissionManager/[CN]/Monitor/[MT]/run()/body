{
  for (; fsnamesystem.isRunning(); ) {
    fsnamesystem.writeLock();
    try {
      check();
    }
  finally {
      fsnamesystem.writeUnlock();
    }
    try {
      Thread.sleep(recheckInterval);
    }
 catch (    InterruptedException ie) {
      LOG.warn(this.getClass().getSimpleName() + " interrupted: " + ie);
    }
  }
}
