{
  doAnswer(new Answer<Container>(){
    @Override public Container answer(    InvocationOnMock invocation) throws Throwable {
      final FiCaSchedulerApp application=(FiCaSchedulerApp)(invocation.getArguments()[0]);
      final ContainerId containerId=TestUtils.getMockContainerId(application);
      Container container=TestUtils.getMockContainer(containerId,((FiCaSchedulerNode)(invocation.getArguments()[1])).getNodeID(),(Resource)(invocation.getArguments()[2]),((Priority)invocation.getArguments()[3]));
      return container;
    }
  }
).when(queue).createContainer(any(FiCaSchedulerApp.class),any(FiCaSchedulerNode.class),any(Resource.class),any(Priority.class));
  CSQueue parent=queue.getParent();
  doNothing().when(parent).completedContainer(any(Resource.class),any(FiCaSchedulerApp.class),any(FiCaSchedulerNode.class),any(RMContainer.class),any(ContainerStatus.class),any(RMContainerEventType.class),any(CSQueue.class));
  return queue;
}
