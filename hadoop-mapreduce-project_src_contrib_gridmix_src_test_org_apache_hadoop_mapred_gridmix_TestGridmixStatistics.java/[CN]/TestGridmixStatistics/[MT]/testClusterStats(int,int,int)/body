{
  assertEquals("Incorrect count of total number of submitted map tasks",numSubmittedMapTasks,ClusterStats.getSubmittedMapTasks());
  assertEquals("Incorrect count of total number of submitted reduce tasks",numSubmittedReduceTasks,ClusterStats.getSubmittedReduceTasks());
  assertEquals("Incorrect submitted jobs",numSubmittedJobs,ClusterStats.getRunningJobStats().size());
}
