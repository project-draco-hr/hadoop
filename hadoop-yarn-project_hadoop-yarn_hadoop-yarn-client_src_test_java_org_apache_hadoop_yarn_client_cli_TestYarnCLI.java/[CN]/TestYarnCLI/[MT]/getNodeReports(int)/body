{
  List<NodeReport> nodeReports=new ArrayList<NodeReport>();
  for (int i=0; i < noOfNodes; i++) {
    NodeReport nodeReport=BuilderUtils.newNodeReport(BuilderUtils.newNodeId("host" + i,0),NodeState.RUNNING,"host" + 1 + ":8888","rack1",Records.newRecord(Resource.class),Records.newRecord(Resource.class),0,Records.newRecord(NodeHealthStatus.class));
    nodeReports.add(nodeReport);
  }
  return nodeReports;
}
