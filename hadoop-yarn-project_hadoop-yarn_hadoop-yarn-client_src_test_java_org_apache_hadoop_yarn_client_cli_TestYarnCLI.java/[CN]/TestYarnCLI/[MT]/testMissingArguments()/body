{
  ApplicationCLI cli=createAndGetAppCLI();
  int result=cli.run(new String[]{"-status"});
  Assert.assertEquals(result,-1);
  Assert.assertEquals("Missing argument for options\n" + createApplicationCLIHelpMessage(),sysOutStream.toString());
  sysOutStream.reset();
  NodeCLI nodeCLI=new NodeCLI();
  nodeCLI.setClient(client);
  nodeCLI.setSysOutPrintStream(sysOut);
  nodeCLI.setSysErrPrintStream(sysErr);
  result=nodeCLI.run(new String[]{"-status"});
  Assert.assertEquals(result,-1);
  Assert.assertEquals("Missing argument for options\n" + createNodeCLIHelpMessage(),sysOutStream.toString());
}
