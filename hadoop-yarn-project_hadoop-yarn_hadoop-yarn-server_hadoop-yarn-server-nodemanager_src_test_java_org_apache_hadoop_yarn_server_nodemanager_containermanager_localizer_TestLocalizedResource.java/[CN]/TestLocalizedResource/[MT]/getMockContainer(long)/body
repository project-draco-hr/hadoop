  when(appAttemptId.getAttemptId()).thenReturn(0);
  ContainerId container=mock(ContainerId.class);
  when(container.getContainerId()).thenReturn(id);
  when(container.getApplicationAttemptId()).thenReturn(appAttemptId);
  return container;
}
