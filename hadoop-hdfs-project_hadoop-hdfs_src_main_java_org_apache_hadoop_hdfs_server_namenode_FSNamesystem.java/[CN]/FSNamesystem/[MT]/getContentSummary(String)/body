{
  checkOperation(OperationCategory.READ);
  readLock();
  boolean success=true;
  try {
    checkOperation(OperationCategory.READ);
    return FSDirStatAndListingOp.getContentSummary(dir,src);
  }
 catch (  AccessControlException ace) {
    success=false;
    throw ace;
  }
 finally {
    readUnlock();
    logAuditEvent(success,"contentSummary",src);
  }
}
