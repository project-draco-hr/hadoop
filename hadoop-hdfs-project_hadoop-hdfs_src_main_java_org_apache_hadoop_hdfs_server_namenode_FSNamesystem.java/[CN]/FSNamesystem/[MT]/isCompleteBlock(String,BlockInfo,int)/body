{
  if (!b.isComplete()) {
    final BlockInfoUnderConstruction uc=(BlockInfoUnderConstruction)b;
    final int numNodes=b.numNodes();
    LOG.info("BLOCK* " + b + " is not COMPLETE (ucState = "+ uc.getBlockUCState()+ ", replication# = "+ numNodes+ (numNodes < minRepl ? " < " : " >= ")+ " minimum = "+ minRepl+ ") in file "+ src);
    return false;
  }
  return true;
}
