{
  checkOperation(OperationCategory.READ);
  boolean success=false;
  readLock();
  try {
    checkOperation(OperationCategory.READ);
    final AclStatus ret=FSDirAclOp.getAclStatus(dir,src);
    success=true;
    return ret;
  }
  finally {
    readUnlock();
    logAuditEvent(success,"getAclStatus",src);
  }
}
