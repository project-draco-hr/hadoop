{
  checkOperation(OperationCategory.READ);
  final AclStatus ret;
  readLock();
  try {
    checkOperation(OperationCategory.READ);
    ret=FSDirAclOp.getAclStatus(dir,src);
  }
 catch (  AccessControlException ace) {
    logAuditEvent(false,"getAclStatus",src);
    throw ace;
  }
 finally {
    readUnlock();
  }
  logAuditEvent(true,"getAclStatus",src);
  return ret;
}
