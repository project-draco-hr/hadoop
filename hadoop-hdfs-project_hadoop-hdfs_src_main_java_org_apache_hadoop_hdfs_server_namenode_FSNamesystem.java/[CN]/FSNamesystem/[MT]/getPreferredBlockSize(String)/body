{
  FSPermissionChecker pc=getPermissionChecker();
  checkOperation(OperationCategory.READ);
  readLock();
  try {
    checkOperation(OperationCategory.READ);
    if (isPermissionEnabled) {
      checkTraverse(pc,filename);
    }
    return dir.getPreferredBlockSize(filename);
  }
  finally {
    readUnlock();
  }
}
