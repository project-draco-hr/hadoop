{
  FileStatus status=null;
  if (stat != null) {
    Path symlink=stat.isSymlink() ? new Path(stat.getSymlink()) : null;
    Path path=dst != null ? new Path(dst) : new Path(src);
    status=new FileStatus(stat.getLen(),stat.isDir(),stat.getReplication(),stat.getBlockSize(),stat.isLazyPersist(),stat.getModificationTime(),stat.getAccessTime(),stat.getPermission(),stat.getOwner(),stat.getGroup(),symlink,path);
  }
  for (  AuditLogger logger : auditLoggers) {
    if (logger instanceof HdfsAuditLogger) {
      HdfsAuditLogger hdfsLogger=(HdfsAuditLogger)logger;
      hdfsLogger.logAuditEvent(succeeded,ugi.toString(),addr,cmd,src,dst,status,ugi,dtSecretManager);
    }
 else {
      logger.logAuditEvent(succeeded,ugi.toString(),addr,cmd,src,dst,status);
    }
  }
}
