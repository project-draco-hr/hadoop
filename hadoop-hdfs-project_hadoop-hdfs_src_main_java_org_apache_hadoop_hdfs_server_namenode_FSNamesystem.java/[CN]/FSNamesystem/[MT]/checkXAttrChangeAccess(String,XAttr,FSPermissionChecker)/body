{
  if (isPermissionEnabled && xAttr.getNameSpace() == XAttr.NameSpace.USER) {
    final INode inode=dir.getINode(src);
    if (inode != null && inode.isDirectory() && inode.getFsPermission().getStickyBit()) {
      if (!pc.isSuperUser()) {
        checkOwner(pc,src);
      }
    }
 else {
      checkPathAccess(pc,src,FsAction.WRITE);
    }
  }
}
