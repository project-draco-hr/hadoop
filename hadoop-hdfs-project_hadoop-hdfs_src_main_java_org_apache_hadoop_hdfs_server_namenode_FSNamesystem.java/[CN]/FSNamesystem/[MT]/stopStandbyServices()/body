{
  LOG.info("Stopping services started for standby state");
  if (standbyCheckpointer != null) {
    standbyCheckpointer.stop();
  }
  if (editLogTailer != null) {
    editLogTailer.stop();
  }
  if (dir != null && getFSImage() != null && getFSImage().editLog != null) {
    getFSImage().editLog.close();
  }
}
