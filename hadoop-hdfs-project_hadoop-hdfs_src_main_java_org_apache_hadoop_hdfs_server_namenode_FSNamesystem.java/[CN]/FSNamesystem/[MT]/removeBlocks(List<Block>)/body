{
  int start=0;
  int end=0;
  while (start < blocks.size()) {
    end=BLOCK_DELETION_INCREMENT + start;
    end=end > blocks.size() ? blocks.size() : end;
    writeLock();
    try {
      for (int i=start; i < end; i++) {
        blockManager.removeBlock(blocks.get(i));
      }
    }
  finally {
      writeUnlock();
    }
    start=end;
  }
}
