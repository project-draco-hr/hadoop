{
  try {
    return startFileInt(src,permissions,holder,clientMachine,flag,createParent,replication,blockSize);
  }
 catch (  AccessControlException e) {
    if (isAuditEnabled() && isExternalInvocation()) {
      logAuditEvent(false,UserGroupInformation.getCurrentUser(),getRemoteIp(),"create",src,null,null);
    }
    throw e;
  }
}
