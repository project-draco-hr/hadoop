{
  try {
    startFileInt(src,permissions,holder,clientMachine,flag,createParent,replication,blockSize);
  }
 catch (  AccessControlException e) {
    if (auditLog.isInfoEnabled() && isExternalInvocation()) {
      logAuditEvent(false,UserGroupInformation.getCurrentUser(),Server.getRemoteIp(),"create",src,null,null);
    }
    throw e;
  }
}
