{
  if (dir.getInode(file.getId()) == null) {
    return true;
  }
  INode tmpChild=file;
  INodeDirectory tmpParent=file.getParent();
  while (true) {
    if (tmpParent == null || tmpParent.searchChildren(tmpChild.getLocalNameBytes()) < 0) {
      return true;
    }
    if (tmpParent.isRoot()) {
      break;
    }
    tmpChild=tmpParent;
    tmpParent=tmpParent.getParent();
  }
  if (file.isWithSnapshot() && file.getFileWithSnapshotFeature().isCurrentFileDeleted()) {
    return true;
  }
  return false;
}
