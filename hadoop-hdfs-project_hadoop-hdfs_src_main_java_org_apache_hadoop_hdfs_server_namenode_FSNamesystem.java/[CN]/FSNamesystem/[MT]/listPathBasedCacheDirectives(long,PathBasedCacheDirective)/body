{
  checkOperation(OperationCategory.READ);
  final FSPermissionChecker pc=isPermissionEnabled ? getPermissionChecker() : null;
  BatchedListEntries<PathBasedCacheDirective> results;
  readLock();
  boolean success=false;
  try {
    checkOperation(OperationCategory.READ);
    results=cacheManager.listPathBasedCacheDirectives(startId,filter,pc);
    success=true;
  }
  finally {
    readUnlock();
    if (isAuditEnabled() && isExternalInvocation()) {
      logAuditEvent(success,"listPathBasedCacheDirectives",null,null,null);
    }
  }
  return results;
}
