{
  writeLock();
  try {
    if (!isInSafeMode()) {
      NameNode.stateChangeLog.info("STATE* Safe mode is already OFF");
      return;
    }
    if (blockManager.leaveSafeMode(force)) {
      setManualAndResourceLowSafeMode(false,false);
      startSecretManagerIfNecessary();
    }
  }
  finally {
    writeUnlock();
  }
}
