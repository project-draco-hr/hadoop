{
  FSPermissionChecker pc=getPermissionChecker();
  try {
    return getBlockLocationsInt(pc,src,offset,length,doAccessTime,needBlockToken,checkSafeMode);
  }
 catch (  AccessControlException e) {
    if (isAuditEnabled() && isExternalInvocation()) {
      logAuditEvent(false,UserGroupInformation.getCurrentUser(),getRemoteIp(),"open",src,null,null);
    }
    throw e;
  }
}
