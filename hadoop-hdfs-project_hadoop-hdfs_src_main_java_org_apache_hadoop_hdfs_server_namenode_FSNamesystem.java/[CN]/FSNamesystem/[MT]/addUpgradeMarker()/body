{
  checkSuperuserPrivilege();
  checkOperation(OperationCategory.WRITE);
  writeLock();
  try {
    checkOperation(OperationCategory.WRITE);
    getEditLog().logUpgradeMarker();
  }
  finally {
    writeUnlock();
  }
  getEditLog().logSync();
  if (auditLog.isInfoEnabled() && isExternalInvocation()) {
    logAuditEvent(true,"upgrade",null,null,null);
  }
}
