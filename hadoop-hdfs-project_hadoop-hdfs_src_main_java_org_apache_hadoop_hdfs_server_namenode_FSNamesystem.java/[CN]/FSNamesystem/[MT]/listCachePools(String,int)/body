{
  final FSPermissionChecker pc=getPermissionChecker();
  List<CachePoolInfo> results;
  checkOperation(OperationCategory.READ);
  readLock();
  try {
    checkOperation(OperationCategory.READ);
    results=cacheManager.listCachePools(pc,prevKey,maxRepliesPerRequest);
  }
  finally {
    readUnlock();
  }
  return results;
}
