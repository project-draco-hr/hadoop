{
  if (!isRollingUpgrade()) {
    throw new RollingUpgradeException("Failed to finalize rolling upgrade since there is no rolling upgrade in progress.");
  }
  final long startTime=rollingUpgradeInfo.getStartTime();
  rollingUpgradeInfo=null;
  return new RollingUpgradeInfo(blockPoolId,true,startTime,finalizeTime);
}
