{
  checkOperation(OperationCategory.READ);
  List<XAttr> fsXattrs;
  readLock();
  try {
    checkOperation(OperationCategory.READ);
    fsXattrs=FSDirXAttrOp.getXAttrs(dir,src,xAttrs);
  }
 catch (  AccessControlException e) {
    logAuditEvent(false,"getXAttrs",src);
    throw e;
  }
 finally {
    readUnlock();
  }
  logAuditEvent(true,"getXAttrs",src);
  return fsXattrs;
}
