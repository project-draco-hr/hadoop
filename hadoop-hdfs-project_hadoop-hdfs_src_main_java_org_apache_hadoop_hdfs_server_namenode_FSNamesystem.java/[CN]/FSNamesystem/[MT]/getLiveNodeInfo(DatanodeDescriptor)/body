{
  return ImmutableMap.<String,Object>builder().put("infoAddr",node.getInfoAddr()).put("infoSecureAddr",node.getInfoSecureAddr()).put("xferaddr",node.getXferAddr()).put("lastContact",getLastContact(node)).put("usedSpace",getDfsUsed(node)).put("adminState",node.getAdminState().toString()).put("nonDfsUsedSpace",node.getNonDfsUsed()).put("capacity",node.getCapacity()).put("numBlocks",node.numBlocks()).put("version",node.getSoftwareVersion()).put("used",node.getDfsUsed()).put("remaining",node.getRemaining()).put("blockScheduled",node.getBlocksScheduled()).put("blockPoolUsed",node.getBlockPoolUsed()).put("blockPoolUsedPercent",node.getBlockPoolUsedPercent()).put("volfails",node.getVolumeFailures()).build();
}
