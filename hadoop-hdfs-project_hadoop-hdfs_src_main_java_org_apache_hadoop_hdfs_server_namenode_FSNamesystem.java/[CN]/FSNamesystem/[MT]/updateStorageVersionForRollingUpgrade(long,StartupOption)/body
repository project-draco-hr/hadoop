{
  boolean rollingStarted=RollingUpgradeStartupOption.STARTED.matches(startOpt) && layoutVersion > HdfsConstants.NAMENODE_LAYOUT_VERSION;
  boolean rollingRollback=RollingUpgradeStartupOption.ROLLBACK.matches(startOpt);
  if (rollingRollback || rollingStarted) {
    fsImage.updateStorageVersion();
  }
}
