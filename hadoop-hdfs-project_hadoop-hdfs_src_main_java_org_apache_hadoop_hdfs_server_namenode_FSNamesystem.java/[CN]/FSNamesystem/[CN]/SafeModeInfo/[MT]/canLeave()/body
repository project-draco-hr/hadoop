{
  if (reached == 0) {
    return false;
  }
  if (monotonicNow() - reached < extension) {
    reportStatus("STATE* Safe mode ON, in safe mode extension.",false);
    return false;
  }
  if (needEnter()) {
    reportStatus("STATE* Safe mode ON, thresholds not met.",false);
    return false;
  }
  return true;
}
