{
  boolean assertsOn=false;
  assert assertsOn=true;
  if (!assertsOn)   return;
  if (blockTotal == -1 && blockSafe == -1) {
    return;
  }
  int activeBlocks=blockManager.getActiveBlockCount();
  if ((blockTotal != activeBlocks) && !(blockSafe >= 0 && blockSafe <= blockTotal)) {
    throw new AssertionError(" SafeMode: Inconsistent filesystem state: " + "SafeMode data: blockTotal=" + blockTotal + " blockSafe="+ blockSafe+ "; "+ "BlockManager data: active="+ activeBlocks);
  }
}
