{
  try {
    if (!isExternalClient && (zkClient != null)) {
      zkClient.close();
    }
    if (seqCounter != null) {
      seqCounter.close();
    }
    if (keyCache != null) {
      keyCache.close();
    }
    if (tokenCache != null) {
      tokenCache.close();
    }
  }
 catch (  Exception e) {
    LOG.error("Could not stop Curator Framework",e);
  }
  super.stopThreads();
}
