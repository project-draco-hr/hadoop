{
  if (available > 0 || remaining <= 0) {
    return;
  }
  int xorlen=(int)Math.min(remaining,bufSize);
  readExact(streams[0],xor,xorlen);
  for (int i=1; i < streams.length; i++) {
    readExact(streams[i],buf,xorlen);
    for (int j=0; j < xorlen; j++) {
      xor[j]^=buf[j];
    }
  }
  remaining-=xorlen;
  available=xorlen;
  readPos=0;
  readPos=0;
}
