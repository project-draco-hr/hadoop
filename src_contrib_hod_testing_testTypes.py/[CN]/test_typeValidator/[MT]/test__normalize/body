def test__normalize(self):
    tmpdir = tempfile.mkdtemp(dir=self.originalDir)
    oldWd = os.getcwd()
    os.chdir('/')
    tmpdirName = re.sub('.*/', '', tmpdir)
    (type, originalVal, normalizedVal) = ('file', tmpdirName, os.path.join(self.originalDir, tmpdirName))
    assert (self.validator.normalize(type, originalVal) == normalizedVal)
    os.chdir(oldWd)
    os.rmdir(tmpdir)
    pass
