{
  OpReadBlockProto proto=OpReadBlockProto.parseFrom(vintPrefixed(in));
  ExtendedBlock b=fromProto(proto.getHeader().getBaseHeader().getBlock());
  Token<BlockTokenIdentifier> token=fromProto(proto.getHeader().getBaseHeader().getToken());
  opReadBlock(in,b,proto.getOffset(),proto.getLen(),proto.getHeader().getClientName(),token);
}
