def test_replacement(self):
    file = tempfile.NamedTemporaryFile()
    file.write('Contents go here')
    file.flush()
    self.assertEqual('Contents go here', InstanceUserData(file.name, {}).read())
    self.assertEqual('Contents were here', InstanceUserData(file.name, {'go': 'were', }).read())
    self.assertEqual('Contents  here', InstanceUserData(file.name, {'go': None, }).read())
    file.close()
