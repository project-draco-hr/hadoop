{
  wl.checkWriteLock();
  assert(syncLatch != null);
  try {
    syncLatch.await();
  }
 catch (  InterruptedException ie) {
    throw new IOException("Interrupted waiting on latch",ie);
  }
  if (transmitResult.get() != BKException.Code.OK) {
    throw new IOException("Failed to write to bookkeeper; Error is (" + transmitResult.get() + ") "+ BKException.getMessage(transmitResult.get()));
  }
  syncLatch=null;
}
