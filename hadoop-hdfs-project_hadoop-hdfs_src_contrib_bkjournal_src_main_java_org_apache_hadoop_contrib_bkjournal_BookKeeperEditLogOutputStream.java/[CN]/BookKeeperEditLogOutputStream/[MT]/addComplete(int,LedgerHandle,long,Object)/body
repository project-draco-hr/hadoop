{
synchronized (this) {
    outstandingRequests.decrementAndGet();
    if (!transmitResult.compareAndSet(BKException.Code.OK,rc)) {
      LOG.warn("Tried to set transmit result to (" + rc + ") \""+ BKException.getMessage(rc)+ "\""+ " but is already ("+ transmitResult.get()+ ") \""+ BKException.getMessage(transmitResult.get())+ "\"");
    }
    CountDownLatch l=syncLatch;
    if (l != null) {
      l.countDown();
    }
  }
}
