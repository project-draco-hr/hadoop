{
  this.conf=conf;
  YarnRPC rpc=YarnRPC.create(this.conf);
  InetSocketAddress rmAddress=NetUtils.createSocketAddr(this.conf.get(YarnConfiguration.RM_ADDRESS,YarnConfiguration.DEFAULT_RM_ADDRESS));
  LOG.info("Connecting to ResourceManager at " + rmAddress);
  applicationsManager=(ClientRMProtocol)rpc.getProxy(ClientRMProtocol.class,rmAddress,this.conf);
  LOG.info("Connected to ResourceManager at " + rmAddress);
}
