{
  if (favoredNodes == null) {
    return null;
  }
 else {
    boolean[] pinnings=new boolean[nodes.length];
    for (int i=0; i < nodes.length; i++) {
      pinnings[i]=false;
      for (int j=0; j < favoredNodes.length; j++) {
        if (nodes[i].getXferAddrWithHostname().equals(favoredNodes[j])) {
          pinnings[i]=true;
          break;
        }
      }
    }
    return pinnings;
  }
}
