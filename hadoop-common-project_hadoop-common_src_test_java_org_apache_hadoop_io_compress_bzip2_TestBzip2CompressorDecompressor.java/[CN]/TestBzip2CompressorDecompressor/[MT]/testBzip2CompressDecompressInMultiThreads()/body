{
  MultithreadedTestUtil.TestContext ctx=new MultithreadedTestUtil.TestContext();
  for (int i=0; i < 10; i++) {
    ctx.addThread(new MultithreadedTestUtil.TestingThread(ctx){
      @Override public void doWork() throws Exception {
        testCompressDecompress();
      }
    }
);
  }
  ctx.startThreads();
  ctx.waitFor(60000);
}
