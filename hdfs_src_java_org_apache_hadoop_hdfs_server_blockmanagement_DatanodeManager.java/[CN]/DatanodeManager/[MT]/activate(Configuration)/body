{
  this.decommissionthread=new Daemon(new DecommissionManager(namesystem).new Monitor(conf.getInt(DFSConfigKeys.DFS_NAMENODE_DECOMMISSION_INTERVAL_KEY,DFSConfigKeys.DFS_NAMENODE_DECOMMISSION_INTERVAL_DEFAULT),conf.getInt(DFSConfigKeys.DFS_NAMENODE_DECOMMISSION_NODES_PER_INTERVAL_KEY,DFSConfigKeys.DFS_NAMENODE_DECOMMISSION_NODES_PER_INTERVAL_DEFAULT)));
  decommissionthread.start();
}
