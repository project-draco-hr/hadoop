{
  BudgetQueue budgetQueue=queueCache.get(queue);
  if (budgetQueue == null) {
    return "";
  }
  return "<budget>" + Float.toString(budgetQueue.budget) + "</budget>\n"+ "<spending>"+ Float.toString(budgetQueue.spending)+ "</spending>\n"+ "<used>"+ Integer.toString(budgetQueue.used)+ "</used>\n"+ "<pending>"+ budgetQueue.pending+ "</pending>\n";
}
