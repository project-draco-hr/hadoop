{
  int ulimit=getChildUlimit(conf);
  if (ulimit <= 0) {
    return null;
  }
  List<String> setup=null;
  String[] ulimitCmd=Shell.getUlimitMemoryCommand(ulimit);
  if (ulimitCmd != null) {
    setup=new ArrayList<String>();
    for (    String arg : ulimitCmd) {
      setup.add(arg);
    }
  }
  return setup;
}
