{
  TBODY<TABLE<Hamlet>> tbody=html.table("#nodes").thead().tr().th(".rack","Rack").th(".nodeaddress","Node Address").th(".nodehttpaddress","Node HTTP Address").th(".healthStatus","Health-status").th(".lastHealthUpdate","Last health-update").th(".healthReport","Health-report").th(".containers","Containers")._()._().tbody();
  for (  RMNode ni : this.rmContext.getRMNodes().values()) {
    NodeHealthStatus health=ni.getNodeHealthStatus();
    tbody.tr().td(ni.getRackName()).td(String.valueOf(ni.getNodeID().toString())).td().a("http://" + ni.getHttpAddress(),ni.getHttpAddress())._().td(health.getIsNodeHealthy() ? "Healthy" : "Unhealthy").td(Times.format(health.getLastHealthReportTime())).td(String.valueOf(health.getHealthReport())).td("n/a")._();
  }
  tbody._()._();
}
