{
  if (!(e instanceof IOException)) {
    return new IOException(e);
  }
  final IOException ioe=(IOException)e;
  if (!(ioe instanceof RemoteException)) {
    return ioe;
  }
  final RemoteException re=(RemoteException)ioe;
  return re.unwrapRemoteException(AccessControlException.class,InvalidToken.class,AuthenticationException.class,AuthorizationException.class,FileAlreadyExistsException.class,FileNotFoundException.class,ParentNotDirectoryException.class,UnresolvedPathException.class,SafeModeException.class,DSQuotaExceededException.class,NSQuotaExceededException.class);
}
