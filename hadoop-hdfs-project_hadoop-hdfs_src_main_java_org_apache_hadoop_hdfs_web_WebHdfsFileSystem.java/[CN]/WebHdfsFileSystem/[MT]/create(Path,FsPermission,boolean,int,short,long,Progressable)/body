{
  statistics.incrementWriteOps(1);
  final HttpOpParam.Op op=PutOpParam.Op.CREATE;
  return new FsPathRunner(op,f,new PermissionParam(applyUMask(permission)),new OverwriteParam(overwrite),new BufferSizeParam(bufferSize),new ReplicationParam(replication),new BlockSizeParam(blockSize)).run().write(bufferSize);
}
