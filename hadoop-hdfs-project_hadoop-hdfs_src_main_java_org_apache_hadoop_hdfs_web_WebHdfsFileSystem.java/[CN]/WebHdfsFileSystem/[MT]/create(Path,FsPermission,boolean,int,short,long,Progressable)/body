{
  statistics.incrementWriteOps(1);
  final HttpOpParam.Op op=PutOpParam.Op.CREATE;
  final HttpURLConnection conn=httpConnect(op,f,new PermissionParam(applyUMask(permission)),new OverwriteParam(overwrite),new BufferSizeParam(bufferSize),new ReplicationParam(replication),new BlockSizeParam(blockSize));
  return write(op,conn,bufferSize);
}
