{
  final long elapsed;
  if (beginTracking != null && beginTracking.beginTime != Long.MIN_VALUE && endTracking != null && endTracking.endTime != Long.MIN_VALUE) {
    elapsed=endTracking.endTime - beginTracking.beginTime;
  }
 else   if (beginTracking != null && beginTracking.beginTime != Long.MIN_VALUE) {
    elapsed=Time.monotonicNow() - beginTracking.beginTime;
  }
 else {
    elapsed=0;
  }
  return Math.max(0,elapsed);
}
