{
  XDR request=new XDR();
  RpcCall.write(request,0x8000004c,Nfs3Constant.PROGRAM,Nfs3Constant.VERSION,Nfs3Constant.NFSPROC3.CREATE.getValue());
  request.writeInt(0);
  request.writeInt(0);
  request.writeInt(0);
  request.writeInt(0);
  SetAttr3 objAttr=new SetAttr3();
  CREATE3Request createReq=new CREATE3Request(new FileHandle("/"),"out-of-order-write" + System.currentTimeMillis(),0,objAttr,0);
  createReq.serialize(request);
  return request;
}
