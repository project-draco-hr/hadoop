{
  ensureInitialized();
  try {
    Set<String> result=new LinkedHashSet<String>(groups.getGroups(getShortUserName()));
    return result.toArray(new String[result.size()]);
  }
 catch (  IOException ie) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Failed to get groups for user " + getShortUserName() + " by "+ ie);
      LOG.trace("TRACE",ie);
    }
    return StringUtils.emptyStringArray;
  }
}
