{
  if (System.getProperty("java.vendor").contains("IBM")) {
    return windows ? (is64Bit ? "com.ibm.security.auth.module.Win64LoginModule" : "com.ibm.security.auth.module.NTLoginModule") : "com.ibm.security.auth.module.LinuxLoginModule";
  }
 else {
    return windows ? "com.sun.security.auth.module.NTLoginModule" : "com.sun.security.auth.module.UnixLoginModule";
  }
}
