{
  ensureInitialized();
  try {
    return groups.getGroups(getShortUserName());
  }
 catch (  IOException ie) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Failed to get groups for user " + getShortUserName() + " by "+ ie);
      LOG.trace("TRACE",ie);
    }
    return Collections.emptyList();
  }
}
