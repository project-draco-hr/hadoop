{
  for (long txid=1; txid <= MAX_TXID; txid++) {
    if (txid == BAD_TXID) {
      byte garbage[]={0x1,0x2,0x3};
      elos.writeRaw(garbage,0,garbage.length);
    }
 else {
      DeleteOp op;
      op=DeleteOp.getInstance(cache);
      op.setTransactionId(txid);
      op.setPath("/foo." + txid);
      op.setTimestamp(txid);
      elos.write(op);
    }
  }
}
