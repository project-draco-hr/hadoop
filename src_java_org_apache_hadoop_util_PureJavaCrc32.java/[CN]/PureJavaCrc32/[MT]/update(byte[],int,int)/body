{
  while (len > 7) {
    int c0=b[off++] ^ crc;
    int c1=b[off++] ^ (crc>>>=8);
    int c2=b[off++] ^ (crc>>>=8);
    int c3=b[off++] ^ (crc>>>=8);
    crc=(T8_7[c0 & 0xff] ^ T8_6[c1 & 0xff]) ^ (T8_5[c2 & 0xff] ^ T8_4[c3 & 0xff]);
    crc^=(T8_3[b[off++] & 0xff] ^ T8_2[b[off++] & 0xff]) ^ (T8_1[b[off++] & 0xff] ^ T8_0[b[off++] & 0xff]);
    len-=8;
  }
  while (len > 0) {
    crc=(crc >>> 8) ^ T8_0[(crc ^ b[off++]) & 0xff];
    len--;
  }
}
