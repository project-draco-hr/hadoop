{
  while (len > 3) {
    int c0=crc ^ b[off++];
    int c1=(crc>>>=8) ^ b[off++];
    int c2=(crc>>>=8) ^ b[off++];
    int c3=(crc>>>=8) ^ b[off++];
    crc=T4[c0 & 0xff] ^ T3[c1 & 0xff] ^ T2[c2 & 0xff]^ T1[c3 & 0xff];
    len-=4;
  }
  while (len > 0) {
    crc=(crc >>> 8) ^ T1[(crc ^ b[off++]) & 0xff];
    len--;
  }
}
