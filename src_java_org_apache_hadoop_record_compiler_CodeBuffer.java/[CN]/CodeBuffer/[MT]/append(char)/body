{
  if (endMarkers.contains(ch)) {
    level--;
  }
  if (firstChar) {
    for (int idx=0; idx < level; idx++) {
      for (int num=0; num < numSpaces; num++) {
        rawAppend(' ');
      }
    }
  }
  rawAppend(ch);
  firstChar=false;
  if (startMarkers.contains(ch)) {
    level++;
  }
  if (ch == '\n') {
    firstChar=true;
  }
}
