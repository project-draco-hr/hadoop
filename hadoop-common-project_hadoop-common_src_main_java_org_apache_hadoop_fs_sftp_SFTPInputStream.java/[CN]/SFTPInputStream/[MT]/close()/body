{
  if (closed) {
    return;
  }
  super.close();
  closed=true;
  if (!channel.isConnected()) {
    throw new IOException(E_CLIENT_NOTCONNECTED);
  }
  try {
    Session session=channel.getSession();
    channel.disconnect();
    session.disconnect();
  }
 catch (  JSchException e) {
    throw new IOException(StringUtils.stringifyException(e));
  }
}
