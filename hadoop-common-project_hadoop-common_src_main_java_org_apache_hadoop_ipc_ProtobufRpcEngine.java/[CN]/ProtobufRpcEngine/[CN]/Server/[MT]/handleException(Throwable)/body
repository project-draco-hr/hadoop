{
  HadoopRpcExceptionProto exception=HadoopRpcExceptionProto.newBuilder().setExceptionName(e.getClass().getName()).setStackTrace(StringUtils.stringifyException(e)).build();
  HadoopRpcResponseProto response=HadoopRpcResponseProto.newBuilder().setStatus(ResponseStatus.ERRROR).setException(exception).build();
  return new RpcResponseWritable(response);
}
