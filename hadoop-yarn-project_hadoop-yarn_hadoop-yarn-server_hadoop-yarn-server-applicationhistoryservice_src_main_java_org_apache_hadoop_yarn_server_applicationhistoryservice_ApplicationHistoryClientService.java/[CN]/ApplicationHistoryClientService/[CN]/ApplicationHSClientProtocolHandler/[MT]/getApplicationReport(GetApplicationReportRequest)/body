{
  ApplicationId applicationId=request.getApplicationId();
  try {
    GetApplicationReportResponse response=GetApplicationReportResponse.newInstance(history.getApplication(applicationId));
    return response;
  }
 catch (  IOException e) {
    String msg="Application with id '" + applicationId + "' doesn't exist in the history store.";
    LOG.error(msg,e);
    throw new ApplicationNotFoundException(msg);
  }
}
