{
  ContainerId containerId=request.getContainerId();
  try {
    GetContainerReportResponse response=GetContainerReportResponse.newInstance(history.getContainer(containerId));
    return response;
  }
 catch (  IOException e) {
    String msg="Container with id '" + containerId + "' doesn't exist in the history store.";
    LOG.error(msg,e);
    throw new ContainerNotFoundException(msg);
  }
}
