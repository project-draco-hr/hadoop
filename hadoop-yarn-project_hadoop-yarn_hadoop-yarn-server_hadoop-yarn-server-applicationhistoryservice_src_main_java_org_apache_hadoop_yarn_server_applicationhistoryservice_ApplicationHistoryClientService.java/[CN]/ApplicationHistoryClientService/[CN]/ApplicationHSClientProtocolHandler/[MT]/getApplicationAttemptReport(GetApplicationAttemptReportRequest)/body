{
  ApplicationAttemptId appAttemptId=request.getApplicationAttemptId();
  try {
    GetApplicationAttemptReportResponse response=GetApplicationAttemptReportResponse.newInstance(history.getApplicationAttempt(appAttemptId));
    return response;
  }
 catch (  IOException e) {
    String msg="ApplicationAttempt with id '" + appAttemptId + "' doesn't exist in the history store.";
    LOG.error(msg,e);
    throw new ApplicationAttemptNotFoundException(msg);
  }
}
