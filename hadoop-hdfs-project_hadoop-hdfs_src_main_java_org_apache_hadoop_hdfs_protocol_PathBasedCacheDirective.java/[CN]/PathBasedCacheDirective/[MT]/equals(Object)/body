{
  if (o == null) {
    return false;
  }
  if (getClass() != o.getClass()) {
    return false;
  }
  PathBasedCacheDirective other=(PathBasedCacheDirective)o;
  return new EqualsBuilder().append(getPath(),other.getPath()).append(getReplication(),other.getReplication()).append(getPool(),other.getPool()).isEquals();
}
