{
  if (!DFSUtil.isValidName(path.toUri().getPath())) {
    throw new InvalidPathNameError(this);
  }
  if (replication <= 0) {
    throw new IOException("Tried to request a cache replication " + "factor of " + replication + ", but that is less than 1.");
  }
  if (pool.isEmpty()) {
    throw new InvalidPoolNameError(this);
  }
}
