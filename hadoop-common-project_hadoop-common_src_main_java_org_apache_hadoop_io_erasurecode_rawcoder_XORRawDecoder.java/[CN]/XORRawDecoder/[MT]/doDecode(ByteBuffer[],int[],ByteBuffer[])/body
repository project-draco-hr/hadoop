{
  resetBuffer(outputs[0]);
  int bufSize=getChunkSize();
  int erasedIdx=erasedIndexes[0];
  for (int i=0; i < inputs.length; i++) {
    if (i == erasedIdx) {
      continue;
    }
    for (int j=0; j < bufSize; j++) {
      outputs[0].put(j,(byte)(outputs[0].get(j) ^ inputs[i].get(j)));
    }
  }
}
