{
  Path fileAbs=new Path("/doesNotExist");
  try {
    fc.getFileLinkStatus(fileAbs);
    fail("Got FileStatus for non-existant file");
  }
 catch (  FileNotFoundException f) {
  }
  try {
    fc.getLinkTarget(fileAbs);
    fail("Got link target for non-existant file");
  }
 catch (  FileNotFoundException f) {
  }
}
