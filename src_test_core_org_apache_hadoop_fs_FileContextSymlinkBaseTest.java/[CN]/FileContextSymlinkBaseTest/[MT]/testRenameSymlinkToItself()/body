{
  Path link=new Path(testBaseDir1(),"linkToFile1");
  fc.createSymlink(new Path("/doestNotExist"),link,false);
  try {
    fc.rename(link,link);
  }
 catch (  IOException e) {
    assertTrue(unwrapException(e) instanceof FileAlreadyExistsException);
  }
  try {
    fc.rename(link,link,Rename.OVERWRITE);
  }
 catch (  IOException e) {
    assertTrue(unwrapException(e) instanceof FileAlreadyExistsException);
  }
}
