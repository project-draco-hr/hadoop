{
  byte[][] bytesArr=new byte[chunks.length][];
  ByteBuffer buffer;
  for (int i=0; i < chunks.length; i++) {
    buffer=chunks[i].getBuffer();
    if (buffer.hasArray()) {
      bytesArr[i]=buffer.array();
    }
 else {
      bytesArr[i]=new byte[buffer.remaining()];
      buffer.mark();
      buffer.get(bytesArr[i]);
      buffer.reset();
    }
  }
  return bytesArr;
}
