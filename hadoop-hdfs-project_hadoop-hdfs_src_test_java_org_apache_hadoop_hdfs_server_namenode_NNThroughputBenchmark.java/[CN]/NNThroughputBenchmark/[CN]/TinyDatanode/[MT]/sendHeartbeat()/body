{
  DatanodeCommand[] cmds=nameNode.sendHeartbeat(dnRegistration,DF_CAPACITY,DF_USED,DF_CAPACITY - DF_USED,DF_USED,0,0,0);
  if (cmds != null) {
    for (    DatanodeCommand cmd : cmds) {
      if (LOG.isDebugEnabled()) {
        LOG.debug("sendHeartbeat Name-node reply: " + cmd.getAction());
      }
    }
  }
}
