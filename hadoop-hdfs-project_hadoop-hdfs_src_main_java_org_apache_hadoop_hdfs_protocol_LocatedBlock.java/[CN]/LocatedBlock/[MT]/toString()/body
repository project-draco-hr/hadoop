{
  return getClass().getSimpleName() + "{" + b+ "; getBlockSize()="+ getBlockSize()+ "; corrupt="+ corrupt+ "; offset="+ offset+ "; locs="+ Arrays.asList(locs)+ "; storageIDs="+ (storageIDs != null ? Arrays.asList(storageIDs) : null)+ "; storageTypes="+ (storageTypes != null ? Arrays.asList(storageTypes) : null)+ "}";
}
