{
  List<DatanodeInfo> cachedList=Lists.newArrayList(cachedLocs);
  if (cachedList.contains(loc)) {
    return;
  }
  for (  DatanodeInfoWithStorage di : locs) {
    if (loc.equals(di)) {
      cachedList.add(di);
      cachedLocs=cachedList.toArray(cachedLocs);
      return;
    }
  }
  cachedList.add(loc);
  cachedLocs=cachedList.toArray(cachedLocs);
}
