{
  this.b=b;
  this.offset=startOffset;
  this.corrupt=corrupt;
  if (locs == null) {
    this.locs=EMPTY_LOCS;
  }
 else {
    this.locs=locs;
  }
  Preconditions.checkArgument(cachedLocs != null,"cachedLocs should not be null, use a different constructor");
  if (cachedLocs.length == 0) {
    this.cachedLocs=EMPTY_LOCS;
  }
 else {
    this.cachedLocs=cachedLocs;
  }
}
