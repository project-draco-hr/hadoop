{
  TaskHistoryLog task=new TaskHistoryLog().parse(tokens);
  if (task.TASKID == null) {
    LOG.error("TASKID = NULL for job " + JOBID);
    return;
  }
  if (tasks == null)   tasks=new HashMap<String,TaskHistoryLog>((int)(TOTAL_MAPS + TOTAL_REDUCES));
  TaskHistoryLog existing=tasks.get(task.TASKID);
  if (existing == null)   tasks.put(task.TASKID,task);
 else   existing.updateWith(task);
}
