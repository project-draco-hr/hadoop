{
  StringTokenizer tokens=new StringTokenizer(line);
  if (!tokens.hasMoreTokens())   return;
  String what=tokens.nextToken();
  if (what.equals("Job"))   updateJob(tokens);
 else   if (what.equals("Task"))   updateTask(tokens);
 else   if (what.indexOf("Attempt") >= 0)   updateTaskAttempt(tokens);
}
