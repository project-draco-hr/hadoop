{
  if (attempt.TASK_ATTEMPT_ID == null) {
    LOG.error("Unexpected TASK_ATTEMPT_ID = null for task " + TASKID);
    return;
  }
  if (attempts == null)   attempts=new HashMap<String,TaskAttemptHistoryLog>();
  TaskAttemptHistoryLog existing=attempts.get(attempt.TASK_ATTEMPT_ID);
  if (existing == null)   attempts.put(attempt.TASK_ATTEMPT_ID,attempt);
 else   existing.updateWith(attempt);
  if (attempt.START_TIME > 0 && (this.START_TIME == 0 || this.START_TIME > attempt.START_TIME))   START_TIME=attempt.START_TIME;
}
