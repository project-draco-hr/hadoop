{
  if (currentParseState == ParseState.PARSING_COMPAREOP || currentParseState == ParseState.PARSING_VALUE) {
    offset++;
    while (offset < exprLength && expr.charAt(offset) == TimelineParseConstants.SPACE_CHAR) {
      offset++;
    }
    if (offset == exprLength) {
      throw new TimelineParseException("Invalid " + exprName + "expression");
    }
    if (expr.charAt(offset) == TimelineParseConstants.OPENING_BRACKET_CHAR) {
      handleOpeningBracketChar(true);
    }
 else {
      throw new TimelineParseException("Invalid " + exprName + "expression");
    }
  }
 else {
    throw new TimelineParseException("Encountered unexpected not(!) char " + "while parsing " + exprName + ".");
  }
}
