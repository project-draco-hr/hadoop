{
  LCEResourcesHandler handler=ReflectionUtils.newInstance(conf.getClass(YarnConfiguration.NM_LINUX_CONTAINER_RESOURCES_HANDLER,DefaultLCEResourcesHandler.class,LCEResourcesHandler.class),conf);
  if (handler instanceof CgroupsLCEResourcesHandler) {
    handler=ReflectionUtils.newInstance(DefaultLCEResourcesHandler.class,conf);
  }
  handler.setConf(conf);
  return handler;
}
