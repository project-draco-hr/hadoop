{
  super.setConf(conf);
  containerExecutorExe=getContainerExecutorExecutablePath(conf);
  resourcesHandler=ReflectionUtils.newInstance(conf.getClass(YarnConfiguration.NM_LINUX_CONTAINER_RESOURCES_HANDLER,DefaultLCEResourcesHandler.class,LCEResourcesHandler.class),conf);
  resourcesHandler.setConf(conf);
  if (conf.get(YarnConfiguration.NM_CONTAINER_EXECUTOR_SCHED_PRIORITY) != null) {
    containerSchedPriorityIsSet=true;
    containerSchedPriorityAdjustment=conf.getInt(YarnConfiguration.NM_CONTAINER_EXECUTOR_SCHED_PRIORITY,YarnConfiguration.DEFAULT_NM_CONTAINER_EXECUTOR_SCHED_PRIORITY);
  }
}
