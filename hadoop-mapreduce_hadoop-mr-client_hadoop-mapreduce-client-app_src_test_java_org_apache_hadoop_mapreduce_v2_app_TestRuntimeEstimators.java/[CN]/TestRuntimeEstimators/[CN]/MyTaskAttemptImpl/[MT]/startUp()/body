{
  startMockTime=clock.getTime();
  overridingState=null;
  slotsInUse.addAndGet(taskTypeSlots(myAttemptID.getTaskId().getTaskType()));
  System.out.println("TLTRE.MyTaskAttemptImpl.startUp starting " + getID());
