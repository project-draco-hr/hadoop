{
  try {
    readLock.lock();
    List<NodeLabel> infos=new ArrayList<NodeLabel>();
    for (    Entry<String,NodeLabel> entry : labelCollections.entrySet()) {
      NodeLabel label=entry.getValue();
      infos.add(label.getCopy());
    }
    Collections.sort(infos);
    return infos;
  }
  finally {
    readLock.unlock();
  }
}
