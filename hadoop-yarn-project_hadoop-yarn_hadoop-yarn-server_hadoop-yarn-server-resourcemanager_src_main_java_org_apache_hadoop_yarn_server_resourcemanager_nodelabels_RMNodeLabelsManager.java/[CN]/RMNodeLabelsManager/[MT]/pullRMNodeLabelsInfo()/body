{
  try {
    readLock.lock();
    List<RMNodeLabel> infos=new ArrayList<RMNodeLabel>();
    for (    Entry<String,RMNodeLabel> entry : labelCollections.entrySet()) {
      RMNodeLabel label=entry.getValue();
      infos.add(label.getCopy());
    }
    Collections.sort(infos);
    return infos;
  }
  finally {
    readLock.unlock();
  }
}
