{
  Preconditions.checkArgument(getConf() != null,"null configuration");
  registry=RegistryOperationsFactory.createInstance(new YarnConfiguration(getConf()));
  registry.start();
  if (args.length > 0) {
    if (args[0].equals("ls")) {
      return ls(args);
    }
 else     if (args[0].equals("resolve")) {
      return resolve(args);
    }
 else     if (args[0].equals("bind")) {
      return bind(args);
    }
 else     if (args[0].equals("mknode")) {
      return mknode(args);
    }
 else     if (args[0].equals("rm")) {
      return rm(args);
    }
  }
  return usageError("Invalid command: " + args[0],USAGE);
}
