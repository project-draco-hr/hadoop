{
  ResponseInfo ri=info("\'" + qinfo.getQueueName() + "\' Queue Status")._("Used Resources:",qinfo.getUsedResources().toString())._("Num Active Applications:",qinfo.getNumActiveApplications())._("Num Pending Applications:",qinfo.getNumPendingApplications())._("Min Resources:",qinfo.getMinResources().toString())._("Max Resources:",qinfo.getMaxResources().toString());
  int maxApps=qinfo.getMaxApplications();
  if (maxApps < Integer.MAX_VALUE) {
    ri._("Max Running Applications:",qinfo.getMaxApplications());
  }
  ri._(STEADY_FAIR_SHARE + ":",qinfo.getSteadyFairShare().toString());
  ri._(INSTANTANEOUS_FAIR_SHARE + ":",qinfo.getFairShare().toString());
  html._(InfoBlock.class);
  ri.clear();
}
