{
  this.params=params;
  if (ugiCache == null) {
synchronized (DataNodeUGIProvider.class) {
      if (ugiCache == null) {
        ugiCache=CacheBuilder.newBuilder().expireAfterAccess(conf.getInt(DFSConfigKeys.DFS_WEBHDFS_UGI_EXPIRE_AFTER_ACCESS_KEY,DFSConfigKeys.DFS_WEBHDFS_UGI_EXPIRE_AFTER_ACCESS_DEFAULT),TimeUnit.MILLISECONDS).build();
      }
    }
  }
}
