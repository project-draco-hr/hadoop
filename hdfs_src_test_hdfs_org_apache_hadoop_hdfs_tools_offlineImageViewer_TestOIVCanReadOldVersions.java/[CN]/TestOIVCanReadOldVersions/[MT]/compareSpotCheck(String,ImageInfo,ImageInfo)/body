{
  assertEquals("Version " + hadoopVersion + ": Same number of total blocks",expected.totalNumBlocks,generated.totalNumBlocks);
  assertEquals("Version " + hadoopVersion + ": Same total file size",expected.totalFileSize,generated.totalFileSize);
  assertEquals("Version " + hadoopVersion + ": Same total replication factor",expected.totalReplications,generated.totalReplications);
  assertEquals("Version " + hadoopVersion + ": One-to-one matching of path names",expected.pathNames,generated.pathNames);
}
