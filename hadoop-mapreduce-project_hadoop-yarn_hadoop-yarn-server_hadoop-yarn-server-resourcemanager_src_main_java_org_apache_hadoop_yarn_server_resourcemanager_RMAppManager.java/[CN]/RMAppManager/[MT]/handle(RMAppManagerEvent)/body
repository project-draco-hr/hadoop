{
  ApplicationId appID=event.getApplicationId();
  LOG.debug("RMAppManager processing event for " + appID + " of type "+ event.getType());
switch (event.getType()) {
case APP_COMPLETED:
{
      addCompletedApp(appID);
      checkAppNumCompletedLimit();
    }
  break;
case APP_SUBMIT:
{
  ApplicationSubmissionContext submissionContext=((RMAppManagerSubmitEvent)event).getSubmissionContext();
  submitApplication(submissionContext);
}
break;
default :
LOG.error("Invalid eventtype " + event.getType() + ". Ignoring!");
}
}
