{
  Credentials credentials=new Credentials();
  DataInputByteBuffer dibb=new DataInputByteBuffer();
  ByteBuffer tokens=application.getAMContainerSpec().getContainerTokens();
  if (tokens != null) {
    dibb.reset(tokens);
    credentials.readTokenStorageStream(dibb);
    tokens.rewind();
  }
  return credentials;
}
