{
  super.init(conf);
  try {
    parse();
    if (completedTasks.size() > 0) {
      recoveryMode=true;
      LOG.info("SETTING THE RECOVERY MODE TO TRUE. NO OF COMPLETED TASKS " + "TO RECOVER " + completedTasks.size());
      LOG.info("Job launch time " + jobInfo.getLaunchTime());
      clock.setTime(jobInfo.getLaunchTime());
    }
  }
 catch (  IOException e) {
    LOG.warn(e);
    LOG.warn("Could not parse the old history file. Aborting recovery. " + "Starting afresh.");
  }
}
