{
  if (overrideStart) {
    try {
      UserGroupInformation ugi=UserGroupInformation.getCurrentUser();
      String user=ugi.getShortUserName();
      this.credentials=ugi.getCredentials();
      stagingDirPath=MRApps.getStagingAreaDir(conf,user);
    }
 catch (    Exception e) {
      fail(e.getMessage());
    }
  }
 else {
    super.start();
  }
}
