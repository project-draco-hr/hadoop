{
  StringBuilder sb=new StringBuilder("[ ");
  for (  String m : methods) {
    try {
      Method method=event.getClass().getMethod(m);
      String value=method.invoke(event).toString();
      sb.append(method.getName()).append("=").append(value).append(" ");
    }
 catch (    Exception e) {
    }
  }
  sb.append("]");
  return sb.toString();
}
