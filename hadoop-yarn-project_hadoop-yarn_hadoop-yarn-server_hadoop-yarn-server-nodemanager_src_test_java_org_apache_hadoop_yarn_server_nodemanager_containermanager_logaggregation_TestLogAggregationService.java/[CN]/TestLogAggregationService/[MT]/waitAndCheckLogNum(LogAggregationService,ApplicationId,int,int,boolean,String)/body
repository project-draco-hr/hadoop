{
  int count=0;
  int logFiles=numOfLogsAvailable(logAggregationService,application,sizeLimited,lastLogFile);
  while ((logFiles != expectNum) && (count <= maxAttempts)) {
    Thread.sleep(500);
    count++;
    logFiles=numOfLogsAvailable(logAggregationService,application,sizeLimited,lastLogFile);
  }
  return (logFiles == expectNum);
}
