def test_bash_quote(self):
    self.assertEqual('', bash_quote(None))
    self.assertEqual("''", bash_quote(''))
    self.assertEqual("'a'", bash_quote('a'))
    self.assertEqual("'a b'", bash_quote('a b'))
    self.assertEqual("'a\x08'", bash_quote('a\x08'))
    self.assertEqual("'a '\\'' b'", bash_quote("a ' b"))
