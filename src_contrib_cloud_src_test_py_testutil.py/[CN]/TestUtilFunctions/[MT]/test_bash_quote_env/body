def test_bash_quote_env(self):
    self.assertEqual('', bash_quote_env(''))
    self.assertEqual('a', bash_quote_env('a'))
    self.assertEqual("a='b'", bash_quote_env('a=b'))
    self.assertEqual("a='b c'", bash_quote_env('a=b c'))
    self.assertEqual("a='b\\c'", bash_quote_env('a=b\\c'))
    self.assertEqual("a='b '\\'' c'", bash_quote_env("a=b ' c"))
