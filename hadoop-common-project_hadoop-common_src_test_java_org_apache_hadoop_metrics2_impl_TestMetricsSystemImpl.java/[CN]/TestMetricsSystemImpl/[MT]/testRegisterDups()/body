{
  MetricsSystem ms=new MetricsSystemImpl();
  TestSource ts1=new TestSource("ts1");
  TestSource ts2=new TestSource("ts2");
  ms.register("ts1","",ts1);
  MetricsSource s1=ms.getSource("ts1");
  assertNotNull(s1);
  ms.register("ts1","",ts2);
  MetricsSource s2=ms.getSource("ts1");
  assertNotNull(s2);
  assertNotSame(s1,s2);
  ms.shutdown();
}
