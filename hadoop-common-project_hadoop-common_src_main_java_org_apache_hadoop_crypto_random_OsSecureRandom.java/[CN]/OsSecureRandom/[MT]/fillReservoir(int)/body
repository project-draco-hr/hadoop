{
  if (pos >= reservoir.length - min) {
    try {
      if (stream == null) {
        stream=new FileInputStream(new File(randomDevPath));
      }
      IOUtils.readFully(stream,reservoir,0,reservoir.length);
    }
 catch (    IOException e) {
      throw new RuntimeException("failed to fill reservoir",e);
    }
    pos=0;
  }
}
