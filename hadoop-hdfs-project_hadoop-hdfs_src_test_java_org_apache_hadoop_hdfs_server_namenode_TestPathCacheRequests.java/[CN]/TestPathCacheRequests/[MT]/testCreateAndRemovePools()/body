{
  CachePoolInfo req=CachePoolInfo.newBuilder().setPoolName("pool1").setOwnerName("bob").setGroupName("bobgroup").setMode(new FsPermission((short)0755)).setWeight(150).build();
  CachePool pool=proto.addCachePool(req);
  try {
    proto.removeCachePool(909);
    Assert.fail("expected to get an exception when " + "removing a non-existent pool.");
  }
 catch (  IOException ioe) {
  }
  proto.removeCachePool(pool.getId());
  try {
    proto.removeCachePool(pool.getId());
    Assert.fail("expected to get an exception when " + "removing a non-existent pool.");
  }
 catch (  IOException ioe) {
  }
  req=new CachePoolInfo("pool2");
  proto.addCachePool(req);
}
