{
  JobStatus oldStatus=(JobStatus)fjob.getStatus().clone();
  fjob.startTime=start;
  fjob.status.setStartTime(start);
  JobStatus newStatus=(JobStatus)fjob.getStatus().clone();
  JobStatusChangeEvent event=new JobStatusChangeEvent(fjob,JobStatusChangeEvent.EventType.START_TIME_CHANGED,oldStatus,newStatus);
  for (  JobInProgressListener listener : listeners) {
    listener.jobUpdated(event);
  }
}
