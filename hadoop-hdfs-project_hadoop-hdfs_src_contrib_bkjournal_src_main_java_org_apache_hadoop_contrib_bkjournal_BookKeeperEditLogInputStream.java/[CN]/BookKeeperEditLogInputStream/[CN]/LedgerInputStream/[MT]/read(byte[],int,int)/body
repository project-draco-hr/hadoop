{
  try {
    int read=0;
    if (entryStream == null) {
      entryStream=nextStream();
      if (entryStream == null) {
        return read;
      }
    }
    while (read < len) {
      int thisread=entryStream.read(b,off + read,(len - read));
      if (thisread == -1) {
        entryStream=nextStream();
        if (entryStream == null) {
          return read;
        }
      }
 else {
        read+=thisread;
      }
    }
    return read;
  }
 catch (  IOException e) {
    throw e;
  }
}
