{
  if (!hasAction()) {
    return false;
  }
  if (!hasBlockPoolId()) {
    return false;
  }
  for (int i=0; i < getBlocksCount(); i++) {
    if (!getBlocks(i).isInitialized()) {
      return false;
    }
  }
  for (int i=0; i < getTargetsCount(); i++) {
    if (!getTargets(i).isInitialized()) {
      return false;
    }
  }
  return true;
}
