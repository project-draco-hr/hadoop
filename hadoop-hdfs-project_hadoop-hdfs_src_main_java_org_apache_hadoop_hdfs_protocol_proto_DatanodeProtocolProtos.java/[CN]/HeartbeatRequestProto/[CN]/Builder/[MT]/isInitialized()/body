{
  if (!hasRegistration()) {
    return false;
  }
  if (!hasCapacity()) {
    return false;
  }
  if (!hasDfsUsed()) {
    return false;
  }
  if (!hasRemaining()) {
    return false;
  }
  if (!hasBlockPoolUsed()) {
    return false;
  }
  if (!hasXmitsInProgress()) {
    return false;
  }
  if (!hasXceiverCount()) {
    return false;
  }
  if (!hasFailedVolumes()) {
    return false;
  }
  if (!getRegistration().isInitialized()) {
    return false;
  }
  return true;
}
