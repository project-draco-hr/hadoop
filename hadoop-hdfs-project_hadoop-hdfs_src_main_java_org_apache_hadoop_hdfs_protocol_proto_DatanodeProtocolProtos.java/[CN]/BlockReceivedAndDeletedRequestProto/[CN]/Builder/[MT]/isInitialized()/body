{
  if (!hasRegistration()) {
    return false;
  }
  if (!hasBlockPoolId()) {
    return false;
  }
  if (!getRegistration().isInitialized()) {
    return false;
  }
  for (int i=0; i < getBlocksCount(); i++) {
    if (!getBlocks(i).isInitialized()) {
      return false;
    }
  }
  return true;
}
