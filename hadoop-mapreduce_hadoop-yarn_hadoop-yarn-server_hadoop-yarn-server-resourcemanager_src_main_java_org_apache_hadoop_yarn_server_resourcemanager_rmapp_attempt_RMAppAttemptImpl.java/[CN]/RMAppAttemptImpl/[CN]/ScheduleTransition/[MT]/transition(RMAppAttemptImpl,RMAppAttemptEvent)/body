{
  appAttempt.eventHandler.handle(new RMAppEvent(event.getApplicationAttemptId().getApplicationId(),RMAppEventType.APP_ACCEPTED));
  ResourceRequest request=BuilderUtils.newResourceRequest(AM_CONTAINER_PRIORITY,"*",appAttempt.submissionContext.getMasterCapability(),1);
  LOG.debug("About to request resources for AM of " + appAttempt.applicationAttemptId + " required "+ request);
  appAttempt.scheduler.allocate(appAttempt.applicationAttemptId,Collections.singletonList(request),EMPTY_CONTAINER_RELEASE_LIST);
}
