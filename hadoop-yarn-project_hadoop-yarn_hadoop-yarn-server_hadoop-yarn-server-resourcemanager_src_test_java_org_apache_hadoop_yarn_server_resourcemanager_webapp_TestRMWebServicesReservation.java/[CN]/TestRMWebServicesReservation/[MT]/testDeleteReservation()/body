{
  rm.start();
  for (int i=0; i < 100; i++) {
    MockNM amNodeManager=rm.registerNode("127.0.0." + i + ":1234",100 * 1024);
    amNodeManager.nodeHeartbeat(true);
  }
  ReservationId rid=testSubmissionReservationHelper("reservation/submit",MediaType.APPLICATION_JSON);
  if (this.isAuthenticationEnabled()) {
    assertNotNull(rid);
  }
  testDeleteReservationHelper("reservation/delete",rid,MediaType.APPLICATION_JSON);
  rm.stop();
}
