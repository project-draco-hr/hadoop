{
  return verify(file.exists(),category,"%s file does not exist: %s",text,file) && verify(file.isFile(),category,"%s path does not refer to a file: %s",text,file) && verify(file.length() != 0,category,"%s file is empty: %s",text,file)&& verify(file.canRead(),category,"%s file is not readable: %s",text,file);
}
