{
  String jaasFilename=System.getProperty(SUN_SECURITY_JAAS_FILE);
  if (jaasRequired) {
    verify(jaasFilename != null,CAT_JAAS,"No JAAS file specified in " + SUN_SECURITY_JAAS_FILE);
  }
  if (jaasFilename != null) {
    title("JAAS");
    File jaasFile=new File(jaasFilename);
    println("JAAS file is defined in %s: %s",SUN_SECURITY_JAAS_FILE,jaasFile);
    verifyFileIsValid(jaasFile,CAT_JAAS,"JAAS file defined in " + SUN_SECURITY_JAAS_FILE);
    dump(jaasFile);
    endln();
  }
}
