{
  if (verify(user.getAuthenticationMethod() == AuthenticationMethod.KERBEROS,CAT_LOGIN,"User %s is not authenticated by Kerberos",user)) {
    verify(user.hasKerberosCredentials(),CAT_LOGIN,"%s: No kerberos credentials for %s",messagePrefix,user);
    verify(user.getAuthenticationMethod() != null,CAT_LOGIN,"%s: Null AuthenticationMethod for %s",messagePrefix,user);
  }
}
