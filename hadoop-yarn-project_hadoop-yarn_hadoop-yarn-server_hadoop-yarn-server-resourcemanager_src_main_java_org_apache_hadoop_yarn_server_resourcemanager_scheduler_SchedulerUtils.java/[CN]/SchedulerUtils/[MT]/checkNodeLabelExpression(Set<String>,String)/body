{
  if (labelExpression == null || labelExpression.trim().isEmpty()) {
    if (!nodeLabels.isEmpty()) {
      return false;
    }
  }
  if (labelExpression != null) {
    for (    String str : labelExpression.split("&&")) {
      if (!str.trim().isEmpty() && (nodeLabels == null || !nodeLabels.contains(str.trim()))) {
        return false;
      }
    }
  }
  return true;
}
