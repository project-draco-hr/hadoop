{
  if (objects == null) {
    return null;
  }
 else   if (objects.length == 0) {
    return EMPTY_DATANODE_INFO_ARRAY;
  }
 else {
    final DatanodeInfo[] array=new DatanodeInfo[objects.length];
    for (int i=0; i < array.length; i++) {
      array[i]=(DatanodeInfo)toDatanodeInfo((Map<?,?>)objects[i]);
    }
    return array;
  }
}
