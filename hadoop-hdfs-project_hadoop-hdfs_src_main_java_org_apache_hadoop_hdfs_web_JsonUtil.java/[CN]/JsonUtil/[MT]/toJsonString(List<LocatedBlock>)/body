{
  if (array == null) {
    return null;
  }
 else   if (array.size() == 0) {
    return "[]";
  }
 else {
    final StringBuilder b=new StringBuilder().append('[').append(toJsonString(array.get(0)));
    for (int i=1; i < array.size(); i++) {
      b.append(",\n  ").append(toJsonString(array.get(i)));
    }
    return b.append(']').toString();
  }
}
