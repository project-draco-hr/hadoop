{
  if (array == null) {
    return null;
  }
 else   if (array.length == 0) {
    return "[]";
  }
 else {
    final StringBuilder b=new StringBuilder().append('[').append(toJsonString(array[0]));
    for (int i=1; i < array.length; i++) {
      b.append(", ").append(toJsonString(array[i]));
    }
    return b.append(']').toString();
  }
}
