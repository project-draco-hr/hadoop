{
  TimelineEntity entity=createContainerEntity(event.getContainerId());
  TimelineEvent tEvent=new TimelineEvent();
  tEvent.setId(ContainerMetricsConstants.CREATED_IN_RM_EVENT_TYPE);
  tEvent.setTimestamp(event.getTimestamp());
  Map<String,Object> eventInfo=new HashMap<String,Object>();
  eventInfo.put(ContainerMetricsConstants.ALLOCATED_MEMORY_ENTITY_INFO,event.getAllocatedResource().getMemorySize());
  eventInfo.put(ContainerMetricsConstants.ALLOCATED_VCORE_ENTITY_INFO,event.getAllocatedResource().getVirtualCores());
  eventInfo.put(ContainerMetricsConstants.ALLOCATED_HOST_ENTITY_INFO,event.getAllocatedNode().getHost());
  eventInfo.put(ContainerMetricsConstants.ALLOCATED_PORT_ENTITY_INFO,event.getAllocatedNode().getPort());
  eventInfo.put(ContainerMetricsConstants.ALLOCATED_PRIORITY_ENTITY_INFO,event.getAllocatedPriority().getPriority());
  eventInfo.put(ContainerMetricsConstants.ALLOCATED_HOST_HTTP_ADDRESS_ENTITY_INFO,event.getNodeHttpAddress());
  tEvent.setInfo(eventInfo);
  entity.addEvent(tEvent);
  putEntity(entity,event.getContainerId().getApplicationAttemptId().getApplicationId());
}
