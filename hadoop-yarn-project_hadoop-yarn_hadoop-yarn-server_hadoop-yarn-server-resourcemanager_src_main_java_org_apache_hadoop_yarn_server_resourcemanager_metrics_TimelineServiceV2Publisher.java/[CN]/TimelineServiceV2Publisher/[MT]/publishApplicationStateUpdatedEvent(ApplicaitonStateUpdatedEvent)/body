{
  ApplicationEntity entity=createApplicationEntity(event.getApplicationId());
  Map<String,Object> eventInfo=new HashMap<String,Object>();
  eventInfo.put(ApplicationMetricsConstants.STATE_EVENT_INFO,event.getAppState());
  TimelineEvent tEvent=new TimelineEvent();
  tEvent.setId(ApplicationMetricsConstants.STATE_UPDATED_EVENT_TYPE);
  tEvent.setTimestamp(event.getTimestamp());
  tEvent.setInfo(eventInfo);
  entity.addEvent(tEvent);
  putEntity(entity,event.getApplicationId());
}
