{
  TaskInProgress tip=taskidToTIPMap.get(taskid);
  if (tip != null) {
    aclsManager.checkAccess(tip.getJob(),UserGroupInformation.getCurrentUser(),shouldFail ? Operation.FAIL_TASK : Operation.KILL_TASK);
    return tip.killTask(taskid,shouldFail);
  }
 else {
    LOG.info("Kill task attempt failed since task " + taskid + " was not found");
    return false;
  }
}
