{
  while (true) {
    try {
      Thread.sleep(tasktrackerExpiryInterval / 3);
      checkExpiredTrackers();
    }
 catch (    InterruptedException iex) {
      break;
    }
catch (    Exception t) {
      LOG.error("Tracker Expiry Thread got exception: " + StringUtils.stringifyException(t));
    }
  }
}
