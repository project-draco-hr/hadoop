{
  if (!fi.getReasonforblacklisting().contains(ReasonForBlackListing.NODE_UNHEALTHY)) {
    Set<TaskTracker> trackers=hostnameToTaskTracker.get(hostName);
synchronized (trackers) {
      for (      TaskTracker t : trackers) {
        TaskTrackerStat stat=statistics.getTaskTrackerStat(t.getTrackerName());
        stat.incrHealthCheckFailed();
      }
    }
  }
}
