def testSuite():
    alltests = unittest.TestSuite([unittest.makeSuite(TestCluster, 'test'), unittest.makeSuite(TestJsonVolumeSpecManager, 'test'), unittest.makeSuite(TestJsonVolumeManager, 'test'), unittest.makeSuite(TestInstanceUserData, 'test'), unittest.makeSuite(TestUtilFunctions, 'test')])
