{
  println("\n\n\n*** runTest: modulus=" + modulus + ", creationExpirationPeriod="+ creationExpirationPeriod+ ", accessExpirationPeriod="+ accessExpirationPeriod);
  for (int i=0; i <= 16; i+=4) {
    final int tablelength=(1 << i);
    final int upper=i + 2;
    final int steps=Math.max(1,upper / 3);
    for (int j=upper; j > 0; j-=steps) {
      final int datasize=1 << j;
      check(tablelength,creationExpirationPeriod,accessExpirationPeriod,datasize,modulus);
    }
  }
}
