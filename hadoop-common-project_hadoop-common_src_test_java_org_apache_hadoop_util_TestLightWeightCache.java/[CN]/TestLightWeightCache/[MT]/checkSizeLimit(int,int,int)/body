{
  final LightWeightCacheTestCase test=new LightWeightCacheTestCase(sizeLimit,sizeLimit,1L << 32,1L << 32,datasize,modulus);
  print("  check size ................. ");
  for (int i=0; i < test.data.size(); i++) {
    test.cache.put(test.data.get(i));
    Assert.assertTrue(test.cache.size() <= sizeLimit);
  }
  println("DONE " + test.stat());
}
