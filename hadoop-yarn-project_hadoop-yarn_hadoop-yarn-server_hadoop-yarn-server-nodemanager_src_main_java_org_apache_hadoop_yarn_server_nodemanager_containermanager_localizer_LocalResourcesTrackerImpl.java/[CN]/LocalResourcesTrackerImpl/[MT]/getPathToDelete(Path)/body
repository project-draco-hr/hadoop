{
  Path delPath=localPath.getParent();
  String name=delPath.getName();
  Matcher matcher=RANDOM_DIR_PATTERN.matcher(name);
  if (matcher.matches()) {
    return delPath;
  }
 else {
    LOG.warn("Random directory component did not match. " + "Deleting localized path only");
    return localPath;
  }
}
