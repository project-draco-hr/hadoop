{
  this.state=state;
  this.proxy=makeMock();
  try {
    Configuration conf=new Configuration();
    conf.set(NodeFencer.CONF_METHODS_KEY,DummyFencer.class.getName());
    this.fencer=Mockito.spy(NodeFencer.create(conf));
