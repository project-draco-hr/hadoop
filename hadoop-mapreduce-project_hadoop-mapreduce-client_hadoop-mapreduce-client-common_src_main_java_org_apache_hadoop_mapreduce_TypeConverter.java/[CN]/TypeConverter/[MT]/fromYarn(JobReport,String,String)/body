{
  JobPriority jobPriority=JobPriority.NORMAL;
  return new org.apache.hadoop.mapred.JobStatus(fromYarn(jobreport.getJobId()),jobreport.getSetupProgress(),jobreport.getMapProgress(),jobreport.getReduceProgress(),jobreport.getCleanupProgress(),fromYarn(jobreport.getJobState()),jobPriority,jobreport.getUser(),jobreport.getJobName(),jobFile,trackingUrl);
}
