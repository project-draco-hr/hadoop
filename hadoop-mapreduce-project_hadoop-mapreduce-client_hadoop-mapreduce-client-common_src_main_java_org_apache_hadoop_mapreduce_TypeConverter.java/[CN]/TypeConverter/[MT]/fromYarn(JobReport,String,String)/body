{
  String user=null, jobName=null;
  JobPriority jobPriority=JobPriority.NORMAL;
  return new org.apache.hadoop.mapred.JobStatus(fromYarn(jobreport.getJobId()),jobreport.getSetupProgress(),jobreport.getMapProgress(),jobreport.getReduceProgress(),jobreport.getCleanupProgress(),fromYarn(jobreport.getJobState()),jobPriority,user,jobName,jobFile,trackingUrl);
}
