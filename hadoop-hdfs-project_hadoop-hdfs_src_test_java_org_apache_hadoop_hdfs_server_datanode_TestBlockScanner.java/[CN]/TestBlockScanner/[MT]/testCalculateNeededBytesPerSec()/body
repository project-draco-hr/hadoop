{
  Assert.assertTrue(VolumeScanner.calculateShouldScan(100,0));
  Assert.assertFalse(VolumeScanner.calculateShouldScan(100,101 * 3600));
  Assert.assertTrue(VolumeScanner.calculateShouldScan(1,3540));
  Assert.assertTrue(VolumeScanner.calculateShouldScan(100000L,354000000L));
  Assert.assertFalse(VolumeScanner.calculateShouldScan(100000L,365000000L));
}
