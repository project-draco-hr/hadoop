{
  List<ProxyAndInfo<ClientProtocol>> proxies=getProxiesForAllNameNodesInNameservice(conf,nsId,ClientProtocol.class);
  List<ClientProtocol> namenodes=new ArrayList<ClientProtocol>(proxies.size());
  for (  ProxyAndInfo<ClientProtocol> proxy : proxies) {
    namenodes.add(proxy.getProxy());
  }
  return namenodes;
}
