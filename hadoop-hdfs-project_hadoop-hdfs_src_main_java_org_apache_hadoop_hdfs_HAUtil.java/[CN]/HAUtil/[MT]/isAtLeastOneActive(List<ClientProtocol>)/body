{
  for (  ClientProtocol namenode : namenodes) {
    try {
      namenode.getFileInfo("/");
      return true;
    }
 catch (    RemoteException re) {
      IOException cause=re.unwrapRemoteException();
      if (cause instanceof StandbyException) {
      }
 else {
