{
  int depth=0, index;
  INode tmp=inode;
  while (tmp != null) {
    depth++;
    tmp=tmp.getParent();
  }
  final byte[][] path=new byte[depth][];
  final INode[] inodes=new INode[depth];
  tmp=inode;
  index=depth;
  while (tmp != null) {
    index--;
    path[index]=tmp.getKey();
    inodes[index]=tmp;
    tmp=tmp.getParent();
  }
  return new INodesInPath(inodes,path);
}
