{
  super(message);
  this.statusCode=method.getStatusCode();
  this.operation=operation;
  this.uri=uri;
  String bodyAsString;
  try {
    bodyAsString=method.getResponseBodyAsString();
    if (bodyAsString == null) {
      bodyAsString="";
    }
  }
 catch (  IOException e) {
    bodyAsString="";
  }
  this.body=bodyAsString;
}
