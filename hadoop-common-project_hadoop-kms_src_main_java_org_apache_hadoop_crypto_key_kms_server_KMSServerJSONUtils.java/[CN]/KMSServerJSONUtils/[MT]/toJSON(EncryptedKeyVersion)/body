{
  Map json=new LinkedHashMap();
  if (encryptedKeyVersion != null) {
    json.put(KMSRESTConstants.VERSION_NAME_FIELD,encryptedKeyVersion.getKeyVersionName());
    json.put(KMSRESTConstants.IV_FIELD,Base64.encodeBase64URLSafeString(encryptedKeyVersion.getIv()));
    json.put(KMSRESTConstants.ENCRYPTED_KEY_VERSION_FIELD,toJSON(encryptedKeyVersion.getEncryptedKey()));
  }
  return json;
}
