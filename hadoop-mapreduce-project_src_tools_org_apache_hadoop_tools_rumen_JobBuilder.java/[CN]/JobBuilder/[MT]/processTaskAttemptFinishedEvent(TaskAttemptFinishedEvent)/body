{
  LoggedTaskAttempt attempt=getOrMakeTaskAttempt(event.getTaskType(),event.getTaskId().toString(),event.getAttemptId().toString());
  if (attempt == null) {
    return;
  }
  attempt.setResult(getPre21Value(event.getTaskStatus()));
  ParsedHost pHost=getAndRecordParsedHost(event.getRackName(),event.getHostname());
  if (pHost != null) {
    attempt.setLocation(pHost.makeLoggedLocation());
  }
  attempt.setFinishTime(event.getFinishTime());
  attempt.incorporateCounters(((TaskAttemptFinished)event.getDatum()).counters);
}
