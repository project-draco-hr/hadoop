{
  ContainerManagementProtocolProxyData proxy=null;
  try {
    proxy=cmProxy.getProxy(nodeId.toString(),containerId);
    ContainerStatus containerStatus=proxy.getContainerManagementProtocol().getContainerStatus(GetContainerStatusRequest.newInstance(containerId)).getStatus();
    return containerStatus;
  }
  finally {
    if (proxy != null) {
      cmProxy.mayBeCloseProxy(proxy);
    }
  }
}
