{
  String srcs=normalizePath(src);
  readLock();
  try {
    INode targetNode=rootDir.getNode(srcs,false);
    if (targetNode == null) {
      throw new FileNotFoundException("File does not exist: " + srcs);
    }
 else {
      return targetNode.computeContentSummary();
    }
  }
  finally {
    readUnlock();
  }
}
