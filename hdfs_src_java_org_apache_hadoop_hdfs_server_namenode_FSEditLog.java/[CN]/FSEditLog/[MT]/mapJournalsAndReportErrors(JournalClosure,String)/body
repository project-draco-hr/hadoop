{
  List<JournalAndStream> badJAS=Lists.newLinkedList();
  for (  JournalAndStream jas : journals) {
    try {
      closure.apply(jas);
    }
 catch (    Throwable t) {
      LOG.error("Error " + status + " (journal "+ jas+ ")",t);
      badJAS.add(jas);
    }
  }
  disableAndReportErrorOnJournals(badJAS);
}
