{
  AddOp op=AddOp.getInstance().setPath(path).setReplication(newNode.getReplication()).setModificationTime(newNode.getModificationTime()).setAccessTime(newNode.getAccessTime()).setBlockSize(newNode.getPreferredBlockSize()).setBlocks(newNode.getBlocks()).setPermissionStatus(newNode.getPermissionStatus()).setClientName(newNode.getClientName()).setClientMachine(newNode.getClientMachine());
  logEdit(op);
}
