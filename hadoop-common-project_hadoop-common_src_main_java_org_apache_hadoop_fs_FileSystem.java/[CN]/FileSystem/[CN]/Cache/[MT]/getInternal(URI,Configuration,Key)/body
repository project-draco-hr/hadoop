{
  FileSystem fs;
synchronized (this) {
    fs=map.get(key);
  }
  if (fs != null) {
    return fs;
  }
  fs=createFileSystem(uri,conf);
synchronized (this) {
    FileSystem oldfs=map.get(key);
    if (oldfs != null) {
      fs.close();
      return oldfs;
    }
    if (map.isEmpty() && !clientFinalizer.isAlive()) {
      Runtime.getRuntime().addShutdownHook(clientFinalizer);
    }
    fs.key=key;
    map.put(key,fs);
    if (conf.getBoolean("fs.automatic.close",true)) {
      toAutoClose.add(key);
    }
    return fs;
  }
}
