{
  visitor.accept(rootData);
  if (allData != null) {
    for (Iterator<StatisticsData> iter=allData.iterator(); iter.hasNext(); ) {
      StatisticsData data=iter.next();
      visitor.accept(data);
      if (data.owner.get() == null) {
        rootData.add(data);
        iter.remove();
      }
    }
  }
  return visitor.aggregate();
}
