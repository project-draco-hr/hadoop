{
synchronized (deleteOnExit) {
    for (Iterator<Path> iter=deleteOnExit.iterator(); iter.hasNext(); ) {
      Path path=iter.next();
      try {
        if (exists(path)) {
          delete(path,true);
        }
      }
 catch (      IOException e) {
        LOG.info("Ignoring failure to deleteOnExit for path " + path);
      }
      iter.remove();
    }
  }
}
