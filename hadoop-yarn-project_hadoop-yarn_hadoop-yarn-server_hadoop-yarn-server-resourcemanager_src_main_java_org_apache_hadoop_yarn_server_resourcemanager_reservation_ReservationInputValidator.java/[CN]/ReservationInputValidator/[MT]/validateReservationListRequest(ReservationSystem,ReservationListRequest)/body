{
  String queue=request.getQueue();
  if (request.getEndTime() < request.getStartTime()) {
    String errorMessage="The specified end time must be greater than " + "the specified start time.";
    RMAuditLogger.logFailure("UNKNOWN",AuditConstants.LIST_RESERVATION_REQUEST,"validate list reservation input","ClientRMService",errorMessage);
    throw RPCUtil.getRemoteException(errorMessage);
  }
  return getPlanFromQueue(reservationSystem,queue,AuditConstants.LIST_RESERVATION_REQUEST);
}
