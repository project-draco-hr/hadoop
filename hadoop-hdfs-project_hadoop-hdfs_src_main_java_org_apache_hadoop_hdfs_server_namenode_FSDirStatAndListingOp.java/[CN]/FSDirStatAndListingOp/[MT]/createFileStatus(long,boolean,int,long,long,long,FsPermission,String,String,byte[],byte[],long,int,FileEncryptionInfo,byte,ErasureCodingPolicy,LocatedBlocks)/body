{
  if (locations == null) {
    return new HdfsFileStatus(length,isdir,replication,blocksize,mtime,atime,permission,owner,group,symlink,path,fileId,childrenNum,feInfo,storagePolicy,ecPolicy);
  }
 else {
    return new HdfsLocatedFileStatus(length,isdir,replication,blocksize,mtime,atime,permission,owner,group,symlink,path,fileId,locations,childrenNum,feInfo,storagePolicy,ecPolicy);
  }
}
