{
  PortmapMapping mapEntry=new PortmapMapping(RpcProgramMountd.PROGRAM,RpcProgramMountd.VERSION_1,PortmapMapping.TRANSPORT_UDP,RpcProgramMountd.PORT);
  XDR mappingRequest=PortmapRequest.create(mapEntry);
  RegistrationClient registrationClient=new RegistrationClient("localhost",Nfs3Constant.SUN_RPCBIND,mappingRequest);
  registrationClient.run();
  Thread t1=new Runtest1();
  t1.start();
  t1.join();
}
