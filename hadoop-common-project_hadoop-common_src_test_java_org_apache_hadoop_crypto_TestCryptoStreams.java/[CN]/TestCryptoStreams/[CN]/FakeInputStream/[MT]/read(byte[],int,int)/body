{
  if (b == null) {
    throw new NullPointerException();
  }
 else   if (off < 0 || len < 0 || len > b.length - off) {
    throw new IndexOutOfBoundsException();
  }
 else   if (len == 0) {
    return 0;
  }
  checkStream();
  if (pos < length) {
    int n=(int)Math.min(len,length - pos);
    System.arraycopy(data,pos,b,off,n);
    pos+=n;
    return n;
  }
  return -1;
}
