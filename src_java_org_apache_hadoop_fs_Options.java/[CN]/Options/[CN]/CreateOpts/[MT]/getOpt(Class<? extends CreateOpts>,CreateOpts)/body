{
  if (opts == null) {
    throw new IllegalArgumentException("Null opt");
  }
  CreateOpts result=null;
  for (int i=0; i < opts.length; ++i) {
    if (opts[i].getClass() == theClass) {
      if (result != null)       throw new IllegalArgumentException("multiple blocksize varargs");
      result=opts[i];
    }
  }
  return result;
}
