{
synchronized (TestDelegationTokensWithHA.this) {
    while (!catchup) {
      try {
        LOG.info("The editlog tailer is waiting to catchup...");
        TestDelegationTokensWithHA.this.wait();
      }
 catch (      InterruptedException e) {
      }
    }
  }
  super.catchupDuringFailover();
}
