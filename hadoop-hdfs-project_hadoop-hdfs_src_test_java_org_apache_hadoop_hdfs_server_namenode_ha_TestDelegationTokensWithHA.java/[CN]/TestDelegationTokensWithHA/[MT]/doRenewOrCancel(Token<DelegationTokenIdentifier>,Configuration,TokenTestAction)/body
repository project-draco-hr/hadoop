{
  UserGroupInformation.createRemoteUser("JobTracker").doAs(new PrivilegedExceptionAction<Void>(){
    @Override public Void run() throws Exception {
switch (action) {
case RENEW:
        token.renew(conf);
      break;
case CANCEL:
    token.cancel(conf);
  break;
default :
fail("bad action:" + action);
}
return null;
}
}
);
}
