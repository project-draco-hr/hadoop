{
  checkOpen();
  Preconditions.checkArgument(length >= 0);
  LocatedBlocks blockLocations=getBlockLocations(src,length);
  FileChecksumHelper.FileChecksumComputer maker=new FileChecksumHelper.ReplicatedFileChecksumComputer(src,length,blockLocations,namenode,this);
  maker.compute();
  return maker.getFileChecksum();
}
