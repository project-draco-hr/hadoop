{
  checkOpen();
  try (TraceScope ignored=newPathTraceScope("getXAttrs",src)){
    return XAttrHelper.buildXAttrMap(namenode.getXAttrs(src,XAttrHelper.buildXAttrs(names)));
  }
 catch (  RemoteException re) {
