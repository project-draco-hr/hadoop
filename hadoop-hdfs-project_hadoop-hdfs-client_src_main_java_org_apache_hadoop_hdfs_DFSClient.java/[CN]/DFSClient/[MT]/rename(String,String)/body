{
  checkOpen();
  try (TraceScope ignored=newSrcDstTraceScope("rename",src,dst)){
    return namenode.rename(src,dst);
  }
 catch (  RemoteException re) {
    throw re.unwrapRemoteException(AccessControlException.class,NSQuotaExceededException.class,DSQuotaExceededException.class,QuotaByStorageTypeExceededException.class,UnresolvedPathException.class,SnapshotAccessControlException.class);
  }
}
