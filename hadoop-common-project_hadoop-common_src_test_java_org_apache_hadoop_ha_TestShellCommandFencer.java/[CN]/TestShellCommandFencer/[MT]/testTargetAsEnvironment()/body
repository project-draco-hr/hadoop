{
  if (!Shell.WINDOWS) {
    fencer.tryFence(TEST_TARGET,"echo $target_host $target_port $target_address");
    Mockito.verify(ShellCommandFencer.LOG).info(Mockito.endsWith("echo $ta...t_address: host 1234 host:1234"));
  }
 else {
    fencer.tryFence(TEST_TARGET,"echo %target_host% %target_port% %target_address%");
    Mockito.verify(ShellCommandFencer.LOG).info(Mockito.endsWith("echo %ta..._address%: host 1234 host:1234"));
  }
}
