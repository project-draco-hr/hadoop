{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Job to be removed for user " + job.getProfile().getUser());
  }
  Integer i=qsc.getNumJobsByUser().get(job.getProfile().getUser());
  i--;
  if (0 == i.intValue()) {
    qsc.getNumJobsByUser().remove(job.getProfile().getUser());
    qsc.getMapTSC().getNumSlotsOccupiedByUser().remove(job.getProfile().getUser());
    qsc.getReduceTSC().getNumSlotsOccupiedByUser().remove(job.getProfile().getUser());
    if (LOG.isDebugEnabled()) {
      LOG.debug("No more jobs for user, number of users = " + qsc.getNumJobsByUser().size());
    }
  }
 else {
    qsc.getNumJobsByUser().put(job.getProfile().getUser(),i);
    if (LOG.isDebugEnabled()) {
      LOG.debug("User still has " + i + " jobs, number of users = "+ qsc.getNumJobsByUser().size());
    }
  }
}
