{
  if (entity == null) {
    assertNull(retrievedEntityInfo);
    return;
  }
  assertEquals(entity,retrievedEntityInfo.getEntityId());
  assertEquals(entityType,retrievedEntityInfo.getEntityType());
  if (events == null)   assertNull(retrievedEntityInfo.getEvents());
 else   assertEquals(events,retrievedEntityInfo.getEvents());
  if (relatedEntities == null)   assertNull(retrievedEntityInfo.getRelatedEntities());
 else   assertEquals(relatedEntities,retrievedEntityInfo.getRelatedEntities());
  if (primaryFilters == null)   assertNull(retrievedEntityInfo.getPrimaryFilters());
 else   assertTrue(primaryFilters.equals(retrievedEntityInfo.getPrimaryFilters()));
  if (otherInfo == null)   assertNull(retrievedEntityInfo.getOtherInfo());
 else   assertTrue(otherInfo.equals(retrievedEntityInfo.getOtherInfo()));
}
