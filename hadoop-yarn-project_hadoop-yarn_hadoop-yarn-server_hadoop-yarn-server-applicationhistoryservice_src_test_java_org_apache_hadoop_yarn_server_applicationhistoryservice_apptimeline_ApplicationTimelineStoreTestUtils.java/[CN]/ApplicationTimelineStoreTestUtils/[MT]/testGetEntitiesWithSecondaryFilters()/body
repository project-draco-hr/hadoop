{
  List<ATSEntity> entities=store.getEntities("type_1",null,null,null,null,goodTestingFilters,EnumSet.allOf(Field.class)).getEntities();
  assertEquals(2,entities.size());
  verifyEntityInfo(entity1,entityType1,events1,EMPTY_REL_ENTITIES,primaryFilters,otherInfo,entities.get(0));
  verifyEntityInfo(entity1b,entityType1,events1,EMPTY_REL_ENTITIES,primaryFilters,otherInfo,entities.get(1));
  entities=store.getEntities("type_1",null,null,null,userFilter,goodTestingFilters,EnumSet.allOf(Field.class)).getEntities();
  assertEquals(2,entities.size());
  verifyEntityInfo(entity1,entityType1,events1,EMPTY_REL_ENTITIES,primaryFilters,otherInfo,entities.get(0));
  verifyEntityInfo(entity1b,entityType1,events1,EMPTY_REL_ENTITIES,primaryFilters,otherInfo,entities.get(1));
  entities=store.getEntities("type_1",null,null,null,null,badTestingFilters,EnumSet.allOf(Field.class)).getEntities();
  assertEquals(0,entities.size());
  entities=store.getEntities("type_1",null,null,null,userFilter,badTestingFilters,EnumSet.allOf(Field.class)).getEntities();
  assertEquals(0,entities.size());
}
