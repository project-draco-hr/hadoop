{
  List<org.apache.hadoop.yarn.api.records.QueueInfo> queues=new ArrayList<org.apache.hadoop.yarn.api.records.QueueInfo>();
  org.apache.hadoop.yarn.api.records.QueueInfo parentQueue=applicationsManager.getQueueInfo(getQueueInfoRequest(parent,false,true,false)).getQueueInfo();
  getChildQueues(parentQueue,queues);
  return TypeConverter.fromYarnQueueInfo(queues,this.conf);
}
