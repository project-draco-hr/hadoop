{
  try {
    new AuthenticationToken(null,"p","t");
    Assert.fail();
  }
 catch (  IllegalArgumentException ex) {
  }
catch (  Throwable ex) {
    Assert.fail();
  }
  try {
    new AuthenticationToken("","p","t");
    Assert.fail();
  }
 catch (  IllegalArgumentException ex) {
  }
catch (  Throwable ex) {
    Assert.fail();
  }
  try {
    new AuthenticationToken("u",null,"t");
    Assert.fail();
  }
 catch (  IllegalArgumentException ex) {
  }
catch (  Throwable ex) {
    Assert.fail();
  }
  try {
    new AuthenticationToken("u","","t");
    Assert.fail();
  }
 catch (  IllegalArgumentException ex) {
  }
catch (  Throwable ex) {
    Assert.fail();
  }
  try {
    new AuthenticationToken("u","p",null);
    Assert.fail();
  }
 catch (  IllegalArgumentException ex) {
  }
catch (  Throwable ex) {
    Assert.fail();
  }
  try {
    new AuthenticationToken("u","p","");
    Assert.fail();
  }
 catch (  IllegalArgumentException ex) {
  }
catch (  Throwable ex) {
    Assert.fail();
  }
  new AuthenticationToken("u","p","t");
}
