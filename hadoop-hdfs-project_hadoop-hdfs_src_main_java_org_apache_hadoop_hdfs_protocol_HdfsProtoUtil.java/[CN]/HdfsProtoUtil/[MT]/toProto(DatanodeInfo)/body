{
  return HdfsProtos.DatanodeInfoProto.newBuilder().setId(toProto((DatanodeID)dni)).setCapacity(dni.getCapacity()).setDfsUsed(dni.getDfsUsed()).setRemaining(dni.getRemaining()).setBlockPoolUsed(dni.getBlockPoolUsed()).setLastUpdate(dni.getLastUpdate()).setXceiverCount(dni.getXceiverCount()).setLocation(dni.getNetworkLocation()).setAdminState(HdfsProtos.DatanodeInfoProto.AdminState.valueOf(dni.getAdminState().name())).build();
}
