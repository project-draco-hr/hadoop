{
  Client client=createClient();
  try {
    URI uri=URI.create("http://localhost:" + serverPort + "/ws/v2/"+ "timeline/entities/cluster1/app1/app?infofilters=info2:3.5");
    ClientResponse resp=getResponse(client,uri);
    Set<TimelineEntity> entities=resp.getEntity(new GenericType<Set<TimelineEntity>>(){
    }
);
    assertEquals(MediaType.APPLICATION_JSON_TYPE,resp.getType());
    assertNotNull(entities);
    assertEquals(1,entities.size());
    assertTrue("Entity with id_3 should have been present in response.",entities.contains(newEntity("app","id_3")));
  }
  finally {
    client.destroy();
  }
}
