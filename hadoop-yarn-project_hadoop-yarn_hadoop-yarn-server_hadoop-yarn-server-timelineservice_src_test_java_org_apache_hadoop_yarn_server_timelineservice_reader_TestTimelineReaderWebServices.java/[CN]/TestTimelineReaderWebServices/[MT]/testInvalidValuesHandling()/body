{
  Client client=createClient();
  try {
    URI uri=URI.create("http://localhost:" + serverPort + "/ws/v2/"+ "timeline/entities/cluster1/app1/app?flowrunid=a23b");
    verifyHttpResponse(client,uri,Status.BAD_REQUEST);
    uri=URI.create("http://localhost:" + serverPort + "/ws/v2/timeline/"+ "entity/cluster1/app1/app/id_1?flowrunid=2ab15");
    verifyHttpResponse(client,uri,Status.BAD_REQUEST);
    uri=URI.create("http://localhost:" + serverPort + "/ws/v2/timeline/"+ "entities/cluster1/app1/app/?limit=#$561av");
    verifyHttpResponse(client,uri,Status.BAD_REQUEST);
  }
  finally {
    client.destroy();
  }
}
