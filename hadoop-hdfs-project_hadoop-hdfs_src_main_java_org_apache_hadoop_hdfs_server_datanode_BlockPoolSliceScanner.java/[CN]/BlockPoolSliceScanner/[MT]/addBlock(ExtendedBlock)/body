{
  BlockScanInfo info=blockMap.get(block.getLocalBlock());
  if (info != null) {
    LOG.warn("Adding an already existing block " + block);
    delBlockInfo(info);
  }
  info=new BlockScanInfo(block.getLocalBlock());
  info.lastScanTime=getNewBlockScanTime();
  addBlockInfo(info,true);
  adjustThrottler();
}
