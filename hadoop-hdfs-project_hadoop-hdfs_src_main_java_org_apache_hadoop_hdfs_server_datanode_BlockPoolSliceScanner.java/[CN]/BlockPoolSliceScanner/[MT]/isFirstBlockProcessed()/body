{
  if (!blockInfoSet.isEmpty()) {
    if (blockInfoSet.first().nextScanType == ScanType.IMMEDIATE_SCAN) {
      return false;
    }
    long blockId=blockInfoSet.first().getBlockId();
    if ((processedBlocks.get(blockId) != null) && (processedBlocks.get(blockId) == 1)) {
      return true;
    }
  }
  return false;
}
