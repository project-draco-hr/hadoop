{
  if (!workRemainingInCurrentPeriod()) {
    return;
  }
  processedBlocks=new HashMap<Long,Integer>();
  if (!assignInitialVerificationTimes()) {
    return;
  }
  try {
    scan();
  }
  finally {
    totalBlocksScannedInLastRun.set(processedBlocks.size());
    lastScanTime.set(Time.now());
  }
}
