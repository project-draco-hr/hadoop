{
  if (bytesLeft <= 0 && Time.monotonicNow() < currentPeriodStart + scanPeriod) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Skipping scan since bytesLeft=" + bytesLeft + ", Start="+ currentPeriodStart+ ", period="+ scanPeriod+ ", now="+ Time.monotonicNow()+ " "+ blockPoolId);
    }
    return false;
  }
 else {
    return true;
  }
}
