{
  FileStatus stat=item.stat;
  String line=String.format(lineFormat,(stat.isDirectory() ? "d" : "-"),stat.getPermission() + (hasAcl(item) ? "+" : ""),(stat.isFile() ? stat.getReplication() : "-"),stat.getOwner(),stat.getGroup(),formatSize(stat.getLen()),dateFormat.format(new Date(stat.getModificationTime())),item);
  out.println(line);
}
