{
  StringBuilder builder=new StringBuilder();
  builder.append("{ id:").append(id).append(", path:").append(path).append(", replication:").append(replication).append(", pool:").append(pool).append(", expiryTime: ").append(getExpiryTimeString()).append(", bytesNeeded:").append(bytesNeeded).append(", bytesCached:").append(bytesCached).append(", filesNeeded:").append(filesNeeded).append(", filesCached:").append(filesCached).append(" }");
  return builder.toString();
}
