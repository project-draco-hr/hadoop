{
  if (other instanceof TupleWritable) {
    TupleWritable that=(TupleWritable)other;
    if (!this.written.equals(that.written)) {
      return false;
    }
    for (int i=0; i < values.length; ++i) {
      if (!has(i))       continue;
      if (!values[i].equals(that.get(i))) {
        return false;
      }
    }
    return true;
  }
  return false;
}
