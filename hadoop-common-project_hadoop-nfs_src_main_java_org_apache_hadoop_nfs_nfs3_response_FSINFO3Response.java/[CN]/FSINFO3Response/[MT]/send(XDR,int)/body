{
  super.send(out,xid);
  out.writeBoolean(true);
  postOpAttr.serialize(out);
  if (getStatus() == Nfs3Status.NFS3_OK) {
    out.writeInt(rtmax);
    out.writeInt(rtpref);
    out.writeInt(rtmult);
    out.writeInt(wtmax);
    out.writeInt(wtpref);
    out.writeInt(wtmult);
    out.writeInt(dtpref);
    out.writeLongAsHyper(maxFileSize);
    timeDelta.serialize(out);
    out.writeInt(properties);
  }
  return out;
}
