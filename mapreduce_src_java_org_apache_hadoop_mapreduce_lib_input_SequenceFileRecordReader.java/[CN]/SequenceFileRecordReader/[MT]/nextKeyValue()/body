{
  if (!more) {
    return false;
  }
  inputByteCounter.increment(in.getPosition() - pos);
  pos=in.getPosition();
  key=(K)in.next(key);
  if (key == null || (pos >= end && in.syncSeen())) {
    more=false;
    key=null;
    value=null;
  }
 else {
    value=(V)in.getCurrentValue(value);
  }
  return more;
}
