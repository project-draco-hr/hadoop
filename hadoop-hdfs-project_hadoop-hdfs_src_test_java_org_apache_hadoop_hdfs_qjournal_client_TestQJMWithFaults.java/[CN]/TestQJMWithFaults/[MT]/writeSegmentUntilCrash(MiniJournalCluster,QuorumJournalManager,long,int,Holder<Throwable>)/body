{
  long firstTxId=txid;
  long lastAcked=txid - 1;
  try {
    EditLogOutputStream stm=qjm.startLogSegment(txid);
    for (int i=0; i < numTxns; i++) {
      QJMTestUtil.writeTxns(stm,txid++,1);
      lastAcked++;
    }
    stm.close();
    qjm.finalizeLogSegment(firstTxId,lastAcked);
  }
 catch (  Throwable t) {
    thrown.held=t;
  }
  return lastAcked;
}
