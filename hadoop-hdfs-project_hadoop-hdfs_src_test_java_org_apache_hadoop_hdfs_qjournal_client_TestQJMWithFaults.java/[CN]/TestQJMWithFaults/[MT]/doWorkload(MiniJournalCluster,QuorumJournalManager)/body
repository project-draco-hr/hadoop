{
  int lastAcked=0;
  try {
    qjm.recoverUnfinalizedSegments();
    writeSegment(cluster,qjm,1,3,true);
    lastAcked=3;
    writeSegment(cluster,qjm,4,3,true);
    lastAcked=6;
  }
 catch (  QuorumException qe) {
    LOG.info("Failed to write at txid " + lastAcked,qe);
  }
  return lastAcked;
}
