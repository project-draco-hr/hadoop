{
  List<String> result=Lists.newArrayList();
  result.add(getPrefix(checkNotNull(pathSpec,"pathSpec")));
  Iterable<String> parts=pathSplitter.split(pathSpec);
  String controller=null, action=null;
  for (  String s : parts) {
    if (controller == null) {
      if (s.charAt(0) == ':') {
        controller="default";
        result.add(controller);
        action="index";
        result.add(action);
      }
 else {
        controller=s;
      }
    }
 else     if (action == null) {
      if (s.charAt(0) == ':') {
        action="index";
        result.add(action);
      }
 else {
        action=s;
      }
    }
    result.add(s);
  }
  if (controller == null) {
    result.add("default");
  }
  if (action == null) {
    result.add("index");
  }
  return result;
}
