{
  if (skip)   return;
  DataOutputStream outValue=null;
  try {
    outValue=writer.prepareAppendValue(6);
    outValue.write("value0".getBytes());
    fail("Cannot add a value without adding key first. ");
  }
 catch (  Exception e) {
  }
 finally {
    if (outValue != null) {
      outValue.close();
    }
  }
}
