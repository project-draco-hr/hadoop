{
  DistributedSchedulingAllocateRequestPBImpl request=new DistributedSchedulingAllocateRequestPBImpl(proto);
  try {
    DistributedSchedulingAllocateResponse response=real.allocateForDistributedScheduling(request);
    return ((DistributedSchedulingAllocateResponsePBImpl)response).getProto();
  }
 catch (  YarnException e) {
    throw new ServiceException(e);
  }
catch (  IOException e) {
    throw new ServiceException(e);
  }
}
