{
  try {
    writeLock.lock();
    RMContainer rmContainer=liveContainers.remove(containerId);
    if (rmContainer != null && rmContainer.isRemotelyAllocated()) {
      this.attemptResourceUsageAllocatedRemotely.decUsed(rmContainer.getAllocatedResource());
    }
  }
  finally {
    writeLock.unlock();
  }
}
