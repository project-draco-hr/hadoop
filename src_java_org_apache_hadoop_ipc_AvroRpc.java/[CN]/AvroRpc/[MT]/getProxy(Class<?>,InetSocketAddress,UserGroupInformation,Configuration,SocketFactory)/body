{
  return Proxy.newProxyInstance(protocol.getClassLoader(),new Class[]{protocol},new InvocationHandler(){
    public Object invoke(    Object proxy,    Method method,    Object[] args) throws Throwable {
      return new ReflectRequestor(protocol,new ClientTransceiver(addr,ticket,conf,factory)).invoke(proxy,method,args);
    }
  }
);
}
