{
  Collection<DatanodeStorageInfo> ret=new ArrayList<>();
  if (rackMap.size() == 2) {
    for (    List<DatanodeStorageInfo> dsi : rackMap.values()) {
      if (dsi.size() >= 2) {
        ret.addAll(dsi);
      }
    }
  }
  if (ret.isEmpty()) {
    ret.addAll(moreThanOne);
    ret.addAll(exactlyOne);
  }
  return ret;
}
