{
  if (!isFirst) {
    return false;
  }
 else   if (delHint == null) {
    return false;
  }
 else   if (!excessTypes.contains(delHint.getStorageType())) {
    return false;
  }
 else {
    if (moreThan1Racks.contains(delHint)) {
      return true;
    }
 else     if (added != null && !moreThan1Racks.contains(added)) {
      return true;
    }
    return false;
  }
}
