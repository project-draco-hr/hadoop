{
  out.write("<property>");
  out.write("<name>");
  out.write(name);
  out.write("</name>");
  out.write("<value>");
  out.write(val);
  out.write("</value>");
  if (isFinal) {
    out.write("<final>true</final>");
  }
  for (  String s : sources) {
    out.write("<source>");
    out.write(s);
    out.write("</source>");
  }
  out.write("</property>\n");
}
