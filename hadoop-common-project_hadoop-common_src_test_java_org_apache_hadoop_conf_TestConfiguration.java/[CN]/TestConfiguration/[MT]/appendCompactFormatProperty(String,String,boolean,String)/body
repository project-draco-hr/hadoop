{
  out.write("<property ");
  out.write("name=\"");
  out.write(name);
  out.write("\" ");
  out.write("value=\"");
  out.write(val);
  out.write("\" ");
  if (isFinal) {
    out.write("final=\"true\" ");
  }
  if (source != null) {
    out.write("source=\"");
    out.write(source);
    out.write("\" ");
  }
  out.write("/>\n");
}
