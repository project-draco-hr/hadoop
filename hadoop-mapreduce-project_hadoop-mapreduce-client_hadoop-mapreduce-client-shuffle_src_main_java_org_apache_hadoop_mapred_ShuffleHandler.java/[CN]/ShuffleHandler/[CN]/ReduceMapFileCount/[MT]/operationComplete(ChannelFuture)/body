{
  if (!future.isSuccess()) {
    future.getChannel().close();
    return;
  }
  int waitCount=this.reduceContext.getMapsToWait().decrementAndGet();
  if (waitCount == 0) {
    metrics.operationComplete(future);
    future.getChannel().close();
  }
 else {
    pipelineFact.getSHUFFLE().sendMap(reduceContext);
  }
}
