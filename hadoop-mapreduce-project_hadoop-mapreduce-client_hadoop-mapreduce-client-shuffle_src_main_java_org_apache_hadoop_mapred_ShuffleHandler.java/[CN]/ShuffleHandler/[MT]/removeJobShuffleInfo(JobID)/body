{
  String jobIdStr=jobId.toString();
  secretManager.removeTokenForJob(jobIdStr);
  userRsrc.remove(jobIdStr);
  if (stateDb != null) {
    try {
      stateDb.delete(bytes(jobIdStr));
    }
 catch (    DBException e) {
      throw new IOException("Unable to remove " + jobId + " from state store",e);
    }
  }
}
