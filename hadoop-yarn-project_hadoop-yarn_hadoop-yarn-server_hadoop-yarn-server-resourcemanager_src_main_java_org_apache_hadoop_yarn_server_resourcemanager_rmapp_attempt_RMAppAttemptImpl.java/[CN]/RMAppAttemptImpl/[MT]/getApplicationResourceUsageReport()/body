{
  this.readLock.lock();
  try {
    ApplicationResourceUsageReport report=scheduler.getAppResourceUsageReport(this.getAppAttemptId());
    if (report == null) {
      Resource none=Resource.newInstance(0,0);
      report=ApplicationResourceUsageReport.newInstance(0,0,none,none,none);
    }
    return report;
  }
  finally {
    this.readLock.unlock();
  }
}
