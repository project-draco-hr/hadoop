{
  this.readLock.lock();
  try {
    ApplicationResourceUsageReport report=scheduler.getAppResourceUsageReport(this.getAppAttemptId());
    if (report == null) {
      report=RMServerUtils.DUMMY_APPLICATION_RESOURCE_USAGE_REPORT;
    }
    return report;
  }
  finally {
    this.readLock.unlock();
  }
}
