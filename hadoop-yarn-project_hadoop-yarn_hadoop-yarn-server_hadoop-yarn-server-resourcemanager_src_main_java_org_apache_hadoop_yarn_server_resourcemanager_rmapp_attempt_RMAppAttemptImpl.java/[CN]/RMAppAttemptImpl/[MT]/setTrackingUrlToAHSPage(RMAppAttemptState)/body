{
  originalTrackingUrl=pjoin(WebAppUtils.getHttpSchemePrefix(conf) + WebAppUtils.getAHSWebAppURLWithoutScheme(conf),"applicationhistory","app",getAppAttemptId().getApplicationId());
switch (stateToBeStored) {
case KILLED:
case FAILED:
    proxiedTrackingUrl=originalTrackingUrl;
  break;
default :
break;
}
}
