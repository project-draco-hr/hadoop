{
  this.readLock.lock();
  try {
    List<ContainerStatus> returnList=new ArrayList<ContainerStatus>();
    for (    Collection<ContainerStatus> containerStatusList : justFinishedContainers.values()) {
      returnList.addAll(containerStatusList);
    }
    return returnList;
  }
  finally {
    this.readLock.unlock();
  }
}
