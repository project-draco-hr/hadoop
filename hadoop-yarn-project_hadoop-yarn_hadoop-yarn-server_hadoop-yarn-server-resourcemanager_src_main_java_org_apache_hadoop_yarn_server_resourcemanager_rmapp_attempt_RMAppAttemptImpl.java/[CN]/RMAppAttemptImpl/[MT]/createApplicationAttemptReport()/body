{
  this.readLock.lock();
  ApplicationAttemptReport attemptReport=null;
  try {
    attemptReport=ApplicationAttemptReport.newInstance(this.getAppAttemptId(),this.getHost(),this.getRpcPort(),this.getTrackingUrl(),this.getDiagnostics(),YarnApplicationAttemptState.valueOf(this.getState().toString()),this.getMasterContainer().getId());
  }
  finally {
    this.readLock.unlock();
  }
  return attemptReport;
}
