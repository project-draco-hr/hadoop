{
  if (!appAttempt.submissionContext.getUnmanagedAM()) {
    ResourceRequest request=BuilderUtils.newResourceRequest(AM_CONTAINER_PRIORITY,ResourceRequest.ANY,appAttempt.getSubmissionContext().getResource(),1);
    Allocation amContainerAllocation=appAttempt.scheduler.allocate(appAttempt.applicationAttemptId,Collections.singletonList(request),EMPTY_CONTAINER_RELEASE_LIST,null,null);
    if (amContainerAllocation != null && amContainerAllocation.getContainers() != null) {
      assert(amContainerAllocation.getContainers().size() == 0);
    }
    return RMAppAttemptState.SCHEDULED;
  }
 else {
    appAttempt.storeAttempt();
    return RMAppAttemptState.LAUNCHED_UNMANAGED_SAVING;
  }
}
