{
  if (application != null) {
synchronized (this) {
      Container container=rmContainer.getContainer();
      boolean removed=false;
      if (rmContainer.getState() == RMContainerState.RESERVED) {
        removed=unreserve(application,rmContainer.getReservedPriority(),node,rmContainer);
      }
 else {
        removed=application.containerCompleted(rmContainer,containerStatus,event);
        node.releaseContainer(container);
      }
      if (removed) {
        releaseResource(clusterResource,application,container.getResource());
        LOG.info("completedContainer" + " container=" + container + " resource="+ container.getResource()+ " queue="+ this+ " usedCapacity="+ getUsedCapacity()+ " absoluteUsedCapacity="+ getAbsoluteUsedCapacity()+ " used="+ usedResources+ " cluster="+ clusterResource);
        getParent().completedContainer(clusterResource,application,node,rmContainer,null,event,this);
      }
    }
  }
}
