{
  Priority priority=rmContainer.getReservedPriority();
  if (application.getTotalRequiredResources(priority) == 0) {
    Container container=rmContainer.getContainer();
    completedContainer(clusterResource,application,node,rmContainer,SchedulerUtils.createAbnormalContainerStatus(container.getId(),SchedulerUtils.UNRESERVED_CONTAINER),RMContainerEventType.RELEASED);
    return container.getResource();
  }
  assignContainersOnNode(clusterResource,node,application,priority,rmContainer);
  return Resources.none();
}
