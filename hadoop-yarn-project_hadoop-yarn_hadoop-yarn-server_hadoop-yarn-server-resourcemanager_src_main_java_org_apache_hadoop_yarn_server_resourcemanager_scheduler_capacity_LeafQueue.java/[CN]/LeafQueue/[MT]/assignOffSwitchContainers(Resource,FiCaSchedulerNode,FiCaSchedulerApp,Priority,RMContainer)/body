{
  ResourceRequest request=application.getResourceRequest(priority,ResourceRequest.ANY);
  if (request != null) {
    if (canAssign(application,priority,node,NodeType.OFF_SWITCH,reservedContainer)) {
      return assignContainer(clusterResource,node,application,priority,request,NodeType.OFF_SWITCH,reservedContainer);
    }
  }
  return Resources.none();
}
