{
  this.currentResourceLimits=currentResourceLimits;
  lastClusterResource=clusterResource;
  updateAbsoluteCapacityResource(clusterResource);
  computeQueueCurrentLimitAndSetHeadroomInfo(clusterResource);
  CSQueueUtils.updateQueueStatistics(resourceCalculator,this,getParent(),clusterResource,minimumAllocation);
  activateApplications();
  for (  FiCaSchedulerApp application : activeApplications) {
synchronized (application) {
      computeUserLimitAndSetHeadroom(application,clusterResource,Resources.none(),null);
    }
  }
}
