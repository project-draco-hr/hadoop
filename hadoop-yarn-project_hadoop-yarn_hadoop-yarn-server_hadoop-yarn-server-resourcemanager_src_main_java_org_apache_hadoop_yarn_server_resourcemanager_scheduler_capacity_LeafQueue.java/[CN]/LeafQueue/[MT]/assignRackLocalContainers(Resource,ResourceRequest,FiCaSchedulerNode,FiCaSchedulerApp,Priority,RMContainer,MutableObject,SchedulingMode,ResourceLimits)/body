{
  if (canAssign(application,priority,node,NodeType.RACK_LOCAL,reservedContainer)) {
    return assignContainer(clusterResource,node,application,priority,rackLocalResourceRequest,NodeType.RACK_LOCAL,reservedContainer,allocatedContainer,schedulingMode,currentResoureLimits);
  }
  return new CSAssignment(Resources.none(),NodeType.RACK_LOCAL);
}
