{
  if (rmContainer.getState().equals(RMContainerState.COMPLETED)) {
    return;
  }
synchronized (this) {
    allocateResource(clusterResource,attempt,rmContainer.getContainer().getResource(),labelManager.getLabelsOnNode(rmContainer.getContainer().getNodeId()));
  }
  getParent().recoverContainer(clusterResource,attempt,rmContainer);
}
