{
  Resources.subtractFrom(usedResources,resource);
  CSQueueUtils.updateQueueStatistics(resourceCalculator,this,getParent(),clusterResource,minimumAllocation);
  --numContainers;
  String userName=application.getUser();
  User user=getUser(userName);
  user.releaseContainer(resource);
  metrics.setAvailableResourcesToUser(userName,application.getHeadroom());
  LOG.info(getQueueName() + " used=" + usedResources+ " numContainers="+ numContainers+ " user="+ userName+ " user-resources="+ user.getConsumedResources());
}
