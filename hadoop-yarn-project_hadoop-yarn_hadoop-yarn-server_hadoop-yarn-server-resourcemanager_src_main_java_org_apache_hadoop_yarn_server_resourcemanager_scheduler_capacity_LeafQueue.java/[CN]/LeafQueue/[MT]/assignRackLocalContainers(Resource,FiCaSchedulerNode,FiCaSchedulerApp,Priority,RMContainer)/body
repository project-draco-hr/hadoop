{
  ResourceRequest request=application.getResourceRequest(priority,node.getRackName());
  if (request != null) {
    if (canAssign(application,priority,node,NodeType.RACK_LOCAL,reservedContainer)) {
      return assignContainer(clusterResource,node,application,priority,request,NodeType.RACK_LOCAL,reservedContainer);
    }
  }
  return Resources.none();
}
