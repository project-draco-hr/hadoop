{
  Resource resourceByLabel=labelManager.getResourceByLabel(nodePartition,clusterResource);
  float consumed=0;
  User user;
  for (  Map.Entry<String,User> entry : users.entrySet()) {
    user=entry.getValue();
    consumed+=user.resetAndUpdateUsageRatio(resourceCalculator,resourceByLabel,nodePartition);
  }
  return consumed;
}
