{
  Resource userLimit=computeUserLimitAndSetHeadroom(application,clusterResource,capability,null);
  if (!canAssignToThisQueue(clusterResource,null,this.currentResourceLimits,capability,Resources.none())) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("was going to reserve but hit queue limit");
    }
    return false;
  }
  if (!assignToUser(clusterResource,application.getUser(),userLimit,application,false,null)) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("was going to reserve but hit user limit");
    }
    return false;
  }
  return true;
}
