{
  try {
    readLock.lock();
    return queueName + ": " + "capacity="+ queueCapacities.getCapacity()+ ", "+ "absoluteCapacity="+ queueCapacities.getAbsoluteCapacity()+ ", "+ "usedResources="+ queueUsage.getUsed()+ ", "+ "usedCapacity="+ getUsedCapacity()+ ", "+ "absoluteUsedCapacity="+ getAbsoluteUsedCapacity()+ ", "+ "numApps="+ getNumApplications()+ ", "+ "numContainers="+ getNumContainers();
  }
  finally {
    readLock.unlock();
  }
}
