{
  ArrayList<UserInfo> usersToReturn=new ArrayList<UserInfo>();
  for (  Map.Entry<String,User> entry : users.entrySet()) {
    usersToReturn.add(new UserInfo(entry.getKey(),Resources.clone(entry.getValue().getUsed()),entry.getValue().getActiveApplications(),entry.getValue().getPendingApplications()));
  }
  return usersToReturn;
}
