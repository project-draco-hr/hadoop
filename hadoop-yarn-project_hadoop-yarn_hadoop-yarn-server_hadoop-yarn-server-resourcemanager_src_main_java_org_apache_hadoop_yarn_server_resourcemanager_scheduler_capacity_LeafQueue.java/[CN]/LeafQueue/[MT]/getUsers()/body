{
  ArrayList<UserInfo> usersToReturn=new ArrayList<UserInfo>();
  for (  Map.Entry<String,User> entry : users.entrySet()) {
    User user=entry.getValue();
    usersToReturn.add(new UserInfo(entry.getKey(),Resources.clone(user.getUsed()),user.getActiveApplications(),user.getPendingApplications(),Resources.clone(user.getConsumedAMResources()),Resources.clone(user.getUserResourceLimit())));
  }
  return usersToReturn;
}
