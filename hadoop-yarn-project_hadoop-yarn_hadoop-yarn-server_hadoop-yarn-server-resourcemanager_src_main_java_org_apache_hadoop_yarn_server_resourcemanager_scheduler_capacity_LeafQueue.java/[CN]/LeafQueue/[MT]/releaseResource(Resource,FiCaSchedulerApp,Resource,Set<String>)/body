{
  super.releaseResource(clusterResource,resource,nodeLabels);
  String userName=application.getUser();
  User user=getUser(userName);
  user.releaseContainer(resource,nodeLabels);
  metrics.setAvailableResourcesToUser(userName,application.getHeadroom());
  LOG.info(getQueueName() + " used=" + usedResources+ " numContainers="+ numContainers+ " user="+ userName+ " user-resources="+ user.getTotalConsumedResources());
}
