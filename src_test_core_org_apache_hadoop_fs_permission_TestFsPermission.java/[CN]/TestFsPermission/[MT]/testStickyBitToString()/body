{
  for (  boolean sb : new boolean[]{false,true}) {
    for (    FsAction u : FsAction.values()) {
      for (      FsAction g : FsAction.values()) {
        for (        FsAction o : FsAction.values()) {
          FsPermission f=new FsPermission(u,g,o,sb);
          if (f.getStickyBit() && f.getOtherAction().implies(EXECUTE))           assertEquals('t',f.toString().charAt(8));
 else           if (f.getStickyBit() && !f.getOtherAction().implies(EXECUTE))           assertEquals('T',f.toString().charAt(8));
 else           if (!f.getStickyBit() && f.getOtherAction().implies(EXECUTE))           assertEquals('x',f.toString().charAt(8));
 else           assertEquals('-',f.toString().charAt(8));
        }
      }
    }
  }
}
