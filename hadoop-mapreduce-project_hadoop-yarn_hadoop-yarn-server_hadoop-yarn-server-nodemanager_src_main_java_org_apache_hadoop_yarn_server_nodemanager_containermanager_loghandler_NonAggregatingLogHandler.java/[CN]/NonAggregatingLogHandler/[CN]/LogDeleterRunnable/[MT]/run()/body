{
  Path[] localAppLogDirs=new Path[NonAggregatingLogHandler.this.rootLogDirs.length];
  int index=0;
  for (  String rootLogDir : NonAggregatingLogHandler.this.rootLogDirs) {
    localAppLogDirs[index]=new Path(rootLogDir,applicationId.toString());
    index++;
  }
  NonAggregatingLogHandler.this.dispatcher.getEventHandler().handle(new ApplicationEvent(this.applicationId,ApplicationEventType.APPLICATION_LOG_HANDLING_FINISHED));
  NonAggregatingLogHandler.this.delService.delete(user,null,localAppLogDirs);
}
