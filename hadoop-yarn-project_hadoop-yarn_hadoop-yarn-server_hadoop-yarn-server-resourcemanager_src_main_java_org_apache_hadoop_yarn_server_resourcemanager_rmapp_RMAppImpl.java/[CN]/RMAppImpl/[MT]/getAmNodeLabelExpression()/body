{
  String amNodeLabelExpression=null;
  if (!getApplicationSubmissionContext().getUnmanagedAM()) {
    amNodeLabelExpression=getAMResourceRequest().getNodeLabelExpression();
    amNodeLabelExpression=(amNodeLabelExpression == null) ? NodeLabel.NODE_LABEL_EXPRESSION_NOT_SET : amNodeLabelExpression;
    amNodeLabelExpression=(amNodeLabelExpression.trim().isEmpty()) ? NodeLabel.DEFAULT_NODE_LABEL_PARTITION : amNodeLabelExpression;
  }
  return amNodeLabelExpression;
}
