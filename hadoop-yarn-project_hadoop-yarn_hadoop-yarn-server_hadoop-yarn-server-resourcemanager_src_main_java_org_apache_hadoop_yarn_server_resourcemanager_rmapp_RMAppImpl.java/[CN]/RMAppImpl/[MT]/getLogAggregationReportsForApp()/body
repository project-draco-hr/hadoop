{
  try {
    this.readLock.lock();
    Map<NodeId,LogAggregationReport> outputs=new HashMap<NodeId,LogAggregationReport>();
    outputs.putAll(logAggregationStatus);
    for (    Entry<NodeId,LogAggregationReport> output : outputs.entrySet()) {
      if (!output.getValue().getLogAggregationStatus().equals(LogAggregationStatus.TIME_OUT) && !output.getValue().getLogAggregationStatus().equals(LogAggregationStatus.FINISHED) && isAppInFinalState(this)&& System.currentTimeMillis() > this.logAggregationStartTime + this.logAggregationStatusTimeout) {
        output.getValue().setLogAggregationStatus(LogAggregationStatus.TIME_OUT);
      }
    }
    return outputs;
  }
  finally {
    this.readLock.unlock();
  }
}
