{
  RMAppRunningOnNodeEvent nodeAddedEvent=(RMAppRunningOnNodeEvent)event;
  if (isAppInFinalState(app)) {
    app.handler.handle(new RMNodeCleanAppEvent(nodeAddedEvent.getNodeId(),nodeAddedEvent.getApplicationId()));
    return;
  }
  app.ranNodes.add(nodeAddedEvent.getNodeId());
  app.logAggregationStatus.put(nodeAddedEvent.getNodeId(),LogAggregationReport.newInstance(app.applicationId,nodeAddedEvent.getNodeId(),app.logAggregationEnabled ? LogAggregationStatus.NOT_START : LogAggregationStatus.DISABLED,""));
}
