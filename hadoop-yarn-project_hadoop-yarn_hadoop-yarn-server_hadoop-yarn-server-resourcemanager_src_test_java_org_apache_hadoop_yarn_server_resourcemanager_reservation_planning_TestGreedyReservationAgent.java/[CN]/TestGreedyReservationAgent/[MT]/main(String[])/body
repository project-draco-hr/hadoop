{
  boolean left=false;
  int numJobs=1000;
  if (arg.length > 0) {
    numJobs=Integer.parseInt(arg[0]);
  }
  if (arg.length > 1) {
    left=Boolean.parseBoolean(arg[1]);
  }
  try {
    TestGreedyReservationAgent test=new TestGreedyReservationAgent(left);
    test.setup();
    test.testStress(numJobs);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
