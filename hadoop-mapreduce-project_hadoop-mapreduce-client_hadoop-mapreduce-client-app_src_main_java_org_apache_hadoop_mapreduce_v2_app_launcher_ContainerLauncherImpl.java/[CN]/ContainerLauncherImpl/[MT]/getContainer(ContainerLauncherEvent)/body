{
  ContainerId id=event.getContainerID();
  Container c=containers.get(id);
  if (c == null) {
    c=new Container(event.getTaskAttemptID(),event.getContainerID(),event.getContainerMgrAddress());
    Container old=containers.putIfAbsent(id,c);
    if (old != null) {
      c=old;
    }
  }
  return c;
}
