{
  dataIn.close();
  if (checksumIn != null) {
    checksumIn.close();
  }
  if (slowReadBuff != null) {
    bufferPool.returnBuffer(slowReadBuff);
    slowReadBuff=null;
  }
  if (checksumBuff != null) {
    bufferPool.returnBuffer(checksumBuff);
    checksumBuff=null;
  }
  startOffset=-1;
  checksum=null;
}
