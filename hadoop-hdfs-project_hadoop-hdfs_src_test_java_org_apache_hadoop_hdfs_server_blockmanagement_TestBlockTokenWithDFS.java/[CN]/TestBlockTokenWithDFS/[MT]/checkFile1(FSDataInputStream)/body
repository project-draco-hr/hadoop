{
  byte[] toRead=new byte[FILE_SIZE];
  int totalRead=0;
  int nRead=0;
  try {
    while ((nRead=in.read(toRead,totalRead,toRead.length - totalRead)) > 0) {
      totalRead+=nRead;
    }
  }
 catch (  IOException e) {
    return false;
  }
  assertEquals("Cannot read file.",toRead.length,totalRead);
  return checkFile(toRead);
}
