def getServices(self):
    'get all the services from the config'
    sdd = {}
    for keys in self.hodConfig:
        if keys.startswith('gridservice-'):
            str = keys.split('-')
            dict = self.hodConfig[keys]
            if ('server-params' in dict):
                dict['attrs'] = dict['server-params']
            if ('final-server-params' in dict):
                dict['final-attrs'] = dict['final-server-params']
            dict['id'] = str[1]
            desc = ServiceDesc(dict)
            sdd[desc.getName()] = desc
    return sdd
