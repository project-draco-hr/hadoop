{
  StringBuilder sb=new StringBuilder("Counters: " + countCounters());
  for (  CounterGroup group : this) {
    sb.append("\n\t" + group.getDisplayName());
    for (    Counter counter : group) {
      sb.append("\n\t\t" + counter.getDisplayName() + "="+ counter.getValue());
    }
  }
  return sb.toString();
}
