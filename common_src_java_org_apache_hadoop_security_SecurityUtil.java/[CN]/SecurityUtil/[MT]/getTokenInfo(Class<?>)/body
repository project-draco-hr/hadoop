{
  for (  SecurityInfo provider : testProviders) {
    TokenInfo result=provider.getTokenInfo(protocol);
    if (result != null) {
      return result;
    }
  }
  for (  SecurityInfo provider : securityInfoProviders) {
    TokenInfo result=provider.getTokenInfo(protocol);
    if (result != null) {
      return result;
    }
  }
  return null;
}
