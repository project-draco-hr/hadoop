{
  LOG.info("Sending assigned event to " + yarnAttemptID);
  ContainerId cId=attemptInfo.getContainerId();
  NodeId nodeId=ConverterUtils.toNodeId(attemptInfo.getHostname() + ":" + attemptInfo.getPort());
  Container container=BuilderUtils.newContainer(cId,nodeId,attemptInfo.getTrackerName() + ":" + attemptInfo.getHttpPort(),null,null,null);
  actualHandler.handle(new TaskAttemptContainerAssignedEvent(yarnAttemptID,container,null));
}
