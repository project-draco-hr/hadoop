{
  final float epsilon=1e-5f;
  assertEquals("capacity",expectedCapacity,q.getCapacity(),epsilon);
  assertEquals("absolute capacity",expectedAbsCapacity,q.getAbsoluteCapacity(),epsilon);
  assertEquals("maximum capacity",expectedMaxCapacity,q.getMaximumCapacity(),epsilon);
  assertEquals("absolute maximum capacity",expectedAbsMaxCapacity,q.getAbsoluteMaximumCapacity(),epsilon);
}
