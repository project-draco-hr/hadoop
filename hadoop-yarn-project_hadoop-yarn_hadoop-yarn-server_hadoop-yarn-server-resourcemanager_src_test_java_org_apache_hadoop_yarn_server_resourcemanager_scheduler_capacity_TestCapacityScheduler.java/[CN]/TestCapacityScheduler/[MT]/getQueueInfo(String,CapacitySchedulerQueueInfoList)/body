{
  if (info != null) {
    for (    CapacitySchedulerQueueInfo queueInfo : info.getQueueInfoList()) {
      if (queueInfo.getQueueName().equals(name)) {
        return queueInfo;
      }
 else {
        CapacitySchedulerQueueInfo result=getQueueInfo(name,queueInfo.getQueues());
        if (result == null) {
          continue;
        }
        return result;
      }
    }
  }
  return null;
}
