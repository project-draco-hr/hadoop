{
  if (isFencedState()) {
    LOG.info("State store is in Fenced state. Can't store RM Delegation Token.");
    return;
  }
  try {
    storeRMDelegationTokenAndSequenceNumberState(rmDTIdentifier,renewDate,latestSequenceNumber);
  }
 catch (  Exception e) {
    notifyStoreOperationFailed(e);
  }
}
