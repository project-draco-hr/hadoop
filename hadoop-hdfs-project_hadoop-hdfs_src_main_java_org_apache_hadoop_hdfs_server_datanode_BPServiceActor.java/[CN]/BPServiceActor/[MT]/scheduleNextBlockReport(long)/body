{
  if (resetBlockReportTime) {
    lastBlockReport=previousReportStartTime - DFSUtil.getRandom().nextInt((int)(dnConf.blockReportInterval));
    resetBlockReportTime=false;
  }
 else {
    lastBlockReport+=(monotonicNow() - lastBlockReport) / dnConf.blockReportInterval * dnConf.blockReportInterval;
  }
}
