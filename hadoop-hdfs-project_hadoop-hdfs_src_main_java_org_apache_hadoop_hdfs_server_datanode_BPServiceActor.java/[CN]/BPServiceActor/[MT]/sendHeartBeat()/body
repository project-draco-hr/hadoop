{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Sending heartbeat from service actor: " + this);
  }
  StorageReport[] report={new StorageReport(bpRegistration.getDatanodeUuid(),false,dn.getFSDataset().getCapacity(),dn.getFSDataset().getDfsUsed(),dn.getFSDataset().getRemaining(),dn.getFSDataset().getBlockPoolUsed(bpos.getBlockPoolId()))};
  return bpNamenode.sendHeartbeat(bpRegistration,report,dn.getXmitsInProgress(),dn.getXceiverCount(),dn.getFSDataset().getNumFailedVolumes());
}
