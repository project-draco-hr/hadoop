{
  LOG.info("heartbeat: " + this);
  return bpNamenode.sendHeartbeat(bpRegistration,dn.getFSDataset().getCapacity(),dn.getFSDataset().getDfsUsed(),dn.getFSDataset().getRemaining(),dn.getFSDataset().getBlockPoolUsed(bpos.getBlockPoolId()),dn.getXmitsInProgress(),dn.getXceiverCount(),dn.getFSDataset().getNumFailedVolumes());
}
