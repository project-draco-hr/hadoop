{
  try {
    if (str != null) {
      str=str.trim();
      if (str.length() > 0) {
        value=parse(str);
      }
    }
  }
 catch (  Exception ex) {
    throw new IllegalArgumentException(MessageFormat.format("Parameter [{0}], invalid value [{1}], value must be [{2}]",getName(),str,getDomain()));
  }
  return value;
}
