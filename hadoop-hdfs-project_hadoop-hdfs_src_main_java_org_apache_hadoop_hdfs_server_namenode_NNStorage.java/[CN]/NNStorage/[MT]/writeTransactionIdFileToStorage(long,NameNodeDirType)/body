{
  for (Iterator<StorageDirectory> it=dirIterator(type); it.hasNext(); ) {
    StorageDirectory sd=it.next();
    try {
      writeTransactionIdFile(sd,txid);
    }
 catch (    IOException e) {
      LOG.warn("writeTransactionIdToStorage failed on " + sd,e);
      reportErrorsOnDirectory(sd);
    }
  }
}
