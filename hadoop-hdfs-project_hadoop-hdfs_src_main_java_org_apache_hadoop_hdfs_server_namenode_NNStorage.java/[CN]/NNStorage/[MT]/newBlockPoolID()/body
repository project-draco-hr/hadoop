{
  String ip;
  try {
    ip=DNS.getDefaultIP("default");
  }
 catch (  UnknownHostException e) {
    LOG.warn("Could not find ip address of \"default\" inteface.");
    throw e;
  }
  int rand=DFSUtil.getSecureRandom().nextInt(Integer.MAX_VALUE);
  return "BP-" + rand + "-"+ ip+ "-"+ Time.now();
}
