{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Loading inode reference section");
  }
  while (true) {
    INodeReferenceSection.INodeReference e=INodeReferenceSection.INodeReference.parseDelimitedFrom(in);
    if (e == null) {
      break;
    }
    refList.add(e);
    if (LOG.isTraceEnabled()) {
      LOG.trace("Loaded inode reference named '" + e.getName() + "' referring to id "+ e.getReferredId()+ "");
    }
  }
  if (LOG.isDebugEnabled()) {
    LOG.debug("Loaded " + refList.size() + " inode references");
  }
}
