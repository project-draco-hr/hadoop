{
  if (isWildCardACLValue(group)) {
    throw new IllegalArgumentException("Group " + group + " can not be added");
  }
  if (!isAllAllowed()) {
    List<String> groupsList=new LinkedList<String>();
    groupsList.add(group);
    groupsMapping.cacheGroupsAdd(groupsList);
    groups.add(group);
  }
}
