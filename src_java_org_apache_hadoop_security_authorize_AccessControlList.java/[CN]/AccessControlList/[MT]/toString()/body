{
  StringBuilder sb=new StringBuilder();
  boolean first=true;
  for (  String user : users) {
    if (!first) {
      sb.append(",");
    }
 else {
      first=false;
    }
    sb.append(user);
  }
  if (!groups.isEmpty()) {
    sb.append(" ");
  }
  first=true;
  for (  String group : groups) {
    if (!first) {
      sb.append(",");
    }
 else {
      first=false;
    }
    sb.append(group);
  }
  return sb.toString();
}
