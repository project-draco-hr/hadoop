{
  readLock();
  try {
    EncryptionZoneInt ezi=getEncryptionZoneForPath(iip);
    if (ezi == null) {
      return false;
    }
    for (    KeyVersion ezVersion : ezi.keyVersions) {
      if (keyVersionName.equals(ezVersion.getVersionName())) {
        return true;
      }
    }
    return false;
  }
  finally {
    readUnlock();
  }
}
