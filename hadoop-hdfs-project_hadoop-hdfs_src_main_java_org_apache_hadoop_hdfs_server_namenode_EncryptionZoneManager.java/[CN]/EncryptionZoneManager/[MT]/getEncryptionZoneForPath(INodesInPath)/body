{
  Preconditions.checkNotNull(iip);
  final INode[] inodes=iip.getINodes();
  for (int i=inodes.length - 1; i >= 0; i--) {
    final INode inode=inodes[i];
    if (inode != null) {
      final EncryptionZoneInt ezi=encryptionZones.get(inode.getId());
      if (ezi != null) {
        return ezi;
      }
    }
  }
  return null;
}
