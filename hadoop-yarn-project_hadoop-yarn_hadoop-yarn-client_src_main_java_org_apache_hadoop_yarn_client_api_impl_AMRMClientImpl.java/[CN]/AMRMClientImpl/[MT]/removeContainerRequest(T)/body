{
  Set<String> allRacks=new HashSet<String>();
  if (req.getRacks() != null) {
    allRacks.addAll(req.getRacks());
  }
  allRacks.addAll(resolveRacks(req.getNodes()));
  if (req.getNodes() != null) {
    for (    String node : new HashSet<String>(req.getNodes())) {
      decResourceRequest(req.getPriority(),node,req.getCapability(),req.getContainerCount(),req);
    }
  }
  for (  String rack : allRacks) {
    decResourceRequest(req.getPriority(),rack,req.getCapability(),req.getContainerCount(),req);
  }
  decResourceRequest(req.getPriority(),ResourceRequest.ANY,req.getCapability(),req.getContainerCount(),req);
}
