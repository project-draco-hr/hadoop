{
  String exp=containerRequest.getNodeLabelExpression();
  if (null == exp || exp.isEmpty()) {
    return;
  }
  if (exp.contains("&&") || exp.contains("||")) {
    throw new InvalidContainerRequestException("Cannot specify more than two node labels" + " in a single node label expression");
  }
  if ((containerRequest.getRacks() != null && (!containerRequest.getRacks().isEmpty())) || (containerRequest.getNodes() != null && (!containerRequest.getNodes().isEmpty()))) {
    throw new InvalidContainerRequestException("Cannot specify node label with rack and node");
  }
}
