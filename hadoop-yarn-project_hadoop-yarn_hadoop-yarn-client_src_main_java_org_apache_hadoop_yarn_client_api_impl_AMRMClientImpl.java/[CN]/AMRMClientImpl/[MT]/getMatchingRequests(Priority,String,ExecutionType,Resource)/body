{
  Preconditions.checkArgument(capability != null,"The Resource to be requested should not be null ");
  Preconditions.checkArgument(priority != null,"The priority at which to request containers should not be null ");
  List<LinkedHashSet<T>> list=new LinkedList<LinkedHashSet<T>>();
  RemoteRequestsTable remoteRequestsTable=getTable(0);
  List<ResourceRequestInfo<T>> matchingRequests=remoteRequestsTable.getMatchingRequests(priority,resourceName,executionType,capability);
  for (  ResourceRequestInfo<T> resReqInfo : matchingRequests) {
    if (canFit(resReqInfo.remoteRequest.getCapability(),capability) && !resReqInfo.containerRequests.isEmpty()) {
      list.add(resReqInfo.containerRequests);
    }
  }
  return list;
}
