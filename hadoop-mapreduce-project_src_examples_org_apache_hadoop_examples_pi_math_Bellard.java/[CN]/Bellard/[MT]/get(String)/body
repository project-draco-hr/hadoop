{
  s=s.trim();
  if (s.charAt(0) == 'P')   s=s.substring(1);
  final String[] parts=s.split("\\D+");
  if (parts.length >= 2) {
    final String name="P" + parts[0] + "_"+ parts[1];
    for (    Parameter p : values())     if (p.name().equals(name))     return p;
  }
  throw new IllegalArgumentException("s=" + s + ", parts="+ Arrays.asList(parts));
}
