{
  while (true) {
    openCurrentStream();
    try {
      int limit=Math.min(blockAvailable(),len);
      int value=currentStream.read(b,offset,limit);
      currentOffset+=value;
      nextLocation=0;
      return value;
    }
 catch (    BlockMissingException e) {
      setAlternateLocations(e,currentOffset);
    }
catch (    ChecksumException e) {
      setAlternateLocations(e,currentOffset);
    }
  }
}
