{
  if (stopped.getAndSet(true)) {
    return;
  }
  if (allocatorThread != null) {
    allocatorThread.interrupt();
    try {
      allocatorThread.join();
    }
 catch (    InterruptedException ie) {
      LOG.warn("InterruptedException while stopping",ie);
    }
  }
  if (shouldUnregister) {
    unregister();
  }
  super.serviceStop();
}
