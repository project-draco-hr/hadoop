{
  int heartbeatInterval=conf.getInt(SLSConfiguration.AM_HEARTBEAT_INTERVAL_MS,SLSConfiguration.AM_HEARTBEAT_INTERVAL_MS_DEFAULT);
  int containerMemoryMB=conf.getInt(SLSConfiguration.CONTAINER_MEMORY_MB,SLSConfiguration.CONTAINER_MEMORY_MB_DEFAULT);
  int containerVCores=conf.getInt(SLSConfiguration.CONTAINER_VCORES,SLSConfiguration.CONTAINER_VCORES_DEFAULT);
  Resource containerResource=BuilderUtils.newResource(containerMemoryMB,containerVCores);
  if (isSLS) {
    startAMFromSLSTraces(containerResource,heartbeatInterval);
  }
 else {
    startAMFromRumenTraces(containerResource,heartbeatInterval);
  }
  numAMs=amMap.size();
  remainingApps=numAMs;
}
