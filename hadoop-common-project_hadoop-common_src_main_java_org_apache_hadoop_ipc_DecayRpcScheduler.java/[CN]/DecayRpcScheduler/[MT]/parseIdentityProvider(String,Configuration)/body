{
  List<IdentityProvider> providers=conf.getInstances(ns + "." + CommonConfigurationKeys.IPC_CALLQUEUE_IDENTITY_PROVIDER_KEY,IdentityProvider.class);
  if (providers.size() < 1) {
    LOG.info("IdentityProvider not specified, " + "defaulting to UserIdentityProvider");
    return new UserIdentityProvider();
  }
  return providers.get(0);
}
