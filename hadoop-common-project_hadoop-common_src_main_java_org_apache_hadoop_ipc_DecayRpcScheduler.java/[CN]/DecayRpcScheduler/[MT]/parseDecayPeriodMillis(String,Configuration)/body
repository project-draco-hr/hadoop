{
  long period=conf.getLong(ns + "." + IPC_FCQ_DECAYSCHEDULER_PERIOD_KEY,0);
  if (period == 0) {
    period=conf.getLong(ns + "." + IPC_SCHEDULER_DECAYSCHEDULER_PERIOD_KEY,IPC_SCHEDULER_DECAYSCHEDULER_PERIOD_DEFAULT);
  }
 else   if (period > 0) {
    LOG.warn((IPC_FCQ_DECAYSCHEDULER_PERIOD_KEY + " is deprecated. Please use " + IPC_SCHEDULER_DECAYSCHEDULER_PERIOD_KEY));
  }
  if (period <= 0) {
    throw new IllegalArgumentException("Period millis must be >= 0");
  }
  return period;
}
