{
  AtomicLong count=this.callCounts.get(identity);
  if (count == null) {
    count=new AtomicLong(0);
    AtomicLong otherCount=callCounts.putIfAbsent(identity,count);
    if (otherCount != null) {
      count=otherCount;
    }
  }
  totalCalls.getAndIncrement();
  return count.getAndIncrement();
}
