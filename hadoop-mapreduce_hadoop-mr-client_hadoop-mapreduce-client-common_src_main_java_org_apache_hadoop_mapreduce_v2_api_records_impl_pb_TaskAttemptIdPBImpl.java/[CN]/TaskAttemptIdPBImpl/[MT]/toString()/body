{
  String identifier=(getTaskId() == null) ? "none" : getTaskId().getJobId().getAppId().getClusterTimestamp() + "_" + jobidFormat.format(getTaskId().getJobId().getAppId().getId())+ "_"+ ((getTaskId().getTaskType() == TaskType.MAP) ? "m" : "r")+ "_"+ idFormat.format(getTaskId().getId())+ "_"+ getId();
  return "attempt_" + identifier;
}
