{
  BlockPlacementStatus defaultStatus=super.verifyBlockPlacement(srcPath,lBlk,numberOfReplicas);
  BlockPlacementStatusWithUpgradeDomain upgradeDomainStatus=new BlockPlacementStatusWithUpgradeDomain(defaultStatus,getUpgradeDomainsFromNodes(lBlk.getLocations()),numberOfReplicas,upgradeDomainFactor);
  return upgradeDomainStatus;
}
