{
  for (  DiffReportEntry entry : entries) {
    System.out.println("DiffEntry is:" + entry.getType() + "\""+ new String(entry.getRelativePath())+ "\"");
    if ((entry.getType() == type) && ((new String(entry.getRelativePath())).compareTo(relativePath) == 0)) {
      return true;
    }
  }
  return false;
}
