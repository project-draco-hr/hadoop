{
  if (skipBuf == null) {
    skipBuf=new byte[bytesPerChecksum];
  }
  long nSkipped=0;
  while (nSkipped < n) {
    int toSkip=(int)Math.min(n - nSkipped,skipBuf.length);
    int ret=read(skipBuf,0,toSkip);
    if (ret <= 0) {
      return nSkipped;
    }
    nSkipped+=ret;
  }
  return nSkipped;
}
