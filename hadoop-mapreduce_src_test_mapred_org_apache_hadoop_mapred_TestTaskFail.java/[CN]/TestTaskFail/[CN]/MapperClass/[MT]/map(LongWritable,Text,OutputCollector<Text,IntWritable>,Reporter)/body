{
  System.err.println(taskLog);
  assertFalse(Boolean.getBoolean(System.getProperty("hadoop.tasklog.iscleanup")));
  if (taskid.endsWith("_0")) {
    throw new IOException();
  }
 else   if (taskid.endsWith("_1")) {
    System.exit(-1);
  }
 else   if (taskid.endsWith("_2")) {
    throw new Error();
  }
}
