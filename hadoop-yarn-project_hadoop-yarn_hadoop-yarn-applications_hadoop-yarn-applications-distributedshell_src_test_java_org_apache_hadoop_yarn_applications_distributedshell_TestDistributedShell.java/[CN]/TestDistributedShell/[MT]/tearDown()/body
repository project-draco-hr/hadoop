{
  if (yarnCluster != null) {
    try {
      yarnCluster.stop();
    }
  finally {
      yarnCluster=null;
    }
  }
  if (hdfsCluster != null) {
    try {
      hdfsCluster.shutdown();
    }
  finally {
      hdfsCluster=null;
    }
  }
  FileContext fsContext=FileContext.getLocalFSFileContext();
  fsContext.delete(new Path(conf.get("yarn.timeline-service.leveldb-timeline-store.path")),true);
}
