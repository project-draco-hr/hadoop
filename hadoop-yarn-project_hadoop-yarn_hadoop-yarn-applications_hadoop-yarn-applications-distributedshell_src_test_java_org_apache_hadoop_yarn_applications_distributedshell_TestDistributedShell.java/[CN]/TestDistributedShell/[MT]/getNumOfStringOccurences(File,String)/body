{
  BufferedReader reader=null;
  String strLine;
  long actualCount=0;
  try {
    reader=new BufferedReader(new FileReader(entityFile));
    while ((strLine=reader.readLine()) != null) {
      if (strLine.trim().contains(searchString)) {
        actualCount++;
      }
    }
  }
  finally {
    reader.close();
  }
  return actualCount;
}
