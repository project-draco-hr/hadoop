{
  if (LOG.isDebugEnabled()) {
    LOG.debug("App " + getName() + " is going to preempt a running "+ "container");
  }
  RMContainer toBePreempted=null;
  for (  RMContainer container : app.getLiveContainers()) {
    if (!app.getPreemptionContainers().contains(container) && (toBePreempted == null || comparator.compare(toBePreempted,container) > 0)) {
      toBePreempted=container;
    }
  }
  return toBePreempted;
}
