{
  byte[] result=new byte[utf8Length(string)];
  try {
synchronized (OBUF) {
      OBUF.reset();
      writeChars(OBUF,string,0,string.length());
      System.arraycopy(OBUF.getData(),0,result,0,OBUF.getLength());
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  return result;
}
