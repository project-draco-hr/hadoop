{
  if (domain == null)   domain=HADOOP_JMX_DOMAIN;
  ObjectName jmxBean;
  try {
    jmxBean=new ObjectName(domain + ":service=" + serviceName+ ",name="+ typeName);
  }
 catch (  MalformedObjectNameException e) {
    LOG.debug(e.getStackTrace());
    throw new IOException(e);
  }
  return jmxBean;
}
