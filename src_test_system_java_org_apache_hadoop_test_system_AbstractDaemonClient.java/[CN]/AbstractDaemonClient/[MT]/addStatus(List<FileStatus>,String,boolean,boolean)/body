{
  FileStatus[] fs=listStatus(f,local);
  if (fs != null) {
    for (    FileStatus fileStatus : fs) {
      if (!f.equals(fileStatus.getPath().toString())) {
        status.add(fileStatus);
        if (recursive) {
          addStatus(status,fileStatus.getPath().toString(),local,recursive);
        }
      }
    }
  }
}
