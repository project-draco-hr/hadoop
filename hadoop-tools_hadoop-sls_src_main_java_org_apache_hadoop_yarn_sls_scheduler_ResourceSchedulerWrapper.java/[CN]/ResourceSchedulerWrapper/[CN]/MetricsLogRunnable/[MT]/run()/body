{
  if (running) {
    String metrics=web.generateRealTimeTrackingMetrics();
    try {
      if (firstLine) {
        metricsLogBW.write(metrics + EOL);
        firstLine=false;
      }
 else {
        metricsLogBW.write("," + metrics + EOL);
      }
      metricsLogBW.flush();
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
}
