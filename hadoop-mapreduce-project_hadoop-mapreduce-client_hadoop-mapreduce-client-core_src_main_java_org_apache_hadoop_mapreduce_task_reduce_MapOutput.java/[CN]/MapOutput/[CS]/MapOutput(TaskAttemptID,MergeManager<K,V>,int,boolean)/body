{
  this.id=ID.incrementAndGet();
  this.mapId=mapId;
  this.merger=merger;
  type=Type.MEMORY;
  byteStream=new BoundedByteArrayOutputStream(size);
  memory=byteStream.getBuffer();
  this.size=size;
  localFS=null;
  disk=null;
  outputPath=null;
  tmpOutputPath=null;
  this.primaryMapOutput=primaryMapOutput;
}
