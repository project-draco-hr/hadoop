{
  File quotaFile=new File(path,CPU.getName() + "." + CGroupsHandler.CGROUP_CPU_QUOTA_US);
  if (quotaFile.exists()) {
    String contents=FileUtils.readFileToString(quotaFile,"UTF-8");
    int quotaUS=Integer.parseInt(contents.trim());
    if (quotaUS != -1) {
      return true;
    }
  }
  return false;
}
