{
  final long inputSizeInMB=1024 * 7;
  String tracePath=getTraceFile("cpu_emul_case1");
  Assert.assertNotNull("Trace file not found!",tracePath);
  String[] runtimeValues={"LOADJOB",SubmitterUserResolver.class.getName(),"STRESS",inputSizeInMB + "m",tracePath};
  String[] otherArgs={"-D",GridMixConfig.GRIDMIX_CPU_EMULATON + "=" + GridMixConfig.GRIDMIX_CPU_USAGE_PLUGIN,"-D",GridMixConfig.GRIDMIX_DISTCACHE_ENABLE + "=false","-D",GridMixConfig.GRIDMIX_CPU_CUSTOM_INTERVAL + "=0.25F"};
  runGridmixAndVerify(runtimeValues,otherArgs,tracePath,execMode);
}
