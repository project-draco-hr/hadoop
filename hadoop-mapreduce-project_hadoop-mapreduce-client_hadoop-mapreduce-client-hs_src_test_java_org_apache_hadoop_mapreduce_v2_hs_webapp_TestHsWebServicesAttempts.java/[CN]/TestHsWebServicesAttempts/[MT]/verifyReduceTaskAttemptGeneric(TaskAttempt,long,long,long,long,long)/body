{
  assertEquals("shuffleFinishTime wrong",ta.getShuffleFinishTime(),shuffleFinishTime);
  assertEquals("mergeFinishTime wrong",ta.getSortFinishTime(),mergeFinishTime);
  assertEquals("elapsedShuffleTime wrong",ta.getLaunchTime() - ta.getShuffleFinishTime(),elapsedShuffleTime);
  assertEquals("elapsedMergeTime wrong",ta.getShuffleFinishTime() - ta.getSortFinishTime(),elapsedMergeTime);
  assertEquals("elapsedReduceTime wrong",ta.getSortFinishTime() - ta.getFinishTime(),elapsedReduceTime);
}
