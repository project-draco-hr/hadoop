{
  GetGroupsForUserRequestProto requestProto=GetGroupsForUserRequestProto.newBuilder().setUser(user).build();
  try {
    GetGroupsForUserResponseProto responseProto=proxy.getGroupsForUser(null,requestProto);
    return (String[])responseProto.getGroupsList().toArray(new String[responseProto.getGroupsCount()]);
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
