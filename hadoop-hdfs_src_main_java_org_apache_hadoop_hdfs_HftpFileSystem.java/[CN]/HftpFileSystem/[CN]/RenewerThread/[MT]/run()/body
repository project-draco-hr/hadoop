{
  RenewAction action=null;
  while (true) {
    try {
      action=queue.take();
      if (action.renew()) {
        action.setNewTime(RENEW_CYCLE + System.currentTimeMillis());
        queue.add(action);
      }
      action=null;
    }
 catch (    InterruptedException ie) {
      return;
    }
catch (    Exception ie) {
      if (action != null) {
        LOG.warn("Failure to renew token " + action,ie);
      }
 else {
        LOG.warn("Failure in renew queue",ie);
      }
    }
  }
}
