{
  maybeInitBuilder();
  builder.clearContainersToDecrease();
  if (this.containersToDecrease == null) {
    return;
  }
  Iterable<ContainerProto> iterable=new Iterable<ContainerProto>(){
    @Override public Iterator<ContainerProto> iterator(){
      return new Iterator<ContainerProto>(){
        private Iterator<Container> iter=containersToDecrease.iterator();
        @Override public boolean hasNext(){
          return iter.hasNext();
        }
        @Override public ContainerProto next(){
          return convertToProtoFormat(iter.next());
        }
        @Override public void remove(){
          throw new UnsupportedOperationException();
        }
      }
;
    }
  }
;
  builder.addAllContainersToDecrease(iterable);
}
