{
  out.print("<" + INODE_REFERENCE_SECTION_REF + ">");
  o(INODE_REFERENCE_SECTION_REFERRED_ID,r.getReferredId()).o(SECTION_NAME,r.getName().toStringUtf8()).o(INODE_REFERENCE_SECTION_DST_SNAPSHOT_ID,r.getDstSnapshotId()).o(INODE_REFERENCE_SECTION_LAST_SNAPSHOT_ID,r.getLastSnapshotId());
  out.print("</" + INODE_REFERENCE_SECTION_REF + ">\n");
}
