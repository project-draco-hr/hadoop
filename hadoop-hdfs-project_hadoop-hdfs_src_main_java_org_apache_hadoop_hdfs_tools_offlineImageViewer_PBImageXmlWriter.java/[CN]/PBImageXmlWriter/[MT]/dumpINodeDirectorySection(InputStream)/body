{
  out.print("<INodeDirectorySection>");
  while (true) {
    INodeDirectorySection.DirEntry e=INodeDirectorySection.DirEntry.parseDelimitedFrom(in);
    if (e == null) {
      break;
    }
    out.print("<directory>");
    o("parent",e.getParent());
    for (    long id : e.getChildrenList()) {
      o("inode",id);
    }
    for (    int refId : e.getRefChildrenList()) {
      o("inodereference-index",refId);
    }
    out.print("</directory>\n");
  }
  out.print("</INodeDirectorySection>\n");
}
