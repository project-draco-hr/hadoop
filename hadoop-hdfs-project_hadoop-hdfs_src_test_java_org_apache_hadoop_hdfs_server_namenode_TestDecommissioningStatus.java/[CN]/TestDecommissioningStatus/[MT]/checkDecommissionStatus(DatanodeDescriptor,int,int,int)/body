{
  assertEquals("Unexpected num under-replicated blocks",expectedUnderRep,decommNode.decommissioningStatus.getUnderReplicatedBlocks());
  assertEquals("Unexpected number of decom-only replicas",expectedDecommissionOnly,decommNode.decommissioningStatus.getDecommissionOnlyReplicas());
  assertEquals("Unexpected number of replicas in under-replicated open files",expectedUnderRepInOpenFiles,decommNode.decommissioningStatus.getUnderReplicatedInOpenFiles());
}
