{
  if (blocks == null) {
    return;
  }
  for (int i=0; i < blocks.length; i++) {
    final String err=checkBlockComplete(blocks,i,numCommittedAllowed,minReplication);
    Preconditions.checkState(err == null,"Unexpected block state: %s, file=%s (%s), blocks=%s (i=%s)",err,this,getClass().getSimpleName(),Arrays.asList(blocks),i);
  }
}
