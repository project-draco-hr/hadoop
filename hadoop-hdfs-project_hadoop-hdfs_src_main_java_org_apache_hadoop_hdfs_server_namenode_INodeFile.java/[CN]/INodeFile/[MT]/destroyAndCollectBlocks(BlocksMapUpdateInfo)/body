{
  if (blocks != null && collectedBlocks != null) {
    for (    BlockInfo blk : blocks) {
      collectedBlocks.addDeleteBlock(blk);
      blk.setBlockCollection(null);
    }
  }
  setBlocks(null);
  clear();
  if (this instanceof FileWithSnapshot) {
    ((FileWithSnapshot)this).getDiffs().clear();
  }
}
