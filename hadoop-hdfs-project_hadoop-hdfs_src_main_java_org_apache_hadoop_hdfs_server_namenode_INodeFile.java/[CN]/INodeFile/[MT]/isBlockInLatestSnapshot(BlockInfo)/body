{
  FileWithSnapshotFeature sf=this.getFileWithSnapshotFeature();
  if (sf == null || sf.getDiffs() == null)   return false;
  BlockInfo[] snapshotBlocks=getDiffs().findEarlierSnapshotBlocks(getDiffs().getLastSnapshotId());
  if (snapshotBlocks == null)   return false;
  return Arrays.asList(snapshotBlocks).contains(block);
}
