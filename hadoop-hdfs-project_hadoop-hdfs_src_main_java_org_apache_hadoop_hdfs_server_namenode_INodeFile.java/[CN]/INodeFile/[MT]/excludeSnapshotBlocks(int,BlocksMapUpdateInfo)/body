{
  if (collectedBlocks == null || collectedBlocks.getToDeleteList().isEmpty())   return;
  FileWithSnapshotFeature sf=getFileWithSnapshotFeature();
  if (sf == null)   return;
  BlockInfo[] snapshotBlocks=getDiffs().findEarlierSnapshotBlocks(snapshotId);
  if (snapshotBlocks == null)   return;
  List<BlockInfo> toDelete=collectedBlocks.getToDeleteList();
  for (  BlockInfo blk : snapshotBlocks) {
    if (toDelete.contains(blk))     collectedBlocks.removeDeleteBlock(blk);
  }
}
