{
  if (bpid == null || bpid.equals("")) {
    throw new InconsistentFSStateException(storage,"file " + STORAGE_FILE_VERSION + " is invalid.");
  }
  if (!blockpoolID.equals("") && !blockpoolID.equals(bpid)) {
    throw new InconsistentFSStateException(storage,"Unexpected blockpoolID " + bpid + ". Expected "+ blockpoolID);
  }
  blockpoolID=bpid;
}
