{
  super(NodeType.DATA_NODE);
  this.namespaceID=namespaceID;
  this.blockpoolID=bpID;
  this.cTime=cTime;
  this.clusterID=clusterId;
  storagesWithRollingUpgradeMarker=Collections.newSetFromMap(new ConcurrentHashMap<String,Boolean>());
  storagesWithoutRollingUpgradeMarker=Collections.newSetFromMap(new ConcurrentHashMap<String,Boolean>());
}
