{
  NMContainerStatus report=((RMContainerRecoverEvent)event).getContainerReport();
  if (report.getContainerState().equals(ContainerState.COMPLETE)) {
    ContainerStatus status=ContainerStatus.newInstance(report.getContainerId(),report.getContainerState(),report.getDiagnostics(),report.getContainerExitStatus());
    new FinishedTransition().transition(container,new RMContainerFinishedEvent(container.containerId,status,RMContainerEventType.FINISHED));
    return RMContainerState.COMPLETED;
  }
 else   if (report.getContainerState().equals(ContainerState.RUNNING)) {
    container.eventHandler.handle(new RMAppRunningOnNodeEvent(container.getApplicationAttemptId().getApplicationId(),container.nodeId));
    return RMContainerState.RUNNING;
  }
 else {
    LOG.warn("RMContainer received unexpected recover event with container" + " state " + report.getContainerState() + " while recovering.");
    return RMContainerState.RUNNING;
  }
}
