{
  try {
    readLock.lock();
    StringBuilder logURL=new StringBuilder();
    logURL.append(WebAppUtils.getHttpSchemePrefix(rmContext.getYarnConfiguration()));
    logURL.append(WebAppUtils.getRunningLogURL(container.getNodeHttpAddress(),ConverterUtils.toString(containerId),user));
    return logURL.toString();
  }
  finally {
    readLock.unlock();
  }
}
