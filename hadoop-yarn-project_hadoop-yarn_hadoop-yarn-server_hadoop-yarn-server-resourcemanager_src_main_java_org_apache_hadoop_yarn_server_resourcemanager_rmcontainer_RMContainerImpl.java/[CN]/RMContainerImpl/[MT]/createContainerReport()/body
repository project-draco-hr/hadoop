{
  this.readLock.lock();
  ContainerReport containerReport=null;
  try {
    containerReport=ContainerReport.newInstance(this.getContainerId(),this.getAllocatedResource(),this.getAllocatedNode(),this.getAllocatedPriority(),this.getCreationTime(),this.getFinishTime(),this.getDiagnosticsInfo(),this.getLogURL(),this.getContainerExitStatus(),this.getContainerState(),this.getNodeHttpAddress());
  }
  finally {
    this.readLock.unlock();
  }
  return containerReport;
}
