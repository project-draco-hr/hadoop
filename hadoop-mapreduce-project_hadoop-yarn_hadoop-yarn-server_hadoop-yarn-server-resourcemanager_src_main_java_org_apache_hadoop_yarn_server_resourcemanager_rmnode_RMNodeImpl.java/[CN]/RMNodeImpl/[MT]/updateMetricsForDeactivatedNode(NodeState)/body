{
  ClusterMetrics metrics=ClusterMetrics.getMetrics();
  metrics.decrNumActiveNodes();
switch (finalState) {
case DECOMMISSIONED:
    metrics.incrDecommisionedNMs();
  break;
case LOST:
metrics.incrNumLostNMs();
break;
case REBOOTED:
metrics.incrNumRebootedNMs();
break;
case UNHEALTHY:
metrics.incrNumUnhealthyNMs();
break;
}
}
