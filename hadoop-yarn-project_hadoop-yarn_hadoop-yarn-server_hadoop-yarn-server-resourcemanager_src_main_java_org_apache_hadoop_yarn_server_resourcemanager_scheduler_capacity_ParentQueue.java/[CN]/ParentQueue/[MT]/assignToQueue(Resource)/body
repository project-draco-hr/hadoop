{
  float currentCapacity=(float)(usedResources.getMemory()) / clusterResource.getMemory();
  if (currentCapacity >= absoluteMaxCapacity) {
    LOG.info(getQueueName() + " used=" + usedResources.getMemory()+ " current-capacity ("+ currentCapacity+ ") "+ " >= max-capacity ("+ absoluteMaxCapacity+ ")");
    return false;
  }
  return true;
}
