{
  float currentCapacity=Resources.divide(resourceCalculator,clusterResource,usedResources,clusterResource);
  if (currentCapacity >= absoluteMaxCapacity) {
    LOG.info(getQueueName() + " used=" + usedResources+ " current-capacity ("+ currentCapacity+ ") "+ " >= max-capacity ("+ absoluteMaxCapacity+ ")");
    return false;
  }
  return true;
}
