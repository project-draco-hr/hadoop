{
  if (application != null) {
synchronized (this) {
      super.releaseResource(clusterResource,rmContainer.getContainer().getResource(),node.getPartition());
      if (LOG.isDebugEnabled()) {
        LOG.debug("completedContainer " + this + ", cluster="+ clusterResource);
      }
      if (sortQueues) {
        for (Iterator<CSQueue> iter=childQueues.iterator(); iter.hasNext(); ) {
          CSQueue csqueue=iter.next();
          if (csqueue.equals(completedChildQueue)) {
            iter.remove();
            if (LOG.isDebugEnabled()) {
              LOG.debug("Re-sorting completed queue: " + csqueue);
            }
            childQueues.add(csqueue);
            break;
          }
        }
      }
      needToResortQueuesAtNextAllocation=!sortQueues;
    }
    if (parent != null) {
      parent.completedContainer(clusterResource,application,node,rmContainer,null,event,this,sortQueues);
    }
  }
}
