{
  Set<String> labelCanAccess=new HashSet<String>(accessibleLabels.contains(CommonNodeLabelsManager.ANY) ? nodeLabels : Sets.intersection(accessibleLabels,nodeLabels));
  if (nodeLabels.isEmpty()) {
    labelCanAccess.add(RMNodeLabelsManager.NO_LABEL);
  }
  boolean canAssign=true;
  for (  String label : labelCanAccess) {
    float currentAbsoluteLabelUsedCapacity=Resources.divide(resourceCalculator,clusterResource,queueUsage.getUsed(label),labelManager.getResourceByLabel(label,clusterResource));
    if (currentAbsoluteLabelUsedCapacity >= getAbsoluteMaximumCapacityByNodeLabel(label)) {
      if (LOG.isDebugEnabled()) {
        LOG.debug(getQueueName() + " used=" + queueUsage.getUsed()+ " current-capacity ("+ queueUsage.getUsed(label)+ ") "+ " >= max-capacity ("+ labelManager.getResourceByLabel(label,clusterResource)+ ")");
      }
      canAssign=false;
      break;
    }
  }
  return canAssign;
}
