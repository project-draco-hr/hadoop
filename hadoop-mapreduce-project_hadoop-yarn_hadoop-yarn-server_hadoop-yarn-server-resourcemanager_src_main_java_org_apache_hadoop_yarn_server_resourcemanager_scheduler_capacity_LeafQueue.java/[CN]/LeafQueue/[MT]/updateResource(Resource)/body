{
  float queueLimit=clusterResource.getMemory() * absoluteCapacity;
  setUtilization(usedResources.getMemory() / queueLimit);
  setUsedCapacity(usedResources.getMemory() / (clusterResource.getMemory() * parent.getAbsoluteCapacity()));
  Resource resourceLimit=Resources.createResource(roundUp((int)queueLimit));
  metrics.setAvailableResourcesToQueue(Resources.subtractFrom(resourceLimit,usedResources));
}
