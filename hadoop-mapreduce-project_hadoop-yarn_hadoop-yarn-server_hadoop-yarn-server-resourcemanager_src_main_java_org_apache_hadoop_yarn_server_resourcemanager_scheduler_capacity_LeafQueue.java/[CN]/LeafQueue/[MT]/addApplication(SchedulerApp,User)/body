{
  user.submitApplication();
  applications.add(application);
  applicationsMap.put(application.getApplicationAttemptId(),application);
  LOG.info("Application added -" + " appId: " + application.getApplicationId() + " user: "+ user+ ","+ " leaf-queue: "+ getQueueName()+ " #user-applications: "+ user.getApplications()+ " #queue-applications: "+ getNumApplications());
}
