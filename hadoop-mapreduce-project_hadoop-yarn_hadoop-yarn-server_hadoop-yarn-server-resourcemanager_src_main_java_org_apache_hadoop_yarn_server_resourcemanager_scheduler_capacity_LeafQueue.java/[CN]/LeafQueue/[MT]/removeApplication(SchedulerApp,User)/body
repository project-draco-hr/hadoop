{
  applications.remove(application);
  applicationsMap.remove(application.getApplicationAttemptId());
  user.finishApplication();
  if (user.getApplications() == 0) {
    users.remove(application.getUser());
  }
  LOG.info("Application removed -" + " appId: " + application.getApplicationId() + " user: "+ application.getUser()+ " queue: "+ getQueueName()+ " #user-applications: "+ user.getApplications()+ " #queue-applications: "+ getNumApplications());
}
