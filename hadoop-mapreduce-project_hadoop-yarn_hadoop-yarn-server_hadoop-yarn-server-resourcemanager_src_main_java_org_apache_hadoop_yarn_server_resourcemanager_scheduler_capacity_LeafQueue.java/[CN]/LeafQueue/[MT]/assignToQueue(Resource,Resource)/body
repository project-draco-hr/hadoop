{
  float potentialNewCapacity=(float)(usedResources.getMemory() + required.getMemory()) / clusterResource.getMemory();
  if (potentialNewCapacity > absoluteMaxCapacity) {
    LOG.info(getQueueName() + " usedResources: " + usedResources.getMemory()+ " currentCapacity "+ ((float)usedResources.getMemory()) / clusterResource.getMemory() + " required " + required.getMemory() + " potentialNewCapacity: " + potentialNewCapacity + " ( " + " > max-capacity (" + absoluteMaxCapacity + ")");
    return false;
  }
  return true;
}
