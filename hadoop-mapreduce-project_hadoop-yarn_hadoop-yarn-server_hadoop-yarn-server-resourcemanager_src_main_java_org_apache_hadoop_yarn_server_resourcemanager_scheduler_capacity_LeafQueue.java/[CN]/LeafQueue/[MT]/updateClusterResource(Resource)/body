{
  maxActiveApplications=computeMaxActiveApplications(clusterResource,maxAMResourcePercent,absoluteCapacity);
  maxActiveApplicationsPerUser=computeMaxActiveApplicationsPerUser(maxActiveApplications,userLimit,userLimitFactor);
  for (  SchedulerApp application : activeApplications) {
    computeAndSetUserResourceLimit(application,clusterResource);
  }
}
