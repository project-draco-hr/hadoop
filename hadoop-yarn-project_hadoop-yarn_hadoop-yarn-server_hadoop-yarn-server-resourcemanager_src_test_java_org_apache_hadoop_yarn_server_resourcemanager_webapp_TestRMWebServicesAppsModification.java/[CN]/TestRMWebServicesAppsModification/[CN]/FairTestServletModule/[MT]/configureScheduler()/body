    out.println("</allocations>");
    out.close();
  }
 catch (  IOException e) {
  }
  conf.set(FairSchedulerConfiguration.ALLOCATION_FILE,FS_ALLOC_FILE);
  conf.set("yarn.resourcemanager.scheduler.class",FairScheduler.class.getName());
