def proxy(self, ssh_options):
    master = self._get_master()
    if (not master):
        sys.exit(1)
    options = '-o "ConnectTimeout 10" -o "ServerAliveInterval 60" -N -D 6666'
    process = subprocess.Popen(('ssh %s %s root@%s' % (xstr(ssh_options), options, master.public_ip)), stdin=subprocess.PIPE, stdout=subprocess.PIPE, stderr=subprocess.PIPE, shell=True)
    print ('export HADOOP_CLOUD_PROXY_PID=%s;\necho Proxy pid %s;' % (process.pid, process.pid))
