{
  type=in.readByte();
  Class<? extends Writable> clazz=getTypes()[type & 0xff];
  try {
    instance=ReflectionUtils.newInstance(clazz,conf);
  }
 catch (  Exception e) {
    e.printStackTrace();
    throw new IOException("Cannot initialize the class: " + clazz);
  }
  instance.readFields(in);
}
