{
  if (isMonitoring) {
    Collection<Updater> myUpdaters;
synchronized (this) {
      myUpdaters=new ArrayList<Updater>(updaters);
    }
    for (    Updater updater : myUpdaters) {
      try {
        updater.doUpdates(this);
      }
 catch (      Throwable throwable) {
        throwable.printStackTrace();
      }
    }
    emitRecords();
  }
}
