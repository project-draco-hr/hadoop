{
  LOG.info("Rolling master-key for container-tokens, got key with id " + masterKeyRecord.getKeyId());
  if (super.currentMasterKey == null) {
    super.currentMasterKey=new MasterKeyData(masterKeyRecord);
  }
 else {
    if (super.currentMasterKey.getMasterKey().getKeyId() != masterKeyRecord.getKeyId()) {
      this.previousMasterKey=super.currentMasterKey;
      super.currentMasterKey=new MasterKeyData(masterKeyRecord);
    }
  }
}
