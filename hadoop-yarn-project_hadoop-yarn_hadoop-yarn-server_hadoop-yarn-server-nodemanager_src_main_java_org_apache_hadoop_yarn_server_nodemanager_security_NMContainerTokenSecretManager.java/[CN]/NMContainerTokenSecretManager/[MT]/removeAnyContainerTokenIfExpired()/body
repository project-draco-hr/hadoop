{
  Iterator<Entry<Long,List<ContainerId>>> containersI=this.recentlyStartedContainerTracker.entrySet().iterator();
  Long currTime=System.currentTimeMillis();
  while (containersI.hasNext()) {
    Entry<Long,List<ContainerId>> containerEntry=containersI.next();
    if (containerEntry.getKey() < currTime) {
      containersI.remove();
    }
 else {
      break;
    }
  }
}
