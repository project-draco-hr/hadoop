{
  if (sleep) {
    try {
      Thread.sleep(RANDOM.nextInt(PING_INTERVAL) + MIN_SLEEP_TIME);
    }
 catch (    InterruptedException e) {
    }
  }
  if (callListener != null) {
    callListener.run();
  }
  if (responseClass != null) {
    try {
      return responseClass.newInstance();
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
  }
 else {
    return param;
  }
}
