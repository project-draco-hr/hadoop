{
  readLock.lock();
  try {
    StringBuffer planStr=new StringBuffer("In-memory Plan: ");
    planStr.append("Parent Queue: ").append(queueName).append("Total Capacity: ").append(totalCapacity).append("Step: ").append(step);
    for (    ReservationAllocation reservation : getAllReservations()) {
      planStr.append(reservation);
    }
    return planStr.toString();
  }
  finally {
    readLock.unlock();
  }
}
