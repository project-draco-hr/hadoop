{
  if (Shell.WINDOWS) {
    return false;
  }
  ShellCommandExecutor shexec;
  boolean supported=true;
  try {
    String[] args={"bash","-c","echo 1000"};
    shexec=new ShellCommandExecutor(args);
    shexec.execute();
  }
 catch (  IOException ioe) {
    LOG.warn("Bash is not supported by the OS",ioe);
    supported=false;
  }
  return supported;
}
