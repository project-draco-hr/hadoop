{
  if (Shell.WINDOWS) {
    if (0 == code) {
      return new String[]{Shell.getWinutilsPath(),"task","isAlive",pid};
    }
 else {
      return new String[]{Shell.getWinutilsPath(),"task","kill",pid};
    }
  }
  if (isSetsidAvailable) {
    return new String[]{"bash","-c","kill -" + code + " -- -"+ pid};
  }
 else {
    return new String[]{"bash","-c","kill -" + code + " "+ pid};
  }
}
