{
  int port=toFind.getPort();
  if (port != 0) {
    return index.get(toFind.getIdentifier() + ":" + port);
  }
 else {
    Map.Entry<String,Entry> ceil=index.ceilingEntry(toFind.getIdentifier());
    if ((ceil != null) && (ceil.getValue().getIdentifier().equals(toFind.getIdentifier()))) {
      return ceil.getValue();
    }
    return null;
  }
}
