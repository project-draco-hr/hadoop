{
  return new RMContainerTokenSecretManager(conf){
    @Override public Token createContainerToken(    ContainerId containerId,    NodeId nodeId,    String appSubmitter,    Resource capability,    Priority priority,    long createTime,    LogAggregationContext logAggregationContext,    String nodeLabelExp,    ContainerType containerType){
      numRetries++;
      return super.createContainerToken(containerId,nodeId,appSubmitter,capability,priority,createTime,logAggregationContext,nodeLabelExp,containerType);
    }
  }
;
}
