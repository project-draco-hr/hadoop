{
  return new RMContainerTokenSecretManager(conf){
    @Override public Token createContainerToken(    ContainerId containerId,    NodeId nodeId,    String appSubmitter,    Resource capability,    Priority priority,    long createTime){
      numRetries++;
      return super.createContainerToken(containerId,nodeId,appSubmitter,capability,priority,createTime);
    }
  }
;
}
