{
  ChannelBuffer buf=(ChannelBuffer)e.getMessage();
  XDR request=new XDR(buf.toByteBuffer().asReadOnlyBuffer(),XDR.State.READING);
  InetAddress remoteInetAddr=((InetSocketAddress)e.getRemoteAddress()).getAddress();
  XDR response=rpcProgram.handle(request,remoteInetAddr,null);
  e.getChannel().write(XDR.writeMessageUdp(response.asReadOnlyWrap()),e.getRemoteAddress());
}
