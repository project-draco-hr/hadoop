{
  long time=System.currentTimeMillis();
  try {
    File file=new File(aclFile);
    long lastModified=file.lastModified();
    if (lastModified > lastSuccessfulReload) {
      reloadACL();
      lastSuccessfulReload=time;
    }
  }
 catch (  Exception e) {
    LOG.error("Failed to reload acl file",e);
  }
}
