{
  Configuration conf=getConfig();
  YarnRPC rpc=YarnRPC.create(conf);
  this.server=rpc.getServer(AMRMProtocol.class,this,masterServiceAddress,conf,this.appTokenManager,conf.getInt(YarnConfiguration.RM_SCHEDULER_CLIENT_THREAD_COUNT,YarnConfiguration.DEFAULT_RM_SCHEDULER_CLIENT_THREAD_COUNT));
  if (conf.getBoolean(CommonConfigurationKeysPublic.HADOOP_SECURITY_AUTHORIZATION,false)) {
    refreshServiceAcls(conf,new RMPolicyProvider());
  }
  this.server.start();
  super.start();
}
