{
  int exitCode=0;
  try {
    exitCode=init(args);
    if (exitCode != 0) {
      return exitCode;
    }
    if (!isHelp) {
      if (command.validate()) {
        command.execute();
      }
 else {
        exitCode=1;
      }
    }
  }
 catch (  Exception e) {
    e.printStackTrace(err);
    return 1;
  }
  return exitCode;
}
