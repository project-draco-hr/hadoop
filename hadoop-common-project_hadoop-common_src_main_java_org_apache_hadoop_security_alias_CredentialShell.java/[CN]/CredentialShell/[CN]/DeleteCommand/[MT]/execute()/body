{
  warnIfTransientProvider();
  out.println("Deleting credential: " + alias + " from CredentialProvider: "+ provider.toString());
  if (cont) {
    try {
      provider.deleteCredentialEntry(alias);
      out.println("Credential " + alias + " has been successfully deleted.");
      provider.flush();
      printProviderWritten();
    }
 catch (    IOException e) {
      out.println("Credential " + alias + " has NOT been deleted.");
      throw e;
    }
  }
}
