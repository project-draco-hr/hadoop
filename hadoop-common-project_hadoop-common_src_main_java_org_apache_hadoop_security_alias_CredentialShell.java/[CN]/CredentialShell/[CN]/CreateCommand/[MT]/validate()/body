{
  boolean rc=true;
  try {
    provider=getCredentialProvider();
    if (provider == null) {
      rc=false;
    }
 else     if (provider.needsPassword()) {
      if (strict) {
        out.println(provider.noPasswordError());
        rc=false;
      }
 else {
        out.println(provider.noPasswordWarning());
      }
    }
  }
 catch (  IOException e) {
    e.printStackTrace(err);
  }
  if (alias == null) {
    out.println("There is no alias specified. Please provide the" + "mandatory <alias>. See the usage description with -help.");
    rc=false;
  }
  return rc;
}
