{
  Configuration conf=new Configuration();
  assertNotNull(FileSystem.get(URI.create("/foo"),conf));
  assertNotNull(FileSystem.get(URI.create("/foo/bar"),conf));
  assertNotNull(FileSystem.get(URI.create("foo"),conf));
  assertNotNull(FileSystem.get(URI.create("foo/bar"),conf));
  assertNotNull(FileSystem.get(URI.create("local"),conf));
}
