{
  String encodedLinkTarget=getMetadataAttribute(blob,LINK_BACK_TO_UPLOAD_IN_PROGRESS_METADATA_KEY,OLD_LINK_BACK_TO_UPLOAD_IN_PROGRESS_METADATA_KEY);
  String linkTarget=null;
