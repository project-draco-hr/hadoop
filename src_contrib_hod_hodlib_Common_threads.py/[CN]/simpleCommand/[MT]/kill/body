def kill(self):
    count = 0
    while self.is_running():
        try:
            if (count > 20):
                os.kill(self.__pid, SIGKILL)
                break
            else:
                os.kill(self.__pid, SIGTERM)
        except:
            break
        time.sleep(0.1)
        count = (count + 1)
    self.stop()
