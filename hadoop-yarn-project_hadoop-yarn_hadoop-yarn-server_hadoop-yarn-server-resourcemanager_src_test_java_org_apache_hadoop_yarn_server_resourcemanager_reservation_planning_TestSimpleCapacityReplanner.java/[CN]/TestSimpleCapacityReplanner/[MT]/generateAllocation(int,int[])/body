{
  Map<ReservationInterval,Resource> req=new TreeMap<ReservationInterval,Resource>();
  for (int i=0; i < alloc.length; i++) {
    req.put(new ReservationInterval(startTime + i,startTime + i + 1),ReservationSystemUtil.toResource(ReservationRequest.newInstance(Resource.newInstance(1024,1),alloc[i])));
  }
  return req;
}
