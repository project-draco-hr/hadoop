{
  assert shouldPostponeBlocksFromFuture;
  if (LOG.isDebugEnabled()) {
    LOG.debug("Queueing reported block " + block + " in state "+ reportedState+ " from datanode "+ storageInfo.getDatanodeDescriptor()+ " for later processing because "+ reason+ ".");
  }
  pendingDNMessages.enqueueReportedBlock(storageInfo,block,reportedState);
}
