{
  final LocatedBlock lb;
  if (info.isStriped()) {
    lb=newLocatedStripedBlock(eb,locs,((BlockInfoStripedUnderConstruction)info).getBlockIndices(),offset,false);
  }
 else {
    lb=newLocatedBlock(eb,locs,offset,false);
  }
  setBlockToken(lb,BlockTokenIdentifier.AccessMode.WRITE);
  return lb;
}
