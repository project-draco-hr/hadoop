{
  for (  BlockInfo b : blocks) {
    if (!b.isComplete()) {
      final BlockInfoUnderConstruction uc=(BlockInfoUnderConstruction)b;
      final int numNodes=b.numNodes();
      final int min=getMinStorageNum(b);
      final BlockUCState state=b.getBlockUCState();
      LOG.info("BLOCK* " + b + " is not COMPLETE (ucState = "+ state+ ", replication# = "+ numNodes+ (numNodes < min ? " < " : " >= ")+ " minimum = "+ min+ ") in file "+ src);
      return false;
    }
  }
  return true;
}
