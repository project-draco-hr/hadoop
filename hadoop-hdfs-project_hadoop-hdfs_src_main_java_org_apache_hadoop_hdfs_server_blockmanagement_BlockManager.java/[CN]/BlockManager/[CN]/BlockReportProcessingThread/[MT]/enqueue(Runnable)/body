{
  if (!queue.offer(action)) {
    if (!isAlive() && namesystem.isRunning()) {
      ExitUtil.terminate(1,getName() + " is not running");
    }
    long now=Time.monotonicNow();
    if (now - lastFull > 4000) {
      lastFull=now;
      LOG.info("Block report queue is full");
    }
    queue.put(action);
  }
}
