{
  ContainerId containerId=RecordFactoryProvider.getRecordFactory(null).newRecordInstance(ContainerId.class);
  containerId.setId(-1);
  ApplicationAttemptId applicationAttemptId=RecordFactoryProvider.getRecordFactory(null).newRecordInstance(ApplicationAttemptId.class);
  applicationAttemptId.setAttemptId(-1);
  ApplicationId applicationId=RecordFactoryProvider.getRecordFactory(null).newRecordInstance(ApplicationId.class);
  applicationId.setClusterTimestamp(-1);
  applicationId.setId(-1);
  applicationAttemptId.setApplicationId(applicationId);
  containerId.setApplicationAttemptId(applicationAttemptId);
  return containerId;
}
