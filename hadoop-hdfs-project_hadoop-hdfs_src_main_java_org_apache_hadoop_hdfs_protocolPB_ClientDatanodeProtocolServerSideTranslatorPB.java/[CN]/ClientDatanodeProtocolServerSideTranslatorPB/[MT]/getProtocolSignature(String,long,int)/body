{
  if (!protocol.equals(RPC.getProtocolName(ClientDatanodeProtocol.class))) {
    throw new IOException("Namenode Serverside implements " + RPC.getProtocolName(ClientDatanodeProtocol.class) + ". The following requested protocol is unknown: "+ protocol);
  }
  return ProtocolSignature.getProtocolSignature(clientMethodsHash,RPC.getProtocolVersion(ClientDatanodeProtocolPB.class),ClientDatanodeProtocolPB.class);
}
