{
  try {
    DiskBalancerWorkStatus result=impl.queryDiskBalancerPlan();
    return QueryPlanStatusResponseProto.newBuilder().setResult(result.getResult().getIntResult()).setPlanID(result.getPlanID()).setCurrentStatus(result.currentStateString()).build();
  }
 catch (  Exception e) {
    throw new ServiceException(e);
  }
}
