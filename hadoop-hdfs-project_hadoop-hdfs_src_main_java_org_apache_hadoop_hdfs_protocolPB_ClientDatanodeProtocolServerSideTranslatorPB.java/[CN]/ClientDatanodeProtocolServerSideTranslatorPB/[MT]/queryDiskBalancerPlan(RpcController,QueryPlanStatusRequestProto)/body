{
  try {
    WorkStatus result=impl.queryDiskBalancerPlan();
    return QueryPlanStatusResponseProto.newBuilder().setResult(result.getResult()).setPlanID(result.getPlanID()).setStatus(result.getStatus()).setCurrentStatus(result.getCurrentState()).build();
  }
 catch (  Exception e) {
    throw new ServiceException(e);
  }
}
