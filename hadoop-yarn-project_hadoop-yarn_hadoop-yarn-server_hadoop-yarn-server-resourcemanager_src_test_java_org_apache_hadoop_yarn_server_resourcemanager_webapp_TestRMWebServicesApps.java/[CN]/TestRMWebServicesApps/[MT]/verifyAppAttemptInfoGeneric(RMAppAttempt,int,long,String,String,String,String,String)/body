{
  assertEquals("id doesn't match",appAttempt.getAppAttemptId().getAttemptId(),id);
  assertEquals("startedTime doesn't match",appAttempt.getStartTime(),startTime);
  WebServicesTestUtils.checkStringMatch("containerId",appAttempt.getMasterContainer().getId().toString(),containerId);
  WebServicesTestUtils.checkStringMatch("nodeHttpAddress",appAttempt.getMasterContainer().getNodeHttpAddress(),nodeHttpAddress);
  WebServicesTestUtils.checkStringMatch("nodeId",appAttempt.getMasterContainer().getNodeId().toString(),nodeId);
  assertTrue("logsLink doesn't match",logsLink.startsWith("http://"));
  assertTrue("logsLink doesn't contain user info",logsLink.endsWith("/" + user));
}
