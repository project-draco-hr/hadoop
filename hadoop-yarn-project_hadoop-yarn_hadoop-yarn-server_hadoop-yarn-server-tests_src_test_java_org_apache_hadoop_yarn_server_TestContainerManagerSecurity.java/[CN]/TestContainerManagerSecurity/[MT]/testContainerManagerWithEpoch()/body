{
  try {
    yarnCluster=new MiniYARNCluster(TestContainerManagerSecurity.class.getName(),1,1,1);
    yarnCluster.init(conf);
    yarnCluster.start();
    testContainerTokenWithEpoch(conf);
  }
  finally {
    if (yarnCluster != null) {
      yarnCluster.stop();
      yarnCluster=null;
    }
  }
}
