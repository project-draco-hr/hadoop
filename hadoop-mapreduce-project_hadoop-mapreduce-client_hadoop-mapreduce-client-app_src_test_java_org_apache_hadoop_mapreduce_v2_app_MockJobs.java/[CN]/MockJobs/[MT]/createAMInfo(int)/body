{
  ApplicationAttemptId appAttemptId=BuilderUtils.newApplicationAttemptId(BuilderUtils.newApplicationId(100,1),attempt);
  ContainerId containerId=BuilderUtils.newContainerId(appAttemptId,1);
  return MRBuilderUtils.newAMInfo(appAttemptId,System.currentTimeMillis(),containerId,NM_HOST,NM_PORT,NM_HTTP_PORT);
}
