{
  assertCreatedPathEquals("/","/","");
  assertCreatedPathEquals("/","","");
  assertCreatedPathEquals("/","","/");
  assertCreatedPathEquals("/","/","/");
  assertCreatedPathEquals("/a","/a","");
  assertCreatedPathEquals("/a","/","a");
  assertCreatedPathEquals("/a/b","/a","b");
  assertCreatedPathEquals("/a/b","/a/","b");
  assertCreatedPathEquals("/a/b","/a","/b");
  assertCreatedPathEquals("/a/b","/a","/b/");
  assertCreatedPathEquals("/a","/a","/");
  assertCreatedPathEquals("/alice","/","/alice");
  assertCreatedPathEquals("/alice","/alice","/");
}
