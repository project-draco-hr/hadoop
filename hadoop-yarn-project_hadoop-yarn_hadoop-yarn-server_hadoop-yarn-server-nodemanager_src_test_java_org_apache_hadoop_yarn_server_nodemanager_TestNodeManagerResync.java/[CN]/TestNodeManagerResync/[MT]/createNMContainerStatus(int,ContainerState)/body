{
  ApplicationId applicationId=ApplicationId.newInstance(0,1);
  ApplicationAttemptId applicationAttemptId=ApplicationAttemptId.newInstance(applicationId,1);
  ContainerId containerId=ContainerId.newInstance(applicationAttemptId,id);
  NMContainerStatus containerReport=NMContainerStatus.newInstance(containerId,containerState,Resource.newInstance(1024,1),"recover container",0);
  return containerReport;
}
