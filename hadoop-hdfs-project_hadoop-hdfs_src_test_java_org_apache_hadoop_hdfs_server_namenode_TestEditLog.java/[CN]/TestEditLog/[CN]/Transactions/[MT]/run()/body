{
  PermissionStatus p=namesystem.createFsOwnerPermissions(new FsPermission((short)0777));
  FSEditLog editLog=namesystem.getEditLog();
  for (int i=0; i < numTransactions; i++) {
    INodeFileUnderConstruction inode=new INodeFileUnderConstruction(p,replication,blockSize,0,"","",null);
    editLog.logOpenFile("/filename" + startIndex + i,inode);
    editLog.logCloseFile("/filename" + startIndex + i,inode);
    editLog.logSync();
  }
}
