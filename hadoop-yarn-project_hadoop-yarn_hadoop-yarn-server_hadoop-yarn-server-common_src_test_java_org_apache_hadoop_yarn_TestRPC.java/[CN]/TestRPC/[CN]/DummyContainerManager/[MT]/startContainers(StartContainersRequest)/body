{
  StartContainersResponse response=RECORD_FACTORY.newRecordInstance(StartContainersResponse.class);
  for (  StartContainerRequest request : requests.getStartContainerRequests()) {
    Token containerToken=request.getContainerToken();
    ContainerTokenIdentifier tokenId=null;
    try {
      tokenId=newContainerTokenIdentifier(containerToken);
    }
 catch (    IOException e) {
      throw RPCUtil.getRemoteException(e);
    }
    ContainerStatus status=RECORD_FACTORY.newRecordInstance(ContainerStatus.class);
    status.setState(ContainerState.RUNNING);
    status.setContainerId(tokenId.getContainerID());
    status.setExitStatus(0);
    statuses.add(status);
  }
  return response;
}
