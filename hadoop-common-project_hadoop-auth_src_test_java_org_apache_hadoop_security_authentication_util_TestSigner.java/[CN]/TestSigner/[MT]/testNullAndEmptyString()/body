{
  Signer signer=new Signer(new StringSignerSecretProvider("secret"));
  try {
    signer.sign(null);
    Assert.fail();
  }
 catch (  IllegalArgumentException ex) {
  }
catch (  Throwable ex) {
    Assert.fail();
  }
  try {
    signer.sign("");
    Assert.fail();
  }
 catch (  IllegalArgumentException ex) {
  }
catch (  Throwable ex) {
    Assert.fail();
  }
}
