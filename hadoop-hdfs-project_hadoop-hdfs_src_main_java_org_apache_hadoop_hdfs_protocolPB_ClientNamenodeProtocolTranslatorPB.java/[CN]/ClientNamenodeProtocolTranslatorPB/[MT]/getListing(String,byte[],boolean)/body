{
  GetListingRequestProto req=GetListingRequestProto.newBuilder().setSrc(src).setStartAfter(ByteString.copyFrom(startAfter)).setNeedLocation(needLocation).build();
  try {
    return PBHelper.convert(rpcProxy.getListing(null,req).getDirList());
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
