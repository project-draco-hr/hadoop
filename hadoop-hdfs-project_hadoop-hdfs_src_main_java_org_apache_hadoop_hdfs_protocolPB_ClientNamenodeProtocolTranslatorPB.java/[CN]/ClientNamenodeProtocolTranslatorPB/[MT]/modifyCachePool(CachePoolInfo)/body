    builder.setGroupName(req.getGroupName());
  }
  if (req.getMode() != null) {
    builder.setMode(req.getMode().toShort());
  }
  if (req.getWeight() != null) {
    builder.setWeight(req.getWeight());
  }
  try {
    rpcProxy.modifyCachePool(null,builder.build());
  }
