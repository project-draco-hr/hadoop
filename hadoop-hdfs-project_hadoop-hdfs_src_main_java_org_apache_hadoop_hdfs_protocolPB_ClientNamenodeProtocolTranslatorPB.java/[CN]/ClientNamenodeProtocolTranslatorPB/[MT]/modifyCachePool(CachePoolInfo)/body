{
  ModifyCachePoolRequestProto.Builder builder=ModifyCachePoolRequestProto.newBuilder();
  builder.setPoolName(req.getPoolName());
  if (req.getOwnerName() != null) {
    builder.setOwnerName(req.getOwnerName());
  }
