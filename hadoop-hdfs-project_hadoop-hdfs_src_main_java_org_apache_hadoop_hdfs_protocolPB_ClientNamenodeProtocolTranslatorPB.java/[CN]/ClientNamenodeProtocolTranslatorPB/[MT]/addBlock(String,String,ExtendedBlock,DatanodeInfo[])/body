{
  AddBlockRequestProto req=AddBlockRequestProto.newBuilder().setSrc(src).setClientName(clientName).setPrevious(PBHelper.convert(previous)).addAllExcludeNodes(Arrays.asList(PBHelper.convert(excludeNodes))).build();
  try {
    return PBHelper.convert(rpcProxy.addBlock(null,req).getBlock());
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
