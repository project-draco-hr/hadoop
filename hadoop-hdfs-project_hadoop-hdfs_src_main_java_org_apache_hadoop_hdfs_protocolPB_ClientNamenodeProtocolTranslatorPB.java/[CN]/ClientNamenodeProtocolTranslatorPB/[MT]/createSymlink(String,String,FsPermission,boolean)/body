{
  CreateSymlinkRequestProto req=CreateSymlinkRequestProto.newBuilder().setTarget(target).setLink(link).setDirPerm(PBHelper.convert(dirPerm)).setCreateParent(createParent).build();
  try {
    rpcProxy.createSymlink(null,req);
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
