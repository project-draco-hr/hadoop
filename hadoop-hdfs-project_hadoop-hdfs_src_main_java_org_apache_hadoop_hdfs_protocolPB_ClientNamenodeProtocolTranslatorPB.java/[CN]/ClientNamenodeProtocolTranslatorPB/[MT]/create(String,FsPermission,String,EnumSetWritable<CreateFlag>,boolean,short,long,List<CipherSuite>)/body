{
  CreateRequestProto.Builder builder=CreateRequestProto.newBuilder().setSrc(src).setMasked(PBHelper.convert(masked)).setClientName(clientName).setCreateFlag(PBHelper.convertCreateFlag(flag)).setCreateParent(createParent).setReplication(replication).setBlockSize(blockSize);
  if (cipherSuites != null) {
    builder.addAllCipherSuites(PBHelper.convertCipherSuites(cipherSuites));
  }
  CreateRequestProto req=builder.build();
  try {
    CreateResponseProto res=rpcProxy.create(null,req);
    return res.hasFs() ? PBHelper.convert(res.getFs()) : null;
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
