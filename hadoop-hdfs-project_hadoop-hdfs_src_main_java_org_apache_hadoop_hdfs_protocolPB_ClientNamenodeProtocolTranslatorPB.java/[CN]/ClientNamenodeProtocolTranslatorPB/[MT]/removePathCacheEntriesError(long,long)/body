{
  if (code == RemovePathCacheEntryErrorProto.INVALID_CACHED_PATH_ID_ERROR_VALUE) {
    return new InvalidIdException(id);
  }
 else   if (code == RemovePathCacheEntryErrorProto.NO_SUCH_CACHED_PATH_ID_ERROR_VALUE) {
    return new NoSuchIdException(id);
  }
 else   if (code == RemovePathCacheEntryErrorProto.REMOVE_PERMISSION_DENIED_ERROR_VALUE) {
    return new RemovePermissionDeniedException(id);
  }
 else {
    return new UnexpectedRemovePathCacheEntryException(id);
  }
}
