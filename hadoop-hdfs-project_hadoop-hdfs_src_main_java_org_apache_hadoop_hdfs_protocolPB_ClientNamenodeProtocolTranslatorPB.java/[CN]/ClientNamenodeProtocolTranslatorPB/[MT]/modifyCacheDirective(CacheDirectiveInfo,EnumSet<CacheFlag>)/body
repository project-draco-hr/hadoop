{
  try {
    ModifyCacheDirectiveRequestProto.Builder builder=ModifyCacheDirectiveRequestProto.newBuilder().setInfo(PBHelper.convert(directive));
    if (!flags.isEmpty()) {
      builder.setCacheFlags(PBHelper.convertCacheFlags(flags));
    }
    rpcProxy.modifyCacheDirective(null,builder.build());
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
