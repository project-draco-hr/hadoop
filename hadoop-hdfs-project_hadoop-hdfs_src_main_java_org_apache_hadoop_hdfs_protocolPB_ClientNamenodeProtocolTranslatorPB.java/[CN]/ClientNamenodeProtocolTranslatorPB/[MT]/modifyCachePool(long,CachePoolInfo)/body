{
  ModifyCachePoolRequestProto.Builder builder=ModifyCachePoolRequestProto.newBuilder().setPool(PBHelper.convert(new CachePool(poolId))).setInfo(PBHelper.convert(info));
  try {
    rpcProxy.modifyCachePool(null,builder.build());
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
