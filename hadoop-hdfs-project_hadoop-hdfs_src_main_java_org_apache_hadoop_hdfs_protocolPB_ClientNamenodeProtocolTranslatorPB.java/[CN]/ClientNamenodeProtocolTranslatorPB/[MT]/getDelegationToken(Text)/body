{
  GetDelegationTokenRequestProto req=GetDelegationTokenRequestProto.newBuilder().setRenewer(renewer.toString()).build();
  try {
    GetDelegationTokenResponseProto resp=rpcProxy.getDelegationToken(null,req);
    return resp.hasToken() ? PBHelper.convertDelegationToken(resp.getToken()) : null;
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
