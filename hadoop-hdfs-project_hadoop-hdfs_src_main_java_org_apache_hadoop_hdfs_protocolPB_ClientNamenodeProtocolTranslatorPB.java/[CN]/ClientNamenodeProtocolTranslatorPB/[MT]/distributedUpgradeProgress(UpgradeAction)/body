{
  DistributedUpgradeProgressRequestProto req=DistributedUpgradeProgressRequestProto.newBuilder().setAction(PBHelper.convert(action)).build();
  try {
    return PBHelper.convert(rpcProxy.distributedUpgradeProgress(null,req).getReport());
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
