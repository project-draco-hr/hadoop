{
  GetECZoneInfoRequestProto req=GetECZoneInfoRequestProto.newBuilder().setSrc(src).build();
  try {
    GetECZoneInfoResponseProto response=rpcProxy.getErasureCodingZoneInfo(null,req);
    if (response.hasECZoneInfo()) {
      return PBHelper.convertECZoneInfo(response.getECZoneInfo());
    }
    return null;
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
