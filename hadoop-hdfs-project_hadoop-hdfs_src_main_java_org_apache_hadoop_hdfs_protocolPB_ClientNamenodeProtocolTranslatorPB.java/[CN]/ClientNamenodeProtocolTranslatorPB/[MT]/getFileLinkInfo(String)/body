{
  GetFileLinkInfoRequestProto req=GetFileLinkInfoRequestProto.newBuilder().setSrc(src).build();
  try {
    GetFileLinkInfoResponseProto result=rpcProxy.getFileLinkInfo(null,req);
    return result.hasFs() ? PBHelper.convert(rpcProxy.getFileLinkInfo(null,req).getFs()) : null;
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
