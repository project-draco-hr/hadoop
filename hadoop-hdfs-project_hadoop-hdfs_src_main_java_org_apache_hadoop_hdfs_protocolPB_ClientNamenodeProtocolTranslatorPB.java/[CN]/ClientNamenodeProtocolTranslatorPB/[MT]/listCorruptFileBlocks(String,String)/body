{
  ListCorruptFileBlocksRequestProto.Builder req=ListCorruptFileBlocksRequestProto.newBuilder().setPath(path);
  if (cookie != null)   req.setCookie(cookie);
  try {
    return PBHelper.convert(rpcProxy.listCorruptFileBlocks(null,req.build()).getCorrupt());
  }
