{
  List<String> groups=new ArrayList<String>();
  NetgroupCache.getNetgroups(user,groups);
  assertEquals(size,groups.size());
  if (size > 0) {
    boolean present=false;
    for (    String groupEntry : groups) {
      if (groupEntry.equals(group)) {
        present=true;
        break;
      }
    }
    assertTrue(present);
  }
}
