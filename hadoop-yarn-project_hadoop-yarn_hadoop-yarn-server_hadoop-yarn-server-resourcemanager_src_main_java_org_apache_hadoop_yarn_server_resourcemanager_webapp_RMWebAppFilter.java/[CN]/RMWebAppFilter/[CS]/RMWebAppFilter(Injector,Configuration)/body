{
  super(injector);
  this.injector=injector;
  InetSocketAddress sock=YarnConfiguration.useHttps(conf) ? conf.getSocketAddr(YarnConfiguration.RM_WEBAPP_HTTPS_ADDRESS,YarnConfiguration.DEFAULT_RM_WEBAPP_HTTPS_ADDRESS,YarnConfiguration.DEFAULT_RM_WEBAPP_HTTPS_PORT) : conf.getSocketAddr(YarnConfiguration.RM_WEBAPP_ADDRESS,YarnConfiguration.DEFAULT_RM_WEBAPP_ADDRESS,YarnConfiguration.DEFAULT_RM_WEBAPP_PORT);
  path=sock.getHostName() + ":" + Integer.toString(sock.getPort());
  path=YarnConfiguration.useHttps(conf) ? "https://" + path : "http://" + path;
}
