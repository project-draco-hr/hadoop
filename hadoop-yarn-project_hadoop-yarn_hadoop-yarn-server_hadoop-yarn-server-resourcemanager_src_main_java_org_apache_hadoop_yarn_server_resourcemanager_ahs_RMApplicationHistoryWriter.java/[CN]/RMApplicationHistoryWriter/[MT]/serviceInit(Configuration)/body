{
  historyServiceEnabled=conf.getBoolean(YarnConfiguration.YARN_HISTORY_SERVICE_ENABLED,YarnConfiguration.DEFAULT_YARN_HISTORY_SERVICE_ENABLED);
  writer=createApplicationHistoryStore(conf);
  addIfService(writer);
  dispatcher=createDispatcher(conf);
  dispatcher.register(WritingHistoryEventType.class,new ForwardingEventHandler());
  addIfService(dispatcher);
  super.serviceInit(conf);
}
