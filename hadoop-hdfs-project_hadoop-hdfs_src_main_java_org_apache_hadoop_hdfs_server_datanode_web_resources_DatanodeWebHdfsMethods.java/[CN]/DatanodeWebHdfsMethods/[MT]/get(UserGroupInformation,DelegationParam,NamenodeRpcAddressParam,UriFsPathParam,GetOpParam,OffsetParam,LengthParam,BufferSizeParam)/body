{
  final InetSocketAddress nnRpcAddr=namenodeRpcAddress.getValue();
  init(ugi,delegation,nnRpcAddr,path,op,offset,length,bufferSize);
  return ugi.doAs(new PrivilegedExceptionAction<Response>(){
    @Override public Response run() throws IOException {
      return get(ugi,delegation,nnRpcAddr,path.getAbsolutePath(),op,offset,length,bufferSize);
    }
  }
);
}
