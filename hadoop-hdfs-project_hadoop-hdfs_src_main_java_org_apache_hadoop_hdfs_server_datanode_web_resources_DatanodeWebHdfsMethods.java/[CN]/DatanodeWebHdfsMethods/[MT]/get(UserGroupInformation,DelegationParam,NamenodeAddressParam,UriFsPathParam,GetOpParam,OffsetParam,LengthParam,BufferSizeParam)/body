{
  final String nnId=namenode.getValue();
  init(ugi,delegation,nnId,path,op,offset,length,bufferSize);
  return ugi.doAs(new PrivilegedExceptionAction<Response>(){
    @Override public Response run() throws IOException {
      return get(nnId,path.getAbsolutePath(),op,offset,length,bufferSize);
    }
  }
);
}
