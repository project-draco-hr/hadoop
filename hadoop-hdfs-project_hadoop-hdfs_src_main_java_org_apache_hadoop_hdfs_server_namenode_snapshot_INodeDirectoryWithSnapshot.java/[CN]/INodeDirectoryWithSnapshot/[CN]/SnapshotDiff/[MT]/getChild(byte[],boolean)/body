{
  for (SnapshotDiff d=this; ; d=d.posteriorDiff) {
    final INode[] array=d.diff.accessPrevious(name);
    if (array != null) {
      return array[0];
    }
 else     if (!checkPosterior) {
      return null;
    }
 else     if (d.posteriorDiff == null) {
      return INodeDirectoryWithSnapshot.this.getChild(name,null);
    }
  }
}
