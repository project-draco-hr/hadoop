{
  return diff.combinePosterior(posterior.diff,new Diff.Processor<INode>(){
    /** 
 * Collect blocks for deleted files. 
 */
    @Override public int process(    INode inode){
      if (inode != null) {
        return inode.destroyAndCollectBlocks(collectedBlocks);
      }
      return 0;
    }
  }
);
}
