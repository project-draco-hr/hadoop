{
  if (inodes == null) {
    return null;
  }
 else   if (inodes.isEmpty()) {
    return "[]";
  }
  final StringBuilder b=new StringBuilder("[").append(inodes.get(0).getLocalName());
  for (int i=1; i < inodes.size(); i++) {
    b.append(", ").append(inodes.get(i).getLocalName());
  }
  return b.append("]").toString();
}
