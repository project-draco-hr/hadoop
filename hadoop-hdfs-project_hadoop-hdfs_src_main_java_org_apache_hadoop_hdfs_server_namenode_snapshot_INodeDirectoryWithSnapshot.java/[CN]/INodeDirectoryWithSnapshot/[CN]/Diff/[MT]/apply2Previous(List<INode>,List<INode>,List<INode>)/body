{
  final List<INode> current=new ArrayList<INode>(previous);
  if (dlist != null) {
    for (    INode d : dlist) {
      current.remove(d);
    }
  }
  if (clist != null) {
    for (    INode c : clist) {
      final int i=search(current,c);
      current.add(-i - 1,c);
    }
  }
  return current;
}
