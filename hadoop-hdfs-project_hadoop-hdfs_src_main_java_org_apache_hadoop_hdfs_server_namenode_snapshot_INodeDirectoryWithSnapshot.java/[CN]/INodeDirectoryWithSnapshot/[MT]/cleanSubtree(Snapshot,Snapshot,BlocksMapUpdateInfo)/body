{
  int n=0;
  if (snapshot == null) {
    recordModification(prior);
    DirectoryDiff lastDiff=diffs.getLast();
    if (lastDiff != null) {
      n+=lastDiff.diff.destroyCreatedList(this,collectedBlocks);
    }
  }
 else {
    Snapshot s=getDiffs().getPrior(snapshot);
    if (s != null && (prior == null || Snapshot.ID_COMPARATOR.compare(s,prior) > 0)) {
      prior=s;
    }
    n+=getDiffs().deleteSnapshotDiff(snapshot,prior,this,collectedBlocks);
  }
  n+=cleanSubtreeRecursively(snapshot,prior,collectedBlocks);
  return n;
}
