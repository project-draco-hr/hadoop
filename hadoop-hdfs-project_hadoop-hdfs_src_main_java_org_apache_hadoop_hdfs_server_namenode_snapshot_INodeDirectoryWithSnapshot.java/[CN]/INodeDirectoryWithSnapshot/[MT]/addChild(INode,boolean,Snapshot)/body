{
  Diff diff=null;
  Integer undoInfo=null;
  if (latest != null) {
    diff=checkAndAddLatestDiff(latest);
    undoInfo=diff.create(inode);
  }
  final boolean added=super.addChild(inode,setModTime,null);
  if (!added && undoInfo != null) {
    diff.undoCreate(inode,undoInfo);
  }
  return added;
}
