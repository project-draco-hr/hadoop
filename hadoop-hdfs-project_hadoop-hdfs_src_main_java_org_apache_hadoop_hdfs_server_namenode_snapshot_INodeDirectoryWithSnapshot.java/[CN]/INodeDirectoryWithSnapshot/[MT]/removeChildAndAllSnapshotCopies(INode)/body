{
  if (!removeChild(child)) {
    return false;
  }
  final List<DirectoryDiff> diffList=diffs.asList();
  for (int i=diffList.size() - 1; i >= 0; i--) {
    final ChildrenDiff diff=diffList.get(i).diff;
    if (diff.removeChild(ListType.CREATED,child)) {
      return true;
    }
  }
  return true;
}
