{
  Configuration conf=new Configuration();
  MiniDFSCluster cluster=new MiniDFSCluster.Builder(conf).nnTopology(MiniDFSNNTopology.simpleHATopology()).numDataNodes(1).build();
  try {
    cluster.waitActive();
    testManualFailoverFailback(cluster,conf,0);
  }
  finally {
    cluster.shutdown();
  }
}
