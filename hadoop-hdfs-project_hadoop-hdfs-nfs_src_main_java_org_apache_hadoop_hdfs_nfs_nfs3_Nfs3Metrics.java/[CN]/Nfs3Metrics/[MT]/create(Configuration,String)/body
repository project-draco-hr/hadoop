{
  String sessionId=conf.get(DFSConfigKeys.DFS_METRICS_SESSION_ID_KEY);
  MetricsSystem ms=DefaultMetricsSystem.instance();
  JvmMetrics jm=JvmMetrics.create(gatewayName,sessionId,ms);
  int[] intervals=conf.getInts(conf.get(NfsConfigKeys.NFS_METRICS_PERCENTILES_INTERVALS_KEY,NfsConfigKeys.NFS_METRICS_PERCENTILES_INTERVALS_DEFAULT));
  return ms.register(new Nfs3Metrics(gatewayName,sessionId,intervals,jm));
}
