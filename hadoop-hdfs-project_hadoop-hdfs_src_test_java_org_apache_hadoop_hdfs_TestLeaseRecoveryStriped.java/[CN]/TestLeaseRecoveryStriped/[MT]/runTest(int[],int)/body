{
  writePartialBlocks(blockLengths);
  recoverLease();
  List<Long> oldGS=new ArrayList<>();
  oldGS.add(1001L);
  StripedFileTestUtil.checkData(dfs,p,safeLength,new ArrayList<DatanodeInfo>(),oldGS);
  cluster.restartNameNode(true);
  StripedFileTestUtil.checkData(dfs,p,safeLength,new ArrayList<DatanodeInfo>(),oldGS);
}
