{
  SetXAttrRequestProto req=SetXAttrRequestProto.newBuilder().setSrc(src).setXAttr(PBHelperClient.convertXAttrProto(xAttr)).setFlag(PBHelperClient.convert(flag)).build();
  try {
    rpcProxy.setXAttr(null,req);
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
