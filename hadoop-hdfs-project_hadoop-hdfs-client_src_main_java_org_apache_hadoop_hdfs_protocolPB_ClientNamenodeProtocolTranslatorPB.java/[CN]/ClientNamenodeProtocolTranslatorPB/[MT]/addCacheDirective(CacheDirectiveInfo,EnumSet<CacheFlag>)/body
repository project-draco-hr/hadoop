{
  try {
    AddCacheDirectiveRequestProto.Builder builder=AddCacheDirectiveRequestProto.newBuilder().setInfo(PBHelperClient.convert(directive));
    if (!flags.isEmpty()) {
      builder.setCacheFlags(PBHelperClient.convertCacheFlags(flags));
    }
    return rpcProxy.addCacheDirective(null,builder.build()).getId();
  }
 catch (  ServiceException e) {
    throw ProtobufHelper.getRemoteException(e);
  }
}
