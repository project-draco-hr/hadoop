{
  if (this._dataType == KeyDataType.LONG) {
    long aa=a.getLongValue(this._sortKey);
    long bb=b.getLongValue(this._sortKey);
    if (aa < bb)     return -1;
    if (aa == bb)     return 0;
    if (aa > bb)     return 1;
  }
 else {
    return a.getStringValue(this._sortKey).compareToIgnoreCase(b.getStringValue(this._sortKey));
  }
  return 0;
}
