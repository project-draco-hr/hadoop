{
  Assert.assertTrue("hostList size is " + hostList.size(),hostList.size() <= 1);
  List<String> returnList=new ArrayList<String>();
  if (hostList.isEmpty()) {
    return returnList;
  }
  if (hostList.get(0).equals("host1")) {
    numHost1++;
    returnList.add("/rack1");
  }
  Assert.assertTrue(numHost1 <= 1);
  return returnList;
}
