{
  String pid="100";
  File procfsRootDir=new File(TEST_ROOT_DIR,"proc");
  try {
    setupProcfsRootDir(procfsRootDir);
    ProcfsBasedProcessTree processTree=createProcessTree(pid,true,procfsRootDir.getAbsolutePath());
    Assert.assertTrue(ProcfsBasedProcessTree.checkPidPgrpidForMatch(Integer.valueOf(pid),procfsRootDir.getAbsolutePath()));
  }
  finally {
    FileUtil.fullyDelete(procfsRootDir);
  }
}
