{
  assertEquals("incorrect number of elements",1,json.length());
  JSONObject clusterinfo=json.getJSONObject("clusterMetrics");
  assertEquals("incorrect number of elements",23,clusterinfo.length());
  verifyClusterMetrics(clusterinfo.getInt("appsSubmitted"),clusterinfo.getInt("appsCompleted"),clusterinfo.getInt("reservedMB"),clusterinfo.getInt("availableMB"),clusterinfo.getInt("allocatedMB"),clusterinfo.getInt("reservedVirtualCores"),clusterinfo.getInt("availableVirtualCores"),clusterinfo.getInt("allocatedVirtualCores"),clusterinfo.getInt("totalVirtualCores"),clusterinfo.getInt("containersAllocated"),clusterinfo.getInt("totalMB"),clusterinfo.getInt("totalNodes"),clusterinfo.getInt("lostNodes"),clusterinfo.getInt("unhealthyNodes"),clusterinfo.getInt("decommissionedNodes"),clusterinfo.getInt("rebootedNodes"),clusterinfo.getInt("activeNodes"));
}
