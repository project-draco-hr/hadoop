{
  TaskTAttemptEvent ev=(TaskTAttemptEvent)event;
  TaskAttemptId attemptID=ev.getTaskAttemptID();
  if (task.commitAttempt == null) {
    task.commitAttempt=attemptID;
    LOG.info(attemptID + " given a go for committing the task output.");
  }
 else {
    LOG.info(task.commitAttempt + " already given a go for committing the task output, so killing " + attemptID);
    task.eventHandler.handle(new TaskAttemptKillEvent(attemptID,SPECULATION + task.commitAttempt + " committed first!"));
  }
}
