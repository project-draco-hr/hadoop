{
  Resource absDelta=Resources.negate(decreaseRequest.getDeltaCapacity());
  if (LOG.isDebugEnabled()) {
    LOG.debug("Decrease container : applicationId=" + applicationId + " container="+ decreaseRequest.getContainerId()+ " host="+ decreaseRequest.getNodeId()+ " user="+ user+ " resource="+ absDelta);
  }
  try {
    this.writeLock.lock();
    queue.getMetrics().releaseResources(user,absDelta);
    appResourceUsage.decUsed(decreaseRequest.getNodePartition(),absDelta);
  }
  finally {
    this.writeLock.unlock();
  }
}
