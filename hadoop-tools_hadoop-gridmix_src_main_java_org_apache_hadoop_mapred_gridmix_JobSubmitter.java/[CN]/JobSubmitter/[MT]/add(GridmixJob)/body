{
  final boolean addToQueue=!shutdown;
  if (addToQueue) {
    final SubmitTask task=new SubmitTask(job);
    LOG.info("Total number of queued jobs: " + (queueDepth - sem.availablePermits()));
    sem.acquire();
    try {
      sched.execute(task);
    }
 catch (    RejectedExecutionException e) {
      sem.release();
    }
  }
}
