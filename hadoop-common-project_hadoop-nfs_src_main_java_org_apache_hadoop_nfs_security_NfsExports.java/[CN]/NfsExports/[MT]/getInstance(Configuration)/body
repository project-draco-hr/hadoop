{
  if (exports == null) {
    String matchHosts=conf.get(Nfs3Constant.EXPORTS_ALLOWED_HOSTS_KEY,Nfs3Constant.EXPORTS_ALLOWED_HOSTS_KEY_DEFAULT);
    int cacheSize=conf.getInt(Nfs3Constant.EXPORTS_CACHE_SIZE_KEY,Nfs3Constant.EXPORTS_CACHE_SIZE_DEFAULT);
    long expirationPeriodNano=conf.getLong(Nfs3Constant.EXPORTS_CACHE_EXPIRYTIME_MILLIS_KEY,Nfs3Constant.EXPORTS_CACHE_EXPIRYTIME_MILLIS_DEFAULT) * 1000 * 1000;
    exports=new NfsExports(cacheSize,expirationPeriodNano,matchHosts);
  }
  return exports;
}
