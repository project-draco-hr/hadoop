{
  rm.start();
  rm.registerNode("amNM:1234",2048);
  long end=System.currentTimeMillis();
  Thread.sleep(1);
  rm.submitApp(1024);
  rm.submitApp(1024);
  rm.submitApp(1024);
  WebResource r=resource();
  JSONObject json=r.path("ws").path("v1").path("cluster").path("apps").queryParam("startedTimeEnd",String.valueOf(end)).accept("application/json").get(JSONObject.class);
  assertEquals("correct number of elements",1,json.length());
  assertEquals("apps is not null",JSONObject.NULL,json.get("apps"));
  rm.stop();
}
