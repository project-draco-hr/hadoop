{
  assertEquals("incorrect number of elements",1,json.length());
  JSONObject clusterinfo=json.getJSONObject("clusterMetrics");
  assertEquals("incorrect number of elements",11,clusterinfo.length());
  verifyClusterMetrics(clusterinfo.getInt("appsSubmitted"),clusterinfo.getInt("reservedMB"),clusterinfo.getInt("availableMB"),clusterinfo.getInt("allocatedMB"),clusterinfo.getInt("containersAllocated"),clusterinfo.getInt("totalMB"),clusterinfo.getInt("totalNodes"),clusterinfo.getInt("lostNodes"),clusterinfo.getInt("unhealthyNodes"),clusterinfo.getInt("decommissionedNodes"),clusterinfo.getInt("rebootedNodes"));
}
