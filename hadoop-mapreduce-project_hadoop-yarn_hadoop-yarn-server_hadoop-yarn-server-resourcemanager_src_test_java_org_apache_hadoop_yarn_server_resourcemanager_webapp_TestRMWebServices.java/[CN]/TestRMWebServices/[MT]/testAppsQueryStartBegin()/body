{
  rm.start();
  long start=System.currentTimeMillis();
  Thread.sleep(1);
  rm.registerNode("amNM:1234",2048);
  rm.submitApp(1024);
  rm.submitApp(1024);
  rm.submitApp(1024);
  WebResource r=resource();
  JSONObject json=r.path("ws").path("v1").path("cluster").path("apps").queryParam("startedTimeBegin",String.valueOf(start)).accept("application/json").get(JSONObject.class);
  assertEquals("correct number of elements",1,json.length());
  JSONObject apps=json.getJSONObject("apps");
  assertEquals("correct number of elements",1,apps.length());
  JSONArray array=apps.getJSONArray("app");
  assertEquals("correct number of elements",3,array.length());
  rm.stop();
}
