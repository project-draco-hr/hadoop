{
  rm.start();
  MockNM amNodeManager=rm.registerNode("amNM:1234",2048);
  RMApp app1=rm.submitApp(1024);
  amNodeManager.nodeHeartbeat(true);
  MockAM am=rm.sendAMLaunched(app1.getCurrentAppAttempt().getAppAttemptId());
  am.registerAppAttempt();
  am.unregisterAppAttempt();
  rm.submitApp(1024);
  rm.submitApp(1024);
  long end=System.currentTimeMillis();
  WebResource r=resource();
  JSONObject json=r.path("ws").path("v1").path("cluster").path("apps").queryParam("finishedTimeEnd",String.valueOf(end)).accept("application/json").get(JSONObject.class);
  assertEquals("correct number of elements",1,json.length());
  JSONObject apps=json.getJSONObject("apps");
  assertEquals("correct number of elements",1,apps.length());
  JSONArray array=apps.getJSONArray("app");
  assertEquals("correct number of elements",3,array.length());
  rm.stop();
}
