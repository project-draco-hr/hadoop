{
  WebResource r=resource();
  rm.registerNode("h1:1234",5120);
  rm.registerNode("h2:1235",5121);
  JSONObject json=r.path("ws").path("v1").path("cluster").path("nodes").queryParam("state",RMNodeState.DECOMMISSIONED.toString()).accept("application/json").get(JSONObject.class);
  assertEquals("correct number of elements",1,json.length());
  assertEquals("nodes is not null",JSONObject.NULL,json.get("nodes"));
}
