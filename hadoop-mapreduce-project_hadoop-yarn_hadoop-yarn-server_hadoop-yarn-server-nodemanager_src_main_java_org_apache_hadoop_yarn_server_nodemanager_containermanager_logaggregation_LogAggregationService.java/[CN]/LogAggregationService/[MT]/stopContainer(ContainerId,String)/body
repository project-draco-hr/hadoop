{
  if (!this.appLogAggregators.containsKey(containerId.getApplicationAttemptId().getApplicationId())) {
    throw new YarnException("Application is not initialized yet for " + containerId);
  }
  this.appLogAggregators.get(containerId.getApplicationAttemptId().getApplicationId()).startContainerLogAggregation(containerId,exitCode.equals("0"));
}
