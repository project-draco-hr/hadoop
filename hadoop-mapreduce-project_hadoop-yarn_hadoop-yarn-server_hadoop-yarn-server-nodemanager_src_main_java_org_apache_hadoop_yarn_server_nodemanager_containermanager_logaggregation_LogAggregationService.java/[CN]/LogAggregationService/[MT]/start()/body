{
  String address=getConfig().get(YarnConfiguration.NM_ADDRESS,YarnConfiguration.DEFAULT_NM_ADDRESS);
  InetSocketAddress cmBindAddress=NetUtils.createSocketAddr(address);
  try {
    this.nodeFile=InetAddress.getLocalHost().getHostAddress() + "_" + cmBindAddress.getPort();
  }
 catch (  UnknownHostException e) {
    throw new YarnException(e);
  }
  super.start();
}
