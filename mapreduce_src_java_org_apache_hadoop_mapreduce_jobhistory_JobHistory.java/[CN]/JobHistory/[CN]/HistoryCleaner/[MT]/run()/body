{
  while (true) {
    try {
      doCleanup();
      Thread.sleep(cleanupFrequency);
    }
 catch (    InterruptedException e) {
      LOG.info("History Cleaner thread exiting");
      return;
    }
catch (    Throwable t) {
      LOG.warn("History cleaner thread threw an exception",t);
    }
  }
}
