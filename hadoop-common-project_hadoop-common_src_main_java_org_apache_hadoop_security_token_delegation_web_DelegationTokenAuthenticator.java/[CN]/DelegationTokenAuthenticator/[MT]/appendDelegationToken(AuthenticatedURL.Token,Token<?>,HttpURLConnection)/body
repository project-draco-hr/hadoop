{
  if (token.isSet()) {
    LOG.debug("Auth token is set, not appending delegation token.");
    return;
  }
  if (dToken == null) {
    LOG.warn("Delegation token is null, cannot set on request header.");
    return;
  }
  conn.setRequestProperty(DelegationTokenAuthenticator.DELEGATION_TOKEN_HEADER,dToken.encodeToUrlString());
}
