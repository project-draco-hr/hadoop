{
  NodesBlock nodesBlock=injector.getInstance(NodesBlock.class);
  nodesBlock.set("node.label","");
  nodesBlock.render();
  PrintWriter writer=injector.getInstance(PrintWriter.class);
  WebAppTests.flushOutput(injector);
  Mockito.verify(writer,Mockito.times(numberOfRacks * (numberOfNodesPerRack - 1) * numberOfActualTableHeaders + numberOfThInMetricsTable)).print("<td");
}
