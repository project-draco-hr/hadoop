{
  ArrayList<String> list=new ArrayList<String>();
  String alias=null;
  try {
    Enumeration<String> e=keyStore.aliases();
    while (e.hasMoreElements()) {
      alias=e.nextElement();
      if (!alias.contains("@")) {
        list.add(alias);
      }
    }
  }
 catch (  KeyStoreException e) {
    throw new IOException("Can't get key " + alias + " from "+ path,e);
  }
  return list;
}
