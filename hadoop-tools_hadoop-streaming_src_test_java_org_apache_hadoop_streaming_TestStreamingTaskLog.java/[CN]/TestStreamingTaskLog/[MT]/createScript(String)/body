{
  File scriptFile=new File(script);
  UtilTest.recursiveDelete(scriptFile);
  FileOutputStream in=new FileOutputStream(scriptFile);
  if (Shell.WINDOWS) {
    in.write("@echo %HADOOP_ROOT_LOGGER% %HADOOP_CLIENT_OPTS%".getBytes());
  }
 else {
    in.write(("cat > /dev/null 2>&1\n" + "echo $HADOOP_ROOT_LOGGER $HADOOP_CLIENT_OPTS").getBytes());
  }
  in.close();
  Shell.execCommand(Shell.getSetPermissionCommand("+x",false,scriptFile.getAbsolutePath()));
  return scriptFile;
}
