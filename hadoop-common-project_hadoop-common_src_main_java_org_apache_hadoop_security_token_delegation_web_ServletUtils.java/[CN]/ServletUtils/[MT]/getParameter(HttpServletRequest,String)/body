{
  String queryString=request.getQueryString();
  if (queryString == null) {
    return null;
  }
  List<NameValuePair> list=URLEncodedUtils.parse(queryString,UTF8_CHARSET);
  if (list != null) {
    for (    NameValuePair nv : list) {
      if (name.equals(nv.getName())) {
        return nv.getValue();
      }
    }
  }
  return null;
}
