{
  RpcRequestHeaderProto.Builder result=RpcRequestHeaderProto.newBuilder();
  result.setRpcKind(convert(rpcKind)).setRpcOp(operation).setCallId(callId).setRetryCount(retryCount).setClientId(ByteString.copyFrom(uuid));
  if (Trace.isTracing()) {
    Span s=Trace.currentSpan();
    result.setTraceInfo(RPCTraceInfoProto.newBuilder().setParentId(s.getSpanId()).setTraceId(s.getTraceId()).build());
  }
  return result.build();
}
