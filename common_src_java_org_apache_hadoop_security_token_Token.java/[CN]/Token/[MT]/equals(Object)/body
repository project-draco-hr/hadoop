{
  if (this == right) {
    return true;
  }
 else   if (right == null || getClass() != right.getClass()) {
    return false;
  }
 else {
    Token<T> r=(Token<T>)right;
    return Arrays.equals(identifier,r.identifier) && Arrays.equals(password,r.password) && kind.equals(r.kind)&& service.equals(r.service);
  }
}
