{
  StringWriter sw=new StringWriter();
  PrintWriter out=new PrintWriter(sw);
  MetricsServlet servlet=new MetricsServlet();
  servlet.printMap(out,servlet.makeMap(contexts));
  String EXPECTED="" + "test1\n" + "  testRecord\n"+ "    {testTag1=testTagValue1,testTag2=testTagValue2}:\n"+ "      testMetric1=1\n"+ "      testMetric2=33\n"+ "test2\n";
  assertEquals(EXPECTED,sw.toString());
}
