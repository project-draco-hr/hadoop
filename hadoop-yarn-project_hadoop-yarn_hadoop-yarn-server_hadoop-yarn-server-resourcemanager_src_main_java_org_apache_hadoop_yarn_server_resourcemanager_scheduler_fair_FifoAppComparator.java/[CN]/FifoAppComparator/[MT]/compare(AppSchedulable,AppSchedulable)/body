{
  int res=a1.getPriority().compareTo(a2.getPriority());
  if (res == 0) {
    if (a1.getStartTime() < a2.getStartTime()) {
      res=-1;
    }
 else {
      res=(a1.getStartTime() == a2.getStartTime() ? 0 : 1);
    }
  }
  if (res == 0) {
    res=a1.getApp().getApplicationId().compareTo(a2.getApp().getApplicationId());
  }
  return res;
}
