{
  StringBuffer command=new StringBuffer();
  for (  String s : cmd) {
    command.append('\'');
    if (isExecutable) {
      command.append(FileUtil.makeShellPath(new File(s)));
      isExecutable=false;
    }
 else {
      command.append(s);
    }
    command.append('\'');
    command.append(" ");
  }
  return command.toString();
}
