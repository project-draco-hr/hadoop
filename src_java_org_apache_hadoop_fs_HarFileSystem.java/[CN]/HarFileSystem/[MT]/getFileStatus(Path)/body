{
  FileStatus archiveStatus=fs.getFileStatus(archiveIndex);
  Path p=makeQualified(f);
  Path harPath=getPathInHar(p);
  if (harPath == null) {
    throw new IOException("Invalid file name: " + f + " in "+ uri);
  }
  String readStr=fileStatusInIndex(harPath);
  if (readStr == null) {
    throw new FileNotFoundException("File: " + f + " does not exist in "+ uri);
  }
  HarStatus hstatus=null;
  hstatus=new HarStatus(readStr);
  return new FileStatus(hstatus.isDir() ? 0 : hstatus.getLength(),hstatus.isDir(),(int)archiveStatus.getReplication(),archiveStatus.getBlockSize(),archiveStatus.getModificationTime(),archiveStatus.getAccessTime(),new FsPermission(archiveStatus.getPermission()),archiveStatus.getOwner(),archiveStatus.getGroup(),makeRelative(this.uri.toString(),new Path(hstatus.name)));
}
