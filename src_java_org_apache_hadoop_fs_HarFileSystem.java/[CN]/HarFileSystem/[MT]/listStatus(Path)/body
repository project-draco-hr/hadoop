{
  List<FileStatus> statuses=new ArrayList<FileStatus>();
  FileStatus archiveStatus=fs.getFileStatus(archiveIndex);
  Path tmpPath=makeQualified(f);
  Path harPath=getPathInHar(tmpPath);
  String readStr=fileStatusInIndex(harPath);
  if (readStr == null) {
    throw new FileNotFoundException("File " + f + " not found in "+ archivePath);
  }
  HarStatus hstatus=new HarStatus(readStr);
  if (!hstatus.isDir())   statuses.add(new FileStatus(hstatus.getLength(),hstatus.isDir(),archiveStatus.getReplication(),archiveStatus.getBlockSize(),archiveStatus.getModificationTime(),archiveStatus.getAccessTime(),new FsPermission(archiveStatus.getPermission()),archiveStatus.getOwner(),archiveStatus.getGroup(),makeRelative(this.uri.toString(),new Path(hstatus.name))));
 else   for (  String child : hstatus.children) {
    FileStatus tmp=getFileStatus(new Path(tmpPath,child));
    statuses.add(tmp);
  }
  return statuses.toArray(new FileStatus[statuses.size()]);
}
