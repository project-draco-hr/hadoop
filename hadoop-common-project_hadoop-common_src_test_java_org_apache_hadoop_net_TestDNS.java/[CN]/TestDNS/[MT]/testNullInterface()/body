{
  try {
    String host=DNS.getDefaultHost(null);
    fail("Expected a NullPointerException, got " + host);
  }
 catch (  NullPointerException npe) {
  }
  try {
    String ip=DNS.getDefaultIP(null);
    fail("Expected a NullPointerException, got " + ip);
  }
 catch (  NullPointerException npe) {
  }
}
