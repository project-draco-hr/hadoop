{
  TestImpl1 impl=new TestImpl1();
  server=RPC.getServer(TestProtocol1.class,impl,ADDRESS,0,2,false,conf,null);
  server.addProtocol(RpcKind.RPC_WRITABLE,TestProtocol0.class,impl);
  server.start();
  addr=NetUtils.getConnectAddress(server);
  proxy=RPC.getProtocolProxy(TestProtocol0.class,TestProtocol0.versionID,addr,conf);
  TestProtocol0 proxy0=(TestProtocol0)proxy.getProxy();
  proxy0.ping();
}
