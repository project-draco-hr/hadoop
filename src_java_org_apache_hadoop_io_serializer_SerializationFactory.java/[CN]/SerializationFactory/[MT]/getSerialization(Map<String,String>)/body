{
  for (  SerializationBase serialization : serializations) {
    if (serialization.accept(metadata)) {
      return (SerializationBase<T>)serialization;
    }
  }
  for (  SerializationBase serialization : legacySerializations) {
    if (serialization.accept(metadata)) {
      return (SerializationBase<T>)serialization;
    }
  }
  return null;
}
