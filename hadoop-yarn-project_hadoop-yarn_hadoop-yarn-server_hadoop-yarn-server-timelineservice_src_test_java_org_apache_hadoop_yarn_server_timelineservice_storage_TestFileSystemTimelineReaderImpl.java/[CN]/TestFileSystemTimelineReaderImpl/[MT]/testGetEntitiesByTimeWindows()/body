      Assert.fail("Incorrect filtering based on created time range");
    }
  }
  result=reader.getEntities("user1","cluster1","flow1",1L,"app1","app",null,1425016502010L,null,null,null,null,null,null,null,null,null,null);
  Assert.assertEquals(1,result.size());
  for (  TimelineEntity entity : result) {
