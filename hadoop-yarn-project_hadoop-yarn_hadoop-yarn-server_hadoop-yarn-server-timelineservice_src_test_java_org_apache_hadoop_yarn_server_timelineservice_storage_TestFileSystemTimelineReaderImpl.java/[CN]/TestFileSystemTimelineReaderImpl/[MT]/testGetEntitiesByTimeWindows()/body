      Assert.fail("Incorrect filtering based on created time range");
    }
  }
  result=reader.getEntities("user1","cluster1","flow1",1L,"app1","app",null,null,null,1425016502090L,1425016503020L,null,null,null,null,null,null,null);
  Assert.assertEquals(2,result.size());
