{
  Set<TimelineEntity> result=reader.getEntities("user1","cluster1","flow1",1L,"app1","app",null,1425016502030L,1425016502060L,null,null,null,null,null,null,null,null,null);
  Assert.assertEquals(1,result.size());
  for (  TimelineEntity entity : result) {
    if (!entity.getId().equals("id_4")) {
      Assert.fail("Incorrect filtering based on created time range");
    }
  }
