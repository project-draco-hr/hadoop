{
  Set<TimelineEntity> result=reader.getEntities("user1","cluster1","flow1",1L,"app1","app",2L,null,null,null,null,null,null,null,null,null,null,null,null,null);
  Assert.assertEquals(2,result.size());
  for (  TimelineEntity entity : result) {
    if (!entity.getId().equals("id_1") && !entity.getId().equals("id_4")) {
      Assert.fail("Entity not sorted by created time");
    }
  }
  result=reader.getEntities("user1","cluster1","flow1",1L,"app1","app",3L,null,null,null,null,null,null,null,null,null,null,null,null,null);
  Assert.assertEquals(3,result.size());
}
