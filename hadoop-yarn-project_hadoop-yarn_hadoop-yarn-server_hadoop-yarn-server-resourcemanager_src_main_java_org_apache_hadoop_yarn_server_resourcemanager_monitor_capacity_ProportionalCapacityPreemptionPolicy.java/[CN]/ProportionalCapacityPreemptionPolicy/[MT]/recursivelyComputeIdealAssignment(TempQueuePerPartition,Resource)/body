{
  Set<String> leafQueueNames=new HashSet<>();
  if (root.getChildren() != null && root.getChildren().size() > 0) {
    computeIdealResourceDistribution(rc,root.getChildren(),totalPreemptionAllowed,root.idealAssigned);
    for (    TempQueuePerPartition t : root.getChildren()) {
      leafQueueNames.addAll(recursivelyComputeIdealAssignment(t,totalPreemptionAllowed));
    }
  }
 else {
    return ImmutableSet.of(root.queueName);
  }
  return leafQueueNames;
}
