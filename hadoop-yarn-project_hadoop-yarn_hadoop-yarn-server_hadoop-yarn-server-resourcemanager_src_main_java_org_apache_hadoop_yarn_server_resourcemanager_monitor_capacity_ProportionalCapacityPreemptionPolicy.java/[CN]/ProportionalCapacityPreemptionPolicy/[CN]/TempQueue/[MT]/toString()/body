{
  StringBuilder sb=new StringBuilder();
  sb.append("CUR: ").append(current).append(" PEN: ").append(pending).append(" GAR: ").append(guaranteed).append(" NORM: ").append(normalizedGuarantee).append(" IDEAL_ASSIGNED: ").append(idealAssigned).append(" IDEAL_PREEMPT: ").append(toBePreempted).append(" ACTUAL_PREEMPT: ").append(actuallyPreempted);
  return sb.toString();
}
