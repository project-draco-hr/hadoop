{
  this.conf=conf;
  haEnabled=HAUtil.isHAEnabled(this.conf);
  if (haEnabled) {
    HAUtil.verifyAndSetConfiguration(conf);
    rm.setConf(this.conf);
    adminAcl=new AccessControlList(conf.get(YarnConfiguration.YARN_ADMIN_ACL,YarnConfiguration.DEFAULT_YARN_ADMIN_ACL));
  }
  rm.createAndInitActiveServices();
  super.serviceInit(this.conf);
}
