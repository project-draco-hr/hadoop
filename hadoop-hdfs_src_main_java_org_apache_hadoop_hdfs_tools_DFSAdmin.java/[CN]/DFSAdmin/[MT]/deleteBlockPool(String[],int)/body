{
  ClientDatanodeProtocol dnProxy=getDataNodeProxy(argv[i]);
  boolean force=false;
  if (argv.length - 1 == i + 2) {
    if ("force".equals(argv[i + 2])) {
      force=true;
    }
 else {
      printUsage("-deleteBlockPool");
      return -1;
    }
  }
  dnProxy.deleteBlockPool(argv[i + 1],force);
  return 0;
}
