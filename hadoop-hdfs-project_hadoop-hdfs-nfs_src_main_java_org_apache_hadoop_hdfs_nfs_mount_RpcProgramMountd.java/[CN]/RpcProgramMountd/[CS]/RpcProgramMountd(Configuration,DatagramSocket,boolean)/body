{
  super("mountd","localhost",config.getInt("nfs3.mountd.port",PORT),PROGRAM,VERSION_1,VERSION_3,registrationSocket,allowInsecurePorts);
  exports=new ArrayList<String>();
  exports.add(config.get(Nfs3Constant.EXPORT_POINT,Nfs3Constant.EXPORT_POINT_DEFAULT));
  this.hostsMatcher=NfsExports.getInstance(config);
  this.mounts=Collections.synchronizedList(new ArrayList<MountEntry>());
  UserGroupInformation.setConfiguration(config);
  SecurityUtil.login(config,DFS_NFS_KEYTAB_FILE_KEY,DFS_NFS_KERBEROS_PRINCIPAL_KEY);
  this.dfsClient=new DFSClient(NameNode.getAddress(config),config);
}
