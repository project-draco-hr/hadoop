{
  if (LOG.isDebugEnabled()) {
    LOG.debug("KILL_CONTAINER: container" + cont.toString());
  }
  recoverResourceRequestForContainer(cont);
  completedContainer(cont,SchedulerUtils.createPreemptedContainerStatus(cont.getContainerId(),SchedulerUtils.PREEMPTED_CONTAINER),RMContainerEventType.KILL);
}
