{
  if (rmContainer == null) {
    LOG.info("Container " + containerStatus.getContainerId() + " completed with event "+ event);
    return;
  }
  Container container=rmContainer.getContainer();
  FiCaSchedulerApp application=getCurrentAttemptForContainer(container.getId());
  ApplicationId appId=container.getId().getApplicationAttemptId().getApplicationId();
  if (application == null) {
    LOG.info("Container " + container + " of"+ " finished application "+ appId+ " completed with event "+ event);
    return;
  }
  FiCaSchedulerNode node=getNode(container.getNodeId());
  LeafQueue queue=(LeafQueue)application.getQueue();
  queue.completedContainer(clusterResource,application,node,rmContainer,containerStatus,event,null,true);
  LOG.info("Application attempt " + application.getApplicationAttemptId() + " released container "+ container.getId()+ " on node: "+ node+ " with event: "+ event);
}
