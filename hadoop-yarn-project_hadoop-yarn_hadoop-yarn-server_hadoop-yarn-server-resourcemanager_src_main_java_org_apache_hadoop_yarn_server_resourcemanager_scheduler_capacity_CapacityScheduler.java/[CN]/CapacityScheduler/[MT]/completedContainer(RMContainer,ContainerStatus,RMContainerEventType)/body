{
  if (rmContainer == null) {
    LOG.info("Null container completed...");
    return;
  }
  Container container=rmContainer.getContainer();
  ApplicationAttemptId applicationAttemptId=container.getId().getApplicationAttemptId();
  FiCaSchedulerApp application=getApplication(applicationAttemptId);
  if (application == null) {
    LOG.info("Container " + container + " of"+ " unknown application "+ applicationAttemptId+ " completed with event "+ event);
    return;
  }
  FiCaSchedulerNode node=getNode(container.getNodeId());
  LeafQueue queue=(LeafQueue)application.getQueue();
  queue.completedContainer(clusterResource,application,node,rmContainer,containerStatus,event,null);
  LOG.info("Application " + applicationAttemptId + " released container "+ container.getId()+ " on node: "+ node+ " with event: "+ event);
}
