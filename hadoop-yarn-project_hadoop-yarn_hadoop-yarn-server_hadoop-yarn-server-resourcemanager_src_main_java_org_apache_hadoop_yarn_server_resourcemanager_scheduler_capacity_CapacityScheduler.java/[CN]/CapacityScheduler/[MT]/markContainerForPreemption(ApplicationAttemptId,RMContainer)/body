{
  if (LOG.isDebugEnabled()) {
    LOG.debug(SchedulerEventType.MARK_CONTAINER_FOR_PREEMPTION + ": appAttempt:" + aid.toString()+ " container: "+ cont.toString());
  }
  FiCaSchedulerApp app=getApplicationAttempt(aid);
  if (app != null) {
    app.markContainerForPreemption(cont.getContainerId());
  }
}
