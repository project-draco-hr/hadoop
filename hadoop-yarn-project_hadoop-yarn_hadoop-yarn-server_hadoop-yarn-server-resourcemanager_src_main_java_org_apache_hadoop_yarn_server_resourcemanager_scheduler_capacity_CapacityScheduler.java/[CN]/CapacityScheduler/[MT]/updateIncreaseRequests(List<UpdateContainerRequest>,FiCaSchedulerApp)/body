{
  if (null == increaseRequests || increaseRequests.isEmpty()) {
    return null;
  }
  List<SchedContainerChangeRequest> schedIncreaseRequests=createSchedContainerChangeRequests(increaseRequests,true);
  LeafQueue leafQueue=(LeafQueue)app.getQueue();
synchronized (leafQueue) {
    if (app.isStopped()) {
      return null;
    }
    if (app.updateIncreaseRequests(schedIncreaseRequests)) {
      return leafQueue;
    }
    return null;
  }
}
