def getTarList(self, hodring):
    lock = self.tarSourceLock
    lock.acquire()
    leastkey = None
    leastval = (-1)
    for (k, v) in self.count.iteritems():
        if (leastval == (-1)):
            leastval = v
            pass
        if ((v <= leastval) and (v < self.maxconnect)):
            leastkey = k
            leastval = v
    if (leastkey == None):
        url = 'none'
    else:
        url = self.dict[leastkey]
        self.count[leastkey] = (leastval + 1)
        self.log.debug(('%s %d' % (leastkey, self.count[leastkey])))
    lock.release()
    self.log.debug(((('sending url ' + url) + ' to ') + hodring))
    return url
