{
  if (argv.length > 0) {
    System.err.println("Usage: HdfsProxy");
    return null;
  }
  if (conf == null) {
    conf=new HdfsConfiguration(false);
    conf.addResource("hdfsproxy-default.xml");
  }
  StringUtils.startupShutdownMessage(HdfsProxy.class,argv,LOG);
  HdfsProxy proxy=new HdfsProxy(conf);
  proxy.start();
  return proxy;
}
