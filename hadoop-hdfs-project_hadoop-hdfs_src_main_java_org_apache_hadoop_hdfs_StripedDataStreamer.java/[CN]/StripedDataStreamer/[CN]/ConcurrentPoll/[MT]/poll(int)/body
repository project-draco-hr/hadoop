{
  for (; ; ) {
synchronized (queue) {
      final T polled=queue.poll(i);
      if (polled != null) {
        return polled;
      }
      if (isReady2Populate()) {
        try {
          populate();
          return queue.poll(i);
        }
 catch (        IOException ioe) {
          LOG.warn("Failed to populate, " + this,ioe);
        }
      }
    }
    sleep(100,"poll");
  }
}
