{
  if (!isLeadingStreamer() && !isParityStreamer()) {
    LocatedBlock finishedBlock=new LocatedBlock(new ExtendedBlock(block.getBlockPoolId(),block.getBlockId(),block.getNumBytes(),block.getGenerationStamp()),null);
    try {
      boolean offSuccess=stripedBlocks.get(0).offer(finishedBlock,30,TimeUnit.SECONDS);
    }
 catch (    InterruptedException ie) {
    }
  }
  super.endBlock();
}
