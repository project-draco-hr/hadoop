{
  int k=key.get();
  final int vali=val.get();
  final String kvstr="Unexpected tuple: " + stringify(key,val);
  if (0 == k % (srcs * srcs)) {
    assertTrue(kvstr,vali == k * 10 / srcs + srcs - 1);
  }
 else {
    final int i=k % srcs;
    assertTrue(kvstr,srcs * (vali - i) == 10 * (k - i));
  }
  context.write(key,one);
  key.set(-1);
  val.set(0);
}
