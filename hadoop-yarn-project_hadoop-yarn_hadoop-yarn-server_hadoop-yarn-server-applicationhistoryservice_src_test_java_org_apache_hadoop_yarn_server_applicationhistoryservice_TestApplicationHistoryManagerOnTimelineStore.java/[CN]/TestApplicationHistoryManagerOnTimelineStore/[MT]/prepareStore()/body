{
  store=createStore(SCALE);
  TimelineEntities entities=new TimelineEntities();
  entities.addEntity(createApplicationTimelineEntity(ApplicationId.newInstance(0,SCALE + 1),true,true,false));
  entities.addEntity(createApplicationTimelineEntity(ApplicationId.newInstance(0,SCALE + 2),true,false,true));
