{
  Map<String,Long> resourceMetrics=new HashMap<String,Long>();
  long mapCPUUsage=getCounterValue(jhInfo.getMapCounters(),TaskCounter.CPU_MILLISECONDS.toString());
  resourceMetrics.put("MAP",mapCPUUsage);
  long reduceCPUUsage=getCounterValue(jhInfo.getReduceCounters(),TaskCounter.CPU_MILLISECONDS.toString());
  resourceMetrics.put("REDUCE",reduceCPUUsage);
  return resourceMetrics;
}
