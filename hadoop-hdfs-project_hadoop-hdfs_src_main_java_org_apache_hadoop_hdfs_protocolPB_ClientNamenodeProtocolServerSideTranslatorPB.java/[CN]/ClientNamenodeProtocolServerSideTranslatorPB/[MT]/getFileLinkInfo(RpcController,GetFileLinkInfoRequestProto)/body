{
  try {
    HdfsFileStatus result=server.getFileLinkInfo(req.getSrc());
    if (result != null) {
      return GetFileLinkInfoResponseProto.newBuilder().setFs(PBHelper.convert(result)).build();
    }
 else {
      return VOID_GETFILELINKINFO_RESPONSE;
    }
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
}
