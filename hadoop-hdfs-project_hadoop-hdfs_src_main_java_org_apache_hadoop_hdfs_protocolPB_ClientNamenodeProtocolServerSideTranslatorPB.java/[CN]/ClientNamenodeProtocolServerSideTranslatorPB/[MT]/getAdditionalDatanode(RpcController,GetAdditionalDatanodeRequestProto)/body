{
  try {
    return GetAdditionalDatanodeResponseProto.newBuilder().setBlock(PBHelper.convert(server.getAdditionalDatanode(req.getSrc(),PBHelper.convert(req.getBlk()),PBHelper.convert((DatanodeInfoProto[])req.getExistingsList().toArray()),PBHelper.convert((DatanodeInfoProto[])req.getExcludesList().toArray()),req.getNumAdditionalNodes(),req.getClientName()))).build();
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
}
