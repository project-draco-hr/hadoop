{
  try {
    UpgradeStatusReport result=server.distributedUpgradeProgress(PBHelper.convert(req.getAction()));
    DistributedUpgradeProgressResponseProto.Builder builder=DistributedUpgradeProgressResponseProto.newBuilder();
    if (result != null) {
      builder.setReport(PBHelper.convert(result));
    }
    return builder.build();
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
}
