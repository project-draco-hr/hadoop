{
  try {
    server.updatePipeline(req.getClientName(),PBHelper.convert(req.getOldBlock()),PBHelper.convert(req.getNewBlock()),PBHelper.convert((DatanodeIDProto[])req.getNewNodesList().toArray()));
    return VOID_UPDATEPIPELINE_RESPONSE;
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
}
