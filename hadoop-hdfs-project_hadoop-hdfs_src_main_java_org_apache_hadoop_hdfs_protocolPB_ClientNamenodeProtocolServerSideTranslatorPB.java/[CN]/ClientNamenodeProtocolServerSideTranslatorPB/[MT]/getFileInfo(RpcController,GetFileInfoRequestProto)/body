{
  try {
    HdfsFileStatus result=server.getFileInfo(req.getSrc());
    if (result != null) {
      return GetFileInfoResponseProto.newBuilder().setFs(PBHelper.convert(result)).build();
    }
    return VOID_GETFILEINFO_RESPONSE;
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
}
