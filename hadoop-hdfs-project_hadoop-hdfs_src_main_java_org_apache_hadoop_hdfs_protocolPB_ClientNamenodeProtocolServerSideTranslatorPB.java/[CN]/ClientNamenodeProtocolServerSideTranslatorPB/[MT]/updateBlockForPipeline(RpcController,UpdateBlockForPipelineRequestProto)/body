{
  try {
    LocatedBlockProto result=PBHelper.convertLocatedBlock(server.updateBlockForPipeline(PBHelper.convert(req.getBlock()),req.getClientName()));
    return UpdateBlockForPipelineResponseProto.newBuilder().setBlock(result).build();
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
}
