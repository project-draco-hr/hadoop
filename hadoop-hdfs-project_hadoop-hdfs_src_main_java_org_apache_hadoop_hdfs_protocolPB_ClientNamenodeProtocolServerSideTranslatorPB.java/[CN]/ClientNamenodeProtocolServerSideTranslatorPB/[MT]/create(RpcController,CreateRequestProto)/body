{
  try {
    server.create(req.getSrc(),PBHelper.convert(req.getMasked()),req.getClientName(),PBHelper.convert(req.getCreateFlag()),req.getCreateParent(),(short)req.getReplication(),req.getBlockSize());
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
  return VOID_CREATE_RESPONSE;
}
