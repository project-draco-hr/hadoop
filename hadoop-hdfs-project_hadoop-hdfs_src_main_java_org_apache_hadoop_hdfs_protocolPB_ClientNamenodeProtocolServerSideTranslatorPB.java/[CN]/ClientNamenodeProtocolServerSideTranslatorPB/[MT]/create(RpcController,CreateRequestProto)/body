{
  try {
    HdfsFileStatus result=server.create(req.getSrc(),PBHelperClient.convert(req.getMasked()),req.getClientName(),PBHelperClient.convertCreateFlag(req.getCreateFlag()),req.getCreateParent(),(short)req.getReplication(),req.getBlockSize(),PBHelperClient.convertCryptoProtocolVersions(req.getCryptoProtocolVersionList()));
    if (result != null) {
      return CreateResponseProto.newBuilder().setFs(PBHelperClient.convert(result)).build();
    }
    return VOID_CREATE_RESPONSE;
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
}
