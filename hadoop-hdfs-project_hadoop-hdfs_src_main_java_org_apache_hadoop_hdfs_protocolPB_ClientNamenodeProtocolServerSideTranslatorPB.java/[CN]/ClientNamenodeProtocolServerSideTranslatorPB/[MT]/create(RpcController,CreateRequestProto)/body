{
  try {
    HdfsFileStatus result=server.create(req.getSrc(),PBHelper.convert(req.getMasked()),req.getClientName(),PBHelper.convertCreateFlag(req.getCreateFlag()),req.getCreateParent(),(short)req.getReplication(),req.getBlockSize());
    if (result != null) {
      return CreateResponseProto.newBuilder().setFs(PBHelper.convert(result)).build();
    }
    return VOID_CREATE_RESPONSE;
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
}
