{
  try {
    final RollingUpgradeInfo info=server.rollingUpgrade(PBHelper.convert(req.getAction()));
    final RollingUpgradeResponseProto.Builder b=RollingUpgradeResponseProto.newBuilder();
    if (info != null) {
      b.setRollingUpgradeInfo(PBHelper.convert(info));
    }
    return b.build();
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
}
