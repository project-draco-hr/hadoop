{
  try {
    ErasureCodingPolicy ecPolicy=req.hasEcPolicy() ? PBHelper.convertErasureCodingPolicy(req.getEcPolicy()) : null;
    server.createErasureCodingZone(req.getSrc(),ecPolicy);
    return CreateErasureCodingZoneResponseProto.newBuilder().build();
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
}
