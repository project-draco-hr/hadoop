{
  try {
    LocatedBlock result=server.append(req.getSrc(),req.getClientName());
    if (result != null) {
      return AppendResponseProto.newBuilder().setBlock(PBHelper.convert(result)).build();
    }
    return NULL_APPEND_RESPONSE;
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
}
