{
  try {
    SnapshotDiffReport report=server.getSnapshotDiffReport(request.getSnapshotRoot(),request.getFromSnapshot(),request.getToSnapshot());
    return GetSnapshotDiffReportResponseProto.newBuilder().setDiffReport(PBHelperClient.convert(report)).build();
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
}
