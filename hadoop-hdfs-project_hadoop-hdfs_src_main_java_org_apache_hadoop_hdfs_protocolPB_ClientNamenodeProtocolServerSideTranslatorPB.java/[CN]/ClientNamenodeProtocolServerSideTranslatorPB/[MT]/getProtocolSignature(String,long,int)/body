{
  if (!protocol.equals(RPC.getProtocolName(ClientNamenodeProtocolPB.class))) {
    throw new IOException("Namenode Serverside implements " + RPC.getProtocolName(ClientNamenodeProtocolPB.class) + ". The following requested protocol is unknown: "+ protocol);
  }
  return ProtocolSignature.getProtocolSignature(clientMethodsHash,RPC.getProtocolVersion(ClientNamenodeProtocolPB.class),ClientNamenodeProtocolPB.class);
}
