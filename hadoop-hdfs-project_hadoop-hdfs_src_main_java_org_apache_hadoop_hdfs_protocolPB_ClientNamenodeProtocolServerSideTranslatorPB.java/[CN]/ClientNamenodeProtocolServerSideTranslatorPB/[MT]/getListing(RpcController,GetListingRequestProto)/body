{
  try {
    DirectoryListing result=server.getListing(req.getSrc(),req.getStartAfter().toByteArray(),req.getNeedLocation());
    if (result != null) {
      return GetListingResponseProto.newBuilder().setDirList(PBHelper.convert(result)).build();
    }
 else {
      return NULL_GETLISTING_RESPONSE;
    }
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
}
