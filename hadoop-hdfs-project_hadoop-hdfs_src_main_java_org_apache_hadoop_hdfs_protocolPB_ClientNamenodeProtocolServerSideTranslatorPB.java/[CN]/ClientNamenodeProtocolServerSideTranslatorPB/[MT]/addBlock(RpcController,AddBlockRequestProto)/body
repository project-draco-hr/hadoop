{
  try {
    return AddBlockResponseProto.newBuilder().setBlock(PBHelper.convert(server.addBlock(req.getSrc(),req.getClientName(),PBHelper.convert(req.getPrevious()),PBHelper.convert((DatanodeInfoProto[])req.getExcludeNodesList().toArray())))).build();
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
}
