{
  if (isDaemonStarted() == true) {
    return;
  }
  daemon=new Daemon(new Runnable(){
    @Override public void run(){
      try {
        PeerCache.this.run();
      }
 catch (      InterruptedException e) {
      }
 finally {
        PeerCache.this.clear();
      }
    }
    @Override public String toString(){
      return String.valueOf(PeerCache.this);
    }
  }
);
  daemon.start();
}
