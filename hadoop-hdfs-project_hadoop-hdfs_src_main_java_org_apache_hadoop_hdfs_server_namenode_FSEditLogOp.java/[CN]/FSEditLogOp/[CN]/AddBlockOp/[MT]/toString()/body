{
  StringBuilder sb=new StringBuilder();
  sb.append("AddBlockOp [path=").append(path).append(", penultimateBlock=").append(penultimateBlock == null ? "NULL" : penultimateBlock).append(", lastBlock=").append(lastBlock);
  appendRpcIdsToString(sb,rpcClientId,rpcCallId);
