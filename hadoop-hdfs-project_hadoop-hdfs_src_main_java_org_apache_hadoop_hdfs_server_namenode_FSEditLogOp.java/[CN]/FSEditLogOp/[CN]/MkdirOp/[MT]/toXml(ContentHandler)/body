{
  XMLUtils.addSaxString(contentHandler,"LENGTH",Integer.valueOf(length).toString());
  XMLUtils.addSaxString(contentHandler,"INODEID",Long.valueOf(inodeId).toString());
  XMLUtils.addSaxString(contentHandler,"PATH",path);
  XMLUtils.addSaxString(contentHandler,"TIMESTAMP",Long.valueOf(timestamp).toString());
  FSEditLogOp.permissionStatusToXml(contentHandler,permissions);
  if (permissions.getPermission().getAclBit()) {
    appendAclEntriesToXml(contentHandler,aclEntries);
  }
}
