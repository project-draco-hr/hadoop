{
  StringBuilder builder=new StringBuilder();
  builder.append("SetReplicationOp [path=");
  builder.append(path);
  builder.append(", replication=");
  builder.append(replication);
  builder.append(", opCode=");
  builder.append(opCode);
  builder.append(", txid=");
  builder.append(txid);
