{
  src=st.getValue("SRC");
  if (!st.hasChildren("ENTRY"))   return;
  List<Stanza> stanzas=st.getChildren("ENTRY");
  for (  Stanza s : stanzas) {
    AclEntry e=new AclEntry.Builder().setScope(AclEntryScope.valueOf(s.getValue("SCOPE"))).setType(AclEntryType.valueOf(s.getValue("TYPE"))).setName(s.getValue("NAME")).setPermission(fsActionFromXml(s)).build();
    aclEntries.add(e);
  }
}
