{
  directive=new CacheDirectiveInfo.Builder().setId(Long.parseLong(st.getValue("ID"))).setPath(new Path(st.getValue("PATH"))).setReplication(Short.parseShort(st.getValue("REPLICATION"))).setPool(st.getValue("POOL")).setExpiration(CacheDirectiveInfo.Expiration.newAbsolute(Long.parseLong(st.getValue("EXPIRATION")))).build();
  readRpcIdsFromXml(st);
}
