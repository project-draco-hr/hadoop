{
  for (int idx=0; idx < len; idx++) {
    if (expected[from + idx] != actual[idx]) {
      Assert.fail(message + " byte " + (from + idx)+ " differs. expected "+ expected[from + idx]+ " actual "+ actual[idx]+ "\nexpected: "+ StringUtils.byteToHexString(expected,from,from + len)+ "\nactual:   "+ StringUtils.byteToHexString(actual,0,len));
    }
  }
}
