{
  for (int i=0; i < availableSlots; i++) {
    for (    JobInProgress job : jobQueue) {
      if ((job.getStatus().getRunState() != JobStatus.RUNNING) || (!map && job.numReduceTasks == 0)) {
        continue;
      }
      if (assignMapRedTask(job,taskTracker,numTrackers,assignedTasks,queueQuota,fill,map)) {
        break;
      }
    }
  }
}
