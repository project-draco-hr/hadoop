{
  DatanodeProtocol mock=Mockito.mock(DatanodeProtocol.class);
  Mockito.doReturn(new NamespaceInfo(1,FAKE_CLUSTERID,FAKE_BPID,0,HdfsConstants.LAYOUT_VERSION)).when(mock).versionRequest();
  Mockito.doReturn(new HeartbeatResponse(null)).when(mock).sendHeartbeat(Mockito.any(DatanodeRegistration.class),Mockito.anyLong(),Mockito.anyLong(),Mockito.anyLong(),Mockito.anyLong(),Mockito.anyInt(),Mockito.anyInt(),Mockito.anyInt());
  return mock;
}
