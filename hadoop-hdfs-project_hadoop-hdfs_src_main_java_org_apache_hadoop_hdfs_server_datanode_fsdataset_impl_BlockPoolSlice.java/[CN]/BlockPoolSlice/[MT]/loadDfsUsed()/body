{
  long cachedDfsUsed;
  long mtime;
  Scanner sc;
  try {
    sc=new Scanner(new File(currentDir,DU_CACHE_FILE));
  }
 catch (  FileNotFoundException fnfe) {
    return -1;
  }
  try {
    if (sc.hasNextLong()) {
      cachedDfsUsed=sc.nextLong();
    }
 else {
      return -1;
    }
    if (sc.hasNextLong()) {
      mtime=sc.nextLong();
    }
 else {
      return -1;
    }
    if (mtime > 0 && (Time.now() - mtime < 600000L)) {
      FsDatasetImpl.LOG.info("Cached dfsUsed found for " + currentDir + ": "+ cachedDfsUsed);
      return cachedDfsUsed;
    }
    return -1;
  }
  finally {
    sc.close();
  }
}
