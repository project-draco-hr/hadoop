{
  Set<KerberosTicket> tickets=subject.getPrivateCredentials(KerberosTicket.class);
  for (  KerberosTicket ticket : tickets) {
    if (SecurityUtil.isOriginalTGT(ticket)) {
      if (LOG.isDebugEnabled()) {
        LOG.debug("Found tgt " + ticket);
      }
      return ticket;
    }
  }
  return null;
}
