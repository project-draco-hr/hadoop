{
  if (lb == null)   return null;
  final int len=lb.length;
  org.apache.hadoop.hdfs.protocol.LocatedBlock[] result=new org.apache.hadoop.hdfs.protocol.LocatedBlock[len];
  for (int i=0; i < len; ++i) {
    result[i]=new org.apache.hadoop.hdfs.protocol.LocatedBlock(ExtendedBlockWritable.convertExtendedBlock(lb[i].getBlock()),DatanodeInfoWritable.convertDatanodeInfo(lb[i].getLocations()),lb[i].getStartOffset(),lb[i].isCorrupt());
  }
  return result;
}
