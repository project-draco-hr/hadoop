{
  if (lb == null)   return null;
  final int len=lb.size();
  List<org.apache.hadoop.hdfs.protocol.LocatedBlock> result=new ArrayList<org.apache.hadoop.hdfs.protocol.LocatedBlock>(len);
  for (int i=0; i < len; ++i) {
    result.add(LocatedBlockWritable.convertLocatedBlock(lb.get(i)));
  }
  return result;
}
