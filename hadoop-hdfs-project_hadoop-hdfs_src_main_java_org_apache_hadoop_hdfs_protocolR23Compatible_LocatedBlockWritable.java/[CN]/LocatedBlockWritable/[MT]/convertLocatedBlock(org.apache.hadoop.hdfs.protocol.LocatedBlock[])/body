{
  if (lb == null)   return null;
  final int len=lb.length;
  LocatedBlockWritable[] result=new LocatedBlockWritable[len];
  for (int i=0; i < len; ++i) {
    result[i]=new LocatedBlockWritable(ExtendedBlockWritable.convertExtendedBlock(lb[i].getBlock()),DatanodeInfoWritable.convertDatanodeInfo(lb[i].getLocations()),lb[i].getStartOffset(),lb[i].isCorrupt());
  }
  return result;
}
