{
  if (lb == null)   return null;
  org.apache.hadoop.hdfs.protocol.LocatedBlock result=new org.apache.hadoop.hdfs.protocol.LocatedBlock(ExtendedBlockWritable.convertExtendedBlock(lb.getBlock()),DatanodeInfoWritable.convertDatanodeInfo(lb.getLocations()),lb.getStartOffset(),lb.isCorrupt());
  TokenWritable tok=lb.getBlockToken();
  result.setBlockToken(new org.apache.hadoop.security.token.Token<BlockTokenIdentifier>(tok.getIdentifier(),tok.getPassword(),tok.getKind(),tok.getService()));
  return result;
}
