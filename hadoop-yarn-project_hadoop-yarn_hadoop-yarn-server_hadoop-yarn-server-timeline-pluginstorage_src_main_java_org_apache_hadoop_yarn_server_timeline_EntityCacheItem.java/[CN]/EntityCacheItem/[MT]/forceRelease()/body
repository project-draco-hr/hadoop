{
  try {
    if (store != null) {
      store.close();
    }
  }
 catch (  IOException e) {
    LOG.warn("Error closing timeline store",e);
  }
  store=null;
  for (  LogInfo log : appLogs.getDetailLogs()) {
    if (log.getFilename().contains(groupId.toString())) {
      log.setOffset(0);
    }
  }
  LOG.debug("Cache for group {} released. ",groupId);
}
