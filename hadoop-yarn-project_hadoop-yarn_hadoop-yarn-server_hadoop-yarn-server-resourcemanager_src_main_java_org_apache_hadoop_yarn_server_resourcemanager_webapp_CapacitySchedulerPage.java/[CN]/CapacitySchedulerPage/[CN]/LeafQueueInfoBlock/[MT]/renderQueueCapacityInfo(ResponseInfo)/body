{
  ri._("Used Capacity:",percent(lqinfo.getUsedCapacity() / 100))._("Configured Capacity:",percent(lqinfo.getCapacity() / 100))._("Configured Max Capacity:",percent(lqinfo.getMaxCapacity() / 100))._("Absolute Used Capacity:",percent(lqinfo.getAbsoluteUsedCapacity() / 100))._("Absolute Configured Capacity:",percent(lqinfo.getAbsoluteCapacity() / 100))._("Absolute Configured Max Capacity:",percent(lqinfo.getAbsoluteMaxCapacity() / 100))._("Used Resources:",lqinfo.getResourcesUsed().toString());
}
