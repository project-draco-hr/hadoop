{
  CommitBlockSynchronizationRequestProto.Builder builder=CommitBlockSynchronizationRequestProto.newBuilder().setBlock(PBHelper.convert(block)).setNewGenStamp(newgenerationstamp).setNewLength(newlength).setCloseFile(closeFile).setDeleteBlock(deleteblock);
  for (int i=0; i < newtargets.length; i++) {
    builder.setNewTaragets(i,PBHelper.convert(newtargets[i]));
  }
  CommitBlockSynchronizationRequestProto req=builder.build();
  try {
    rpcProxy.commitBlockSynchronization(NULL_CONTROLLER,req);
  }
 catch (  ServiceException se) {
    throw ProtobufHelper.getRemoteException(se);
  }
}
