{
  String key=null;
  try {
    Configuration c=ProviderUtils.excludeIncompatibleCredentialProviders(conf,NativeAzureFileSystem.class);
    char[] keyChars=c.getPassword(getStorageAccountKeyName(accountName));
    if (keyChars != null) {
      key=new String(keyChars);
    }
  }
 catch (  IOException ioe) {
    LOG.warn("Unable to get key from credential providers.",ioe);
  }
  return key;
}
