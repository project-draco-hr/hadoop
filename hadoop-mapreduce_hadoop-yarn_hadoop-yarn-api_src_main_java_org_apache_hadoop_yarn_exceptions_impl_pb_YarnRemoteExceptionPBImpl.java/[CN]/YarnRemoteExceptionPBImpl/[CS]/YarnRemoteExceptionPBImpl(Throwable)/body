{
  super(t);
  maybeInitBuilder();
  if (t.getCause() == null) {
  }
 else {
    builder.setCause(new YarnRemoteExceptionPBImpl(t.getCause()).getProto());
    builder.setClassName(t.getClass().getName());
  }
  StringWriter sw=new StringWriter();
  PrintWriter pw=new PrintWriter(sw);
  t.printStackTrace(pw);
  pw.close();
  if (sw.toString() != null)   builder.setTrace(sw.toString());
  if (t.getMessage() != null)   builder.setMessage(t.getMessage());
}
