{
  try {
    if (!doPipe_) {
      LOG.info("mapRedFinished");
      return;
    }
    try {
      if (clientOut_ != null) {
        clientOut_.flush();
        clientOut_.close();
      }
      waitOutputThreads();
    }
 catch (    IOException io) {
      LOG.warn(io);
    }
    if (sim != null)     sim.destroy();
    LOG.info("mapRedFinished");
  }
 catch (  RuntimeException e) {
    LOG.info("PipeMapRed failed!",e);
    throw e;
  }
}
