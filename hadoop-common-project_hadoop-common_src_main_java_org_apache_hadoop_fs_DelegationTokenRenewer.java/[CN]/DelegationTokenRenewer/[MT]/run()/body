{
  for (; ; ) {
    RenewAction<T> action=null;
    try {
      action=queue.take();
      if (action.renew()) {
        action.updateRenewalTime();
        queue.add(action);
      }
    }
 catch (    InterruptedException ie) {
      return;
    }
catch (    Exception ie) {
      T.LOG.warn("Failed to renew token, action=" + action,ie);
    }
  }
}
