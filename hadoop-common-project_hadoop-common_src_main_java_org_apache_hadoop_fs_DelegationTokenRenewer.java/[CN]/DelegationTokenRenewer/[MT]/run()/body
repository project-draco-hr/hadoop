{
  for (; ; ) {
    RenewAction<?> action=null;
    try {
synchronized (this) {
        action=queue.take();
        if (action.renew()) {
          action.updateRenewalTime();
          queue.add(action);
        }
      }
    }
 catch (    InterruptedException ie) {
      return;
    }
catch (    Exception ie) {
      action.weakFs.get().LOG.warn("Failed to renew token, action=" + action,ie);
    }
  }
}
