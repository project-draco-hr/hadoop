{
  final Call saslCall=new Call(AuthProtocol.SASL.callId,RpcConstants.INVALID_RETRY_COUNT,null,this);
  final ByteArrayOutputStream saslResponse=new ByteArrayOutputStream();
  setupResponse(saslResponse,saslCall,RpcStatusProto.SUCCESS,null,new RpcResponseWrapper(message),null,null);
  saslCall.sendResponse();
}
