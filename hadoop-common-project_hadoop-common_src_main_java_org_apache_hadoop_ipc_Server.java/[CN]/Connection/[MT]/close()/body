{
  disposeSasl();
  data=null;
  dataLengthBuffer=null;
  if (!channel.isOpen())   return;
  try {
    socket.shutdownOutput();
  }
 catch (  Exception e) {
    LOG.debug("Ignoring socket shutdown exception",e);
  }
  if (channel.isOpen()) {
    IOUtils.cleanup(null,channel);
  }
  IOUtils.cleanup(null,socket);
}
