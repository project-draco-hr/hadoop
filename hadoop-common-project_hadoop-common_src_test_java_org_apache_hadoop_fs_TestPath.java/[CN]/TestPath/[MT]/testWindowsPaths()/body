{
  if (!Path.WINDOWS) {
    return;
  }
  assertEquals(new Path("c:\\foo\\bar").toString(),"c:/foo/bar");
  assertEquals(new Path("c:/foo/bar").toString(),"c:/foo/bar");
  assertEquals(new Path("/c:/foo/bar").toString(),"c:/foo/bar");
  assertEquals(new Path("file://c:/foo/bar").toString(),"file://c:/foo/bar");
}
