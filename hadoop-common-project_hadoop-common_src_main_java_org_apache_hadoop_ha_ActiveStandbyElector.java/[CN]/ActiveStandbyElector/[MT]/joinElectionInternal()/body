{
  Preconditions.checkState(appData != null,"trying to join election without any app data");
  if (zkClient == null) {
    if (!reEstablishSession()) {
      fatalError("Failed to reEstablish connection with ZooKeeper");
      return;
    }
  }
  createRetryCount=0;
  wantToBeInElection=true;
  createLockNodeAsync();
}
