{
  if (state != State.ACTIVE) {
    try {
      Stat oldBreadcrumbStat=fenceOldActive();
      writeBreadCrumbNode(oldBreadcrumbStat);
    }
 catch (    Exception e) {
      LOG.warn("Exception handling the winning of election",e);
      reJoinElection();
      return;
    }
    LOG.debug("Becoming active");
    state=State.ACTIVE;
    appClient.becomeActive();
  }
}
