{
  hasReceivedEvent.countDown();
  try {
    hasSetZooKeeper.await(zkSessionTimeout,TimeUnit.MILLISECONDS);
    ActiveStandbyElector.this.processWatchEvent(zk,event);
  }
 catch (  Throwable t) {
    fatalError("Failed to process watcher event " + event + ": "+ StringUtils.stringifyException(t));
  }
}
