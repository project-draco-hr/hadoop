{
  List<String> actualExpansions=GlobExpander.expand(filePattern);
  assertEquals("Different number of expansions",expectedExpansions.length,actualExpansions.size());
  for (int i=0; i < expectedExpansions.length; i++) {
    assertEquals("Expansion of " + filePattern,expectedExpansions[i],actualExpansions.get(i));
  }
}
