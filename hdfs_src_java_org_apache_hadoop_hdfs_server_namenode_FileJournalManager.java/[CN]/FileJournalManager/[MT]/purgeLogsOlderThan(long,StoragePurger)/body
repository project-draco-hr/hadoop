{
  File[] files=FileUtil.listFiles(sd.getCurrentDir());
  List<FoundEditLog> editLogs=FSImageTransactionalStorageInspector.matchEditLogs(files);
  for (  FoundEditLog log : editLogs) {
    if (log.getStartTxId() < minTxIdToKeep && log.getLastTxId() < minTxIdToKeep) {
      purger.purgeLog(log);
    }
  }
}
