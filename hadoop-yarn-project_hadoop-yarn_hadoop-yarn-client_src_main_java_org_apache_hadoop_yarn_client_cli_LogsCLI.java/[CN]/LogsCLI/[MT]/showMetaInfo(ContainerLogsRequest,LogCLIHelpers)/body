{
  if (!request.isAppFinished()) {
    System.err.println("The -show_meta_info command can be only used " + "with finished applications");
    return -1;
  }
 else {
    logCliHelper.printLogMetadata(request.getAppId(),request.getContainerId(),request.getNodeId(),request.getAppOwner(),System.out,System.err);
    return 0;
  }
}
