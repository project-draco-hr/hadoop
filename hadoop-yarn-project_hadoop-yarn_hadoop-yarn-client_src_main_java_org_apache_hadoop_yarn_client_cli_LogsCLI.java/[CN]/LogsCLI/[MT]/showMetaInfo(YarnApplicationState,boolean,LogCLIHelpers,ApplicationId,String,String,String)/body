{
  if (!isApplicationFinished(appState) && appStateObtainedSuccessfully) {
    System.err.println("The -show_meta_info command can be only used " + "with finished applications");
    return -1;
  }
 else {
    logCliHelper.printLogMetadata(appId,containerIdStr,nodeAddress,appOwner,System.out,System.err);
    return 0;
  }
}
