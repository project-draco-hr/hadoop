{
  int count=refCount.decrementAndGet();
  if (count < 0) {
    throw new IllegalArgumentException("can't decrement the " + "reference count on this ClientMmap lower than 0.");
  }
 else   if (count == 0) {
    ClientMmapManager man=manager.get();
    if (man == null) {
      unmap();
    }
 else {
      man.makeEvictable(this);
    }
  }
}
