{
  final DataTransferTest test=getDataTransferTest();
  if (test.isNotSuccessAndLastPipelineContains(index,id)) {
    FiTestUtil.LOG.info(toString(id));
    if (maxDuration <= 0) {
      for (; FiTestUtil.sleep(1000); )       ;
    }
 else {
      FiTestUtil.sleep(minDuration,maxDuration);
    }
  }
}
