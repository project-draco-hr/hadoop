{
  Iterable<DatanodeAdminProperties> datanode=Iterables.filter(allDNs.get(address.getAddress()),new Predicate<DatanodeAdminProperties>(){
    public boolean apply(    DatanodeAdminProperties input){
      return input.getAdminState().equals(AdminStates.IN_MAINTENANCE) && (input.getPort() == 0 || input.getPort() == address.getPort());
    }
  }
);
  return datanode.iterator().hasNext() ? datanode.iterator().next().getMaintenanceExpireTimeInMS() : 0;
}
