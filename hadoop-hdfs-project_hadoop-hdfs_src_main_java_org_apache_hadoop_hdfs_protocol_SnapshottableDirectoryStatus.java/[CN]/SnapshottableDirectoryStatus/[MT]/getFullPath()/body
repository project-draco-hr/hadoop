{
  String parentFullPathStr=(parentFullPath == null || parentFullPath.length == 0) ? null : DFSUtil.bytes2String(parentFullPath);
  if (parentFullPathStr == null && dirStatus.getLocalNameInBytes().length == 0) {
    return new Path("/");
  }
 else {
    return parentFullPathStr == null ? new Path(dirStatus.getLocalName()) : new Path(parentFullPathStr,dirStatus.getLocalName());
  }
}
