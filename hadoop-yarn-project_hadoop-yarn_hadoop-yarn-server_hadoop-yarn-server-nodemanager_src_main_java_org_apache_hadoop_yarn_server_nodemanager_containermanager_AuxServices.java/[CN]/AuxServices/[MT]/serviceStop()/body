{
  try {
synchronized (serviceMap) {
      for (      Service service : serviceMap.values()) {
        if (service.getServiceState() == Service.STATE.STARTED) {
          service.unregisterServiceListener(this);
          service.stop();
        }
      }
      serviceMap.clear();
      serviceMeta.clear();
    }
  }
  finally {
    super.serviceStop();
  }
}
