{
  if (!renameSupported()) {
    return;
  }
  Path testdir=path("test/dir");
  fs.mkdirs(testdir);
  Path parent=testdir.getParent();
  fs.rename(testdir,parent);
  assertEquals("Source exists: " + testdir,true,fs.exists(testdir));
  assertEquals("Destination exists" + parent,true,fs.exists(parent));
}
