{
  FileContext fc;
  try {
    fc=FileContext.getFileContext(jobConf);
  }
 catch (  IOException ioe) {
    System.err.println("Can not initialize the file system: " + ioe.getLocalizedMessage());
    return;
  }
  FSDataOutputStream o=fc.create(FileOutputFormat.getTaskOutputPath(jobConf,"Results"),EnumSet.of(CreateFlag.CREATE));
  PrintStream out=new PrintStream(o);
  printResults(out);
  out.close();
  o.close();
}
