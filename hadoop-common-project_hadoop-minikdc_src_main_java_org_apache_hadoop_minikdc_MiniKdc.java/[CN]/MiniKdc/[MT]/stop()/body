{
  if (kdc != null) {
    System.getProperties().remove(JAVA_SECURITY_KRB5_CONF);
    System.getProperties().remove(SUN_SECURITY_KRB5_DEBUG);
    kdc.stop();
    try {
      ds.shutdown();
    }
 catch (    Exception ex) {
      LOG.error("Could not shutdown ApacheDS properly: {}",ex.toString(),ex);
    }
  }
  delete(workDir);
}
