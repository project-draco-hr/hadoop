{
  acc+=ratio;
  while (acc >= 1.0 && !reduces.isEmpty()) {
    key.setSeed(r.nextLong());
    val.setSeed(r.nextLong());
    final int idx=r.nextInt(reduces.size());
    final RecordFactory f=reduces.get(idx);
    if (!f.next(key,val)) {
      reduces.remove(idx);
      continue;
    }
    context.write(key,val);
    acc-=1.0;
  }
}
