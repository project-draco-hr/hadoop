{
  for (  RecordFactory factory : reduces) {
    key.setSeed(r.nextLong());
    while (factory.next(key,val)) {
      context.write(key,val);
      key.setSeed(r.nextLong());
      try {
        matcher.match();
      }
 catch (      Exception e) {
        LOG.debug("Error in resource usage emulation! Message: ",e);
      }
    }
  }
  matcher.start();
}
