{
  try {
    WriteBatch batch=db.createWriteBatch();
    try {
      String key=APPLICATIONS_KEY_PREFIX + appId;
      batch.delete(bytes(key));
      key=FINISHED_APPS_KEY_PREFIX + appId;
      batch.delete(bytes(key));
      db.write(batch);
    }
  finally {
      batch.close();
    }
  }
 catch (  DBException e) {
    throw new IOException(e);
  }
}
