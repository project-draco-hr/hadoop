{
  if (LOG.isDebugEnabled()) {
    LOG.debug("storeContainerResourceChanged: containerId=" + containerId + ", capability="+ capability);
  }
  String key=CONTAINERS_KEY_PREFIX + containerId.toString() + CONTAINER_RESOURCE_CHANGED_KEY_SUFFIX;
  try {
    db.put(bytes(key),((ResourcePBImpl)capability).getProto().toByteArray());
  }
 catch (  DBException e) {
    throw new IOException(e);
  }
}
