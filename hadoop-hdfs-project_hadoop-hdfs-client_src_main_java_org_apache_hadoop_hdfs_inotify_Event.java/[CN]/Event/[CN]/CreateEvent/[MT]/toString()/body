{
  StringBuilder content=new StringBuilder();
  content.append("CreateEvent [INodeType=" + iNodeType + ", path="+ path+ ", ctime="+ ctime+ ", replication="+ replication+ ", ownerName="+ ownerName+ ", groupName="+ groupName+ ", perms="+ perms+ ", ");
  if (symlinkTarget != null) {
    content.append("symlinkTarget=" + symlinkTarget + ", ");
  }
  content.append("overwrite=" + overwrite + ", defaultBlockSize="+ defaultBlockSize+ "]");
  return content.toString();
}
