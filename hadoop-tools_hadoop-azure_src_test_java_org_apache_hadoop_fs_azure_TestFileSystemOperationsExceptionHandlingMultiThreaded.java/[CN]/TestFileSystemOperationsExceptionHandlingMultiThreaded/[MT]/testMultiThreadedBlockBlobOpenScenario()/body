{
  ExceptionHandlingTestHelper.createEmptyFile(ExceptionHandlingTestHelper.getPageBlobTestStorageAccount(),testPath);
  Thread t=new Thread(new DeleteThread(fs,testPath));
  t.start();
  while (t.isAlive()) {
    inputStream=fs.open(testPath);
    inputStream.close();
  }
  inputStream=fs.open(testPath);
  inputStream.close();
}
