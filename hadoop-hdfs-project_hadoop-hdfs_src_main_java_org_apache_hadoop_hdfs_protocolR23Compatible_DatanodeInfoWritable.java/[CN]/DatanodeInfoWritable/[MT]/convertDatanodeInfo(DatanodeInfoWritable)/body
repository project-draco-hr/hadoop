{
  if (di == null)   return null;
  return new DatanodeInfo(new org.apache.hadoop.hdfs.protocol.DatanodeID(di.getName(),di.getStorageID(),di.getInfoPort(),di.getIpcPort()),di.getNetworkLocation(),di.getHostName(),di.getCapacity(),di.getDfsUsed(),di.getRemaining(),di.getBlockPoolUsed(),di.getLastUpdate(),di.getXceiverCount(),DatanodeInfo.AdminStates.fromValue(di.getAdminState().value));
}
