{
  if (service != null && !isGenericUrl()) {
    LOG.error("Only provide -service with http/https URL.");
    return false;
  }
  if (service == null && isGenericUrl()) {
    LOG.error("Must provide -service with http/https URL.");
    return false;
  }
  if (url.indexOf("://") == -1) {
    LOG.error("URL does not contain a service specification: " + url);
    return false;
  }
  return true;
}
