{
  if (this == to) {
    return true;
  }
 else   if (to instanceof ContentSummary) {
    return getLength() == ((ContentSummary)to).getLength() && getFileCount() == ((ContentSummary)to).getFileCount() && getDirectoryCount() == ((ContentSummary)to).getDirectoryCount() && super.equals(to);
  }
 else {
    return super.equals(to);
  }
}
