{
  List<String> containerLocalDirs=new ArrayList<>(localDirs.size());
  String user=container.getUser();
  String appIdStr=app.getAppId().toString();
  String relativeContainerLocalDir=ContainerLocalizer.USERCACHE + Path.SEPARATOR + user+ Path.SEPARATOR+ ContainerLocalizer.APPCACHE+ Path.SEPARATOR+ appIdStr+ Path.SEPARATOR;
  for (  String localDir : localDirs) {
    containerLocalDirs.add(localDir + Path.SEPARATOR + relativeContainerLocalDir);
  }
  return containerLocalDirs;
}
