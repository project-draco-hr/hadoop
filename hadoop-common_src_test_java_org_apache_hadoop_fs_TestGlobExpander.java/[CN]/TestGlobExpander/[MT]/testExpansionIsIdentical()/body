{
  checkExpansionIsIdentical("");
  checkExpansionIsIdentical("/}");
  checkExpansionIsIdentical("/}{a,b}");
  checkExpansionIsIdentical("{/");
  checkExpansionIsIdentical("{a}");
  checkExpansionIsIdentical("{a,b}/{b,c}");
  checkExpansionIsIdentical("p\\{a/b,c/d\\}s");
  checkExpansionIsIdentical("p{a\\/b,c\\/d}s");
}
