{
  ProgressSplitsBlock result=splitsBlocks.get(statusAttemptID);
  if (result == null) {
    result=new ProgressSplitsBlock(conf.getInt(JTConfig.JT_JOBHISTORY_TASKPROGRESS_NUMBER_SPLITS,ProgressSplitsBlock.DEFAULT_NUMBER_PROGRESS_SPLITS));
    splitsBlocks.put(statusAttemptID,result);
  }
  return result;
}
