{
  if (isCleanupAttempt(id)) {
    return TaskType.TASK_CLEANUP;
  }
 else   if (isJobSetupTask()) {
    return TaskType.JOB_SETUP;
  }
 else   if (isJobCleanupTask()) {
    return TaskType.JOB_CLEANUP;
  }
 else   if (isMapTask()) {
    return TaskType.MAP;
  }
 else {
    return TaskType.REDUCE;
  }
}
