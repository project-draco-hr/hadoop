{
  new FileStatus();
  new FileStatus(1,false,3,3,4,new Path("/"));
  new FileStatus(1,false,5,3,4,5,null,null,null,new Path("/a/b"));
  new FileStatus(1,false,5,3,4,5,null,null,null,null,new Path("/a/b"));
  new FileStatus(1,false,5,3,4,5,new FsPermission((short)7),"bla","bla",new Path("/b/c"),new Path("/a/b"));
  boolean error=false;
  try {
    new FileStatus(1,false,3,3,4,null);
  }
 catch (  IllegalArgumentException exc) {
    error=true;
  }
  assertTrue("Accepted null path",error);
  try {
    new FileStatus(1,false,5,3,4,5,null,null,null,null);
  }
 catch (  IllegalArgumentException exc) {
    error=true;
  }
  assertTrue("Accepted null path",error);
  try {
    new FileStatus(1,false,5,3,4,5,null,null,null,null,null);
  }
 catch (  IllegalArgumentException exc) {
    error=true;
  }
  assertTrue("Accepted null path",error);
  try {
    new FileStatus(1,false,5,3,4,5,new FsPermission((short)7),"bla","bla",new Path("/b/c"),null);
  }
 catch (  IllegalArgumentException exc) {
    error=true;
  }
  assertTrue("Accepted null path",error);
}
