{
  if (!protocol.equals(RPC.getProtocolName(DatanodeProtocolPB.class))) {
    throw new IOException("Namenode Serverside implements " + RPC.getProtocolName(DatanodeProtocolPB.class) + ". The following requested protocol is unknown: "+ protocol);
  }
  return ProtocolSignature.getProtocolSignature(clientMethodsHash,RPC.getProtocolVersion(DatanodeProtocolPB.class),DatanodeProtocolPB.class);
}
