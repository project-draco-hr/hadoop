{
  FileStatus fi;
  if (!Shell.WINDOWS) {
    fi=getFileLinkStatusInternal(f,false);
  }
 else {
    fi=getFileLinkStatusInternal(f);
  }
  if (fi.isSymlink()) {
    Path targetQual=FSLinkResolver.qualifySymlinkTarget(this.getUri(),fi.getPath(),fi.getSymlink());
    fi.setSymlink(targetQual);
  }
  return fi;
}
