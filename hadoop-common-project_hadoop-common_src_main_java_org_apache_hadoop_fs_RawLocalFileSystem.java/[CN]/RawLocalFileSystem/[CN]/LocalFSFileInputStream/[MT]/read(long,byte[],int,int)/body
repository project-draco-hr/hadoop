{
  validatePositionedReadArgs(position,b,off,len);
  if (len == 0) {
    return 0;
  }
  ByteBuffer bb=ByteBuffer.wrap(b,off,len);
  try {
    int value=fis.getChannel().read(bb,position);
    if (value > 0) {
      statistics.incrementBytesRead(value);
    }
    return value;
  }
 catch (  IOException e) {
    throw new FSError(e);
  }
}
