{
  Scan scan=new Scan();
  if (createdTimeBegin == DEFAULT_BEGIN_TIME && createdTimeEnd == DEFAULT_END_TIME) {
    scan.setRowPrefixFilter(FlowActivityRowKey.getRowKeyPrefix(clusterId));
  }
 else {
    scan.setStartRow(FlowActivityRowKey.getRowKeyPrefix(clusterId,createdTimeEnd));
    scan.setStopRow(FlowActivityRowKey.getRowKeyPrefix(clusterId,(createdTimeBegin <= 0 ? 0 : (createdTimeBegin - 1))));
  }
  scan.setFilter(new PageFilter(limit));
  return table.getResultScanner(hbaseConf,conn,scan);
}
