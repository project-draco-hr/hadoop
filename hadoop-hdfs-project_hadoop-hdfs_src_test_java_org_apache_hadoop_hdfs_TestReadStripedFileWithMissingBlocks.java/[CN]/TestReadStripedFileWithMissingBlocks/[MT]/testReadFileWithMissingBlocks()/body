{
  for (int missingData=1; missingData <= NUM_PARITY_BLOCKS; missingData++) {
    for (int missingParity=0; missingParity <= NUM_PARITY_BLOCKS - missingData; missingParity++) {
      try {
        setup();
        readFileWithMissingBlocks(new Path("/foo"),fileLength,missingData,missingParity);
      }
  finally {
        tearDown();
      }
    }
  }
}
