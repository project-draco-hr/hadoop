{
  xdr.writeInt(type);
  xdr.writeInt(mode);
  xdr.writeInt(nlink);
  xdr.writeInt(uid);
  xdr.writeInt(gid);
  xdr.writeLongAsHyper(size);
  xdr.writeLongAsHyper(used);
  xdr.writeInt(rdev.getSpecdata1());
  xdr.writeInt(rdev.getSpecdata2());
  xdr.writeLongAsHyper(fsid);
  xdr.writeLongAsHyper(fileid);
  atime.serialize(xdr);
  mtime.serialize(xdr);
  ctime.serialize(xdr);
}
