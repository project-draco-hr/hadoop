{
  if (exists(dst)) {
    if (isDirectory(dst)) {
      if (null == srcName) {
        throw new IOException("Target " + dst + " is a directory");
      }
      checkDest(null,new Path(dst,srcName),overwrite);
    }
 else     if (!overwrite) {
      throw new IOException("Target " + dst + " already exists");
    }
  }
}
