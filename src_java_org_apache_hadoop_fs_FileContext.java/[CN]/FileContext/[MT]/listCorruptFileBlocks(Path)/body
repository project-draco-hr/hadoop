{
  final Path absF=fixRelativePart(path);
  return new FSLinkResolver<RemoteIterator<Path>>(){
    @Override public RemoteIterator<Path> next(    final AbstractFileSystem fs,    final Path p) throws IOException, UnresolvedLinkException {
      return fs.listCorruptFileBlocks(p);
    }
  }
.resolve(this,absF);
}
