{
  this.conf=conf;
  try {
    this.hostsReader=new HostsFileReader(conf.get(RMConfig.RM_NODES_INCLUDE_FILE,RMConfig.DEFAULT_RM_NODES_INCLUDE_FILE),conf.get(RMConfig.RM_NODES_EXCLUDE_FILE,RMConfig.DEFAULT_RM_NODES_EXCLUDE_FILE));
    printConfiguredHosts();
  }
 catch (  IOException ioe) {
    LOG.warn("Failed to init hostsReader, disabling",ioe);
    try {
      this.hostsReader=new HostsFileReader(RMConfig.DEFAULT_RM_NODES_INCLUDE_FILE,RMConfig.DEFAULT_RM_NODES_EXCLUDE_FILE);
    }
 catch (    IOException ioe2) {
      this.hostsReader=null;
      throw new YarnException(ioe2);
    }
  }
  super.init(conf);
}
