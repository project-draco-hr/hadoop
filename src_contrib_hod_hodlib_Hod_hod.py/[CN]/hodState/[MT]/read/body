def read(self, id=None):
    info = {}
    self.__set_state_file(id)
    if self.__stateFile:
        if os.path.isfile(self.__stateFile):
            stateFile = open(self.__stateFile, 'r')
            try:
                info = cPickle.load(stateFile)
            except EOFError:
                pass
            stateFile.close()
    return info
