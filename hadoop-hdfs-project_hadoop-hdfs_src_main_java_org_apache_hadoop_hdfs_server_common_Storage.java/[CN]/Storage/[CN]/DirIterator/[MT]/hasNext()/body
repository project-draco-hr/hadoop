{
  if (storageDirs.isEmpty() || nextIndex >= storageDirs.size())   return false;
  if (dirType != null || !includeShared) {
    while (nextIndex < storageDirs.size()) {
      if (shouldReturnNextDir())       break;
      nextIndex++;
    }
    if (nextIndex >= storageDirs.size())     return false;
  }
  return true;
}
