{
  Configuration conf=context.getConfiguration();
  this.fieldSeparator=conf.get(FieldSelectionHelper.DATA_FIELD_SEPERATOR,"\t");
  this.reduceOutputKeyValueSpec=conf.get(FieldSelectionHelper.REDUCE_OUTPUT_KEY_VALUE_SPEC,"0-:");
  allReduceValueFieldsFrom=FieldSelectionHelper.parseOutputKeyValueSpec(reduceOutputKeyValueSpec,reduceOutputKeyFieldList,reduceOutputValueFieldList);
  LOG.info(FieldSelectionHelper.specToString(fieldSeparator,reduceOutputKeyValueSpec,allReduceValueFieldsFrom,reduceOutputKeyFieldList,reduceOutputValueFieldList));
}
