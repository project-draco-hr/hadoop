{
  Set<String> nodeLabels=null;
  if (accessLocal) {
    nodeLabels=getNodeLabelManagerInstance(getConf()).getClusterNodeLabels();
  }
 else {
    nodeLabels=client.getClusterNodeLabels();
  }
  sysout.println(String.format("Node Labels: %s",StringUtils.join(sortStrSet(nodeLabels).iterator(),",")));
}
