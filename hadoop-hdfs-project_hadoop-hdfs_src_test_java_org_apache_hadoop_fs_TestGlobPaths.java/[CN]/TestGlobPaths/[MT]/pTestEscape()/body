{
  org.junit.Assume.assumeTrue(!Path.WINDOWS);
  try {
    String[] files=new String[]{USER_DIR + "/ab\\[c.d"};
    Path[] matchedPath=prepareTesting(USER_DIR + "/ab\\[c.d",files);
    assertEquals(matchedPath.length,1);
    assertEquals(matchedPath[0],path[0]);
  }
  finally {
    cleanupDFS();
  }
}
