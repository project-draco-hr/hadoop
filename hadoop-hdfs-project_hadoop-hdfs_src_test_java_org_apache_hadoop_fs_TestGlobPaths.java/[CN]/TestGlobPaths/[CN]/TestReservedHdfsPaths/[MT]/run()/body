{
  String reservedRoot="/.reserved/.inodes/" + INodeId.ROOT_INODE_ID;
  Assert.assertEquals(reservedRoot,TestPath.mergeStatuses(wrap.globStatus(new Path(reservedRoot),new AcceptAllPathFilter())));
  Assert.assertEquals("",TestPath.mergeStatuses(wrap.globStatus(new Path("/.reserved/*"),new AcceptAllPathFilter())));
}
