{
  boolean fromAntJunit=System.getProperty("test.build.data") != null;
  if (fromAntJunit) {
    new File(antTestDir_).mkdirs();
    File outFile=new File(antTestDir_,testName_ + ".log");
    PrintStream out=new PrintStream(new FileOutputStream(outFile));
    System.setOut(out);
    System.setErr(out);
  }
}
