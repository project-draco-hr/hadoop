{
  if (args.length == 0) {
    printUsage();
    return -1;
  }
  try {
    return UserGroupInformation.getCurrentUser().doAs(new PrivilegedExceptionAction<Integer>(){
      @Override public Integer run() throws Exception {
        return doWork(args);
      }
    }
);
  }
 catch (  InterruptedException e) {
    throw new IOException(e);
  }
}
