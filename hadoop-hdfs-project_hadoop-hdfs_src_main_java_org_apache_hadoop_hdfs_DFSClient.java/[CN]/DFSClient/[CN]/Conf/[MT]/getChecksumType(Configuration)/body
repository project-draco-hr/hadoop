{
  final String checksum=conf.get(DFSConfigKeys.DFS_CHECKSUM_TYPE_KEY,DFSConfigKeys.DFS_CHECKSUM_TYPE_DEFAULT);
  try {
    return DataChecksum.Type.valueOf(checksum);
  }
 catch (  IllegalArgumentException iae) {
    LOG.warn("Bad checksum type: " + checksum + ". Using default "+ DFSConfigKeys.DFS_CHECKSUM_TYPE_DEFAULT);
    return DataChecksum.Type.valueOf(DFSConfigKeys.DFS_CHECKSUM_TYPE_DEFAULT);
  }
}
