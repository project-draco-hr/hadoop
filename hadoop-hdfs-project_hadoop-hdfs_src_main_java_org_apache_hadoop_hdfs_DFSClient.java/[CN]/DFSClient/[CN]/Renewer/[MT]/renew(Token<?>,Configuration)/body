{
  Token<DelegationTokenIdentifier> delToken=(Token<DelegationTokenIdentifier>)token;
  LOG.info("Renewing " + DelegationTokenIdentifier.stringifyToken(delToken));
  ClientProtocol nn=DFSUtil.createNamenode(SecurityUtil.getTokenServiceAddr(delToken),conf,UserGroupInformation.getCurrentUser());
  try {
    return nn.renewDelegationToken(delToken);
  }
 catch (  RemoteException re) {
    throw re.unwrapRemoteException(InvalidToken.class,AccessControlException.class);
  }
}
