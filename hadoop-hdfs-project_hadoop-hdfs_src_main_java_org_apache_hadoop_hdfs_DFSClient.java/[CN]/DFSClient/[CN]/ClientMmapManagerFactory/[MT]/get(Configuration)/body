{
  if (refcnt++ == 0) {
    mmapManager=ClientMmapManager.fromConf(conf);
  }
 else {
    String mismatches=mmapManager.verifyConfigurationMatches(conf);
    if (!mismatches.isEmpty()) {
      LOG.warn("The ClientMmapManager settings you specified " + "have been ignored because another thread created the " + "ClientMmapManager first.  "+ mismatches);
    }
  }
  return mmapManager;
}
