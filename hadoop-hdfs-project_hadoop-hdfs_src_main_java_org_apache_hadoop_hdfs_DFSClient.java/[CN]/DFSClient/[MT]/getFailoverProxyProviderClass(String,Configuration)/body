{
  String configKey=DFS_CLIENT_FAILOVER_PROXY_PROVIDER_KEY_PREFIX + "." + authority;
  try {
    return conf.getClass(configKey,null);
  }
 catch (  RuntimeException e) {
    if (e.getCause() instanceof ClassNotFoundException) {
      throw new IOException("Could not load failover proxy provider class " + conf.get(configKey) + " which is configured for authority "+ authority,e);
    }
 else {
      throw e;
    }
  }
}
