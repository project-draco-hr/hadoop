{
  try {
    FsPermission dirPerm=FsPermission.getDefault().applyUMask(dfsClientConf.uMask);
    namenode.createSymlink(target,link,dirPerm,createParent);
  }
 catch (  RemoteException re) {
    throw re.unwrapRemoteException(AccessControlException.class,FileAlreadyExistsException.class,FileNotFoundException.class,ParentNotDirectoryException.class,NSQuotaExceededException.class,DSQuotaExceededException.class,UnresolvedPathException.class);
  }
}
