{
  checkOpen();
  TraceScope scope=getSrcDstTraceScope("rename",src,dst);
  try {
    return namenode.rename(src,dst);
  }
 catch (  RemoteException re) {
    throw re.unwrapRemoteException(AccessControlException.class,NSQuotaExceededException.class,DSQuotaExceededException.class,UnresolvedPathException.class,SnapshotAccessControlException.class);
  }
 finally {
    scope.close();
  }
}
