{
  try {
    return namenode.setReplication(src,replication);
  }
 catch (  RemoteException re) {
    throw re.unwrapRemoteException(AccessControlException.class,FileNotFoundException.class,SafeModeException.class,DSQuotaExceededException.class,UnresolvedPathException.class,SnapshotAccessControlException.class);
  }
}
