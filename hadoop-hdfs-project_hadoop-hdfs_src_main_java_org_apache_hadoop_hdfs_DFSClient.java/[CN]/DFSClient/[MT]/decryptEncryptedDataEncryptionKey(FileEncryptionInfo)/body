{
  if (provider == null) {
    throw new IOException("No KeyProvider is configured, cannot access" + " an encrypted file");
  }
  EncryptedKeyVersion ekv=EncryptedKeyVersion.createForDecryption(null,feInfo.getEzKeyVersionName(),feInfo.getIV(),feInfo.getEncryptedDataEncryptionKey());
  try {
    return provider.decryptEncryptedKey(ekv);
  }
 catch (  GeneralSecurityException e) {
    throw new IOException(e);
  }
}
