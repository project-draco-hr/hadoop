{
  try {
    container.nmClientAsync.callbackHandler.onStartContainerError(event.getContainerId(),RPCUtil.getRemoteException(STOP_BEFORE_START_ERROR_MSG));
  }
 catch (  Throwable thr) {
    LOG.info("Unchecked exception is thrown from onStartContainerError for " + "Container " + event.getContainerId(),thr);
  }
}
