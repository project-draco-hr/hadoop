{
  assumeTrue(!emulatingSymlinksOnWindows());
  Path file=new Path(testBaseDir1(),"file");
  Path link=new Path(testBaseDir1(),"linkToFile");
  createAndWriteFile(file);
  wrapper.createSymlink(file,link,false);
  try {
    wrapper.createSymlink(file,link,false);
    fail("link already exists");
  }
 catch (  IOException x) {
  }
}
