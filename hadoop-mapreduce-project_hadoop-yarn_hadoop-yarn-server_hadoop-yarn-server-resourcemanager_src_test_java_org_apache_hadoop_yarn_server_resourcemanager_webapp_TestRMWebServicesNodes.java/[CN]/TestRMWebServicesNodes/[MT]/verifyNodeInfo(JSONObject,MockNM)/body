{
  assertEquals("incorrect number of elements",11,nodeInfo.length());
  verifyNodeInfoGeneric(nm,nodeInfo.getString("state"),nodeInfo.getString("rack"),nodeInfo.getString("healthStatus"),nodeInfo.getString("id"),nodeInfo.getString("nodeHostName"),nodeInfo.getString("nodeHTTPAddress"),nodeInfo.getLong("lastHealthUpdate"),nodeInfo.getString("healthReport"),nodeInfo.getInt("numContainers"),nodeInfo.getLong("usedMemoryMB"),nodeInfo.getLong("availMemoryMB"));
}
