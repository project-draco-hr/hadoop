{
  if (JIFFY_LENGTH_IN_MILLIS < 0) {
    return UNAVAILABLE;
  }
  long incJiffies=0;
  boolean isAvailable=false;
  for (  ProcessInfo p : processTree.values()) {
    if (p != null) {
      incJiffies+=p.getDtime();
      isAvailable=true;
    }
  }
  if (isAvailable) {
    if (cpuTime == UNAVAILABLE) {
      cpuTime=0L;
    }
    cpuTime+=incJiffies * JIFFY_LENGTH_IN_MILLIS;
  }
  return cpuTime;
}
