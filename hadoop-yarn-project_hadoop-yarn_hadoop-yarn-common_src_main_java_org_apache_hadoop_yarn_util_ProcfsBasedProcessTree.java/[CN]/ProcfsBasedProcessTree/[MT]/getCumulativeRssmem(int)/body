{
  if (PAGE_SIZE < 0) {
    return 0;
  }
  if (smapsEnabled) {
    return getSmapBasedCumulativeRssmem(olderThanAge);
  }
  long totalPages=0;
  for (  ProcessInfo p : processTree.values()) {
    if ((p != null) && (p.getAge() > olderThanAge)) {
      totalPages+=p.getRssmemPage();
    }
  }
  return totalPages * PAGE_SIZE;
}
