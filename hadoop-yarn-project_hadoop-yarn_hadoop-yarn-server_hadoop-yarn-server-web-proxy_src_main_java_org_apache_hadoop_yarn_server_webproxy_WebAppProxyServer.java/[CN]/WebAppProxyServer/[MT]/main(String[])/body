{
  Thread.setDefaultUncaughtExceptionHandler(new YarnUncaughtExceptionHandler());
  StringUtils.startupShutdownMessage(WebAppProxyServer.class,args,LOG);
  try {
    WebAppProxyServer proxy=new WebAppProxyServer();
    ShutdownHookManager.get().addShutdownHook(new CompositeServiceShutdownHook(proxy),SHUTDOWN_HOOK_PRIORITY);
    YarnConfiguration conf=new YarnConfiguration();
    proxy.init(conf);
    proxy.start();
  }
 catch (  Throwable t) {
    LOG.fatal("Error starting Proxy server",t);
    System.exit(-1);
  }
}
