{
  maybeInitBuilder();
  builder.clearIncreaseRequest();
  if (increaseRequests == null) {
    return;
  }
  Iterable<ContainerResourceIncreaseRequestProto> iterable=new Iterable<ContainerResourceIncreaseRequestProto>(){
    @Override public Iterator<ContainerResourceIncreaseRequestProto> iterator(){
      return new Iterator<ContainerResourceIncreaseRequestProto>(){
        Iterator<ContainerResourceIncreaseRequest> iter=increaseRequests.iterator();
        @Override public boolean hasNext(){
          return iter.hasNext();
        }
        @Override public ContainerResourceIncreaseRequestProto next(){
          return convertToProtoFormat(iter.next());
        }
        @Override public void remove(){
          throw new UnsupportedOperationException();
        }
      }
;
    }
  }
;
  builder.addAllIncreaseRequest(iterable);
}
