{
  if (this.ask != null) {
    addAsksToProto();
  }
  if (this.release != null) {
    addReleasesToProto();
  }
  if (this.increaseRequests != null) {
    addIncreaseRequestsToProto();
  }
  if (this.blacklistRequest != null) {
    builder.setBlacklistRequest(convertToProtoFormat(this.blacklistRequest));
  }
}
