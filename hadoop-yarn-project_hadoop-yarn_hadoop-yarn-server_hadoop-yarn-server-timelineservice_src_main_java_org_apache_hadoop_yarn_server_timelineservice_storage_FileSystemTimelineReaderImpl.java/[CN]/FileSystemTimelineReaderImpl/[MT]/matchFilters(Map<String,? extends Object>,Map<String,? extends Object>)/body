{
  if (entityInfo == null || entityInfo.isEmpty()) {
    return false;
  }
  for (  Map.Entry<String,? extends Object> filter : filters.entrySet()) {
    Object infoValue=entityInfo.get(filter.getKey());
    if (infoValue == null) {
      return false;
    }
    if (!matchFilter(infoValue,filter.getValue())) {
      return false;
    }
  }
  return true;
}
