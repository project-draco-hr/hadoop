{
  for (  Map.Entry<String,Set<String>> relation : relations.entrySet()) {
    Set<String> ids=entityRelations.get(relation.getKey());
    if (ids == null) {
      return false;
    }
    for (    String id : relation.getValue()) {
      if (!ids.contains(id)) {
        return false;
      }
    }
  }
  return true;
}
