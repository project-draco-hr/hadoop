{
  String flowRunPath=getFlowRunPath(context.getUserId(),context.getClusterId(),context.getFlowName(),context.getFlowRunId(),context.getAppId());
  File dir=new File(new File(rootPath,ENTITIES_DIR),context.getClusterId() + File.separator + flowRunPath+ File.separator+ context.getAppId()+ File.separator+ context.getEntityType());
  return getEntities(dir,context.getEntityType(),filters,dataToRetrieve);
}
