{
  final String lcRollingPeriod=conf.get(YarnConfiguration.TIMELINE_SERVICE_ROLLING_PERIOD,YarnConfiguration.DEFAULT_TIMELINE_SERVICE_ROLLING_PERIOD);
  this.rollingPeriod=RollingPeriod.valueOf(lcRollingPeriod.toUpperCase(Locale.ENGLISH));
  fdf=FastDateFormat.getInstance(rollingPeriod.dateFormat(),TimeZone.getTimeZone("GMT"));
  sdf=new SimpleDateFormat(rollingPeriod.dateFormat());
  sdf.setTimeZone(fdf.getTimeZone());
}
