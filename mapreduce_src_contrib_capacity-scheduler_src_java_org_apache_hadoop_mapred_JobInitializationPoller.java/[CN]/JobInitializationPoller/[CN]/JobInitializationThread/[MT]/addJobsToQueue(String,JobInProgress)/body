{
  TreeMap<JobSchedulingInfo,JobInProgress> jobs=jobsPerQueue.get(queue);
  if (jobs == null) {
    LOG.error("Invalid queue passed to the thread : " + queue + " For job :: "+ job.getJobID());
    return;
  }
synchronized (jobs) {
    JobSchedulingInfo schedInfo=new JobSchedulingInfo(job);
    jobs.put(schedInfo,job);
    currentJobCount.getAndIncrement();
  }
}
