{
  out.print("<tr>");
  for (int i=0; i < columns.length; i++) {
    if (row / 2 * 2 == row) {
      out.print("<td style=\"vertical-align: top;background-color:LightGrey;\"><B>" + columns[i] + "</B><br></td>");
    }
 else {
      out.print("<td style=\"vertical-align: top;background-color:LightBlue;\"><B>" + columns[i] + "</B><br></td>");
    }
  }
  out.print("</tr>");
}
