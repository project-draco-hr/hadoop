{
  if (xAttrs == null) {
    return null;
  }
  Map<String,byte[]> xAttrMap=Maps.newHashMap();
  for (  XAttr xAttr : xAttrs) {
    String name=getPrefixName(xAttr);
    byte[] value=xAttr.getValue();
    if (value == null) {
      value=new byte[0];
    }
    xAttrMap.put(name,value);
  }
  return xAttrMap;
}
