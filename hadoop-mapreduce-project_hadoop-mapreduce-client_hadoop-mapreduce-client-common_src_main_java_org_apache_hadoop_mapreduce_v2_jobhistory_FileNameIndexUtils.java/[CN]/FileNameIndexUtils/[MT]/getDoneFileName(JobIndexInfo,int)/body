{
  StringBuilder sb=new StringBuilder();
  sb.append(encodeJobHistoryFileName(escapeDelimiters(TypeConverter.fromYarn(indexInfo.getJobId()).toString())));
  sb.append(DELIMITER);
  sb.append(encodeJobHistoryFileName(String.valueOf(indexInfo.getSubmitTime())));
  sb.append(DELIMITER);
  sb.append(encodeJobHistoryFileName(escapeDelimiters(getUserName(indexInfo))));
  sb.append(DELIMITER);
  sb.append(trimURLEncodedString(encodeJobHistoryFileName(escapeDelimiters(getJobName(indexInfo))),jobNameLimit));
  sb.append(DELIMITER);
  sb.append(encodeJobHistoryFileName(String.valueOf(indexInfo.getFinishTime())));
  sb.append(DELIMITER);
  sb.append(encodeJobHistoryFileName(String.valueOf(indexInfo.getNumMaps())));
  sb.append(DELIMITER);
  sb.append(encodeJobHistoryFileName(String.valueOf(indexInfo.getNumReduces())));
  sb.append(DELIMITER);
  sb.append(encodeJobHistoryFileName(indexInfo.getJobStatus()));
  sb.append(DELIMITER);
  sb.append(escapeDelimiters(encodeJobHistoryFileName(getQueueName(indexInfo))));
  sb.append(DELIMITER);
  sb.append(encodeJobHistoryFileName(String.valueOf(indexInfo.getJobStartTime())));
  sb.append(encodeJobHistoryFileName(JobHistoryUtils.JOB_HISTORY_FILE_EXTENSION));
  return sb.toString();
}
