{
  long val=defaultVal;
  if (file.exists()) {
    BufferedReader br=new BufferedReader(new InputStreamReader(new FileInputStream(file),Charsets.UTF_8));
    try {
      val=Long.parseLong(br.readLine());
      br.close();
      br=null;
    }
  finally {
      IOUtils.cleanup(LOG,br);
    }
  }
  return val;
}
