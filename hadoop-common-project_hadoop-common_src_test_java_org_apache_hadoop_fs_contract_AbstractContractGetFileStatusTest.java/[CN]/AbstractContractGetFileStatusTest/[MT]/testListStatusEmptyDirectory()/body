{
  FileSystem fs=getFileSystem();
  assertTrue(fs.delete(getContract().getTestPath(),true));
  Path subfolder=getContract().getTestPath().suffix("/" + testPath.getName());
  assertTrue(fs.mkdirs(subfolder));
  assertEquals("ls on an empty directory not of length 0",0,fs.listStatus(subfolder).length);
  assertTrue("ls on a non-empty directory of length 0",fs.listStatus(getContract().getTestPath()).length > 0);
}
