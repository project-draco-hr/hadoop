{
synchronized (collectors) {
    TimelineCollector collector=collectors.remove(id);
    if (collector == null) {
      String msg="the collector for " + id + " does not exist!";
      LOG.error(msg);
      return false;
    }
 else {
      collector.stop();
      LOG.info("the collector service for " + id + " was removed");
      return true;
    }
  }
}
