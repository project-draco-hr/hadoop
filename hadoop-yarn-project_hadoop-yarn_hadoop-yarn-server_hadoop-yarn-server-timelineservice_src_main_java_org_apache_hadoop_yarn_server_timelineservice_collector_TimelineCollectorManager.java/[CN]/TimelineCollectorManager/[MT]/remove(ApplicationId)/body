{
  TimelineCollector collector=collectors.remove(appId);
  if (collector == null) {
    LOG.error("the collector for " + appId + " does not exist!");
  }
 else {
    postRemove(appId,collector);
    collector.stop();
    LOG.info("the collector service for " + appId + " was removed");
  }
  return collector != null;
}
