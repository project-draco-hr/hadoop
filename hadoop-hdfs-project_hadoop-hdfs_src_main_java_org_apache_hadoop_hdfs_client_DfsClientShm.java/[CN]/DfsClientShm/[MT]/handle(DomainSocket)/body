{
  manager.unregisterShm(getShmId());
synchronized (this) {
    Preconditions.checkState(!stale);
    stale=true;
    boolean hadSlots=false;
    for (Iterator<Slot> iter=slotIterator(); iter.hasNext(); ) {
      Slot slot=iter.next();
      slot.makeInvalid();
      hadSlots=true;
    }
    if (!hadSlots) {
      free();
    }
  }
  return true;
}
