{
  if (tries == 0) {
    throw new IOException(e);
  }
  LOG.warn("Some exceptions occurred in oss connection, try to reopen oss" + " connection at position '" + position + "', "+ e.getMessage());
  try {
    Thread.sleep(100);
  }
 catch (  InterruptedException e2) {
    LOG.warn(e2.getMessage());
  }
  reopen(position);
}
