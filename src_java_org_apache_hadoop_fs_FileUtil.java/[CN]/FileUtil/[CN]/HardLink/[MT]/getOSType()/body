{
  String osName=System.getProperty("os.name");
  if (osName.indexOf("Windows") >= 0 && (osName.indexOf("XP") >= 0 || osName.indexOf("2003") >= 0 || osName.indexOf("Vista") >= 0))   return OSType.OS_TYPE_WINXP;
 else   if (osName.indexOf("SunOS") >= 0)   return OSType.OS_TYPE_SOLARIS;
 else   if (osName.indexOf("Mac") >= 0)   return OSType.OS_TYPE_MAC;
 else   return OSType.OS_TYPE_UNIX;
}
