{
  int l=0, r=inodes.length;
  while (l < r) {
    int mid=l + (r - l) / 2;
    FsImageProto.INodeSection.INode n=FsImageProto.INodeSection.INode.parseFrom(inodes[mid]);
    long nid=n.getId();
    if (id > nid) {
      l=mid + 1;
    }
 else     if (id < nid) {
      r=mid;
    }
 else {
      return n;
    }
  }
  return null;
}
