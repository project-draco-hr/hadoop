{
  this(conf,getLocalFileContext(conf),Executors.newFixedThreadPool(conf.getInt(YarnConfiguration.NM_LOCALIZER_FETCH_THREAD_COUNT,YarnConfiguration.DEFAULT_NM_LOCALIZER_FETCH_THREAD_COUNT)),new HashMap<Future<Path>,LocalizerResourceRequestEvent>(),new HashMap<LocalResourceRequest,List<LocalizerResourceRequestEvent>>());
}
