{
  JobStatus jobStatus=createTestJobStatus("job_20130313155005308_0001",JobStatus.SUCCEEDED);
  JobConf jobConf=createTestJobConf(new Configuration(),0,baseUrl + "jobend?jobid=$jobId&status=$jobStatus");
  JobEndNotifier.localRunnerNotification(jobConf,jobStatus);
  assertEquals(1,JobEndServlet.calledTimes);
  assertEquals("jobid=job_20130313155005308_0001&status=SUCCEEDED",JobEndServlet.requestUri.getQuery());
}
