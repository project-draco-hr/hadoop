{
  assertEquals(RMAppAttemptState.SCHEDULED,applicationAttempt.getAppAttemptState());
  assertEquals(0,applicationAttempt.getJustFinishedContainers().size());
  assertNull(applicationAttempt.getMasterContainer());
  assertEquals(0.0,(double)applicationAttempt.getProgress(),0.0001);
  assertEquals(0,applicationAttempt.getRanNodes().size());
  assertNull(applicationAttempt.getFinalApplicationStatus());
  verify(application).handle(any(RMAppEvent.class));
  verify(scheduler).allocate(any(ApplicationAttemptId.class),any(List.class),any(List.class));
}
