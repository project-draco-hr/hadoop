{
  TaskSchedulingContext tsi=getTSC(qsc);
  if (tsi.getMaxCapacity() >= 0) {
    if ((tsi.getNumSlotsOccupied() + noOfSlotsPerTask) > tsi.getMaxCapacity()) {
      if (LOG.isDebugEnabled()) {
        LOG.debug("Queue " + qsc.getQueueName() + " "+ "has reached its  max "+ type+ "Capacity");
        LOG.debug("Current running tasks " + tsi.getCapacity());
      }
      return true;
    }
  }
  return false;
}
