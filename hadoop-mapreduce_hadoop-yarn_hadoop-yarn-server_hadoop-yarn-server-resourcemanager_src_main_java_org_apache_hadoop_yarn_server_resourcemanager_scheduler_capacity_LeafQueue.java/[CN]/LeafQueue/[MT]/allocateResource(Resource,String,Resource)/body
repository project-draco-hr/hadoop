{
  Resources.addTo(usedResources,resource);
  updateResource(clusterResource);
  ++numContainers;
  User user=getUser(userName);
  user.assignContainer(resource);
  LOG.info(getQueueName() + " used=" + usedResources+ " numContainers="+ numContainers+ " user="+ userName+ " resources="+ user.getConsumedResources());
}
