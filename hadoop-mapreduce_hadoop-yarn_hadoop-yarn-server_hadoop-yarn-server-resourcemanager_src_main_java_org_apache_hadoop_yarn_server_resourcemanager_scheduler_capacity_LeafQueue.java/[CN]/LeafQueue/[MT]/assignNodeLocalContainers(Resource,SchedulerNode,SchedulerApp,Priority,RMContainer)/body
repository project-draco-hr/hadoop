{
  ResourceRequest request=application.getResourceRequest(priority,node.getHostName());
  if (request != null) {
    if (canAssign(application,priority,node,NodeType.NODE_LOCAL,reservedContainer)) {
      return assignContainer(clusterResource,node,application,priority,request,NodeType.NODE_LOCAL,reservedContainer);
    }
  }
  return Resources.none();
}
