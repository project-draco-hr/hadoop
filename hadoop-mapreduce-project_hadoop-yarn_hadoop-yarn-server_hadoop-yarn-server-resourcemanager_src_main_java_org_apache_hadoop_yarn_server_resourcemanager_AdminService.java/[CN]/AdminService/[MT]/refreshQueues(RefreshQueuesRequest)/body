{
  UserGroupInformation user=checkAcls("refreshQueues");
  try {
    scheduler.reinitialize(conf,null,null);
    RMAuditLogger.logSuccess(user.getShortUserName(),"refreshQueues","AdminService");
    return recordFactory.newRecordInstance(RefreshQueuesResponse.class);
  }
 catch (  IOException ioe) {
    LOG.info("Exception refreshing queues ",ioe);
    RMAuditLogger.logFailure(user.getShortUserName(),"refreshQueues",adminAcl.toString(),"AdminService","Exception refreshing queues");
    throw RPCUtil.getRemoteException(ioe);
  }
}
