{
  UserGroupInformation user=checkAcls("refreshAdminAcls");
  Configuration conf=new Configuration();
  adminAcl=new AccessControlList(conf.get(YarnConfiguration.RM_ADMIN_ACL,YarnConfiguration.DEFAULT_RM_ADMIN_ACL));
  RMAuditLogger.logSuccess(user.getShortUserName(),"refreshAdminAcls","AdminService");
  return recordFactory.newRecordInstance(RefreshAdminAclsResponse.class);
}
