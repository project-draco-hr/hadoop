def test_print_status(self):
    cluster = RackspaceCluster('cluster1', None, TestCluster.DriverStub())
    out = StringIO.StringIO()
    cluster.print_status(None, 'running', out)
    self.assertEquals('nn,jt cluster1-nj-000 cluster1-nj-000 100.0.0.1 10.0.0.1 running\ndn,tt cluster1-dt-000 cluster1-dt-000 100.0.0.1 10.0.0.1 running\ndn,tt cluster1-dt-001 cluster1-dt-001 100.0.0.1 10.0.0.1 running\n', out.getvalue().replace('\t', ' '))
    out = StringIO.StringIO()
    cluster.print_status(['dn'], 'running', out)
    self.assertEquals('dn,tt cluster1-dt-000 cluster1-dt-000 100.0.0.1 10.0.0.1 running\ndn,tt cluster1-dt-001 cluster1-dt-001 100.0.0.1 10.0.0.1 running\n', out.getvalue().replace('\t', ' '))
