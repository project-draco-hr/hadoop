{
  String startupInfo=Environment.runCommand("grep --max-count=1 STARTUP_MSG:\\s*host " + file.getName()).toString();
  Pattern pattern=Pattern.compile("\\s+(\\w+/.+)\\s+");
  Matcher matcher=pattern.matcher(startupInfo);
  if (matcher.find(0)) {
    hostname=matcher.group(1).split("/")[0];
    ips=new String[1];
    ips[0]=matcher.group(1).split("/")[1];
  }
}
