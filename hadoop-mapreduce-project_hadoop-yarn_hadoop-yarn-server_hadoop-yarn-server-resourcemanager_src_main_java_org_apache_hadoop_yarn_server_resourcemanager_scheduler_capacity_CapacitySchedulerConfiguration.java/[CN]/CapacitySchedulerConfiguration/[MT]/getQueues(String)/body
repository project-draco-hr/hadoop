{
  LOG.info("CSConf - getQueues called for: queuePrefix=" + getQueuePrefix(queue));
  String[] queues=getStrings(getQueuePrefix(queue) + QUEUES);
  LOG.info("CSConf - getQueues: queuePrefix=" + getQueuePrefix(queue) + ", queues="+ ((queues == null) ? "" : StringUtils.arrayToString(queues)));
  return queues;
}
