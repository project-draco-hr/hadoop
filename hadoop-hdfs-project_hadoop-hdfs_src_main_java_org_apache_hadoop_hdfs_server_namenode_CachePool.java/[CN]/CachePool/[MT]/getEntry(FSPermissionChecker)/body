{
  boolean hasPermission=true;
  if (pc != null) {
    try {
      pc.checkPermission(this,FsAction.READ);
    }
 catch (    AccessControlException e) {
      hasPermission=false;
    }
  }
  return new CachePoolEntry(getInfo(hasPermission),hasPermission ? getStats() : new CachePoolStats.Builder().build());
}
