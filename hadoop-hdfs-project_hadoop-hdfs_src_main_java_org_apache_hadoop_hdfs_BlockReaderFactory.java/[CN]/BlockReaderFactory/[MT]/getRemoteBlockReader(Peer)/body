{
  if (conf.useLegacyBlockReader) {
    return RemoteBlockReader.newBlockReader(fileName,block,token,startOffset,length,conf.ioBufferSize,verifyChecksum,clientName,peer,datanode,clientContext.getPeerCache(),cachingStrategy);
  }
 else {
    return RemoteBlockReader2.newBlockReader(fileName,block,token,startOffset,length,verifyChecksum,clientName,peer,datanode,clientContext.getPeerCache(),cachingStrategy);
  }
}
