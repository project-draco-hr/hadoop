{
  Map<AsyncLogger,ListenableFuture<GetEditLogManifestResponseProto>> calls=Maps.newHashMap();
  for (  AsyncLogger logger : loggers) {
    ListenableFuture<GetEditLogManifestResponseProto> future=logger.getEditLogManifest(fromTxnId);
    calls.put(logger,future);
  }
  return QuorumCall.create(calls);
}
