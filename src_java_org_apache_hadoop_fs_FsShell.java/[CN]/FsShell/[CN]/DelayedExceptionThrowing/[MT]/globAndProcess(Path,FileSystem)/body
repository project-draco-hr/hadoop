{
  List<IOException> exceptions=new ArrayList<IOException>();
  for (  Path p : FileUtil.stat2Paths(srcFs.globStatus(srcPattern),srcPattern))   try {
    process(p,srcFs);
  }
 catch (  IOException ioe) {
    exceptions.add(ioe);
  }
  if (!exceptions.isEmpty())   if (exceptions.size() == 1)   throw exceptions.get(0);
 else   throw new IOException("Multiple IOExceptions: " + exceptions);
}
