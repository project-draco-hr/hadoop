{
  final String serviceAddr=mrCluster.getConfig().get(JHAdminConfig.MR_HISTORY_ADDRESS);
  final YarnRPC rpc=YarnRPC.create(conf);
  MRClientProtocol historyClient=(MRClientProtocol)rpc.getProxy(MRClientProtocol.class,NetUtils.createSocketAddr(serviceAddr),mrCluster.getConfig());
  return historyClient;
}
