{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  VolumePair that=(VolumePair)o;
  return sourceVolUuid.equals(that.sourceVolUuid) && sourceVolBasePath.equals(that.sourceVolBasePath) && destVolUuid.equals(that.destVolUuid)&& destVolBasePath.equals(that.destVolBasePath);
}
