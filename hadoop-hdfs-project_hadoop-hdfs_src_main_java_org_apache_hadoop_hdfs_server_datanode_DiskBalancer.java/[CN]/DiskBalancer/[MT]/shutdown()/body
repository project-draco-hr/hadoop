{
  lock.lock();
  try {
    this.isDiskBalancerEnabled=false;
    this.currentResult=Result.NO_PLAN;
    if ((this.future != null) && (!this.future.isDone())) {
      this.currentResult=Result.PLAN_CANCELLED;
      this.blockMover.setExitFlag();
      shutdownExecutor();
    }
  }
  finally {
    lock.unlock();
  }
}
