{
  if (!protocol.equals(RPC.getProtocolName(JournalProtocolPB.class))) {
    throw new IOException("Namenode Serverside implements " + RPC.getProtocolName(JournalProtocolPB.class) + ". The following requested protocol is unknown: "+ protocol);
  }
  return ProtocolSignature.getProtocolSignature(clientMethodsHash,RPC.getProtocolVersion(JournalProtocolPB.class),JournalProtocolPB.class);
}
