{
  List<ApplicationAttemptId> activeApps=scheduler.getAppsInQueue(expiredReservation);
  if (activeApps.isEmpty()) {
    return;
  }
  for (  ApplicationAttemptId app : activeApps) {
    try {
      scheduler.moveApplication(app.getApplicationId(),defReservationQueue);
    }
 catch (    YarnException e) {
      LOG.warn("Encountered unexpected error during migration of application: {} from reservation: {}",app,expiredReservation,e);
    }
  }
}
