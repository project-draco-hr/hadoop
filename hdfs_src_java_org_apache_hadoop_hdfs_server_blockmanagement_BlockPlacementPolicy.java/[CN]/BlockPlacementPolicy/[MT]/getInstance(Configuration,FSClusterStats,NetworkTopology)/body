{
  Class<? extends BlockPlacementPolicy> replicatorClass=conf.getClass("dfs.block.replicator.classname",BlockPlacementPolicyDefault.class,BlockPlacementPolicy.class);
  BlockPlacementPolicy replicator=(BlockPlacementPolicy)ReflectionUtils.newInstance(replicatorClass,conf);
  replicator.initialize(conf,stats,clusterMap);
  return replicator;
}
