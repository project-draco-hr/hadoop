{
  if (SCHEME_NAME.equals(providerName.getScheme())) {
    try {
      return new FailureInjectingJavaKeyStoreProvider((JavaKeyStoreProvider)new JavaKeyStoreProvider.Factory().createProvider(new URI(providerName.toString().replace(SCHEME_NAME,JavaKeyStoreProvider.SCHEME_NAME)),conf));
    }
 catch (    URISyntaxException e) {
      throw new RuntimeException(e);
    }
  }
  return null;
}
