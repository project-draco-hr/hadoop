{
  if (app != null) {
    String trackingUrl=app.getTrackingUrl();
    this.trackingUrlIsNotReady=trackingUrl == null || trackingUrl.isEmpty() || "N/A".equalsIgnoreCase(trackingUrl);
    this.trackingUI=this.trackingUrlIsNotReady ? "UNASSIGNED" : (app.getFinishTime() == 0 ? "ApplicationMaster" : "History");
    if (!trackingUrlIsNotReady) {
      this.trackingUrl=join("http://",trackingUrl);
    }
    this.trackingUrlPretty=trackingUrlIsNotReady ? "UNASSIGNED" : join("http://",trackingUrl);
    this.applicationId=app.getApplicationId();
    this.appIdNum=String.valueOf(app.getApplicationId().getId());
    this.id=app.getApplicationId().toString();
    this.user=app.getUser().toString();
    this.name=app.getName().toString();
    this.queue=app.getQueue().toString();
    this.state=app.getState();
    this.progress=app.getProgress() * 100;
    this.diagnostics=app.getDiagnostics().toString();
    if (diagnostics == null || diagnostics.isEmpty()) {
      this.diagnostics="";
    }
    this.finalStatus=app.getFinalApplicationStatus();
    this.clusterId=ResourceManager.clusterTimeStamp;
    if (hasAccess) {
      this.startedTime=app.getStartTime();
      this.finishedTime=app.getFinishTime();
      this.elapsedTime=Times.elapsed(app.getStartTime(),app.getFinishTime());
      RMAppAttempt attempt=app.getCurrentAppAttempt();
      if (attempt != null) {
        Container masterContainer=attempt.getMasterContainer();
        if (masterContainer != null) {
          this.amContainerLogsExist=true;
          String url=join("http://",masterContainer.getNodeHttpAddress(),"/node","/containerlogs/",ConverterUtils.toString(masterContainer.getId()));
          this.amContainerLogs=url;
          this.amHostHttpAddress=masterContainer.getNodeHttpAddress();
        }
      }
    }
  }
}
