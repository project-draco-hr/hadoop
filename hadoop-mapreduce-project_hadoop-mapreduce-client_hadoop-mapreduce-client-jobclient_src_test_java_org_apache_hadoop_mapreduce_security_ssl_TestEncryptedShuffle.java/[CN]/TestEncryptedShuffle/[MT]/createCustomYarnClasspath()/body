{
  String classpathDir=KeyStoreTestUtil.getClasspathDir(TestEncryptedShuffle.class);
  URL url=Thread.currentThread().getContextClassLoader().getResource("mrapp-generated-classpath");
  File f=new File(url.getPath());
  BufferedReader reader=new BufferedReader(new FileReader(f));
  String cp=reader.readLine();
  cp=cp + ":" + classpathDir;
  f=new File(classpathDir,"mrapp-generated-classpath");
  Writer writer=new FileWriter(f);
  writer.write(cp);
  writer.close();
  new File(classpathDir,"core-site.xml").delete();
}
