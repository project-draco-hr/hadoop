{
  final int numDirDiff=in.readInt();
  if (numDirDiff >= 0) {
    v.visitEnclosingElement(ImageElement.SNAPSHOT_DIR_DIFFS,ImageElement.NUM_SNAPSHOT_DIR_DIFF,numDirDiff);
    for (int i=0; i < numDirDiff; i++) {
      processDirectoryDiff(in,v,currentINodeName);
    }
    v.leaveEnclosingElement();
  }
}
