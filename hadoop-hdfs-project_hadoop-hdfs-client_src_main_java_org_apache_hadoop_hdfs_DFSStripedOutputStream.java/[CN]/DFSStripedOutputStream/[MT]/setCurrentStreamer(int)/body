{
  if (streamer != null) {
    int oldIdx=streamers.indexOf(getCurrentStreamer());
    if (oldIdx >= 0) {
      currentPackets[oldIdx]=currentPacket;
    }
  }
  streamer=getStripedDataStreamer(newIdx);
  currentPacket=currentPackets[newIdx];
  adjustChunkBoundary();
  return getCurrentStreamer();
}
