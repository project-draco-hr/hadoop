{
  if (pauseMonitor == null) {
    pauseMonitor=new JvmPauseMonitor(config);
    pauseMonitor.start();
    metrics.getJvmMetrics().setPauseMonitor(pauseMonitor);
  }
  writeManager.startAsyncDataSerivce();
  try {
    infoServer.start();
  }
 catch (  IOException e) {
    LOG.error("failed to start web server",e);
  }
}
