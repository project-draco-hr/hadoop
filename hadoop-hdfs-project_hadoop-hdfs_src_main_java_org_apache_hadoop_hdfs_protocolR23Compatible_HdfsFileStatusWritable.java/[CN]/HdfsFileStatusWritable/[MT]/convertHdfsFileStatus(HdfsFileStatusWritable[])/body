{
  if (fs == null)   return null;
  final int len=fs.length;
  org.apache.hadoop.hdfs.protocol.HdfsFileStatus[] result=new org.apache.hadoop.hdfs.protocol.HdfsFileStatus[len];
  for (int i=0; i < len; ++i) {
    if (fs[i] instanceof HdfsLocatedFileStatusWritable) {
      result[i]=HdfsLocatedFileStatusWritable.convertLocatedHdfsFileStatus((HdfsLocatedFileStatusWritable)fs[i]);
    }
 else {
      result[i]=convertHdfsFileStatus(fs[i]);
    }
  }
  return result;
}
