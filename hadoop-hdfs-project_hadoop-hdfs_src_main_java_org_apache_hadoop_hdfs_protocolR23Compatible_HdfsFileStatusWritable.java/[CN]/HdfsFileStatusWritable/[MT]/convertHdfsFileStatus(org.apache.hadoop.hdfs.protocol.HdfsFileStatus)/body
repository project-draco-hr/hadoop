{
  if (fs == null)   return null;
  return new HdfsFileStatusWritable(fs.getLen(),fs.isDir(),fs.getReplication(),fs.getBlockSize(),fs.getModificationTime(),fs.getAccessTime(),org.apache.hadoop.hdfs.protocolR23Compatible.FsPermissionWritable.convertPermission(fs.getPermission()),fs.getOwner(),fs.getGroup(),fs.getSymlinkInBytes(),fs.getLocalNameInBytes());
}
