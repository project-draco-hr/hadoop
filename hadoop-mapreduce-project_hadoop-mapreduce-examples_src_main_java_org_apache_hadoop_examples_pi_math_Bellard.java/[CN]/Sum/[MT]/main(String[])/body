{
  final Util.Timer t=new Util.Timer(false);
  computePi(t,0);
  computePi(t,1);
  computePi(t,2);
  computePi(t,3);
  computePi(t,4);
  Util.printBitSkipped(1008);
  computePi(t,1008);
  computePi(t,1012);
  long b=10;
  for (int i=0; i < 7; i++) {
    Util.printBitSkipped(b);
    computePi(t,b - 4);
    computePi(t,b);
    computePi(t,b + 4);
    b*=10;
  }
}
