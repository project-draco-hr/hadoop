{
  Configuration conf=new Configuration();
  conf.set(ProxyUsers.getProxySuperuserGroupConfKey(REAL_USER_NAME),StringUtils.join(",",Arrays.asList(GROUP_NAMES,GROUP_NAMES)));
  conf.set(ProxyUsers.getProxySuperuserIpConfKey(REAL_USER_NAME),PROXY_IP);
  ProxyUsers.refreshSuperUserGroupsConfiguration(conf);
  Collection<String> groupsToBeProxied=ProxyUsers.getProxyGroups().get(ProxyUsers.getProxySuperuserGroupConfKey(REAL_USER_NAME));
  assertEquals(1,groupsToBeProxied.size());
}
