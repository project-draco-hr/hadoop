{
  Configuration conf=new Configuration();
  conf.set(ProxyUsers.getProxySuperuserGroupConfKey(REAL_USER_NAME),StringUtils.join(",",Arrays.asList(GROUP_NAMES)));
  conf.set(ProxyUsers.getProxySuperuserIpConfKey(REAL_USER_NAME),StringUtils.join(",",Arrays.asList(PROXY_IP,PROXY_IP)));
  ProxyUsers.refreshSuperUserGroupsConfiguration(conf);
  Collection<String> hosts=ProxyUsers.getProxyHosts().get(ProxyUsers.getProxySuperuserIpConfKey(REAL_USER_NAME));
  assertEquals(1,hosts.size());
}
