{
  ContainerTokenIdentifier identifier=new ContainerTokenIdentifier(cId,containerVersion,host + ":" + port,user,r,expiryTime,masterKeyId,rmIdentifier,Priority.newInstance(0),0,null,CommonNodeLabelsManager.NO_LABEL,ContainerType.TASK,ExecutionType.GUARANTEED);
  return newContainerToken(BuilderUtils.newNodeId(host,port),password,identifier);
}
