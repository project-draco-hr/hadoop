{
switch (info.getErasureCode()) {
case XOR:
    return new RaidFilter.PreferenceFilter(conf,rsDestinationPath(conf),xorDestinationPath(conf),info,allPolicies,startTime,stats);
case RS:
  return new RaidFilter.TimeBasedFilter(conf,rsDestinationPath(conf),info,allPolicies,startTime,stats);
default :
return null;
}
}
