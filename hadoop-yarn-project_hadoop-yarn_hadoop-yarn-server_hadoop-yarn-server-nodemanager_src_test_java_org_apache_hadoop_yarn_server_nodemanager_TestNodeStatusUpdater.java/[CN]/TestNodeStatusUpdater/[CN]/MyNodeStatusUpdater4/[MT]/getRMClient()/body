{
  if (!triggered) {
    long t=System.currentTimeMillis();
    long duration=t - waitStartTime;
    if (duration <= rmStartIntervalMS || rmNeverStart) {
      throw new YarnRuntimeException("Faking RM start failure as start " + "delay timer has not expired.");
    }
 else {
      triggered=true;
      durationWhenTriggered=duration;
    }
  }
  return resourceTracker;
}
