{
  ApplicationId applicationId=BuilderUtils.newApplicationId(System.currentTimeMillis(),id);
  ApplicationAttemptId applicationAttemptId=BuilderUtils.newApplicationAttemptId(applicationId,id);
  ContainerId contaierId=BuilderUtils.newContainerId(applicationAttemptId,id);
  ContainerStatus containerStatus=BuilderUtils.newContainerStatus(contaierId,containerState,"test_containerStatus: id=" + id + ", containerState: "+ containerState,0);
  return containerStatus;
}
