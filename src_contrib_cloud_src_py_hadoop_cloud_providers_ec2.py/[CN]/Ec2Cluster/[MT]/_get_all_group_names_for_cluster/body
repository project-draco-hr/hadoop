def _get_all_group_names_for_cluster(self):
    all_group_names = self._get_all_group_names()
    r = []
    if (self.name not in all_group_names):
        return r
    for group in all_group_names:
        if re.match(('^%s(-[a-zA-Z0-9_]+)?$' % self.name), group):
            r.append(group)
    return r
