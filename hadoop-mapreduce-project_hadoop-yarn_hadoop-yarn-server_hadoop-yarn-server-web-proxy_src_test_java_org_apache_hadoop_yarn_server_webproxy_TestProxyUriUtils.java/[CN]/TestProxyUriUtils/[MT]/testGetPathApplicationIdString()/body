{
  assertEquals("/proxy/application_6384623_0005",ProxyUriUtils.getPath(new TestAppId(5,6384623l),null));
  assertEquals("/proxy/application_6384623_0005/static/app",ProxyUriUtils.getPath(new TestAppId(5,6384623l),"/static/app"));
  assertEquals("/proxy/application_6384623_0005/",ProxyUriUtils.getPath(new TestAppId(5,6384623l),"/"));
  assertEquals("/proxy/application_6384623_0005/some/path",ProxyUriUtils.getPath(new TestAppId(5,6384623l),"some/path"));
}
