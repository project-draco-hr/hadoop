{
  assertEquals("/proxy/application_6384623_0005/static/app?foo=bar",ProxyUriUtils.getPathAndQuery(new TestAppId(5,6384623l),"/static/app","?foo=bar",false));
  assertEquals("/proxy/application_6384623_0005/static/app?foo=bar&bad=good&proxyapproved=true",ProxyUriUtils.getPathAndQuery(new TestAppId(5,6384623l),"/static/app","foo=bar&bad=good",true));
}
