{
  for (  INode c : created) {
    SnapshotDiffSection.CreatedListEntry.newBuilder().setName(ByteString.copyFrom(c.getLocalNameBytes())).build().writeDelimitedTo(out);
  }
  for (  INodeReference ref : deletedRefs) {
    INodeSection.INodeReference.Builder rb=buildINodeReference(ref);
    rb.build().writeDelimitedTo(out);
  }
}
