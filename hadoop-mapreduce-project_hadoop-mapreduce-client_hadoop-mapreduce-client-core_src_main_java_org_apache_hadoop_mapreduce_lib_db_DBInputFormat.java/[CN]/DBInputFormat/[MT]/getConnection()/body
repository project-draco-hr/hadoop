{
  try {
    if (null == this.connection) {
      this.connection=dbConf.getConnection();
      this.connection.setAutoCommit(false);
      this.connection.setTransactionIsolation(Connection.TRANSACTION_SERIALIZABLE);
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  return connection;
}
