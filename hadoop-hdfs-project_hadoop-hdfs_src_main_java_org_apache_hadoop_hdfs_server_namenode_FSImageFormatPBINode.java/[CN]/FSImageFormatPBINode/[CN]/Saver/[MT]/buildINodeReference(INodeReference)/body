{
  INodeSection.INodeReference.Builder rb=INodeSection.INodeReference.newBuilder().setReferredId(ref.getId());
  if (ref instanceof WithName) {
    rb.setLastSnapshotId(((WithName)ref).getLastSnapshotId()).setName(ByteString.copyFrom(ref.getLocalNameBytes()));
  }
 else   if (ref instanceof DstReference) {
    rb.setDstSnapshotId(((DstReference)ref).getDstSnapshotId());
  }
  return rb;
}
