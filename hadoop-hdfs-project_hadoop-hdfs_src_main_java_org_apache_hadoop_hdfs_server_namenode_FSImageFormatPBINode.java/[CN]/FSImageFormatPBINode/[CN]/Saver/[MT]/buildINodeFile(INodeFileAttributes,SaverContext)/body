{
  INodeSection.INodeFile.Builder b=INodeSection.INodeFile.newBuilder().setAccessTime(file.getAccessTime()).setModificationTime(file.getModificationTime()).setPermission(buildPermissionStatus(file,state.getStringMap())).setPreferredBlockSize(file.getPreferredBlockSize()).setReplication(file.getFileReplication());
  AclFeature f=file.getAclFeature();
  if (f != null) {
    b.setAcl(buildAclEntries(f,state.getStringMap()));
  }
  return b;
}
