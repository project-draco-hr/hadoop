{
  int k=key.get();
  final String kvstr="Unexpected tuple: " + stringify(key,val);
  assertTrue(kvstr,0 == k % (srcs * srcs));
  for (int i=0; i < val.size(); ++i) {
    final int vali=((IntWritable)val.get(i)).get();
    assertTrue(kvstr,(vali - i) * srcs == 10 * k);
  }
  out.collect(key,one);
}
