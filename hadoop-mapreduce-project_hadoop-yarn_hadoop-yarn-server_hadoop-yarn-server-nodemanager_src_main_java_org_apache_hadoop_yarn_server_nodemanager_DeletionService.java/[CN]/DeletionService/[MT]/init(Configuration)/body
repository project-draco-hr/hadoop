{
  if (conf != null) {
    sched=new ScheduledThreadPoolExecutor(conf.getInt(NM_MAX_DELETE_THREADS,DEFAULT_MAX_DELETE_THREADS));
    debugDelay=conf.getInt(DEBUG_DELAY_SEC,0);
  }
 else {
    sched=new ScheduledThreadPoolExecutor(DEFAULT_MAX_DELETE_THREADS);
  }
  sched.setKeepAliveTime(60L,SECONDS);
  super.init(conf);
}
