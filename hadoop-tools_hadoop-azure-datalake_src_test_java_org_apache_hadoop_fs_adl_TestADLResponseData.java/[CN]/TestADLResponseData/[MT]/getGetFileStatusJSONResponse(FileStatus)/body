{
  String str="{\"FileStatus\":{\"length\":" + status.getLen() + ","+ "\"pathSuffix\":\"\",\"type\":\""+ (status.isDirectory() ? "DIRECTORY" : "FILE")+ "\""+ ",\"blockSize\":"+ status.getBlockSize()+ ",\"accessTime\":"+ status.getAccessTime()+ ",\"modificationTime\":"+ status.getModificationTime()+ ""+ ",\"replication\":"+ status.getReplication()+ ",\"permission\":\""+ status.getPermission()+ "\",\"owner\":\""+ status.getOwner()+ "\",\"group\":\""+ status.getGroup()+ "\"}}";
  return str;
}
