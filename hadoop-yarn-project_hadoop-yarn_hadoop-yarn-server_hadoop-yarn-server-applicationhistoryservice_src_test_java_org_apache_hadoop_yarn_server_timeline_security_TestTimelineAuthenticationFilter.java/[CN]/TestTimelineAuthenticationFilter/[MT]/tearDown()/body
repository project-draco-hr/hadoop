{
  if (testMiniKDC != null) {
    testMiniKDC.stop();
  }
  if (testTimelineServer != null) {
    testTimelineServer.stop();
  }
  if (client != null) {
    client.stop();
  }
  if (withSsl) {
    KeyStoreTestUtil.cleanupSSLConfig(keystoresDir,sslConfDir);
    File base=new File(BASEDIR);
    FileUtil.fullyDelete(base);
  }
}
