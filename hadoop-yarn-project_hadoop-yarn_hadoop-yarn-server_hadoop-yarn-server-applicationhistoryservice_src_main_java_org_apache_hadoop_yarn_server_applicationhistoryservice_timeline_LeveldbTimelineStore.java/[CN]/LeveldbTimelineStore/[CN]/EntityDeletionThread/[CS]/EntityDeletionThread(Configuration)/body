{
  ttl=conf.getLong(YarnConfiguration.TIMELINE_SERVICE_TTL_MS,YarnConfiguration.DEFAULT_TIMELINE_SERVICE_TTL_MS);
  ttlInterval=conf.getLong(YarnConfiguration.TIMELINE_SERVICE_LEVELDB_TTL_INTERVAL_MS,YarnConfiguration.DEFAULT_TIMELINE_SERVICE_LEVELDB_TTL_INTERVAL_MS);
  LOG.info("Starting deletion thread with ttl " + ttl + " and cycle "+ "interval "+ ttlInterval);
}
